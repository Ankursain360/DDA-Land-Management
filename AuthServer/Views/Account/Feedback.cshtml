                        @*<i class="ri-menu-fold-line"></i>*@
                        <i class="ri-menu-unfold-line"></i>
                    </div>
                    <div class="logo py-1 me-auto me-md-4 order-md-1">
                        <a href="/" class="d-block">
                            <figure class="m-0">
                                <img src="~/images/header-logo.png" alt="DDA Logo" class="img-fluid">
                            </figure>
                        </a>
                    </div>
                    @*<div class="dropdown noti text-white my-2  me-3 order-md-3">
                    <a href="~/docs/Acquiredlandinformation.pdf" target="_blank" role="button" title="User Manual" class='p-2 d-block text-white no-dec'>
                    <i class="ri-folder-user-fill" style="font-size:20px"></i>
                    </a>
                    </div>*@
                    <div class="text-white my-2  me-3 order-md-3">
                        @*  <a href="https://ddalmis.org.in/" role="button" id="notification" title="Go back to modules!" class='p-2 d-block text-white no-dec'>
                                <i class="ri-home-3-fill" style="font-size:20px"></i>
                            </a> *@
                        <ul class="nav nav-tabs mb-3" id="navigationTabs" role="tablist">
                            <li class="nav-item" role="presentation">
                                <a class="nav-link text-white"
                                   href="https://auth.managemybusinessess.com/Account/Login"
                                   aria-label="Home">
                                    <i class="ri-home-3-fill" style="font-size:20px"></i>
                                </a>
                                @* <a class="nav-link text-white"
                                   href="https://ddalmis.org.in/"
                                   aria-label="Home">
                                    <i class="ri-home-3-fill" style="font-size:20px"></i>
                                </a> *@
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </header>
    </div>

    <div class="pt-5 mt-5">
    </div>

    @* <aside class="left-menu small slow" id="leftMenu">
    <ul class="accordion list-unstyled mb-0">

    </ul>
    </aside>*@

    <partial name="_CookieConsentPartial" />
    <section class="slow bg-gray py-5">
        <div class="container d-flex justify-content-center align-items-center">
            <form method="post" asp-action="Feedback" class="card p-4 shadow-sm bg-white border-0 responsive-form" enctype="multipart/form-data" autocomplete="off">


                <partial name="_ValidationSummary" />

                <h2 class="text-center text-primary mb-3">Feedback</h2>
                <p class="text-center text-muted mb-4">We value your input. Please fill out the form below.</p>

                <div class="mb-3">
                    <label asp-for="Name" class="form-label">Name <span class="text-danger">*</span></label>
                    <div class="input-group">
                        <span class="input-group-text"><i class="mdi mdi-account-outline"></i></span>
                        <input asp-for="Name" class="form-control" placeholder="Enter your name" />
                    </div>
                    <span asp-validation-for="Name" class="text-danger small"></span>
                </div>

                <div class="mb-3">
                    <label asp-for="Address" class="form-label">Address<span class="text-danger">*</span></label>
                    <div class="input-group">
                        <span class="input-group-text"><i class="mdi mdi-home-variant-outline"></i></span>
                        <textarea asp-for="Address" class="form-control" rows="2" placeholder="Enter your address"></textarea>
                    </div>
                    <span asp-validation-for="Address" class="text-danger small"></span>
                </div>

                <div class="mb-3">
                    <label asp-for="Email" class="form-label">Email <span class="text-danger">*</span></label>
                    <div class="input-group">
                        <span class="input-group-text"><i class="mdi mdi-email-outline"></i></span>
                        <input asp-for="Email" type="email" class="form-control" placeholder="Enter your email" />
                    </div>
                    <span asp-validation-for="Email" class="text-danger small"></span>
                </div>

                <div class="mb-3">
                    <label asp-for="Feedback" class="form-label">Feedback <span class="text-danger">*</span></label>
                    <div class="input-group">
                        <span class="input-group-text"><i class="mdi mdi-chat-processing-outline"></i></span>
                        <textarea asp-for="Feedback" class="form-control" rows="3" placeholder="Share your feedback"></textarea>
                    </div>
                    <span asp-validation-for="Feedback" class="text-danger small"></span>
                </div>

                <div class="text-center mt-4">
                    <button type="submit" class="btn btn-primary px-4">Submit</button>
                    @Html.ActionLink("Back", "Login", "Account", null, new { @class = "btn btn-outline-secondary ms-2" })
                </div>
            </form>

        </div>
    </section>

    <br />
    <br />
    <br />
    <footer class="footers slow">
        <div class="container-fluid">
            <div class="text-center mt-2">
                <a href="#" id="digitalindiaLink" class="m-2">
                    <img src="~/images/logos/digital_india.png" alt="Digital India" height="40" />
                </a>
                <a href="#" id="data_Gov_Link" class="m-2">
                    <img src="~/images/logos/data_gov_in.png" alt="data.gov.in" height="40" />
                </a>
                <a href="#" id="makeinindiaLink" class="m-2">
                    <img src="~/images/logos/make_in_india.png" alt="Make in India" height="40" />
                </a>
                <a href="#" id="india_Gov_Link" class="m-2">
                    <img src="~/images/logos/india_gov_in.png" alt="india.gov.in" height="40" />
                </a>
             @*    <a href="#" id="my_gov_Link" class="m-2">
                    <img src="~/images/logos/mygov_logo.jpg" alt="MyGov" height="40" />
                </a> *@
            </div>
            <p class="my-0 py-2 text-center">
                <a asp-action="Login" asp-controller="Account">Home</a>               
                | <a asp-action="copyRight" asp-controller="Account">Copyright Policy</a>
                | <a asp-action="CMAP" asp-controller="Account">CMAP</a>
                | <a asp-action="ContentArchivalCAPPolicy" asp-controller="Account">Content Archival Policy (CAP)</a>
                | <a asp-action="ContentReviewPolicy" asp-controller="Account">Content Review Policy (CRP)</a>
                | <a asp-action="HyperlinkPolicy" asp-controller="Account">Hyperlink Policy</a>
                | <a asp-action="PrivacyPolicy" asp-controller="Account">Privacy Policy</a>
                | <a asp-action="TermsandConditions" asp-controller="Account">Terms & Conditions</a>
                | <a asp-action="WebsiteMonitoringPlan" asp-controller="Account">Website Monitoring Plan</a>
                | <a asp-action="ContingencyManagementPlan" asp-controller="Account">Contingency Management Plan</a>
                | <a asp-action="SecurityPolicy" asp-controller="Account">Security Policy</a>                
                | <a asp-action="Contactus" asp-controller="Account">Contact Us</a>
                | <a asp-action="Help" asp-controller="Account">Help</a>
                | <a asp-action="Disclaimer" asp-controller="Account">Disclaimer</a>
                | <a asp-action="AboutUs" asp-controller="Account">About Us</a>
                | <a asp-action="WebInformationManager" asp-controller="Account">Web Information Manager</a>
                | <a asp-action="Feedback" asp-controller="Account">Feedback</a>
                | <a asp-action="Sitemap" asp-controller="Account">Site map</a><br>
                &nbsp; &copy; Content Owned by DDA, Government of India. &nbsp;&nbsp;
