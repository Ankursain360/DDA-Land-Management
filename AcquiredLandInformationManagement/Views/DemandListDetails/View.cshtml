@model Libraries.Model.Entity.Demandlistdetails

@await Component.InvokeAsync("PageHeader", new { pageName = "Demand List Details" })
<br />
<br />
<form asp-action="Edit" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data" autocomplete="off">
    <h3 class="mb-3 d-flex">
        View Details
        <span style="margin-left:800px"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span>
    </h3>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input type="hidden" asp-for="Id" />
    <div id="allDetails">
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="DemandListNo" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="DemandListNo" class="text-danger"></span>
                    <label class="floating-label">Demand List No.<span style="color:red">*</span></label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="Enmsno" type="text" disabled class="floating-label-field floating-label-field--s3 onlynumbers">
                    <span asp-validation-for="Enmsno" class="text-danger"></span>
                    <label class="floating-label">ENM Sno.</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="form-group col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input type="hidden" asp-for="ENMDocumentName" />
                    @{
                        @if (Model.ENMDocumentName != null)
                        {
                            <input disabled type="file" asp-for="ENMDocumentIFormFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg">
                            <label class="badge badge-primary px-2">(View File)</label>
                        }
                        else
                        {
                            <input disabled type="file" asp-for="ENMDocumentIFormFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg">
                        }
                    }
                    <span class="d-block text-danger field-validation-valid" asp-validation-for="ENMDocumentName"></span>
                    <label class="floating-label">Upload ENM Document</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="LacfileNo" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="LacfileNo" class="text-danger"></span>
                    <label class="floating-label">LAC File No.</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="LbrefDate" type="date" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="LbrefDate" class="text-danger"></span>
                    <label class="floating-label">L&B Ref. Date</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="Lbno" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="Lbno" class="text-danger"></span>
                    <label class="floating-label">L&B No.</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="Rfano" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="Rfano" class="text-danger"></span>
                    <label class="floating-label">RFA No.</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="Lacno" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="Lacno" class="text-danger"></span>
                    <label class="floating-label">LAC No.</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="DdafileNo" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="DdafileNo" class="text-danger"></span>
                    <label class="floating-label">DDA File No.</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="NotificationDate" type="date" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="NotificationDate" class="text-danger"></span>
                    <label class="floating-label">Date of Notification(U/S-6)</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="AwardNo" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="AwardNo" class="text-danger"></span>
                    <label class="floating-label">Award No.</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="AwardDate" type="date" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="AwardDate" class="text-danger"></span>
                    <label class="floating-label">Award Date</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="VillageId" asp-items="@(new SelectList(Model.VillageList, "Id","Name"))" onchange="return GetKhasraList(this.options[this.selectedIndex].value);">
                        <option selected="selected" disabled="disabled" value="">--Select-- </option>
                    </select>
                    <span asp-validation-for="VillageId" class="text-danger"></span>
                    <label asp-for="VillageId" class="floating-label">Village<span style="color:red">*</span></label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="KhasraNoId" asp-items="@(new SelectList(Model.KhasraNoList, "Id","Name"))">
                        <option selected="selected" disabled="disabled" value="">--Select-- </option>
                    </select>
                    <span asp-validation-for="KhasraNoId" class="text-danger"></span>
                    <label asp-for="KhasraNoId" class="floating-label">Khasra No.<span style="color:red">*</span></label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <label>Payable/Apealable</label>
                    </div>
                    <input type="hidden" asp-for="PayableAppealable" value="Payable" />
                    <div class="col-md-4">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="PayableAppealable" name="grpPayableApealable" value="Payable" checked>
                            <label class="form-check-label">Payable </label>

                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="PayableAppealable" name="grpPayableApealable" value="Apealable">
                            <label class="form-check-label">Apealable </label>
                        </div>
                    </div>
                </div>
                <span asp-validation-for="PayableAppealable" class="text-danger"></span>
            </div>
            <div class="col-md-6">
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <label>Balance Interest Case</label>
                    </div>
                    <input type="hidden" asp-for="BalanceInterestCase" value="Yes" />
                    <div class="col-md-4">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="BalanceInterestCase" name="grpBalanceInterestCase" value="Yes" checked>
                            <label class="form-check-label">Yes </label>

                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="BalanceInterestCase" name="grpBalanceInterestCase" value="No">
                            <label class="form-check-label">No </label>
                        </div>
                    </div>
                </div>
                <span asp-validation-for="BalanceInterestCase" class="text-danger"></span>
            </div>
        </div>

        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <label>Case Involves To Which Court</label>
                    </div>
                    <input type="hidden" asp-for="CourtInvolves" value="A.D.J" />
                    <div class="col-md-2">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="CourtInvolves" id="rbtadj" name="grpCourtInvolves" value="A.D.J" checked>
                            <label class="form-check-label">A.D.J </label>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="CourtInvolves" id="rbthc" name="grpCourtInvolves" value="H.C">
                            <label class="form-check-label">H.C </label>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" disabled asp-for="CourtInvolves" id="rbtsc" name="grpCourtInvolves" value="S.C">
                            <label class="form-check-label">S.C </label>
                        </div>
                    </div>
                </div>
                <span asp-validation-for="CourtInvolves" class="text-danger"></span>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="EnhancedRatePerBigha" type="text" disabled class="floating-label-field floating-label-field--s3 numbers">
                    <span asp-validation-for="EnhancedRatePerBigha" class="text-danger"></span>
                    <label class="floating-label">Enhanced Rate(per Bigha)</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="ExistingRatePerBigha" type="text" disabled class="floating-label-field floating-label-field--s3 numbers">
                    <span asp-validation-for="ExistingRatePerBigha" class="text-danger"></span>
                    <label class="floating-label">Existing Rate(per Bigha)</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="ApealableAmt" type="text" disabled class="floating-label-field floating-label-field--s3 numbers">
                    <span asp-validation-for="ApealableAmt" class="text-danger"></span>
                    <label class="floating-label">Apealable Amount(Rs.)</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="PayableAmt" type="text" disabled class="floating-label-field floating-label-field--s3 numbers">
                    <span asp-validation-for="PayableAmt" class="text-danger"></span>
                    <label class="floating-label">Payable Amount(Rs.)</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="TotalAmount" type="text" disabled class="floating-label-field floating-label-field--s3 numbers">
                    <span asp-validation-for="TotalAmount" class="text-danger"></span>
                    <label class="floating-label">Total Amount(Rs.)</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="PartyName" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="PartyName" class="text-danger"></span>
                    <label class="floating-label">Party Name</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="Slpno" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="Slpno" class="text-danger"></span>
                    <label class="floating-label">SLP No.</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="ReasonForNonPay" type="text" disabled class="floating-label-field floating-label-field--s3 ">
                    <span asp-validation-for="ReasonForNonPay" class="text-danger"></span>
                    <label class="floating-label">Reason of Non Payment</label>
                </div>
            </div>
            <div class=" col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="IsActive">
                        <option value="1">Active</option>
                        <option value="0">Inactive</option>
                    </select>
                    <span asp-validation-for="IsActive" class="text-danger"></span>
                    <label asp-for="IsActive" class="floating-label">Status <span style="color:red">*</span></label>
                </div>
            </div>
        </div>
        @foreach (var item in Model.Appealdetail)
        {
            <div class="row mt-md-3">
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.AppealNo" placeholder="Appeal No" type="text"
                               class="floating-label-field floating-label-field--s3" limit="200" />

                        <span asp-validation-for="@item.AppealNo" class="text-danger"></span>

                        <label class="floating-label">Appeal No<span style="color:red">*</span> </label>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="row">
                        <div class="col-md-3">
                            <label>
                                Appeal By Department:<span style="color:red">*</span>
                            </label>
                        </div>
                        <div class="col-md-3">
                            <input type="hidden" asp-for="AppealByDept" />

                            <div class="form-check theme">
                                <input class="form-check-input" type="radio" asp-for="@item.AppealByDept" name="villageradio" id="rUOI" value="UOI" checked="checked">
                                <label class="form-check-label" for="radio">
                                    UOI
                                </label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-check theme">
                                <input class="form-check-input" type="radio" asp-for="@item.AppealByDept" name="villageradio" id="rDDA" value="DDA">
                                <label class="form-check-label" for="radio">
                                    DDA
                                </label>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-check theme">
                                <input class="form-check-input" asp-for="@item.AppealByDept" type="radio" name="villageradio" id="OtherDDA" value="Department">
                                <label class="form-check-label" for="radio">
                                    Other
                                </label>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div class="row mt-md-3" id="Departmentdiv" style="display:none">
                <div class="col-md-6 ">
                    <input asp-for="@item.Department" placeholder="Department" type="text"
                           class="floating-label-field floating-label-field--s3" />

                    <span asp-validation-for="@item.Department" class="text-danger"></span>

                    <label class="floating-label"> @item.Department </label>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.DateOfAppeal" placeholder="Date Of Appeal" type="Date"
                               class="floating-label-field floating-label-field--s3" />

                        <span asp-validation-for="@item.DateOfAppeal" class="text-danger"></span>

                        <label class="floating-label">Date Of Appeal </label>
                    </div>
                </div>
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="PanelLawer" placeholder="Panel Lawyer" type="text"
                               class="floating-label-field floating-label-field--s3" limit="500" />

                        <span asp-validation-for="PanelLawer" class="text-danger"></span>

                        <label class="floating-label">Panel Lawyer </label>
                    </div>
                </div>
            </div>}
        <h3 class="mb-3 d-flex">
            Payment Details

        </h3>
        @foreach (var item in Model.Paymentdetail)
        {
            <div class="row mt-md-3">
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.AmountPaid" placeholder="Amount Paid" type="text"
                               class="floating-label-field floating-label-field--s3" limit="18" />

                        <span asp-validation-for="@item.AmountPaid" class="text-danger"></span>

                        <label class="floating-label">Amount Paid(Rs.) <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.ChequeDate" placeholder="Date Of Appeal" type="Date"
                               class="floating-label-field floating-label-field--s3" />

                        <span asp-validation-for="@item.ChequeDate" class="text-danger"></span>

                        <label class="floating-label">Cheque Date <span style="color:red">*</span></label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.ChequeNo" type="text"
                               class="floating-label-field floating-label-field--s3" limit="50" />

                        <span asp-validation-for="@item.ChequeNo" class="text-danger"></span>

                        <label class="floating-label">Cheque/RTGS/NEFT/IMPS No<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.BankName" placeholder="Bank Name" type="text"
                               class="floating-label-field floating-label-field--s3" limit="100" />

                        <span asp-validation-for="@item.BankName" class="text-danger"></span>

                        <label class="floating-label">Bank Name <span style="color:red">*</span></label>
                    </div>
                </div>

            </div>
            <div class="row mt-md-3">
                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="@item.VoucherNo" placeholder="Cheque No" type="text"
                               class="floating-label-field floating-label-field--s3" limit="100" />

                        <span asp-validation-for="@item.VoucherNo" class="text-danger"></span>

                        <label class="floating-label">Voucher No<span style="color:red">*</span> </label>
                    </div>
                </div>

                <div class="col-md-6 ">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="PercentPaid" placeholder="Panel Lawyer" type="text"
                               class="floating-label-field floating-label-field--s3" limit="18" />

                        <span asp-validation-for="PercentPaid" class="text-danger"></span>

                        <label class="floating-label">Percent Paid<span style="color:red">*</span> </label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="form-group col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="@item.PaymentProofDocumentName" />
                        @{
                            @if (@item.PaymentProofDocumentName != null)
                            {
                                <input type="file" asp-for="@item.PaymentProofDocumentIFormFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg">
                                <a class="badge badge-primary px-2" asp-action="ViewPaymentProofDocument" asp-route-id="@item.Id" target="_blank"> (View File)</a>
                            }
                            else
                            {
                                <input type="file" asp-for="@item.PaymentProofDocumentIFormFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg">
                            }
                        }
                        <span class="d-block text-danger field-validation-valid" asp-validation-for="@item.PaymentProofDocumentName"></span>
                        <label class="floating-label">Upload Copy of Payment Proof</label>
                    </div>
                </div>
            </div>
        }
        <div class="row mt-md-3">
            <div class="col-md-12">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <textarea asp-for="Remarks" type="text" disabled class="floating-label-field floating-label-field--s3 "></textarea>
                    <span asp-validation-for="Remarks" class="text-danger"></span>
                    <label class="floating-label">Remarks</label>
                </div>
            </div>
        </div>
    </div>
    <div class="row mt-md-3">
        <div class=" col-md-12">
            @Html.ActionLink("Back", "Index", "DemandListDetails", null, new { @class = "btn btn-theme " })

        </div>
    </div>

</form>




<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/demandlistdetailsedit.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/demandlistdetailsedit.min.js" asp-append-version="true"></script>
</environment>

