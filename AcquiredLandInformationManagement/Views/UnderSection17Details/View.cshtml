
@model Libraries.Model.Entity.Undersection17





<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Notification U/S-17 Details</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="View">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Notification No</label>
                        <select class="form-control dda-dropdown" asp-for="LandNotificationId" asp-items="@(new SelectList(Model.LandNotificationList, "Id","Name"))">
                            <option>Select</option>
                        </select>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm"> Notification Date</label>
                        <input asp-for="NotificationDate" type="date" class="form-control" />
                    </div>
                </div>

                <div id="Divclsification">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Notification Type</h6>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Notification U/S-6</label>
                            <select class="form-control dda-dropdown" asp-for="UnderSection6Id" asp-items="@(new SelectList(Model.Undersection6List, "Id","UnderSectionNotificationNumber"))">
                                <option>Select</option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm"> Status</label>
                            <select asp-for="IsActive" class="form-control dda-dropdown">
                                <option value="1">Active</option>
                                <option value="0">InActive</option>
                            </select>
                            <span asp-validation-for="IsActive" class="text-danger"></span>
                        </div>
                    </div>
                </div>











                
                <div class="form-row">
                    <div class=" form-group  col-md-12">
                        @Html.ActionLink("Back", "Index", "UnderSection17Details", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>
    <partial name="../Shared/_ValidationScriptsPartial.cshtml" />

