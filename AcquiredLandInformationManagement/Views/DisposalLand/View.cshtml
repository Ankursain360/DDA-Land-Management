@model Libraries.Model.Entity.Disposalland
@{
    ViewData["Title"] = "View";
}
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Disposal Land Master  </h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Edit">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="Id" />

                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Village Name</label>
                        <select asp-for="VillageId" asp-items="@(new SelectList(Model.VillageList, "Id","Name"))"
                                class="form-control dda-dropdown">
                            <option value="0">---Select---</option>

                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Khasra No.</label>
                        <select asp-for="KhasraId" asp-items="@(new SelectList(Model.KhasraList, "Id","Name"))"
                                class="form-control dda-dropdown">
                            <option value="0">---Select---</option>

                        </select>
                    </div>
                </div>
                <div id="divdisposaldetails">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Disposal Details</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Transfer to Which Dept </label>
                            <input asp-for="TransferToWhichDept" required type="text" class="form-control">
                        </div>

                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Area Disposed   </label>
                            <input asp-for="AreaDisposed" required type="text" class="form-control">

                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Date of Disposed   </label>
                            <input asp-for="DateOfDisposed" required type="date" class="form-control">
                        </div>
                    </div>
                </div>
                <div id="divlanddetails">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Land  Details</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Transfer To </label>
                            <input asp-for="TransferTo" required type="text" class="form-control">
                        </div>

                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Transfer By  </label>
                            <input asp-for="TransferBy" required type="text" class="form-control">

                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Proposal Name</label>
                            <select asp-for="UtilizationtypeId" asp-items="@(new SelectList(Model.UtilizationtypeList, "Id","Name"))"
                                    class="form-control dda-dropdown">
                                <option value="0">---Select---</option>

                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">File No/Ref No   </label>
                            <input asp-for="FileNoRefNo" required type="text" class="form-control">
                        </div>


                    </div>
                </div>



                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Status</label>
                        <select asp-for="IsActive" class="form-control dda-dropdown">
                            <option value="1">Active</option>
                            <option value="0">Inactive</option>
                        </select>
                        <span asp-validation-for="IsActive" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-12">
                        <label class="control-label lb-sm">Remarks</label>
                        <textarea asp-for="Remarks" type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                    </div>
                </div>


                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        @Html.ActionLink("Back", "Index", "DisposalLand", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>

