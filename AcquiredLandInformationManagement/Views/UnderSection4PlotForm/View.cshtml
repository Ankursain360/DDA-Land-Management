@model Libraries.Model.Entity.Undersection4plot
@await Component.InvokeAsync("PageHeader", new { pageName = "Notification U/S-4 Plot Form" })




<div class="pt-3">
    <form asp-action="Edit" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <h3 class="mb-3 d-flex">View Details</h3>

        <div class="row">
            <div class="col-md-12">
                <div class="floating-label-wrap">

                    <select class="floating-label-field floating-label-field--s3" asp-for="UnderSection4Id" asp-items="@(new SelectList(Model.NotificationList, "Id","Number"))" disabled>
                        <option>Select</option>
                    </select>
                    <span asp-validation-for="UnderSection4Id" class="text-danger"></span>
                    <label class="floating-label">Notification No(U/S-4)</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">

                    <select class="floating-label-field floating-label-field--s3" asp-for="VillageId" asp-items="@(new SelectList(Model.VillageList, "Id","Name"))" disabled>
                        <option>Select</option>
                    </select>
                    <span asp-validation-for="VillageId" class="text-danger"></span>
                    <label class="floating-label">Village Name</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" asp-for="KhasraId" asp-items="@(new SelectList(Model.KhasraList, "Id","Name"))" disabled>
                        <option>Select</option>
                    </select>
                    <span asp-validation-for="KhasraId" class="text-danger"></span>
                    <label class="floating-label">Khasra No</label>
                </div>
            </div>
        </div>
        <div class="pt-3">
            <h3 class="mb-3 d-flex">Area</h3>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">



                    <input type="text" asp-for="Bigha" class="floating-label-field floating-label-field--s3" disabled/>
                    <label class="floating-label">Bigha</label>

                </div>
            </div>

            <div class="ol-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">



                    <input type="text" asp-for="Biswa" class="floating-label-field floating-label-field--s3" placeholder="Biswa" disabled />
                    <label class="floating-label">Biswa</label>
                </div>
            </div>
            <div class="col-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">


                    <input type="text" asp-for="Biswanshi" class="floating-label-field floating-label-field--s3" placeholder="Biswanshi" disabled />
                    <label class="floating-label">Biswanshi</label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">


            <div class="col-md-12">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <textarea type="text" asp-for="Remarks" class="floating-label-field floating-label-field--s3" placeholder="Remarks" disabled></textarea>
                    <label class="floating-label">Remarks</label>
                </div>
            </div>
        </div>



        <div class="row mt-md-3">
            <div class=" col-md-12">

                @Html.ActionLink("Back", "Index", "UnderSection4PlotForm", null, new { @class = "btn btn-theme " })
            </div>
        </div>



    </form>
</div>




<environment include="Development">
    <script src="~/js/UnderSection4PlotForm.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/UnderSection4PlotForm.min.js" asp-append-version="true"></script>
</environment>
