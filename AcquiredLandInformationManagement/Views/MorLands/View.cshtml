
@model Libraries.Model.Entity.Morland
@await Component.InvokeAsync("PageHeader", new { pageName = "Mor Lands" })




<div class="pt-3">
    <form asp-action="View" disabled="disabled" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="Id" />
        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap">


                    <select class="floating-label-field floating-label-field--s3" asp-for="LandNotificationId" asp-items="@(new SelectList(Model.LandNotificationList, "Id","NotificationNumber"))">
                        <option disabled="disabled">Select</option>
                    </select>
                    <span asp-validation-for="LandNotificationId" class="text-danger"></span>
                    <label class="floating-label">Notification No.</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="NotificationDate" type="date" class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="NotificationDate" class="text-danger"></span>
                    <label class="floating-label">Notification Date</label>

                </div>
            </div>


        </div>

        <div class="row pt-3">
            <div class="form-group col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input type="hidden" asp-for="GOINotificationDocumentName" />
                    @{
                        @if (Model.GOINotificationDocumentName != null)
                        {
                            <input disabled type="file" asp-for="GOINotificationDocumentIFormFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg">
                            <label class="badge badge-primary px-2">(View File)</label>
                        }
                        else
                        {
                            <input disabled type="file" asp-for="GOINotificationDocumentIFormFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg">
                        }
                    }
                    <span class="d-block text-danger field-validation-valid" asp-validation-for="GOINotificationDocumentName"></span>
                    <label class="floating-label">Upload GOI Notification(MOR)</label>
                </div>
            </div>

            <div class="col-md-6">
                <div class="floating-label-wrap">

                    <input asp-for="SerialNumber" placeholder="Serial No" class="floating-label-field floating-label-field--s3" />

                    <span asp-validation-for="SerialNumber" class="text-danger"></span>
                    <label class="floating-label" asp-for="SerialNumber">Serial No</label>

                </div>
            </div>
        </div>


        <div class="row pt-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="PropertySiteNo" class="floating-label-field floating-label-field--s3" disabled />
                    <span asp-validation-for="PropertySiteNo" class="text-danger"></span>
                    <label class="floating-label">Property/Site/KhasraNo</label>

                </div>


            </div>

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="Name" class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                    <label class="floating-label">Location Name(Village)</label>

                </div>
            </div>



        </div>
        <div class="row pt-3">

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input placeholder="Site Description" asp-for="SiteDescription" class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="SiteDescription" class="text-danger"></span>
                    <label class="floating-label">Site Description</label>

                </div>
            </div>

        </div>
        <div id="Divclsification">

            <h2 class="my-3 sub-heading">Area</h2>
            <div class="row pt-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">

                        <input asp-for="Area" placeholder="Bigha" class="floating-label-field floating-label-field--s3" />
                        <span asp-validation-for="Area" class="text-danger"></span>

                        <label class="floating-label">(Sq. Yards)</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">

                        <input asp-for="OccupiedBy" placeholder="Occupied By" class="floating-label-field floating-label-field--s3" />
                        <span asp-validation-for="OccupiedBy" class="text-danger"></span>
                        <label class="floating-label">Occupied By</label>

                    </div>
                </div>
            </div>

        </div>

        <div id="Divclsification">
            <h2 class="my-3 sub-heading">
                <label class="control-label lb-sm" asp-for="StatusOfLand">   Status of Land</label>
            </h2>

            <div class="row pt-3">

                <div class="col-md-6">
                    <input type="hidden" asp-for="StatusOfLand" id="StatusOfLand" value="Green" />
                    <span asp-validation-for="StatusOfLand" class="text-danger"></span>

                    <div class="form-row">

                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="StatusOfLand" type="radio" class="floating-label-field  " placeholder="Name" name="grpLand" value="Allotted" id="A">Allotted
                            </label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="StatusOfLand" type="radio" class="floating-label-field " placeholder="Name" name="grpLand" value="Vacant" id="V">Vacant
                            </label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="StatusOfLand" type="radio" class="floating-label-field floating-label-field--s3 " placeholder="Name" name="grpLand" value="Buildup/Encroached" id="E">Buildup/Encroached
                            </label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="StatusOfLand" type="radio" class="floating-label-field floating-label-field--s3 " placeholder="Name" name="grpLand" value="Green" id="G">Green
                            </label>
                        </div>

                    </div>
                </div>

            </div>
        </div>
        <div id="Divclsification">
            <h2 class="my-3 sub-heading">
                Developed
                <label class="floating-label" asp-for="Developed">Developed</label>

            </h2>

            <div class="row pt-3">

                <div class="col-md-6">
                    <input type="hidden" asp-for="Developed" id="Developed" value="Yes" />


                    <span asp-validation-for="Developed" class="text-danger"></span>

                    <div class="form-row">

                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="Developed" type="radio" class="floating-label-field floating-label-field--s3" placeholder="Name" name="grpDevloped" value="Yes" id="Yes">Yes
                            </label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="Developed" type="radio" class="floating-label-field floating-label-field--s3" placeholder="Name" name="grpDevloped" value="No" id="No">No
                            </label>
                        </div>


                    </div>
                </div>

            </div>
        </div>


        <div id="Divclsification">
            <h2 class="my-3 sub-heading">
                Land Type
                <label class="floating-label" asp-for="LandType">Land Type</label>

            </h2>

            <div class="row pt-3">

                <div class="col-md-6">
                    <input type="hidden" asp-for="LandType" id="LandType" value="Surveyed" />

                    <span asp-validation-for="LandType" class="text-danger"></span>
                    <div class="form-row">

                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="LandType" type="radio" class="floating-label-field  floating-label-field--s3 " placeholder="Name" name="grpLandType" value="Surveyed" id="Surveied">Surveyed
                            </label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="LandType" type="radio" class="floating-label-field floating-label-field--s3 " placeholder="Name" name="grpLandType" value="Retained" id="Retained">Retained
                            </label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="radio-inline  ">
                                <input asp-for="LandType" type="radio" class="floating-label-field floating-label-field--s3 " placeholder="Name" name="grpLandType" value="UnSurveyed" id="UnSurveied">Un-Surveyed
                            </label>
                        </div>


                    </div>
                </div>

            </div>
        </div>

        <div class="row pt-3">




            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">


                </div>
            </div>


        </div>
        <div class="row pt-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <select asp-for="IsActive" class="floating-label-field floating-label-field--s3">
                        <option value="1">Active</option>
                        <option value="0">InActive</option>
                    </select>
                    <span asp-validation-for="IsActive" class="text-danger"></span>
                    <label class="floating-label"> Status</label>
                </div>
            </div>
        </div>

        <div class="row pt-3">
            <div class="col-md-12">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <textarea asp-for="Remarks" placeholder="Remarks" class="floating-label-field floating-label-field--s3"></textarea>
                    <span asp-validation-for="Remarks" class="text-danger"></span>
                    <label class="floating-label">Remarks</label>
                </div>
            </div>
        </div>

        <div class="row mt-md-3">
            <div class=" col-md-12">


                @Html.ActionLink("Back", "Index", "MorLands", null, new { @class = "btn btn-theme " })




            </div>
        </div>
    </form>
</div>
<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/morlandcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/morlandcreate.min.js" asp-append-version="true"></script>
</environment>

