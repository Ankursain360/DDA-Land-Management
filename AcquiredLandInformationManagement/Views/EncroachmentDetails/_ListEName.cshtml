@*@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.EncrocherPeople>
    @{int rowNo = 0; }*@
@model IEnumerable<Dto.Search.EncrochpeopleListDataDto>
@{int rowNo = 0; }
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>
            <th>Sr.No</th>
            <th>Encrocher Name</th>
            <th>Address</th>
            <th>FileNo</th>
        </tr>
    </thead>
    <tbody class="tbody">
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
        <tr>
            <td>@(rowNo += 1)</td>
            <td>@item.Name </td>
            <td>@item.Address</td>
            <td>@item.FileNo</td>

        </tr>
            }}

        </tbody>
</table>
