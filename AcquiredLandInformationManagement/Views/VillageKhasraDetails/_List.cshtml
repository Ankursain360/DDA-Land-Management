
@model IEnumerable<Dto.Search.AcquiredLandVillageListSearchDto>
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>


            <th>Village Name</th>
            <th>Khasra No</th>
            <th>Area (Bhigha-Biswa-Biswana)</th>
            <th>Notification(s) US-4 </th>
            <th>Notification(s) US-6 </th>
            <th>Notification(s) US-17 </th>
            <th>Notification(s) US-22 </th>
            <th>Award(s)</th>
            <th>Date of Possesion</th>
        </tr>
    </thead>
    <tbody>
        @{

            @if (Model != null && Model.Count() > 0)
            {
                @foreach (var item in Model)
                {
                    {
                        <tr>

                            <td>@item.VillageName</td>
                            <td>@item.KhasraName</td>
                            <td>@item.Bigha-@item.Biswa-@item.Biswanshi</td>
                            <td>@(item.un4Number == null ? "-" : item.un4Number) @(item.um4Date == null ? "-" : item.um4Date)</td>
                            <td>@(item.un6Number == null ? "-" : item.un6Number) @(item.um6Date == null ? "-" : item.um6Date)</td>
                            <td>@(item.un17Number == null ? "-" : item.un17Number)</td>
                            <td>@(item.un22Number == null ? "-" : item.un22Number)  @(item.un22Date == null ? "-" : item.un22Date)</td>
                            <td>@(item.AwardNumber == null ? "-" : item.AwardNumber)  @(item.AwardDate == null ? "-" : item.AwardDate)</td>
                            <td>@(item.PossDate == null ? "-" : item.PossDate)</td>
                        </tr>
                    }
                }
            }
            else
            {
                <tr>
                    <th colspan="14" style="text-align:center"> No Record Available! </th>
                </tr>
            }


        }
    </tbody>
</table>


