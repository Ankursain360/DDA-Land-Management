                                <option selected="selected" disabled="disabled" value="">--Select-- </option>
                            </select>
                            <span asp-validation-for="ZoneId" class="text-danger"></span>
                            <label class="floating-label" asp-for="ZoneId">Zone Name<span style="color:red">*&nbsp;</span></label>

                        </div>
                    </div>
                    <div class=" col-md-6">
                        <div class="floating-label-wrap">
                            <select disabled class="floating-label-field floating-label-field--s3" asp-for="VillageId"
