                <div class="pt-3">
                    <h3 class="mb-3 d-flex">Other Details</h3>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <textarea type="text" asp-for="TypeOfStructureOnGramLand" class="floating-label-field floating-label-field--s3" placeholder="Remarks"></textarea>

                            <label class="floating-label">Details/Type of Structure on Gram Sabha Land</label>
                        </div>
                    </div>


                    <div class=" col-md-6">
                        <div class="floating-label-wrap">
                            <select asp-for="WhetherTssSurveyDone" class="floating-label-field floating-label-field--s3" id="survey_done">
                                <option value="1">Yes</option>
                                <option value="0" selected>No</option>
                            </select>
                            <span class="text-danger"></span>
                            <label class="floating-label">Whether TSS Survey Done </label>
                        </div>
                    </div>

                </div>

                <div class="row mt-md-3" id="tss_div" style="display:none">


                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input asp-for="TssSurveyDocument4" type="file" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                            <label class="floating-label">Upload TSS Survey Report</label>
                        </div>
                    </div>


                </div>

                <div class="row mt-md-3">

                    <div class=" col-md-6">
                        <div class="floating-label-wrap">
                            <select asp-for="BoundaryWallDone" class="floating-label-field floating-label-field--s3">
                                <option value="1">Yes</option>
                                <option value="0">No</option>
                            </select>
                            <span class="text-danger"></span>
                            <label class="floating-label">Boundary Wall Done </label>
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input type="file" asp-for="KabzaDocument5" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                            <label class="floating-label">Kabza Proceeding</label>
                        </div>
                    </div>

                </div>



                <div class="row mt-md-3">


                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input type="text" asp-for="TakenFrom" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                            <label class="floating-label">Taken From</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input type="date" asp-for="DateofTakenOver" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                            <label class="floating-label">Date of Taken over</label>
                        </div>
                    </div>


                </div>


                <div class="row mt-md-3">


                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input type="text" asp-for="HandedOverTo" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                            <label class="floating-label">Handed Over to</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input type="date" asp-for="HandedOverDate" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                            <label class="floating-label">Handed Over Date</label>
                        </div>
                    </div>


                </div>

                <div class="row mt-md-3">


                    <div class=" col-md-6">
                        <div class="floating-label-wrap">
                            <select asp-for="LandRecordReceivedGnctd" class="floating-label-field floating-label-field--s3">
                                <option value="1">Yes</option>
                                <option value="0">No</option>
                            </select>
                            <span class="text-danger"></span>
                            <label class="floating-label">Whether Land Record pertaining to GS Land received from GNCTD </label>
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <textarea asp-for="Remarks" type="text" class="floating-label-field floating-label-field--s3" placeholder="Remarks"></textarea>
                            <label class="floating-label">Remarks</label>
                        </div>
                    </div>

                </div>
                <div class="row mt-md-3">

                    <div class=" col-md-6">

                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="IsActive">

                                <option value="1">Active</option>
                                <option value="0">Inactive</option>
                            </select>
                            <span asp-validation-for="IsActive" class="text-danger"></span>
                            <label asp-for="IsActive" class="floating-label">Status</label>
                        </div>

                    </div>


                </div>



                    <div class="row mt-md-3">
                        <div class=" col-md-12">
                            <button class="btn btn-theme " type="submit">Submit</button>
                            @Html.ActionLink("Back", "Index", "GramSabhaLand", null, new { @class = "btn btn-theme " })
                        </div>
                    </div>



            </form>
        </div>
    </div>
</div>





