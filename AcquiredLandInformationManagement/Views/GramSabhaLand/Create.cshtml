@model Libraries.Model.Entity.Gramsabhaland
@await Component.InvokeAsync("PageHeader", new { pageName = "Gram Sabha Land " })

<div class="container-fluid" id="MainContentForm">
    <div class="panel">

        <div class="panel-body">
            <form asp-action="Create" enctype="multipart/form-data" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <h3 class="mb-3 d-flex">Add Details</h3>

                <div class="row mt-md-3">
                    <div class=" col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="ZoneId"
                                    asp-items="@(new SelectList(Model.ZoneList, "Id","Name"))"
                                    onchange="return">
                                <option selected="selected" disabled="disabled" value="">--Select-- </option>
                            </select>
                            <span asp-validation-for="ZoneId" class="text-danger"></span>
                            <label class="floating-label" asp-for="ZoneId">Zone Name<span style="color:red">*&nbsp;</span></label>

                        </div>
                    </div>
                    <div class=" col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="VillageId"
                                    asp-items="@(new SelectList(Model.VillageList, "Id","Name"))">
                                <option selected="selected" disabled="disabled" value="">--Select-- </option>
                            </select>
                            <span asp-validation-for="VillageId" class="text-danger"></span>
                            <label class="floating-label" asp-for="VillageId">Village<span style="color:red">*&nbsp;</span></label>

                        </div>
                    </div>
                </div>




                <div class="pt-3">
                    <h3 class="mb-3 d-flex">Khasra Details</h3>
                    <div class="row mt-md-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="KhasraNo" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <span asp-validation-for="KhasraNo" class="text-danger"></span>
                                <label class="floating-label">Khasra No<span style="color:red">*&nbsp;</span></label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pt-3">
                    <h3 class="mb-3 d-flex">Total Area</h3>

                    <div class="row mt-md-3">

                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" asp-for="TotalAreaBigha" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Bigha</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="TotalAreaBiswa" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswa</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" asp-for="TotalAreaBiswanshi" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswanshi</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pt-3">
                    <h3 class="mb-3 d-flex">Vacant Area</h3>

                    <div class="row mt-md-3">

                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="VacantAreaBigha" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Bigha</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="VacantAreaBiswa" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswa</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="VacantAreaBiswanshi" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswanshi</label>
                            </div>
                        </div>

                    </div>
                </div>


                <div class="pt-3">
                    <h3 class="mb-3 d-flex">Builtup Area</h3>

                    <div class="row mt-md-3">



                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="BuiltupAreaBigha" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Bigha</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="BuiltupAreaBiswa" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswa</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="BuiltupAreaBiswanshi" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswanshi</label>
                            </div>
                        </div>

                    </div>
                </div>



                <div class="pt-3">

                    <h3 class="mb-3 d-flex">Encroached Area</h3>

                    <div class="row mt-md-3">



                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="EncroachedAreaBigha" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Bigha</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="EncroachedAreaBiswa" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswa</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="text" asp-for="EncroachedAreaBiswanshi" class="floating-label-field floating-label-field--s3" placeholder="Remarks" />
                                <label class="floating-label">Biswanshi</label>
                            </div>
                        </div>

                    </div>
                </div>


                <div class="pt-3">

                    <h3 class="mb-3 d-flex">U/S 507 Notification Details</h3>
                    <div class="row mt-md-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" asp-for="Us507notificationNo" class="floating-label-field floating-label-field--s3 " placeholder="Remarks" />
