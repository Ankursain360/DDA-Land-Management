@model Libraries.Model.Entity.Khasra





<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Khasra</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Edit" id="create_user">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Village Name</label>
                        <select class="form-control dda-dropdown" asp-for="VillageId" asp-items="@(new SelectList(Model.VillageList, "Id","Name"))">
                            <option>Select</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Rect No.</label>
                        <input type="text" class="form-control" />
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Land Category</label>
                        <select class="form-control dda-dropdown" asp-for="LandCategoryId" asp-items="@(new SelectList(Model.LandCategoryList, "Id","Name"))">
                            <option>Select</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Khasra No.</label>
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div id="Divclsification">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Area</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">Bigha</label>
                            <input type="text" class="form-control" />
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">Biswa</label>
                            <input type="text" class="form-control" />
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">Biswanshi</label>
                            <input type="text" class="form-control" placeholder="0" />
                        </div>
                    </div>
                </div>

                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Status</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">Active</option>
                            <option value="1">InActive</option>
                        </select>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-12">
                        <label class="control-label lb-sm">Remarks</label>
                        <textarea type="text" class="form-control"></textarea>
                    </div>
                </div>

                <br />







                <div class="form-row">
                    <div class=" form-group  col-md-12">
                        <input type="submit" value="Update" class="btn btn-primary dda-pageheading" onclick="return PasswordMatch();" />
                        @Html.ActionLink("Back", "Index", "KhasraMaster", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>
<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<script src="~/js/UserManagement.js"></script>
