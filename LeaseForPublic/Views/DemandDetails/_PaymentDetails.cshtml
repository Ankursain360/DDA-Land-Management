
@model IEnumerable<Dto.Search.DemandPaymentDetailsDto>

<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">

        <tr>

            <th>Demand Period</th>
            <th>Ground Rent /License Fee </th>
            <th>InterestAmount </th>
            <th>Total Dues </th>

        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
        <tr>
            <td>@item.DemandPeriod</td>
            <td>@item.GroundRentLeaseRent</td>
            <td>@item.InterestAmount</td>
            <td>@item.TotalDues</td>           
        
        </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="13" style="text-align:center"> Data Not Available </th>
            </tr>
        }

    </tbody>

</table>


