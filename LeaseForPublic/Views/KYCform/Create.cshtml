                            <span asp-validation-for="AreaSqmt" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Area in SQMT <span style="color:red">*</span></label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                   
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="AllotmentLetterDate" class="floating-label-field floating-label-field--s3" placeholder="Period to">
                            <span asp-validation-for="AllotmentLetterDate" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Date of Allotment Letter<span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="PossessionDate" class="floating-label-field floating-label-field--s3" placeholder="Period to">
                            <span asp-validation-for="PossessionDate" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Date of Possession <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="LeaseExecutionDate" class="floating-label-field floating-label-field--s3" placeholder="FAR Permissible">
                            <span asp-validation-for="LeaseExecutionDate" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Date of Lease Execution<span style="color:red">*</span></label>
                        </div>
                    </div>
                </div>
                    <div class="row mt-md-3">
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="date" asp-for="GroundRentPayableFromDate" class="floating-label-field floating-label-field--s3" placeholder="Ground Rent Payable from">
                                <span asp-validation-for="GroundRentPayableFromDate" class="text-danger"></span>
                                <label for="field-1" class="floating-label">Ground Rent Payable from <span style="color:red">*</span></label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="number" asp-for="LandPremiumPaid" class="floating-label-field floating-label-field--s3" value="0" placeholder="Land Premium Paid at the time of Allotment Rs">
                                <span asp-validation-for="LandPremiumPaid" class="text-danger"></span>
                                <label for="field-1" class="floating-label">Land Premium Paid at the time of Allotment Rs<span style="color:red">*</span></label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="number" asp-for="GroundRentPayableasonDate" class="floating-label-field floating-label-field--s3" value="0" placeholder="Period to">
                                <span asp-validation-for="GroundRentPayableasonDate" class="text-danger"></span>
                                <label for="field-1" class="floating-label">Ground Rent payable as on date Rs.<span style="color:red">*</span></label>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End of Plot details -->
                <!-- Lessee details -->
