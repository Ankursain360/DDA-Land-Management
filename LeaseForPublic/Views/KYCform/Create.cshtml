                                <thead class="text-nowrap">
                                    <tr>
                                        <th>Sr.No</th>
                                        <th>Challan No./Transaction No./UTR No.<span style="color:red">*</span></th>
                                        <th>Date <span style="color:red">*</span></th>
                                        <th>Amount(Rs) <span style="color:red">*</span></th>
