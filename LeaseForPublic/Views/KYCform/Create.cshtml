@model Libraries.Model.Entity.Kycform
@await Component.InvokeAsync("PageHeader", new { pageName = "Lessee/License KYC Details" })

<div class="pt-3">

    <form asp-action="Create" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">

        <h3 class="mb-3 d-flex">KYC (Know your Customer) Form<span style="margin-left:600px"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span></h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>


        <div class="row">
            <div class="col-md-4">
            </div>
            <div class="col-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <select class="floating-label-field floating-label-field--s3" asp-for="Property" >
                        @*<option selected value="">--Select--</option>*@
                        <option value="Lease">Lease Property</option>
                        <option value="License">License Property</option>
                    </select>
                    <span asp-validation-for="Property" class="text-danger"></span>
                    <label for="field-1" class="floating-label">Choose Property <span style="color:red">*</span></label>
                </div>
            </div>
            <div class="col-md-4">
            </div>
        </div>
        <br />

        <div id="divLeaseproperty">
            <h3 class="my-3 sub-heading">Details of Property </h3>
            <div class="row mt-md-3">
                <div class="col-md-3">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <select class="floating-label-field floating-label-field--s3" asp-for="PropertyTypeId"
                                asp-items="@(new SelectList(Model.PropertyTypeList, "Id","Name"))">
                            <option disabled="disabled" selected value="">Select</option>
                            @*<option selected value="">--Select--</option>
                            <option value="Lease">Residential</option>
                            <option value="License">Commercial</option>
                            <option value="Lease">Institutional</option>
                            <option value="License">Industrial </option>*@
                        </select>
                        <span asp-validation-for="PropertyTypeId" class="text-danger"></span>
                        <label for="field-1" class="floating-label">Nature of Property <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="text" class="floating-label-field floating-label-field--s3" asp-for="FileNo" placeholder="Name">
                        <span asp-validation-for="FileNo" class="text-danger"></span>
                        <label for="field-1" class="floating-label">File No.<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <select class="floating-label-field floating-label-field--s3" asp-for="BranchId"
                                asp-items="@(new SelectList(Model.BranchList, "Id","Name"))">
                            <option disabled="disabled" selected value="">Select</option>
                            @*<option selected value="">--Select--</option>*@
                            @*<option value="Lease">Commercial Estate</option>
                            <option value="License">Commercial Land</option>
                            <option value="Lease">License Property Cell</option>
                            <option value="License">IL </option>
                            <option value="License">Industrial Land</option>
                            <option value="Lease">OSB</option>
                            <option value="License">LAB Residential </option>
                            <option value="License">LAB Rohini </option>
                            <option value="License">GH</option>
                            <option value="License">CS</option>*@
                        </select>
                        <span asp-validation-for="BranchId" class="text-danger"></span>
                        <label for="field-1" class="floating-label">Branch Pertaining To <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <select class="floating-label-field floating-label-field--s3" asp-for="LeaseTypeId">
                                @*asp-items="@(new SelectList(Model.LeasetypeList, "Id","Type"))"*@
                            <option disabled="disabled" selected value="">Select</option>
                            @*<option selected value="">--Select--</option>*@
                            @*<option value="Lease">Perpetual</option>
        <option value="License">Tenure </option>*@

                           
                            @foreach (var item in Model.LeasetypeList)
                            {
                                <option value="@item.Id"> @item.Type </option>
                            }
                        </select>
                        <span asp-validation-for="LeaseTypeId" class="text-danger"></span>
                        <label for="field-1" class="floating-label">Lease Type <span style="color:red">*</span></label>
                    </div>
                </div>

            </div>
            <!-- if Lease type is tenure -->
            <div id="divTenure">
                <h3 class="my-3 sub-heading">Tenure Details </h3>
                <div class="row mt-md-3">
                    <div class=" col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="TenureFrom" class="floating-label-field floating-label-field--s3" placeholder="Period From">
                            <span asp-validation-for="TenureFrom" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Tenure Period From <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="TenureTo" class="floating-label-field floating-label-field--s3" placeholder="Period to">
                            <span asp-validation-for="TenureTo" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Tenure Period To <span style="color:red">*</span></label>
                        </div>
                    </div>

                </div>

            </div>
            <!-- Start of Property Details -->
            <div id="divPropertyDetails">
                @*<h3 class="my-3 sub-heading">Plot Details </h3>*@
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="PlotNo" class="floating-label-field floating-label-field--s3" placeholder="Period to">
                            <span asp-validation-for="PlotNo" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Plot No/ Khasra No <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea type="text" asp-for="PlotDescription" class="floating-label-field floating-label-field--s3" placeholder="FAR Permissible"></textarea>
                            <span asp-validation-for="PlotDescription" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Plot Description <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <select class="floating-label-field floating-label-field--s3" asp-for="ZoneId"
                                    asp-items="@(new SelectList(Model.ZoneList, "Id","Name"))">
                                <option disabled="disabled" selected value="">Select</option>

                            </select>
                            <span asp-validation-for="ZoneId" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Zone <span style="color:red">*</span></label>
                        </div>
                    </div>

                </div>
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <select class="floating-label-field floating-label-field--s3" asp-for="LocalityId"
                                    asp-items="@(new SelectList(Model.LocalityList, "Id","Name"))">
                                <option disabled="disabled" selected value="">Select</option>

                            </select>
                            <span asp-validation-for="LocalityId" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Locality <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Phase" class="floating-label-field floating-label-field--s3" placeholder="Phase">
                            <span asp-validation-for="Phase" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Phase<span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Sector" class="floating-label-field floating-label-field--s3" placeholder="Sector">
                            <span asp-validation-for="Sector" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Sector <span style="color:red">*</span></label>
                        </div>
                    </div>


                </div>
                <div class="row mt-md-3">

                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Block" class="floating-label-field floating-label-field--s3" placeholder="Block">
                            <span asp-validation-for="Block" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Block <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Pocket" class="floating-label-field floating-label-field--s3" placeholder="Pocket">
                            <span asp-validation-for="Pocket" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Pocket<span style="color:red">*</span></label>
                        </div>
                    </div>


                </div>


            </div>

            <!-- End of Property details -->
            <div id="divPlotDetails">
                <h3 class="my-3 sub-heading">Applicant Details </h3>

                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Name" class="floating-label-field floating-label-field--s3" placeholder="First Name">
                            <span asp-validation-for="Name" class="text-danger"></span>
                            <label for="field-1" class="floating-label"> Name   <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="FatherName" class="floating-label-field floating-label-field--s3" placeholder="Middle Name">
                            <span asp-validation-for="FatherName" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Father Name</label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <select class="floating-label-field floating-label-field--s3" asp-for="Gender">
                                @*<option selected value="">--Select--</option>*@
                                <option value="Male">Male</option>
                                <option value="Female">Female</option>
                                <option value="Other">Other</option>
                            </select>
                            <span asp-validation-for="Gender" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Gender <span style="color:red">*</span></label>
                        </div>

                    </div>

                </div>

                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="Email" asp-for="EmailId" class="floating-label-field floating-label-field--s3" placeholder="Last Name">
                            <span asp-validation-for="EmailId" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Email ID<span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="MobileNo" type="text" class="floating-label-field floating-label-field--s3" placeholder="Address">
                            <span asp-validation-for="MobileNo" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Mobile No.[Not add +91]<span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea type="number" asp-for="Address" class="floating-label-field floating-label-field--s3" placeholder="Pin Code"></textarea>
                            <span asp-validation-for="Address" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Correspondence Address</label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">


                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="number" asp-for="AadhaarNo" class="floating-label-field floating-label-field--s3" placeholder="Last Name">
                            <span asp-validation-for="AadhaarNo" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Aadhaar No.<span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <select class="floating-label-field floating-label-field--s3" asp-for="Relationship">
                                @*<option selected value="">--Select--</option>*@
                                <option value="Male">Father</option>
                                <option value="Female">Mother</option>
                                <option value="Other">Other</option>
                            </select>
                            <span asp-validation-for="Relationship" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Relationship <span style="color:red">*</span></label>
                        </div>

                    </div>
                </div>
            </div>
            <!-- Lessee details -->
            <div id="divLesseeDetails">
                <h3 class="my-3 sub-heading">Details of Lessee as per Allotment Letter/Lease deed</h3>

                <input type="hidden" asp-for="AllotteeApplicantDetailsSame" />
                <div class="form-check theme">
                    <input class="form-check-input" name="chksame" type="checkbox" id="chk1" asp-for="AllotteeApplicantDetailsSame">
                    <label class="form-check-label" for="checkbox">
                        If the Allottee Details are same as Applicant Details then tick the checkbox
                    </label>
                </div>
                @*<b>  If the Allottee Details are same as Applicant Details then tick the checkbox  &nbsp;&nbsp;&nbsp;&nbsp;</b>*@
                @*<input type="checkbox" asp-for="AllotteeApplicantDetailsSame" value="yes" placeholder="Name">*@

                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="AllotteeLicenseeName" class="floating-label-field floating-label-field--s3" placeholder="Name">
                            <span asp-validation-for="AllotteeLicenseeName" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Name of the Allottee/Lessee <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea class="floating-label-field floating-label-field--s3" asp-for="AllotteeLicenseeAddress" placeholder="Address"> </textarea>
                            <span asp-validation-for="AllotteeLicenseeAddress" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Correspondence Address <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="number" asp-for="AllotteeLicenseeMobileNo" class="floating-label-field floating-label-field--s3" placeholder="PIN Code">
                            <span asp-validation-for="AllotteeLicenseeMobileNo" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Mobile No.[Not add +91]<span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="email" asp-for="AllotteeLicenseeEmailId" class="floating-label-field floating-label-field--s3" placeholder="Email ID">
                            <span asp-validation-for="AllotteeLicenseeEmailId" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Email ID</label>
                        </div>
                    </div>

                </div>
            </div>
            <!-- End of Lessee Details -->
            <!-- Plot details -->
            <div id="divPlotDetails">
                <h3 class="my-3 sub-heading">Plot Details </h3>

                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Area" class="floating-label-field floating-label-field--s3 numbers" placeholder="Address">
                            <span asp-validation-for="Area" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Area <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="AllotmentLetterDate" class="floating-label-field floating-label-field--s3" placeholder="Email ID">
                            <span asp-validation-for="AllotmentLetterDate" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Date of Allotment Letter  </label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="PossessionDate" class="floating-label-field floating-label-field--s3" placeholder="PIN Code">
                            <span asp-validation-for="PossessionDate" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Date of Possession <span style="color:red">*</span></label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="date" asp-for="LeaseLicenseExecutionDate" class="floating-label-field floating-label-field--s3" placeholder="Address">
                            <span asp-validation-for="LeaseLicenseExecutionDate" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Date of Lease Execution <span style="color:red">*</span></label>
                        </div>
                    </div>


                </div>

            </div>

            <!-- End Of Plot Details -->
            <div id="divDocDetails">
                <h3 class="my-3 sub-heading"> Document Required </h3>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="file" asp-for="Aadhar" class="floating-label-field floating-label-field--s3" placeholder="file">
                            <span asp-validation-for="Aadhar" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Aadhaar No. of Allottee/Lessee</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="file" asp-for="Letter" class="floating-label-field floating-label-field--s3" placeholder="file">
                            <span asp-validation-for="Letter" class="text-danger"></span>
                            <label for="field-1" class="floating-label">Possession Letter /Allotment Letter /Executed Lease deed (Any One )</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br />

        @* ********************************************** div  License  property *************************************@
        <div id="divLicenseproperty">
            <h3 class="my-3 sub-heading">Details of Property </h3>
            <div class="row mt-md-3">
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <select class="floating-label-field floating-label-field--s3" asp-for="PropertyTypeId"
                                asp-items="@(new SelectList(Model.PropertyTypeList, "Id","Name"))" style="width:100%;">
                            <option disabled="disabled" selected value="">Select</option>
                            @*<option selected value="">--Select--</option>
                            <option value="Lease">Residential</option>
                            <option value="License">Commercial</option>
                            <option value="Lease">Institutional</option>
                            <option value="License">Industrial </option>*@
                        </select>
                        <span asp-validation-for="PropertyTypeId" class="text-danger"></span>
                        <label for="field-1" class="floating-label">Nature of Property <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="text" class="floating-label-field floating-label-field--s3" asp-for="FileNo" placeholder="Name">
                        <span asp-validation-for="FileNo" class="text-danger"></span>
                        <label for="field-1" class="floating-label">File No.<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <select class="floating-label-field floating-label-field--s3" asp-for="BranchId"
                                asp-items="@(new SelectList(Model.BranchList, "Id","Name"))" style="width:100%;">
                            <option disabled="disabled" selected value="">Select</option>
                            @*<option selected value="">--Select--</option>*@
                            @*<option value="Lease">Commercial Estate</option>
                            <option value="License">Commercial Land</option>
                            <option value="Lease">License Property Cell</option>
                            <option value="License">IL </option>
                            <option value="License">Industrial Land</option>
