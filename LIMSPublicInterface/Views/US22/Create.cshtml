</script>

<script>
    $(document).ready(function () {
        //Open popup
        $('body').on("click", "a.popup", function (e) {
            e.preventDefault();
            var page = $(this).attr('href');
            OpenPopup(page);
        });
    });


    //open popup
    function OpenPopup(Page) {
        var $pageContent = $('<div/>');
        $pageContent.load(Page);
        $dialog = $('<div class="popupWindow" style="overflow:hidden"></div>')
            .html($pageContent)
            .dialog({
                draggable: true,
                autoOpen: false,
                resizable: true,
                model: true,
                height: 600,
                width: 600,
                close: function () {
                    $dialog.dialog('destroy').remove();
                }
            })
        $dialog.dialog('open');
    }

</script>



