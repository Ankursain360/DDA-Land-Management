@model Libraries.Model.Entity.Acquiredlandvillage
@await Component.InvokeAsync("PageHeader", new { pageName = "Village all details Report" })


<div class="pt-3">
    <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">Details</h3>
        <div class="row">
            <div class="form-group col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" asp-for="Name"
                          >
                        <option value="0">All</option>



                        @foreach (var item in Model.VillageList)
                        {
                            <option value="@item.Id" mytag="@item.Name">@item.Name </option>
                        }



                    </select>
                    <span class="d-block text-danger field-validation-valid" asp-validation-for="Name"></span>
                    <label asp-for="Name" class="floating-label">Village </label>
                </div>
            </div>
        </div>
      
        <div class="row mt-md-3">
            <div class="col-md-12 col-xl-3">
                <div class="row">
                    <div class="col-6 col-md-5 col-xl-6">
                        <button id="btnReset" class="btn btn-link btn-block d-flex align-items-center justify-content-center" type="reset">
                            <i class="ri-refresh-line mr-2"></i>Reset
                        </button>
                    </div>
                    <div class="col-6 col-md-7 col-xl-6">

                        <input type="submit" value="Generate" id="btnGenerate" class="btn btn-theme" />

                    </div>
                </div>
            </div>
        </div>

    </div>


    <div class="table-section bg-white border shadow rounded">
     

        <div class="table-responsive px-2">
            <form class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
            <p> <h3 class="mb-3 d-flex" style="text-align:center;" id="village_tag"> Village Name - All</h3></p>    




                <div id="LoadReportView">

                </div>
            </form>
        </div>
    </div>

</div>


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/villagedetailsindex.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/villagedetailsindex.min.js" asp-append-version="true"></script>
</environment>

