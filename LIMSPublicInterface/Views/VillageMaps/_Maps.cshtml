@model Dto.Search.VillageReportDetailsSearchDto
<div class="lightbox">
    <div class="row">
        <div class="col-lg-12 text-center">
            <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
                <h3 class="mb-3 d-flex">Village Map </h3>
                @{
                    var iPhotoUrl = "/VillageMaps/MAPS/" + Model.VillageSmallMap;
                    var fullurl = "/VillageMaps/MAPS/" + Model.VillageFullMap;
                    <a href="@fullurl" data-toggle="lightbox" data-mdb-img="@fullurl">
                        <img class="img-fluid" src="@iPhotoUrl" height="700" data-caption="Village Maps" />
                    </a>
                }
            </div>
            <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
                <h3 class="mb-3 d-flex">Massavies </h3>
                <div class="row mt-md-3">
                    @foreach (var items in Model.VillageMassaviesMap)
                    {
                        var Massaviesurl = "/VillageMaps/ScannedSheets/" + items;

                        <a href="@Massaviesurl" data-toggle="lightbox" data-gallery="example-gallery" class="col-sm-4">
                            <img src="@Massaviesurl" class="img-fluid" alt="Dark Roast Iced Coffee" data-mdb-img="@Massaviesurl">
                        </a>


                    }
                </div>
            </div>
        </div>

    </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/bs5-lightbox@1.8.0/dist/index.bundle.min.js"></script>
