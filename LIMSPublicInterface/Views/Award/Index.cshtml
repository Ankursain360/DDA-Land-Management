
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Award  </h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%">
            UNDER AWARD DELHI DEVELOPMENT AUTHORITY GIVE NOTICE TO GENERAL PUBLIC FOR ACQUISITION OF LAND</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Index">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Award Number(Date) </label>
                       
                        <select class="form-control">

                            <option value="awd-1859">1016 [DATE : 1960/05/26] </option>
                            <option value="Awd-428">1131 [DATE : 1961/04/10] </option>
                            <option value="awd-727">1111 [DATE : 1961/01/31] </option>
                            <option value="Awd-459">1141 [DATE : 1961/04/26] </option>
                            <option value="awd-1457">1146 [DATE : 1961/05/15] </option>
                         </select>
                    </div>
                </div>
                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Result" id="btnFind" class="btn btn-primary dda-pageheading" />
                        @Html.ActionLink("Back", "Index", "Home", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
           
        <div id="divDataLoad" >


            <div class="row" style="margin-bottom:35px;">
                <div class="col-sm-12">
                    <div class="dda-innerheading">
                        <div class="col-md-12">
                            <h6 class="pull-left" style="color:white;margin-top:5px;">NOTIFIED AREA THROUGH AWARD </h6>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="form-group col-md-12">
                    <div class="table-responsive">
                        <table id="divDataLoad" class="table table-striped table-bordered" border="0">
                            <thead class="thead">
                                <tr>
                                    <th>Sr.No.</th>
                                    <th>Village Name</th>
                                    <th>Khasra No</th>
                                    <th>Area (Bigha - Biswa - Biswansi)(ACRE)</th>
                                    <th>Notified Area (B-B-B) (ACRE)</th>
                                </tr>

                            </thead>
                            <tbody class="tbody">
                                <tr>
                                    <td>1</td>
                                    <td>AZADPUR	</td>
                                    <td>0//255</td>
                                    <td>4-06-00 (.896)</td>
                                    <td>4-06-00 (.896)</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>
          
        </div>
    </div>
</div>

<script>
   


    $(document).ready(function () {
        $("#btnFind").click(function () {
            $("#divDataLoad").show();
        });
    });
</script>
