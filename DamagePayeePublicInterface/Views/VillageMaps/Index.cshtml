@model Dto.Search.VillageReportDetailsSearchDto
@await Component.InvokeAsync("PageHeader", new { pageName = "Village maps/massavies" })
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form asp-action="getVillageMaps" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" >
    <div class="pt-3">
        <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
            <h3 class="mb-3 d-flex">Details </h3>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select class="floating-label-field floating-label-field--s3" asp-for="VillageId" asp-items="@(new SelectList( @ViewBag.VillageList, "Id","Name"))">
                            <option value="0">Select</option>
                        </select>
                        <label asp-validation-for="FileNo" class="error"></label>
                        <label class="floating-label" asp-for="VillageId">Village<span style="color:red">*</span></label>
                    </div>
                </div>
            </div>
            <div class="col-6 col-md-7 col-xl-6">
                <button type="submit" value="Result" class="btn btn-theme" onclick="return villageDetails();">Result</button>
            </div>
        </div>

    </div>
 </form>

    <partial name="../Shared/_ValidationScriptsPartial.cshtml" />
    <environment include="Development">
        <script src="~/js/villageMapsIndex.js"></script>

    </environment>
    <environment exclude="Development">
        <script src="~/js/villageMapsIndex.min.js" asp-append-version="true"></script>
    </environment>

