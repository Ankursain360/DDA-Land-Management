                            <input asp-for="LeaseDocumentFile" type="file" class="floating-label-field floating-label-field--s3" placeholder="Please upload pdf documents" id="Assignfile" name="Assignfile" accept="application/pdf" />
                            <label class="floating-label">Please upload pdf documents<span style="color:red">(.pdf only)</span></label>
                        </div>
                    </div>

                </div>
            </div>
