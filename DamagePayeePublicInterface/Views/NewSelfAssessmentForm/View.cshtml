@model Libraries.Model.Entity.Newdamagepayeeregistration
@await Component.InvokeAsync("PageHeader", new { pageName = "New Damage Property Registration Form" })
<div class="pt-3">
    <form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <input type="hidden" asp-for="ApprovedStatus" />
        <input type="hidden" asp-for="Id" />

        <h3 class="mb-3 d-flex">View Details <span class="ms-auto"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span> </h3>
        <div id="COLONYDETAILS">
            <div class="row">
                <div class="col-md-4">
                    <span style="color: #2196F3"> Whether property is damage Payee :</span><span style="color:red">*</span> <select asp-for="WhetherDamageProp" class="floating-label-field floating-label-field--s3">
                        <option value="">---Select---</option>
                        <option value="Yes">Yes</option>
                        <option value="No">No</option>
                    </select>
                    <span asp-validation-for="WhetherDamageProp" class="d-block text-danger field-validation-valid"></span>
                </div>
                <div class="col-md-4">
                    <span style="color: #2196F3">
                        Whether the current occupant is a :<span style="color:red">*</span>
                    </span><select asp-for="Occupanttype" class="floating-label-field floating-label-field--s3">
                        <option value="">---Select---</option>
                        <option value="Original">Original</option>
                        <option value="Legal Heir">Legal Heir</option>
                        <option value="Subsequent">Subsequent buyer</option>
                    </select>
                    <span asp-validation-for="Occupanttype" class="d-block text-danger field-validation-valid"></span>
                </div>
                <div class="col-md-4" style="display:none" id="divLegal">
                    <span style="color: #2196F3"> Has the Assessee Name change requested in DDA :</span><span style="color:red">*</span> <select asp-for="IsNameChanged" class="floating-label-field floating-label-field--s3">
                        <option value="">---Select---</option>
                        <option value="Yes">Yes</option>
                        <option value="No">No</option>
                    </select>
                    <span asp-validation-for="IsNameChanged" class="d-block text-danger field-validation-valid"></span>
                </div>

            </div>
            <h3 class="my-3 d-flex sub-heading">COLONY DETAILS</h3>
            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="FileNo" type="text" class="floating-label-field floating-label-field--s3" placeholder="RegNo" />
                        <span asp-validation-for="FileNo" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">File No</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="DistrictId" class="floating-label-field floating-label-field--s3" onchange="return onChange(this.options[this.selectedIndex].value)"
                                asp-items="@(new SelectList(Model.districtList,"Id","Name"))">
                            <option value="">---Select---</option>
                        </select>
                        <span asp-validation-for="DistrictId" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">District<span style="color:red">*</span></label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-4">
                    <div class="floating-label-wrap">
                        <select asp-for="VillageId" class="floating-label-field floating-label-field--s3" onchange="return onChangeVillage(this.options[this.selectedIndex].value)"
                                asp-items="@(new SelectList(Model.damagevillageList,"Id","Name"))">
                            <option value="">---Select---</option>
                        </select>
                        <span asp-validation-for="VillageId" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Village/Revenue state<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap">
                        <select asp-for="ColonyId" class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(Model.ColonyList,"Id","Name"))">
                            <option value="">---Select---</option>
                        </select>
                        <span asp-validation-for="ColonyId" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Colony<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-4" style="display:none" id="divotherColony">
                    <div class="floating-label-wrap">
                        <input asp-for="OtherColonyName" type="text" class="floating-label-field floating-label-field--s3" placeholder="Other Colony Name" />
                        <span asp-validation-for="OtherColonyName" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Other Colony Name<span style="color:red">*</span></label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <input asp-for="PinCode" type="text" class="floating-label-field floating-label-field--s3 numbers" maxlength="6" placeholder="PIN" />
                        <span asp-validation-for="PinCode" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Pin code</label>
                    </div>
                </div>
            </div>
        </div>

        <div id="PROPERTYDETAILS">
            <h3 class="my-3 d-flex sub-heading">PROPERTY DETAILS</h3>
            <div class="row">
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        @*<input asp-for="TypeProperty" type="text" class="floating-label-field floating-label-field--s3" placeholder="Property Type" />*@
                        <select asp-for="TypeOfProperty" class="floating-label-field floating-label-field--s3">
                            <option value="">---Select---</option>
                            <option value="Residential">Built-up</option>
                            <option value="Commercial">Vacant</option>
                        </select>
                        <span asp-validation-for="TypeOfProperty" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Type of property<span style="color:red">*</span></label>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <select asp-for="UseType" class="floating-label-field floating-label-field--s3">
                            <option value="">---Select---</option>
                            <option value="Residential">Residential</option>
                            <option value="Commercial">Commercial</option>
                            <option value="Industrial">Industrial</option>
                            <option value="Institutional">Institutional</option>
                            <option value="Other">Other</option>
                        </select>
                        <span asp-validation-for="UseType" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Type of use<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-4" style="display:none" id="divUsetype">
                    <div class="floating-label-wrap">
                        <input type="text" class="floating-label-field floating-label-field--s3" placeholder="Other Property Use" />
                        <span asp-validation-for="AreaBuildingFootprint" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Other Property Use </label>
                    </div>
                </div>
            </div>

            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="LandArea" type="text" class="floating-label-field floating-label-field--s3 numbers" placeholder="Landarea" />
                        <span asp-validation-for="LandArea" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Land area (sqm)</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <input asp-for="AreaBuildingFootprint" type="text" class="floating-label-field floating-label-field--s3 numbers" placeholder="Area of building" />
                        <span asp-validation-for="AreaBuildingFootprint" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Area of building footprint on plot (sqm) </label>
                    </div>
                </div>
            </div>

            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <input asp-for="TotalConstructedArea" type="text" class="floating-label-field floating-label-field--s3 numbers" placeholder="Constructed Area  (sqm)" />
                        <span asp-validation-for="TotalConstructedArea" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Total constructed area (sqm) (including all floors)<span style="color:red">*</span></label>
                    </div>
                </div>


                <div class="col-md-6">
                    @*changed*@
                    <div class="floating-label-wrap">
                        <input asp-for="VacantArea" type="text" class="floating-label-field floating-label-field--s3 numbers" placeholder="Vacant Area (sqm)" />
                        <span asp-validation-for="VacantArea" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Vacant area (sqm)<span style="color:red">*</span></label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="HousePropertyNo" type="text" class="floating-label-field floating-label-field--s3" placeholder="House No" />
                        <span asp-validation-for="HousePropertyNo" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">House No/Property No<span style="color:red">*</span></label>

                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <input asp-for="PlotNo" type="text" class="floating-label-field floating-label-field--s3" placeholder="Plot No" />
                        <span asp-validation-for="PlotNo" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Plot No</label>
                    </div>
                </div>


            </div>
            <div class="row mt-md-3">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="Street" type="text" class="floating-label-field floating-label-field--s3" placeholder="Street" />
                        <span asp-validation-for="Street" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Street</label>

                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="NoofFloorConstructed" type="text" class="floating-label-field floating-label-field--s3" placeholder="No of Floor" />
                        <span asp-validation-for="NoofFloorConstructed" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">No of floors costructed</label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">

                <div class="col-md-6">
                    @*changed*@
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="KhasraNo" type="text" class="floating-label-field floating-label-field--s3" placeholder="Khasra No" />
                        <span asp-validation-for="KhasraNo" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Khasra No ( as per revenue records )</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="LandMark" type="text" class="floating-label-field floating-label-field--s3" placeholder="Khasra No" />
                        <span asp-validation-for="LandMark" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">Land mark (if any)</label>
                    </div>
                </div>
            </div>
        </div>

        <div id="PROPERTYSCHEDULE">
            <h3 class="my-3 d-flex sub-heading">PROPERTY SCHEDULE (please indicate the details of the surrounding property)</h3>
            <div class="row">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="North" type="text" class="floating-label-field floating-label-field--s3" placeholder="North" />
                        <span asp-validation-for="North" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">North</label>

                    </div>
                </div>

                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <input asp-for="South" type="text" class="floating-label-field floating-label-field--s3" placeholder="South" />
                        <span asp-validation-for="South" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">South</label>
                    </div>
                </div>


            </div>
            <div class="row mt-md-3">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="East" type="text" class="floating-label-field floating-label-field--s3" placeholder="East" />
                        <span asp-validation-for="East" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">East</label>

                    </div>
                </div>

                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <input asp-for="West" type="text" class="floating-label-field floating-label-field--s3" placeholder="West" />
                        <span asp-validation-for="West" class="d-block text-danger field-validation-valid"></span>
                        <label class="floating-label">West</label>
                    </div>
                </div>


            </div>
        </div>
        <div id="divWhetherSub_Property">
            <h2 class="my-3 sub-heading"> Whether the subject property was on lease.</h2>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="WhetherSubProperty" class="floating-label-field floating-label-field--s3">
                            <option value="No">No</option>
                            <option value="Yes">Yes</option>
                        </select>
                        <label class="floating-label"></label>
                    </div>
                </div>
            </div>
            <div id="divWhetherSub_PropertyYesSeceltion" style="display:none">
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input asp-for="NameOfOriginalLessee" type="text" class="floating-label-field floating-label-field--s3" placeholder="Name of the original lessee (As per the lease deed)" />
                            <label class="floating-label">Name of the original lessee (As per the lease deed)</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input asp-for="TenureTermOfLease" type="text" class="floating-label-field floating-label-field--s3" placeholder="Tenure/term of lease (As per the lease deed)" />
                            <label class="floating-label">Tenure/term of lease (As per the lease deed)</label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input asp-for="DateOfLeaseCommenced" type="date" class="floating-label-field floating-label-field--s3" placeholder="Date from which lease commenced." />
                            <label class="floating-label">Date from which lease commenced.</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select asp-for="UseLeaseDeed" class="floating-label-field floating-label-field--s3">
                                <option value="">---Select---</option>
                                <option value="Residential">Residential</option>
                                <option value="Commercial">Commercial</option>
                            </select>
                            <label class="floating-label">Use as per the lease deed</label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input asp-for="SpecifyUse" type="text" class="floating-label-field floating-label-field--s3" placeholder="Specify use as mentioned in the lease deed" />
                            <label class="floating-label">Specify use as mentioned in the lease deed</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input asp-for="AreaLeaseDeed" type="text" class="floating-label-field floating-label-field--s3 numbers" placeholder="Area as mentioned in the lease deed" />
                            <label class="floating-label">Area as mentioned in the lease deed</label>
                        </div>
                    </div>
