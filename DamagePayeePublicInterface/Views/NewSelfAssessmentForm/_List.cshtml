@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Newdamagepayeeregistration>

<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">

        <tr>
            <th>File No</th>
            <th>Damage Payee</th>
            <th>Current Occupant</th>
            <th>Type of use</th>
            <th>Total Constructed Area</th>
            <th>House No</th>
            <th>Revenue Estate</th>
            <th>Colony</th>
            <th>Operations</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Results)
        {
        <tr>
            <td>@item.FileNo</td>
            <td>@item.WhetherDamageProp</td>
            <td>@item.Occupanttype</td>
            <td>@item.UseType</td>
            <td>@item.TotalConstructedArea</td>
            <td>@item.HousePropertyNo</td>
            <td>@item.GetVillage.Name</td>
            <td>@item.GetColony.Name</td>

            @*<td>
                @if (item.IsActive == 1)
                {
                    <span class="badge bg-primary px-2 py-1">Active</span> }
                else
                {
                    <span class="badge bg-danger px-2 py-1">Inactive</span>}
            </td>*@
            <td>
                <div class="action-btns">
                    <a asp-action="View" asp-route-id="@item.Id" title="View" class="view"><i class="ri-eye-fill"></i> </a>
                    @*<a asp-action="Edit" asp-route-id="@item.Id" title="edit" class="edit"><i class="ri-edit-line"></i> </a>
                <a asp-action="Delete" asp-route-id="@item.Id" title="delete" class="remove" onclick="return confirm('Are you sure, you want to delete this record?')">  <i class="ri-delete-bin-5-fill"></i></a>*@
                </div>
            </td>

        </tr>
        }
    </tbody>
</table>
@await Component.InvokeAsync("Pager", new { totalCount = Model.RowCount, pageNumber = Model.CurrentPage, pageSize = Model.PazeSize })
