                    <script src="~/lib/bootstrap/select2/select2.js" defer></script>

                </div>
            </div>
        </div>
        <div class="permission mt-2 accordion list-unstyled m-0 ">
            <div class="card">
                <a class="p-2 no-dec d-flex align-items-center" data-toggle="collapse" href="#collapseApprroval2" role="button" aria-expanded="true"
                   aria-controls="collapseApprroval">
                    <i class="ri-arrow-right-s-fill"></i>
                    <span class="text ml-1 mr-auto">View Allottee Evidance Document Details  </span>
                    <i class="arrow ri-arrow-drop-down-line slow"></i>
                </a>
                <div class="list-unstyled m-0 pl-2 pr-1 pl-md-4 pr-md-3 collapse" id="collapseApprroval2">

                    <div class="table-section bg-white border shadow rounded">
                        <div class="table-responsive ">
                            <table class="table table-sm table-bordered mb-0 table-striped">
                                <thead class="text-nowrap">

                                    <tr>
                                        <th>User Name</th>
                                        <th>Document Details</th>
                                        <th>Document</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>

                                        <td> Test DD Name</td>
                                        <td>Doc Name </td>



                                        <td>
                                            <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter"> View File </a>
                                        </td>


                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>


                </div>
            </div>
        </div>


        <div class="permission mt-2 accordion list-unstyled m-0 ">
            <div class="card">
                <a class="p-2 no-dec d-flex align-items-center" data-toggle="collapse" href="#collapseApprroval3" role="button" aria-expanded="true"
                   aria-controls="collapseApprroval">
                    <i class="ri-arrow-right-s-fill"></i>
                    <span class="text ml-1 mr-auto">View Hearing Details </span>
                    <i class="arrow ri-arrow-drop-down-line slow"></i>
                </a>
                <div class="list-unstyled m-0 pl-2 pr-1 pl-md-4 pr-md-3 collapse" id="collapseApprroval3">

                    <div class="table-section bg-white border shadow rounded">
                        <div class="table-responsive ">
                            <table class="table table-sm table-bordered mb-0 table-striped">
                                <thead class="text-nowrap">

                                    <tr>
                                        <th>Hearing Venue</th>
                                        <th>Hearing Date</th>
                                        <th>Hearing Time</th>

                                        <th>Attendee</th>
                                        <th>Hearing  Remarks</th>
                                        <th>Document</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td> High Court</td>
                                        <td> 23-mar-2021</td>
                                        <td>10:00 AM</td>
                                        <td>All</td>
                                        <td>dummy data  </td>
                                        <td>
                                            <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter"> View File </a>
                                        </td>

                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
        </div>


        

        <h3 class="my-3 d-flex sub-heading pt-4">Judgement Details</h3>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3">
                        <option disabled="disabled" selected value="">Select</option>
                       
                    </select>
                    @*<span asp-validation-for="LeasesTypeId" class="text-danger"></span>*@
                    <label class="floating-label">Forward To</label>
                </div>
            </div>
            <div class=" col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input type="file" class="floating-label-field floating-label-field--s3" placeholder="Name">
                    @*<span asp-validation-for="DocumentForFile" class="d-block text-danger field-validation-valid"></span>*@
                    <label for="field-1" class="floating-label"> Upload copy of the document selected </label>

                </div>
            </div>

        </div>
        <div class="row mt-md-3">
            <div class=" col-md-12">
                <button class="btn btn-theme " type="submit">Create</button>
                @Html.ActionLink("Back", "Index", "Judgement", null, new { @class = "btn btn-theme " })
            </div>
        </div>
    </form>
</div>



<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
