@model Dto.Search.KycFormApprovalDetailsSearchDto
@await Component.InvokeAsync("PageHeader", new { pageName = "Kyc Approval Details" })
<div class="pt-3">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>  
    <input type="hidden" asp-for="approvalType" />
    <input type="hidden" asp-for="approvalNo" />

    <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">List</h3>
     
     
    </div>
    <div class="table-section bg-white border shadow rounded">
        

        <div class="table-responsive px-2">
            <div id="divkycTable"></div>
        </div>

      
    </div>
</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/KycFormApprovalDetails.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/KycFormApprovalDetails.min.js" asp-append-version="true"></script>
</environment>
