@model IEnumerable<Dto.Search.KycFormDemandPaymentApprovalSearchDto>
@{int rowNo = 0; }
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>
            <th>Sr.No</th>
            <th> Property </th>
            <th>FileNo</th>
            <th>LeaseGroundRentDepositFrequency </th>
            <th> PlotNo </th>
            <th> Phase </th>
            <th> Sector </th>
            <th>EmailId</th>
            <th>MobileNo</th>
            <th>Total Payble</th>
            <th>Total interest</th>
            <th>Total Dues</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
        <tr>
            <td>@(rowNo += 1)</td>
            <td>@item.Property</td>
            <td>@item.FileNo</td>
            <td>@item.LeaseGroundRentDepositFrequency</td>
            <td>@item.PlotNo</td>
            <td>@item.Phase</td>
            <td>@item.Sector</td>
            <td>@item.EmailId</td>
            <td>@item.MobileNo</td>
            <td>@item.TotalPayable</td>
            <td>@item.TotalPayableInterest</td>
            <td>@item.TotalDues</td>
        </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="14" style="text-align:center"> No Record Available! </th>
            </tr>
        }
    </tbody>

</table>
