
@await Component.InvokeAsync("PageHeader", new { pageName = "Evidance Submitted by Allottee" })

<div class="pt-3">
    <form asp-action="Create" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">View Details</h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <div class="accordion mb-4" id="accordionExample">
            <div class="card">
                <div class="card-header p-0" id="headingOne">
                    <h2 class="mb-0">
                        <button class="btn btn-block btn-link collapsed d-flex sub-heading px-3 py-2 border-0 align-items-center" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            View Request of Eviction Proceeding Details<i class="ri-arrow-drop-down-line ml-auto" style="font-size: 1.2rem;font-weight: 100;"></i>
                        </button>
                    </h2>
                </div>

                <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">

                        <div class="pt-3">




                            <div class="row mt-md-3">
                                <div class="col-md-6">
                                    <div class="floating-label-wrap">


                                        <select class="floating-label-field floating-label-field--s3" onchange="get_socity()" style="width:100%;" disabled>

                                            <option selected disabled="disabled" value="">---Select---</option>
                                            @*@foreach (var item in Model.AllotmententryList)
                        {
                            <option value="@item.Id" data-id=" @item.SocietyName"> @item.LRefNo </option>
                        }*@
                                        </select>
                                        @*<span asp-validation-for="AllotmentId" class="text-danger"></span>*@
                                        <label class="floating-label">File No</label>
                                    </div>
                                </div>


                                <div class=" col-md-6">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input placeholder="Address" type="text"
                                               class="floating-label-field floating-label-field--s3" disabled/>
                                        @*<span asp-validation-for="LetterReferenceNo" class="text-danger"></span>*@
                                        <label class="floating-label">Letter Reference No</label>
                                    </div>
                                </div>

                            </div>

                            <div class="row mt-md-3">
                                <div class="col-md-6">
                                    <div class="floating-label-wrap">
                                        <input disabled type="text" id="Society_name" class="floating-label-field floating-label-field--s3 " placeholder="Remarks" />

                                        <span class="text-danger"></span>
                                        <label class="floating-label">Name Of the Society/Allottee </label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled placeholder="Contact No" type="text"
                                               class="floating-label-field floating-label-field--s3" />
                                        <span class="text-danger"></span>
                                        <label class="floating-label">Use as per allotment letter/Lease Deed  </label>
                                    </div>
                                </div>
                            </div>


                            <div class="row mt-md-3">
                                <div class="col-md-6">
                                    <div class="floating-label-wrap">
                                        <textarea disabled type="text" class="floating-label-field floating-label-field--s3 " placeholder="Remarks"></textarea>

                                        <span class="text-danger"></span>
                                        <label class="floating-label">Subject </label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled placeholder="Contact No" type="text"
                                               class="floating-label-field floating-label-field--s3" />
                                        @*<span asp-validation-for="GroundOfViolations" class="text-danger"></span>*@
                                        <label class="floating-label">Ground of violations  </label>
                                    </div>
                                </div>
                            </div>

                            <div class="row mt-md-3">
                                <div class="col-md-6">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled placeholder="Contact No" type="date" value=""
                                               class="floating-label-field floating-label-field--s3" />
                                        @*<span asp-validation-for="DateOfCancellationofLease" class="text-danger"></span>*@
                                        <label class="floating-label">Date of cancellation of lease Deed/Allotment </label>
                                    </div>
                                </div>



                                <div class="col-md-6">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3" style="width:100%;">
                                            <option selected disabled="disabled" value="">---Select---</option>
                                        </select>
                                        @*<span asp-validation-for="HonebleLgOrCommon" class="text-danger"></span>*@
                                        <label class="floating-label">Honble</label>
                                    </div>
                                </div>




                            </div>
                            <div class="row mt-md-3">
                                <div class=" col-md-6">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input placeholder="Address" type="text"
                                               class="floating-label-field floating-label-field--s3" disabled/>
                                        @*<span asp-validation-for="ProceedingEvictionPossession" class="text-danger"></span>*@
                                        <label class="floating-label">Proceeding for eviction of possession Assessment of damage</label>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input type="text"
                                               class="floating-label-field floating-label-field--s3" disabled/>
                                        @*<span asp-validation-for="CourtCaseifAny" class="text-danger"></span>*@
                                        <label class="floating-label">Court case if any   </label>
                                    </div>
                                </div>
                            </div>


                            <div class="row mt-md-3">

                                <div class="col-md-6">
                                    <label class="err" style="color:red;">
                                        Only Upload 10 Mb File
                                    </label>
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        @*<input type="file"  class="floating-label-field floating-label-field--s3" />


                    <label class="floating-label">Demand Letter </label>*@
                                        <a>View Demand Letter  </a>
                                    </div>


                                </div>

                                <div class="col-md-6">
                                    <label class="err" style="color:red;">
                                        Only Upload 10 Mb File
                                    </label>
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        @*<input type="file" class="floating-label-field floating-label-field--s3" />


                    <label class="floating-label">NOC</label>*@
                                        <a>View NOC Document  </a>
                                    </div>
                                </div>
                            </div>


                            <div class="row mt-md-3">

                                <div class="col-md-6">
                                    <label class="err" style="color:red;">
                                        Only Upload 10 Mb File
                                    </label>
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <a>View Cancellation Order Document  </a>
                                    </div>
                                </div>


                                <div class="col-md-6">
                                    <div class="floating-label-wrap">


                                        <select class="floating-label-field floating-label-field--s3" style="width:100%;">
                                            <option selected disabled="disabled" value="">---Select---</option>
                                        </select>
                                        @*<span asp-validation-for="UserId" class="text-danger"></span>*@
                                        <label class="floating-label">UserName</label>
                                    </div>
                                </div>

                            </div>
                        </div>



                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-header p-0" id="headingTwo">
                    <h2 class="mb-0">
                        <button class="btn-link btn btn-block collapsed d-flex sub-heading px-3 py-2 border-0 align-items-center" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                            View Notice Generations Details<i class="ri-arrow-drop-down-line ml-auto" style="font-size: 1.2rem;font-weight: 100;"></i>
                        </button>
                    </h2>
                </div>
                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
                    <div class="card-body">
                        <div id="divGenerate">


                            <div class="table-section bg-white border shadow rounded">
                                <div class="table-responsive ">
                                    <table class="table table-sm table-bordered mb-0 table-striped">
                                        <thead class="text-nowrap">

                                            <tr>
                                                <th>Meeting Date</th>
                                                <th>Meeting Time</th>
                                                <th>Meeting Place</th>

                                                <th>Operations</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>

                                                <td> 23-mar-2021</td>
                                                <td>10:00 AM</td>
                                                <td>New Delhi</td>

                                                <td>
                                                    <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter" 
                   > View Document</a>
                                                </td>

                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                            </div>



                        </div>
                    </div>
                    <link href="~/lib/bootstrap/select2/select2.css" rel="stylesheet" />

                </div>


            </div>



            </div>

            <h3 class="my-3 d-flex sub-heading pt-4">Evidance document Upload</h3>
            <div id="DivFamilyDetails">



                <div class="table-section bg-white border shadow rounded">
                    <div class="table-responsive ">
                        <table id="tbl_posts" class="table table-sm table-bordered mb-0 table-striped">
                            <thead class="text-nowrap">
                                <tr>
                                    <th>Sr.No</th>
                                    <th>Document Name</th>
                                    <th>Document Upload</th>

                                    <th>Operation</th>
                                </tr>
                            </thead>
                            <tbody id="tbl_posts_body">
                            </tbody>
                            <tbody class="tbody" id="add">
                                <tr id="">
                                    <td><span class="sn">1</span></td>
                                    <td>
                                        <input type="text"
                                               class="floating-label-field floating-label-field--s3" />
                                    </td>
                                    <td>
                                        <input type="file" class="floating-label-field floating-label-field--s3" />
                                    </td>

                                    <td>
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <a class="btn btn-theme delete-record" data-id="0" style="display:none;color:white;"><i class="fa fa-trash"></i></a>
                                            <a class="btn btn-theme add-record" data-added="0" style="color:white;">+</a>
                                        </div>
                                    </td>
                                </tr>

                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
            <div class="row pt-3">
                <div class="col-md-12">

                    @Html.ActionLink("Back", "Index", "AllotteeEvidence", null, new { @class = "btn btn-theme " })
                </div>
            </div>
</form>
</div>
