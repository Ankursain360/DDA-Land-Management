@model Libraries.Model.Entity.Requestforproceeding

@await Component.InvokeAsync("PageHeader", new { pageName = "Request for procceding" })
<div class="pt-3">
    <form asp-action="Edit" enctype="multipart/form-data" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">
            View<span style="margin-left:800px"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span>
        </h3>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.CancellationList, "Id","Allotment.Application.RefNo"))" onchange="return GetRelativeData(this.options[this.selectedIndex].value);"
                            asp-for="CancellationId">
                        <option selected disabled="disabled" value="">---Select---</option>
                        @*@foreach (var item in Model.AllotmententryList)
                            {
                                <option value="@item.Id" data-id="@item.SocietyName,@item.PurposeName" > @item.LRefNo </option>
                            }*@
                    </select>
                    <span asp-validation-for="CancellationId" class="text-danger"></span>
                    <label class="floating-label">File No<span style="color:red">*</span></label>
                </div>
            </div>
        </div>



        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <input disabled type="text" id="Society_name" class="floating-label-field floating-label-field--s3 " placeholder="Remarks" />

                    <span class="text-danger"></span>
                    <label class="floating-label">Name Of the Society </label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input disabled placeholder="Contact No" id="Purpose_name" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span class="text-danger"></span>
                    <label class="floating-label">Use as per allotment letter/Lease Deed  </label>
                </div>
            </div>
        </div>


        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <textarea type="text" asp-for="Subject" class="floating-label-field floating-label-field--s3 " placeholder="Remarks" disabled></textarea>

                    <span class="text-danger"></span>
                    <label class="floating-label">Subject </label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input placeholder="Contact No" asp-for="GroundOfViolations" type="text"
                           class="floating-label-field floating-label-field--s3" disabled />
                    <span asp-validation-for="GroundOfViolations" class="text-danger"></span>
                    <label class="floating-label">Ground of violations <span style="color:red">*</span> </label>
                </div>
            </div>
        </div>





        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input placeholder="Contact No" asp-for="DateOfCancellationofLease" type="date"
                           class="floating-label-field floating-label-field--s3" disabled />
                    <span asp-validation-for="DateOfCancellationofLease" class="text-danger"></span>
                    <label class="floating-label">Date of cancellation of lease Deed/Allotment<span style="color:red">*</span> </label>
                </div>
            </div>



            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select disabled class="floating-label-field floating-label-field--s3"
                            asp-for="HonebleLgOrCommon" asp-items="@(new SelectList(Model.HonbleList, "Id","HonbleName"))">
                        <option selected disabled="disabled" value="">---Select---</option>
                    </select>
                    <span asp-validation-for="HonebleLgOrCommon" class="text-danger"></span>
                    <label asp-for="HonebleLgOrCommon" class="floating-label">Cancellation Approving Authority<span style="color:red">*</span></label>
                </div>
            </div>




        </div>
        <div class="row mt-md-3">
            <div class=" col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input disabled asp-for="ProceedingEvictionPossession" placeholder="Address" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="ProceedingEvictionPossession" class="text-danger"></span>
                    <label class="floating-label">Proceeding for eviction of possession Assessment of damage<span style="color:red">*</span></label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input disabled asp-for="CourtCaseifAny" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="CourtCaseifAny" class="text-danger"></span>
                    <label class="floating-label">Court case if any<span style="color:red">*</span>   </label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <label style="color:#2196F3;font-size:.7rem;">
                    Demand Letter <span style="color:red"> (Only Upload 10 Mb File)</span>
                </label>
                <input type="hidden" asp-for="DemandLetter" />
                @{ if (Model.Noc != null)
                    {
                        <a class="badge bg-primary px-2" target="_blank" asp-action="ViewLetter" asp-route-id="@Model.Id"> (View File )</a>
                    }
                    else
                    {
                        <label class="badge bg-primary px-2">No File</label>
                    }
                }
            </div>
            <div class="col-md-6">
                <label style="color:#2196F3;font-size:.7rem;">
                    NOC <span style="color:red"> (Only Upload 10 Mb File)</span>
                </label>
                <input type="hidden" asp-for="Noc" />
                @{ if (Model.Noc != null)
                    {
                        <a class="badge bg-primary px-2" target="_blank" asp-action="ViewLetter1" asp-route-id="@Model.Id"> (View File )</a>
                    }
                    else
                    {
                        <label class="badge bg-primary px-2">No File</label>
                    }
                }
            </div>
        </div>


        <div class="row mt-md-3">
            <div class="col-md-6">
                <label style="color:#2196F3;font-size:.7rem;">
                    Cancellation Order <span style="color:red"> (Only Upload 10 Mb File)</span>
                </label>
                <input type="hidden" asp-for="CancellationOrder" />
                @{ if (Model.CancellationOrder != null)
                    {<a class="badge bg-primary px-2" target="_blank" asp-action="ViewLetter2" asp-route-id="@Model.Id"> (View File )</a>
                    }
                    else
                    {
                        <label class="badge bg-primary px-2">No File</label>
                    }
                }
            </div>
            <div class=" col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" asp-for="IsActive" disabled>
                        <option value="1">Active</option>
                        <option value="0">Inactive</option>
                    </select>
                    <span asp-validation-for="IsActive" class="text-danger"></span>
                    <label asp-for="IsActive" class="floating-label">Status </label>
                </div>

            </div>
        </div>

        <div class="row mt-md-3">
            <div class=" col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="LetterReferenceNo" placeholder="Address" type="text" disabled
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="LetterReferenceNo" class="text-danger"></span>
                    <label class="floating-label">Letter Reference No<span style="color:red">*</span></label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" disabled
                            asp-for="UserId" asp-items="@(new SelectList(Model.UserNameList, "Id","Name"))">
                        <option selected disabled="disabled" value="">---Select---</option>
                    </select>
                    <span asp-validation-for="UserId" class="text-danger"></span>
                    <label class="floating-label">UserName<span style="color:red">*</span></label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class=" col-md-12">
                @Html.ActionLink("Back", "Index", "RequestForProceedingEviction", null, new { @class = "btn btn-theme " })
            </div>
        </div>
    </form>


</div>


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/requestforproceedingview.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/requestforproceedingview.min.js" asp-append-version="true"></script>
</environment>