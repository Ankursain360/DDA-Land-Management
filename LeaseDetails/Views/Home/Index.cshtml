@model Dto.Master.UserProfileDto

<h1 class="border-bottom">Welcome to DDA Land Information Management Application</h1>
<div class="profile-box bg-white rounded border p-3 my-4 shadow d-md-flex align-items-center py-lg-4 px-xl-5">
    <figure class="mx-auto my-0">
        <img class="d-block rounded-circle mx-auto shadow-sm" src="~/images/noimage.jpg" alt="">
    </figure>

    <div class="text-center detail py-3 text-md-left px-md-4 mx-lg-auto px-xl-5 ml-xl-0">
        <h2 class="mb-2">@Model.User.Name</h2>
        @*<p class="text-muted mb-2 mb-md-1">Last Login : <strong>22 Oct 2020 02:33 PM</strong></p>
            <p class="text-muted">Full time designer <strong>(DDA)</strong></p>*@
    </div>
    <div class="py-3 pl-md-4 mx-lg-auto pr-xl-5 ml-xl-0 pl-xl-2">
        <table class="table table-borderless table-sm m-0">
            <tbody>
                <tr>
                    <th scope="row">Designation</th>
                    <td>@Model.Role.Name</td>
                </tr>

                <tr>
                    <th scope="row">Name</th>
                    <td colspan="2">@Model.User.Name</td>
                </tr>
                <tr>
                    <th scope="row">Email Id</th>
                    <td colspan="2">@Model.User.Email</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="row">

    <div class=" col-md-6">
        <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
            <h3 class="mb-3 d-flex">Received Kyc Application Form Status </h3>
            <canvas id="ChartForKycForm" aria-label="chart" role="img" width="400" height="300">
            </canvas>
        </div>
    </div>
    <div class=" col-md-6">
        <div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
            <h3 class="mb-3 d-flex">Received Kyc Demand Payment Form Status </h3>
            <canvas id="ChartForKycPaymentForm" aria-label="chart" role="img" width="400" height="300">
            </canvas>
        </div>
    </div>
    </div>

    <environment include="Development">
        <script src="~/js/Chart.js"></script>
        <script src="~/js/custom.js"></script>
    </environment>
    <environment exclude="Development">
       
        <script src="~/js/Chart.js" asp-append-version="true"></script>
        <script src="~/js/custom.js" asp-append-version="true"></script>
    </environment>
    @*<figure class="m-0 mb-md-n3">
            <img class="img-fluid d-block mx-auto" src="~/images/brand.png" alt="dda">
        </figure>*@
