                                    <span class="text-danger"></span>
                                    <label class="floating-label">Use as per allotment letter/Lease Deed  </label>
                                </div>
                            </div>
                        </div>


                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap">
                                    <textarea disabled type="text" class="floating-label-field floating-label-field--s3 " placeholder="Remarks"></textarea>

                                    <span class="text-danger"></span>
                                    <label class="floating-label">Subject </label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled placeholder="Contact No" type="text"
                                           class="floating-label-field floating-label-field--s3" />
                                    @*<span asp-validation-for="GroundOfViolations" class="text-danger"></span>*@
                                    <label class="floating-label">Ground of violations  </label>
                                </div>
                            </div>
                        </div>

                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled placeholder="Contact No" type="date" value=""
                                           class="floating-label-field floating-label-field--s3" />
                                    @*<span asp-validation-for="DateOfCancellationofLease" class="text-danger"></span>*@
                                    <label class="floating-label">Date of cancellation of lease Deed/Allotment </label>
                                </div>
                            </div>



                            <div class="col-md-6">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3" style="width:100%;">
                                        <option selected disabled="disabled" value="">---Select---</option>
                                    </select>
                                    @*<span asp-validation-for="HonebleLgOrCommon" class="text-danger"></span>*@
                                    <label class="floating-label">Honble</label>
                                </div>
                            </div>




                        </div>
                        <div class="row mt-md-3">
                            <div class=" col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input placeholder="Address" type="text"
                                           class="floating-label-field floating-label-field--s3" disabled />
                                    @*<span asp-validation-for="ProceedingEvictionPossession" class="text-danger"></span>*@
                                    <label class="floating-label">Proceeding for eviction of possession Assessment of damage</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input type="text"
                                           class="floating-label-field floating-label-field--s3" disabled />
                                    @*<span asp-validation-for="CourtCaseifAny" class="text-danger"></span>*@
                                    <label class="floating-label">Court case if any   </label>
                                </div>
                            </div>
                        </div>


                        <div class="row mt-md-3">

                            <div class="col-md-6">
                               
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    @*<input type="file"  class="floating-label-field floating-label-field--s3" />


                                    <label class="floating-label">Demand Letter </label>*@
                                    <a>View Demand Letter  </a>
                                </div>


                            </div>

                            <div class="col-md-6">
                                
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    @*<input type="file" class="floating-label-field floating-label-field--s3" />


                                    <label class="floating-label">NOC</label>*@
                                    <a>View NOC Document  </a>
                                </div>
                            </div>
                        </div>


                        <div class="row mt-md-3">

                            <div class="col-md-6">
                               
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <a>View Cancellation Order Document  </a>
                                </div>
                            </div>


                            <div class="col-md-6">
                                <div class="floating-label-wrap">


                                    <select class="floating-label-field floating-label-field--s3" style="width:100%;">
                                        <option selected disabled="disabled" value="">---Select---</option>
                                    </select>
                                    @*<span asp-validation-for="UserId" class="text-danger"></span>*@
                                    <label class="floating-label">UserName</label>
                                </div>
                            </div>

                        </div>
                    </div>



                </div>
            </div>
        </div>
        <div class="card">
            <div class="card-header p-0" id="headingTwo">
                <h2 class="mb-0">
                    <button class="btn-link btn btn-block collapsed d-flex sub-heading px-3 py-2 border-0 align-items-center" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                        View Notice Generations Details<i class="ri-arrow-drop-down-line ml-auto" style="font-size: 1.2rem;font-weight: 100;"></i>
                    </button>
                </h2>
            </div>
            <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
                <div class="card-body">
                    <div id="divGenerate">


                        <div class="table-section bg-white border shadow rounded">
                            <div class="table-responsive ">
                                <table class="table table-sm table-bordered mb-0 table-striped">
                                    <thead class="text-nowrap">

                                        <tr>
                                            <th>Meeting Date</th>
                                            <th>Meeting Time</th>
                                            <th>Meeting Place</th>

                                            <th>Operations</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>

                                            <td> 23-mar-2021</td>
                                            <td>10:00 AM</td>
                                            <td>New Delhi</td>

                                            <td>
                                                <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter"> View Document</a>
                                            </td>

                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>



                    </div>
                </div>
                <link href="~/lib/bootstrap/select2/select2.css" rel="stylesheet" />

            </div>


        </div>

        <div class="card">
            <div class="card-header p-0" id="headingThree">
                <h2 class="mb-0">
                    <button class="btn-link btn btn-block collapsed d-flex sub-heading px-3 py-2 border-0 align-items-center" type="button" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                        Evidance Upload Document Details <i class="ri-arrow-drop-down-line ml-auto" style="font-size: 1.2rem;font-weight: 100;"></i>
                    </button>
                </h2>
            </div>
            <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordionExample">
                <div class="card-body">
                    <div id="divGenerate">


                        <div class="table-section bg-white border shadow rounded">
                            <div class="table-responsive ">
                                <table class="table table-sm table-bordered mb-0 table-striped">
                                    <thead class="text-nowrap">

                                        <tr>
                                            <th>User Name</th>
                                            <th>Document Details</th>
                                            <th>Operations</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>

                                            <td> Test DD Name</td>
                                            <td>Doc Name </td>
                                            

                                            <td>
                                                <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter"> View Document</a>
                                            </td>

                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>



                    </div>
                </div>
                <link href="~/lib/bootstrap/select2/select2.css" rel="stylesheet" />
            </div>
        </div>
        
        <div class="card">
            <div class="card-header p-0" id="headingFive">
                <h2 class="mb-0">
                    <button class="btn-link btn btn-block collapsed d-flex sub-heading px-3 py-2 border-0 align-items-center" type="button" data-toggle="collapse" data-target="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                      Previous Hearing Details<i class="ri-arrow-drop-down-line ml-auto" style="font-size: 1.2rem;font-weight: 100;"></i>
                    </button>
                </h2>
            </div>
            <div id="collapseFive" class="collapse" aria-labelledby="headingFive" data-parent="#accordionExample">
                <div class="card-body">
                    <div class="table-section bg-white border shadow rounded">
                        <div class="table-responsive ">
                            <table class="table table-sm table-bordered mb-0 table-striped">
                                <thead class="text-nowrap">

                                    <tr>
                                        <th>Hearing Venue</th>
                                        <th>Hearing Date</th>
                                        <th>Hearing Time</th>
                                         <th>Attendee</th>
                                        <th>Hearing  Remarks</th>
                                        <th>Document</th>
                                        <th>Operation</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>High Court, Delhi</td>
                                        <td>01-Apr -2021</td>
                                        <td>04:30 Pm</td>
                                        <td>Attandee Details</td>
                                        <td>This is dummy data only.</td>
                                        <td> <a class="badge badge-primary px-2"> View Document</a></td>

                                        <td>
                                            <div class="action-btns">
                                                <a class="edit" asp-action="Create"><i class="ri-edit-line"></i> </a>
                                                </div></td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <h3 class="my-3 d-flex sub-heading">Add New Hearing Details </h3>



    <div class="row">


        <div class="col-md-6">
            <div class="floating-label-wrap mb-3 mb-md-0">


                <input class="floating-label-field floating-label-field--s3" placeholder="Hearing Date" />
                <span class="text-danger"></span>
                <label class="floating-label">Hearing Date</label>
            </div>
        </div>
        <div class="col-md-6">
            <div class="floating-label-wrap mb-3 mb-md-0">

                <input class="floating-label-field floating-label-field--s3" placeholder="Hearing Venue" />
                <span class="text-danger"></span>
                <label class="floating-label">Hearing Time  </label>
            </div>
        </div>
    </div>

    <div class="row mt-md-3">
        <div class="col-md-6">

            <div class="floating-label-wrap mb-3 mb-md-0">

                <input class="floating-label-field floating-label-field--s3" placeholder="Hearing Time" />
                <span class="text-danger"></span>
                <label class="floating-label">Hearing Venue </label>
            </div>
        </div>

        <div class="col-md-6">
            <div class="floating-label-wrap mb-3 mb-md-0">

                <input class="floating-label-field floating-label-field--s3" placeholder="Hearing Venue" disabled/>
                <span class="text-danger"></span>
                <label class="floating-label">Attendee </label>
            </div>
        </div>
    </div>
    <div class="row mt-md-3">
        <div class="col-md-12">
            <div class="floating-label-wrap mb-3 mb-md-0">

                <textarea class="floating-label-field floating-label-field--s3" placeholder="Hearing Remarks" disabled>
                </textarea>
                <span class="text-danger"></span>
                <label class="floating-label">Hearing Remarks </label>
            </div>
        </div>

    </div>
    <div class="row mt-md-3">
        <div class="col-md-6">
            <div class="floating-label-wrap mb-3 mb-md-0">


                <input class="floating-label-field floating-label-field--s3" placeholder="Hearing Documents" type="file" disabled/>
                <span class="text-danger"></span>
                <label class="floating-label">Hearing Documents (if any)</label>
            </div>
        </div>
        <div class=" col-md-6">
            <div class="floating-label-wrap">
                <select class="floating-label-field floating-label-field--s3">
                    <option value="1">Active</option>
                    <option value="0">Inactive</option>
                </select>
                <span class="text-danger"></span>
                <label class="floating-label">Status </label>
            </div>
        </div>
    </div>







    <div class="row pt-3">
        <div class="col-md-12">

            <div class="row pt-3">
                <div class="col-md-12">
                    <input type="submit" value="Submit" class="btn btn-theme " />
                   
                    @Html.ActionLink("Back", "Index", "Hearing Details", null, new { @class = "btn btn-theme " })
                </div>
            </div>
        </div>
    </div>
</form>

