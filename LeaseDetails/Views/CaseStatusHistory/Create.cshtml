        <div class="permission mt-2">
            <h3 class=" accordion list-unstyled m-0">View Notice Generations Details </h3>

            <div class="pt-4" id="NoticeGenerationDiv">

            </div>
        </div>

        <br />


        <div class="permission mt-2">
            <h3 class=" accordion list-unstyled m-0">View Allottee Evidance Document Details </h3>

            <div class="pt-4" id="AllotteeEvidenceDiv">

            </div>
        </div>





        <div class="permission mt-2">
            <h3 class=" accordion list-unstyled m-0">View Hearing Details</h3>

            <div class="pt-4" id="HearingDetailsDiv">

            </div>
        </div>




        <div class="permission mt-2">
            <h3 class=" accordion list-unstyled m-0">Judgement Details</h3>

            <div class="pt-4" id="judgement">

                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" style="width:100%;"
                                    asp-for="ForwardToUserId" asp-items="@(new SelectList(Model.UserNameList, "Id","Name"))">
                                <option disabled="disabled" selected value="">Select</option>

                            </select>
                            <span asp-validation-for="ForwardToUserId" class="text-danger"></span>
                            <label class="floating-label">Forward To</label>
                        </div>
                    </div>
                    <div class=" col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            @{ if (Model.FilePath != null)
                                {
                                    <a class="badge badge-primary px-2" asp-action="DownloadJudgementFile" asp-route-id="@Model.RequestForProceedingId"> (View Existing Document)</a>
                                    <input type="file" asp-for="File" class="floating-label-field floating-label-field--s3" placeholder="Name">
                                    <span asp-validation-for="File" class="d-block text-danger field-validation-valid"></span>
                                }
                                else
                                {
                                    <input type="file" asp-for="File" class="floating-label-field floating-label-field--s3" placeholder="Name">
                                    <span asp-validation-for="File" class="d-block text-danger field-validation-valid"></span>
                                    <label for="field-1" class="floating-label"> Upload copy of the document selected </label> } }


                        </div>


                    </div>

                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" style="width:100%;"
                                    asp-for="JudgementStatusId" asp-items="@(new SelectList(Model.JudgementStatusList, "Id","Status"))">
                                <option disabled="disabled" selected value="">Select</option>

                            </select>
                            <span asp-validation-for="JudgementStatusId" class="text-danger"></span>
                            <label class="floating-label">Judgement Status</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea asp-for="Remarks" placeholder="Description" class="floating-label-field floating-label-field--s3"></textarea>

                            <span asp-validation-for="Remarks" class="text-danger"></span>
                            <label class="floating-label"> Remarks </label>
                        </div>
                    </div>
                </div>



            </div>
        </div>
        <div class="permission mt-2">
            <h3 class=" accordion list-unstyled m-0">View Action Taken By DDA Details</h3>

            <div class="pt-4" id="ActionDetailsDiv">

            </div>
        </div>

        <div class="row mt-md-3">
            <div class=" col-md-12">
                @Html.ActionLink("Back", "Index", "CaseStatusHistory", null, new { @class = "btn btn-theme " })
            </div>
        </div>
    </form>
</div>



<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/casestatushistory.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/casestatushistory.min.js" asp-append-version="true"></script>
</environment>
