@model Libraries.Model.Entity.Requestforproceeding

@*<div class="permission mt-2 accordion list-unstyled m-0 ">
    <div class="card">
        <a class="p-2 no-dec d-flex align-items-center" data-toggle="collapse" href="#collapseRequestProceeding" role="button" aria-expanded="true"
           aria-controls="collapseRequestProceeding">
            <i class="ri-arrow-right-s-fill"></i>
            <span class="text ml-1 mr-auto">View Request Proceeding For Eviction Details </span>
            <i class="arrow ri-arrow-drop-down-line slow"></i>
        </a>*@
        @*<div class="list-unstyled m-0 pl-2 pr-1 pl-md-4 pr-md-3 collapse" id="collapseRequestProceeding">*@
            <div id="allDetails">
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3"
                                    asp-for="AllotmentId" asp-items="@(new SelectList(Model.AllotmententryList, "Id","LRefNo"))" disabled>
                                <option selected disabled="disabled" value="">---Select---</option>
                            </select>
                            <label class="floating-label">File No</label>
                        </div>
                    </div>
                    <div class=" col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="LetterReferenceNo" placeholder="Address" type="text"
                                   class="floating-label-field floating-label-field--s3" disabled />
                            <span class="text-danger"></span>
                            <label class="floating-label">Letter Reference No</label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input disabled type="text" class="floating-label-field floating-label-field--s3 " placeholder="Remarks" />
                            <span class="text-danger"></span>
                            <label class="floating-label">Name Of the Society </label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input disabled placeholder="Contact No" type="text"
                                   class="floating-label-field floating-label-field--s3" />
                            <span class="text-danger"></span>
                            <label class="floating-label">Use as per allotment letter/Lease Deed  </label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <textarea type="text" asp-for="Subject" class="floating-label-field floating-label-field--s3 " placeholder="Remarks" disabled></textarea>

                            <span class="text-danger"></span>
                            <label class="floating-label">Subject </label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input placeholder="Contact No" asp-for="GroundOfViolations" type="text"
                                   class="floating-label-field floating-label-field--s3" disabled />
                            <span class="text-danger"></span>
                            <label class="floating-label">Ground of violations  </label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input placeholder="Contact No" asp-for="DateOfCancellationofLease" type="date" value=""
                                   class="floating-label-field floating-label-field--s3" disabled />
                            <span class="text-danger"></span>
                            <label class="floating-label">Date of cancellation of lease Deed/Allotment </label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3"
                                    asp-for="HonebleLgOrCommon" asp-items="@(new SelectList(Model.HonbleList, "Id","HonbleName"))">
                                <option selected disabled="disabled" value="">---Select---</option>
                            </select>
                            <span asp-validation-for="HonebleLgOrCommon" class="text-danger"></span>
                            <label asp-for="HonebleLgOrCommon" class="floating-label">Honble</label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class=" col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input asp-for="ProceedingEvictionPossession" placeholder="Address" type="text"
                                   class="floating-label-field floating-label-field--s3" disabled />
                            <span class="text-danger"></span>
                            <label class="floating-label">Proceeding for eviction of possession Assessment of damage</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input asp-for="CourtCaseifAny" type="text"
                                   class="floating-label-field floating-label-field--s3" disabled />
                            <span class="text-danger"></span>
                            <label class="floating-label">Court case if any   </label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <label style="color:#2196F3;font-size:.7rem;">
                                Demand Letter
                            </label>
                            <input type="hidden" asp-for="DemandLetter" />

                            @{ if (Model.DemandLetter != null)
                                {
                                    <input type="file" onchange="upload_check();" placeholder="Layout Plan" id="file_id" class="floating-label-field floating-label-field--s3" asp-for="DemandLetterPhoto" disabled />
                                    <label for="field-1" class="floating-label">Demand Letter </label>
                                    <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter" asp-route-id="@Model.Id"> (View File )</a>
                                }
                                else
                                {
                                    <label class="badge badge-primary px-2">No File</label>
                                    <input type="file" asp-for="DemandLetterPhoto" disabled class="floating-label-field floating-label-field--s3">

                                }
                            }
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <label style="color:#2196F3;font-size:.7rem;">
                                NOC
                            </label>
                            <input type="hidden" asp-for="Noc" />
                            @{ if (Model.Noc != null)
                                {
                                    <input type="file" onchange="upload_check();" placeholder="Layout Plan" id="file_id" class="floating-label-field floating-label-field--s3" asp-for="NocPhoto" disabled />
                                    <label for="field-1" class="floating-label">NOC </label>
                                    <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter" asp-route-id="@Model.Id"> (View File )</a>
                                }
                                else
                                {
                                    <label class="badge badge-primary px-2">No File</label>
                                    <input type="file" asp-for="NocPhoto" class="floating-label-field floating-label-field--s3" disabled>

                                }
                            }
                        </div>
                    </div>
                </div>

                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <label style="color:#2196F3;font-size:.7rem;">
                                Cancellation Order
                            </label>
                            <input type="hidden" asp-for="CancellationOrder" />

                            @{ if (Model.CancellationOrder != null)
                                {
                                    <input type="file" onchange="upload_check();" placeholder="Layout Plan" id="file_id" class="floating-label-field floating-label-field--s3" asp-for="CancellationPhoto" disabled />
                                    <label for="field-1" class="floating-label">NOC </label>
                                    <a class="badge badge-primary px-2" target="_blank" asp-action="ViewLetter" asp-route-id="@Model.Id"> (View File )</a>
                                }
                                else
                                {
                                    <label class="badge badge-primary px-2">No File</label>
                                    <input type="file" asp-for="CancellationPhoto" class="floating-label-field floating-label-field--s3" disabled>

                                }
                            }

                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" style="width:100%;">
                                <option selected disabled="disabled" value="">---Select---</option>
                            </select>
                            @*<span asp-validation-for="UserId" class="text-danger"></span>*@
                            <label class="floating-label">UserName</label>
                        </div>
                    </div>
                </div>

            </div>
        @*</div>
    </div>
</div>*@
<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
