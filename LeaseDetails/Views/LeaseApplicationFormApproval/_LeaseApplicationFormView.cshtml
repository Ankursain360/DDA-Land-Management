                            <span asp-validation-for="FileNo" class="text-danger"></span>
                            <label class="floating-label">Document Upload</label>
                        </div>
                    </div>*@
            </div>
            <div class="row mt-md-3">
                <div class="col-md-12">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <textarea disabled asp-for="SponsorshipDescription" type="text" class="floating-label-field floating-label-field--s3 " limit="4000"></textarea>
                        <span asp-validation-for="SponsorshipDescription" class="text-danger"></span>
                        <label class="floating-label">Description</label>
                    </div>
                </div>
            </div>
            <h3 class="my-3 d-flex sub-heading">
