                <div class="col-md-12">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <textarea disabled asp-for="NotarizedUndertakingDescription" type="text" class="floating-label-field floating-label-field--s3 " limit="4000"></textarea>
                        <span asp-validation-for="NotarizedUndertakingDescription" class="text-danger"></span>
                        <label class="floating-label">Description</label>
                    </div>
                </div>
                @*<div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input  disabled asp-for="FileNo" type="file" class="floating-label-field floating-label-field--s3 ">
                            <span asp-validation-for="FileNo" class="text-danger"></span>
                            <label class="floating-label">Document Upload</label>
                        </div>
                    </div>*@
            </div>
