            <div class="row mt-md-3">
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input disabled asp-for="RefNoOfAllotmentLetterDate" type="text" class="floating-label-field floating-label-field--s3 " limit="200" />
                        <span asp-validation-for="RefNoOfAllotmentLetterDate" class="text-danger"></span>
                        <label class="floating-label">Ref. no. of allotment letter & date</label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input disabled asp-for="AreaSqlMt" type="text" class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="AreaSqlMt" class="text-danger"></span>
                        <label class="floating-label">Area in sq. mtrs.</label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input disabled asp-for="Locality" type="text" class="floating-label-field floating-label-field--s3 " limit="500" />
                        <span asp-validation-for="Locality" class="text-danger"></span>
                        <label class="floating-label">Locality</label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input disabled asp-for="Purpose" type="text" class="floating-label-field floating-label-field--s3 " limit="4000" />
                        <span asp-validation-for="Purpose" class="text-danger"></span>
                        <label class="floating-label">Purpose</label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input disabled asp-for="Rate" type="text" class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="Rate" class="text-danger"></span>
                        <label class="floating-label">Rate</label>
                    </div>
                </div>
            </div>

            <div id="DivDocument">
                <h3 class="my-3 d-flex sub-heading">Document</h3>
                <div class="table-section bg-white border shadow rounded">
                    <div class="table-responsive ">
                        <table class="table table-sm table-bordered mb-0 table-striped">
                            <thead class="text-nowrap">
                                <tr>
                                    <th>Document Name</th>
                                    <th>Uploaded Document</th>
                                </tr>
                            </thead>
                            <tbody class="tbody">
                                @foreach (var item in Model.Leasedocuments)
                                {
                                    <tr>
                                        <td>
                                            @item.DocumentChecklist.Name
                                        </td>
                                        @if (@item.DocumentFileName != "")
                                        {
                                            <td>
                                                <div class="floating-label-wrap mb-3 mb-md-0">
                                                    <a class="badge badge-primary px-2" asp-action="ViewDocumentLeaseApplicationForm" asp-route-id="@item.Id" target="_blank"> (Enclosed)</a>
                                                    @*<a>Enclosed</a>*@
                                                </div>
                                            </td>
                                        }
                                        else
                                        {
                                            <td>  <a>N/A</a></td>
                                        }
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</form>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/encroachmentregisterationview.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/encroachmentregisterationview.min.js" asp-append-version="true"></script>
</environment>