                <div class="col-md-12">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <textarea disabled asp-for="LandDetailsArea" type="text" class="floating-label-field floating-label-field--s3  " limit="4000"></textarea>
                        <span asp-validation-for="LandDetailsArea" class="text-danger"></span>
                    </div>
                </div>
            </div>
