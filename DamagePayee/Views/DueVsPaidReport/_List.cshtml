@model IEnumerable<Dto.Search.PaymentTransactionReportListDataDto>


<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>

            <th>File No.</th>

            <th>Locality </th>
            <th>Payee Name</th>
            <th>Property No</th>
          
            <th>Property/Plot Area (Sq.Yrds)</th>
            <th>Demand No.</th>
            <th>Demand Date</th>
            <th>Demand Amount</th>
        </tr>

    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
        <tr>

            <td>@item.FileNo</td>
            <td>@item.LocalityName</td>
           
            <td>@item.PayeeName</td>
            <td>@item.PropertyNo</td>
           
            <td>@item.PropertyArea</td>
            <td>@item.DemandNo</td>
            <td>@item.DemandDate</td>
            <td>@item.DemandAmount</td>
        </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="14" style="text-align:center"> No Record Available! </th>
            </tr>
        }
    </tbody>

</table>

