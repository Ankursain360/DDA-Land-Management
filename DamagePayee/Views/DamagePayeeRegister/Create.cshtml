                                    </td>
                                    <td>

                                        <input asp-for="AadharNo" limit="12"  type="text" maxlength="15" class="floating-label-field floating-label-field--s3 numbers" />
                                        <span asp-validation-for="AadharNo" class="d-block text-danger field-validation-valid msg3"></span>

                                        <input type="hidden" asp-for="AadharNoFilePath" />
                                        <br />
                                        @**********  new upload btn design ***********@
                                        @*<div class="d-flex align-items-center">

                <div class="file-btn my-3 d-flex align-items-center me-3">
                    <input asp-for="Aadhar" type="file" class="browes d-none" placeholder="Name"  value="">
                    <label for="Aadhar" class="btn btn-theme px-4 mb-0">Browse</label>
