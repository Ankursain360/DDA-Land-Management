                </div>
            </div>*@
                     <input asp-for="Aadhar"  type="file" class="floating-label-field floating-label-field--s3 checkExtension" />
                                        <span asp-validation-for="Aadhar" class="d-block text-danger field-validation-valid"></span>


                                    </td>
                                    <td>

                                        <input asp-for="PanNo"  maxlength="10" type="text" class="floating-label-field floating-label-field--s3 "onchange="return chkvalid(this);" />
                                        <span asp-validation-for="PanNo" class="d-block text-danger field-validation-valid msgpan"></span>

                                        <input type="hidden" asp-for="PanNoFilePath" />
                                        <br />
                                        <input asp-for="Pan" type="file" class="floating-label-field floating-label-field--s3 checkExtension" />
                                        <span asp-validation-for="Pan" class="d-block text-danger field-validation-valid msg4"></span>


                                    </td>
                                    <td>
                                        <input type="hidden" asp-for="PhotographFilePath" />

                                        <input  onchange="fileCheck(this);" asp-for="Photograph" required type="file" class="floating-label-field floating-label-field--s3 checkExtension" />
                                        <span asp-validation-for="Photograph" class="d-block text-danger field-validation-valid"></span>

                                    </td>
                                    <td>
                                        <input type="hidden" asp-for="SignatureFilePath" />

                                        <input asp-for="SignatureFile" required type="file" class="floating-label-field floating-label-field--s3 checkExtension" />
                                        <span asp-validation-for="SignatureFile" class="d-block text-danger field-validation-valid"></span>

                                    </td>
                                    @*<td>
                                        <input type="hidden" asp-for="OtherDocFilePath" />

                                        <input asp-for="OtherDocFile" type="file" class="floating-label-field floating-label-field--s3" />
                                        <span asp-validation-for="OtherDocFile" class="d-block text-danger field-validation-valid"></span>

                                    </td>*@
                                    <td>
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <a class="btn btn-theme delete-record" data-id="0" style="display:none;color:white;"><i class="fa fa-trash"></i></a>
