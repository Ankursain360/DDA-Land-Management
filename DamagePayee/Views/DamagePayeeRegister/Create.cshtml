                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="CourtName" type="text" placeholder="Name of Court" class="floating-label-field floating-label-field--s3" />
                            <span asp-validation-for="CourtName" class="d-block text-danger field-validation-valid"></span>

                            <label class="floating-label">Name of Court <span style="color:red">*</span></label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="CaseNo" type="text" placeholder="Case Number" class="floating-label-field floating-label-field--s3" />
                            <span asp-validation-for="CaseNo" class="d-block text-danger field-validation-valid"></span>

                            <label class="floating-label">Case Number <span style="color:red">*</span></label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
