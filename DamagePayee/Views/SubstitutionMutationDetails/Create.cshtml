@model Libraries.Model.Entity.Mutationdetails

@await Component.InvokeAsync("PageHeader", new { pageName = "Apply For Mutation" })
<br />
<br />

<form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">
    <h3 class="mb-3 d-flex">File Information</h3>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>


    <div id="DivForFileName">

        <div class="row">

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="FileNo" type="text" placeholder="File No" required class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="FileNo" class="d-block text-danger field-validation-valid"></span>
                    <label class="floating-label">File No.</label>
                    <br />
                </div>
            </div>

        </div>

    </div>

    <div id="DivForPreviousDamagee">

