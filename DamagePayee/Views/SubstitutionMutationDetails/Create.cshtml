<form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">
    <h3 class="mb-3 d-flex">File Information</h3>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div id="DivFileInformation">

        <h3 class="my-3 d-flex sub-heading">File Information</h3>

        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="DamagePayeeRegister.FileNo" type="text" disabled placeholder="File No." class="floating-label-field floating-label-field--s3" />
                    <label class="floating-label">File No.</label>
                </div>
            </div>
        </div>
    </div>

    <div id="DivPersonalInformationofDamageAssessee">
        <h3 class="my-3 d-flex sub-heading">Personal Information of Damage Assessee</h3>

        <div class="table-section bg-white border shadow rounded">
            <div class="table-responsive">
                <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_posts">
                    <thead class="text-nowrap">

                        <tr>
                            <th>Sr.No</th>
                            <th>Name</th>
                            <th>Father's/Husband's Name</th>
                            <th style="min-width:100px;">Gender</th>
                            <th>Address</th>
                            <th>Mobile No.</th>
                            <th>Email-Id</th>
                            <th style="min-width:200px;">Aadhar No.</th>
                            <th style="min-width:200px;">Pan No.</th>
                            <th style="min-width:200px;">Photo</th>
                            <th style="min-width:200px;">Signature</th>
                            <th>Operation</th>
                        </tr>
                    </thead>
                    <tbody id="tbl_posts_body">
                    </tbody>
                    <tbody class="tbody" id="add">

                        <tr id="">
                            <td style="text-align:center;"><span class="sn">1</span></td>
                            <td>
                                <input asp-for="DamagePayeeRegister.payeeName" type="text" id="txtPersonalName" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.payeeName" class="d-block text-danger field-validation-valid"></span>
                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.payeeFatherName" type="text" id="txtPersonalFatherName" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.payeeFatherName" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>

                                <select asp-for="DamagePayeeRegister.Gender" class="floating-label-field floating-label-field--s3" id="drpPersonalGender">
                                    <option value="0">---Select---</option>
                                    <option value="1">Male</option>
                                    <option value="2">Female</option>
                                    <option value="3">Other</option>
                                </select>
                                <span asp-validation-for="DamagePayeeRegister.Gender" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.Address" type="text" id="txtPersonalAddress" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.Address" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.MobileNo" type="number" id="txtPersonalMobileNo" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.MobileNo" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.EmailId" type="text" id="txtPersonalEmailid" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.EmailId" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="DamagePayeeRegister.AadharNo" type="text" id="txtAadharNo" placeholder="Aadhar No." class=" floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.AadharNo" class="d-block text-danger field-validation-valid"></span>

                                    <input asp-for="DamagePayeeRegister.Aadhar" type="file" id="AadharFile" class="floating-label-field floating-label-field--s3 " />
                                    <span asp-validation-for="DamagePayeeRegister.Aadhar" class="d-block text-danger field-validation-valid"></span>

                                </div>
                            </td>
                            <td>
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="DamagePayeeRegister.PanNo" type="text" id="txtPanNo" placeholder="Pan No." class="  floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.PanNo" class="d-block text-danger field-validation-valid"></span>

                                    <input asp-for="DamagePayeeRegister.Pan" type="file" id="PanNoFile" class="floating-label-field floating-label-field--s3 " />
                                    <span asp-validation-for="DamagePayeeRegister.Pan" class="d-block text-danger field-validation-valid"></span>

                                </div>
                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.Photograph" type="file" id="Photograph" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.Photograph" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.SignatureFile" type="file" id="Signature" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.SignatureFile" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <a class="btn btn-theme delete-record" data-id="0" style="display:none;color:white;"><i class="fa fa-trash"></i></a>
                                    <a class="btn btn-theme add-record" data-added="0" style="color:white;"><i class="fa fa-plus"></i></a>
                                </div>
                            </td>

                        </tr>
                    </tbody>
                </table>
            </div>

        </div>

    </div>
    <br />
    <div id="Divclsification" class="mt-md-3">
        <h3 class="my-3 d-flex sub-heading">Type of Damage Assessee</h3>

        <div class="row">
            <div class="col-md-3">
                <input type="hidden" asp-for="DamagePayeeRegister.TypeOfDamageAssessee" disabled id="TypeOfDamageAssessee" value="Original" />

                <div class="form-check theme">
                    <input class="form-check-input" type="radio" asp-for="DamagePayeeRegister.TypeOfDamageAssessee" name="grpDamageAssesseeType" id="rOriginal" value="Original" checked>
                    <label class="form-check-label" for="radio">
                        Original
                    </label>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-check theme">
                    <input class="form-check-input" type="radio" asp-for="DamagePayeeRegister.TypeOfDamageAssessee" name="grpDamageAssesseeType" id="rSubsequent" value="Subsequent">
                    <label class="form-check-label" for="radio">
                        Subsequent Purchaser/Occupier
                    </label>
                </div>
            </div>
        </div>
    </div>

    <div id="DivForSubsequentPurchaser" style="display:none">

        <h3 class="my-3 d-flex sub-heading">Chain of Previous Damage Assessee, starting from Original Damage Assessee</h3>

        <div class="table-section bg-white border shadow rounded">
            <div class="table-responsive">
                <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_DamageAssessee">
                    <thead class="text-nowrap">
                        <tr>
                            <th>Sr.No</th>
                            <th>Name</th>
                            <th>Father's Name</th>
                            <th>Date of GPA/Will/Other Deed</th>
                            <th>Upload ATS & GPA/Will</th>
                            <th>Option</th>
                        </tr>
                    </thead>
                    <tbody id="tbl_DamageAssessee_body">
                    </tbody>
                    <tbody class="tbody" id="addDamageAssessee">
                        <tr id="">
                            <td style="text-align:center;"><span class="sn1">1</span></td>
                            <td>
                                <input asp-for="DamagePayeeRegister.Name" maxlength="45" type="text" class="floating-label-field floating-label-field--s3" id="txtDamageAssesseeName" />
                                <span asp-validation-for="DamagePayeeRegister.Name" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.FatherName" type="text" class="floating-label-field floating-label-field--s3" id="txtDamageAssesseeFather" />
                                <span asp-validation-for="FatherName" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.Date" type="date" class="floating-label-field floating-label-field--s3" id="txtDateofWill" />
                                <span asp-validation-for="DamagePayeeRegister.Date" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input asp-for="DamagePayeeRegister.ATSGPA" type="file" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="DamagePayeeRegister.ATSGPA" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <a class="btn btn-theme delete-recordDamageAssessee" data-id="0" style="display:none;color:white;"><i class="fa fa-trash"></i></a>
                                <a class="btn btn-theme add-recordDamageAssessee" data-added="0" style="color:white;"><i class="fa fa-plus"></i></a>
                            </td>

                        </tr>

                    </tbody>
                </table>
            </div>
        </div>

    </div>

    <div id="MainContentForm">
        <h3 class="my-3 d-flex sub-heading">Property Identification Details</h3>

