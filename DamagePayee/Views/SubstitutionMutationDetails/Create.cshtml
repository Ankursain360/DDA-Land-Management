                </div>
            </div>
        </div>
        <div class="row pt-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="OppositionPartyName" type="text" placeholder="Name Of Opposition Party" required class="floating-label-field floating-label-field--s3" />

                    <label class="floating-label">Name Of Opposition Party<span class="text-danger" style="color:red">*</span></label>
                </div>
            </div>


            <div class="col-md-3">
                <input type="hidden" asp-for="PetitionerRespondent" value="Petitioner" id="PetitionerRespondent" />
                <div class="form-check theme">
                    @*<label class="control-label lb-sm">I am</label>*@

                    @*<input type="hidden" asp-for="PetitionerRespondent" id="PossessionType" value="0" />
                        <input type="hidden" asp-for="Id" id="hdnId" />
                        <input type="hidden" asp-for="IsActive" value="1" />*@

                    <input class="form-check-input" asp-for="PetitionerRespondent" type="radio" name="grpPetitione" value="Petitioner" checked="checked" />
                    <label class="form-check-label">Petitioner </label>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-check theme">
                    <input class="form-check-input" asp-for="PetitionerRespondent" type="radio" name="grpPetitione" value="Respondant" />
                    <label class="form-check-label">Respondant</label>

                </div>
            </div>

        </div>
