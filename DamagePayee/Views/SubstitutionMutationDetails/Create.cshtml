                                    <input type="text" asp-for="Name" id="txtPersonalName" class="floating-label-field floating-label-field--s3" />
                                </div>
                            </td>
                            <td>
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="FatherName" type="text" id="txtPersonalFatherName" class="floating-label-field floating-label-field--s3" />
                                </div>
                            </td>
                            <td>
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="DateGpadead" type="date" class="floating-label-field floating-label-field--s3" />
                                </div>
                            </td>
                            <td>
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="GpastafilePath" type="file" class="floating-label-field floating-label-field--s3" />
                                </div>
                            </td>


                            <td>

                                @*<div class="floating-label-wrap mb-3 mb-md-0">
                                        <a class="btn btn-theme delete-record" data-id="0" style="display:none;color:white;"><h3><i class="glyphicon glyphicon-minus-sign"></i></h3></a>
