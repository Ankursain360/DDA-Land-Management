    <div id="Purpose">

        <h2 class="my-3 sub-heading">Purpose Of Mutation/Change Of Title</h2>
        <div class="row">
            <input type="hidden" asp-for="MutationPurpose" disabled id="MutationPurpose" value="Purchaser" />

            <div class="col-md-6">
                <div class="form-check theme">
                    <input class="form-check-input" type="radio" name="grpPurpose" id="rdbPurchaser" value="Purchaser" checked="checked" />

                    <label class="form-check-label">Purchaser</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-check theme">

                    <input class="form-check-input" type="radio" name="grpPurpose" value="rdbInheritance" id="rdbInheritance" />
                    <label class="form-check-label">Inheritance </label>
                </div>

            </div>
        </div>
    </div>

    <div id="DivForInheritancePurpose">

        <h2 class="my-3 sub-heading">If Purchased</h2>
        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="PurchaseDate" type="date" required class="floating-label-field floating-label-field--s3" />

                    <label class="floating-label">Date Of Purchase<span class="text-danger" style="color:red">*</span></label>

                </div>
            </div>
        </div>

        <div class="row pt-3">

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="AtsfilePathNew" type="File" placeholder="Upload ATS(Agreement To sell)" required class="floating-label-field floating-label-field--s3" />

                    <label class="floating-label">Upload ATS(Agreement To sell)<span class="text-danger" style="color:red">*</span></label>
                </div>
            </div>

            <div class="col-md-6">
                <br />
                <input type="submit" value="Upload" class="btn btn-theme " />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="GpafilePathNew" type="file" placeholder="Upload GPA(General Power Attorny If any)" required class="floating-label-field floating-label-field--s3" />

                    <label class="floating-label">Upload GPA(General Power Attorny If any)<span class="text-danger" style="color:red">*</span> </label>
                </div>
            </div>
            <div class="col-md-6">
                <br />
                <input type="submit" value="Upload" class="btn btn-theme " />
            </div>
        </div>
        <div class="row">

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="MoneyfilePathNew" type="file" placeholder="Upload Money Receipts( If any)" required class="floating-label-field floating-label-field--s3" />

                    <label class="floating-label">Upload Money Receipts( If any)<span class="text-danger" style="color:red">*</span> </label>
                </div>
            </div>

            <div class="col-md-6">
                <br />
                <input type="submit" value="Upload" class="btn btn-theme " />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="SignSpecPathNew" type="file" placeholder="Signature Specimen" required class="floating-label-field floating-label-field--s3" />

                    <label class="floating-label">Signature Specimen<span class="text-danger" style="color:red">*</span></label>
                </div>
            </div>
            <div class="col-md-6">
                <br />
                <input type="submit" value="Upload" class="btn btn-theme " />
            </div>
        </div>
    </div>
