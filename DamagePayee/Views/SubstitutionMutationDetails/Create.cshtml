@model Libraries.Model.Entity.Mutationdetailstemp
@await Component.InvokeAsync("PageHeader", new { pageName = "Apply For Mutation" })
<br />
<br />

<form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">
    <h3 class="mb-3 d-flex">Details</h3>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input asp-for="DamagePayeeRegisterId" type="hidden" value="@ViewBag.DamagePayeeId" />
    <input asp-for="Id" type="hidden" value="@ViewBag.Id" />
    <div id="DivFileInformation">

        <h3 class="my-3 d-flex sub-heading">File Information</h3>

        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="DamagePayeeRegister.FileNo" type="text" disabled placeholder="File No." class="floating-label-field floating-label-field--s3" />
                    <label class="floating-label">File No.</label>
                </div>
            </div>
        </div>
    </div>
    <div id="OtherDetails">
        <div id="DivForSubsequentPurchaser">

            <h3 class="my-3 d-flex sub-heading">Chain of Previous Damage Assessee, starting from Original Damage Assessee</h3>
            <div class="table-section bg-white border shadow rounded">
                <div class="table-responsive">
                    <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_DamageAssessee">
                        <thead class="text-nowrap">
                            <tr>
                                <th>Sr.No</th>
                                <th>Name</th>
                                <th>Father's Name</th>
                                <th>Date of GPA/Will/Other Deed</th>
                                <th>Upload ATS & GPA/Will</th>
                            </tr>
                        </thead>
                        <tbody id="tbl_DamageAssessee_body">
                        </tbody>
                        <tbody class="tbody" id="addDamageAssessee">
                            <tr id="">

                                <td style="text-align:center;"><span class="sn">1</span></td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.Name" required maxlength="45" type="text" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.Name" class="d-block text-danger field-validation-valid"></span>

                                </td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.FatherName" required maxlength="45" type="text" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.FatherName" class="d-block text-danger field-validation-valid"></span>

                                </td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.Date" type="date" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.Date" class="d-block text-danger field-validation-valid"></span>

                                </td>
                                <td>
                                    <a id="viewATSGPAId" asp-action="ViewATSGPAFile" class="badge badge-primary px-2" target="_blank" style="display:none;">(View File)</a>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.ATSGPA" type="file" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.ATSGPA" class="d-block text-danger field-validation-valid"></span>

                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>
            </div>

        </div>
        <div id="DivPersonalInformationofDamageAssessee">
            <h3 class="my-3 d-flex sub-heading">Personal Information of Damage Assessee</h3>

            <div class="table-section bg-white border shadow rounded">
                <div class="table-responsive">
                    <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_posts">
                        <thead class="text-nowrap">

                            <tr>
                                <th>Sr.No</th>
                                <th>Name</th>
                                <th>Father's/Husband's Name</th>
                                <th style="min-width:100px;">Gender</th>
                                <th>Address</th>
                                <th>Mobile No.</th>
                                <th>Email-Id</th>
                                <th style="min-width:200px;">Aadhar No.</th>
                                <th style="min-width:200px;">Pan No.</th>
                                <th style="min-width:200px;">Photo</th>
                                <th style="min-width:200px;">Signature</th>
                            </tr>
                        </thead>
                        <tbody id="tbl_posts_body">
                        </tbody>
                        <tbody class="tbody" id="add">
                            <tr id="">
                                <td style="text-align:center;"><span class="sn">1</span></td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.payeeName" maxlength="45" required type="text" class="floating-label-field floating-label-field--s3 Namecheck" />
                                    <span asp-validation-for="DamagePayeeRegister.payeeName" class="d-block text-danger field-validation-valid msg"></span>
                                </td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.payeeFatherName" maxlength="45" required type="text" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.payeeFatherName" class="d-block text-danger field-validation-valid"></span>

                                </td>
                                <td>
                                    <div class="floating-label-wrap">
                                        <select readonly="readonly" asp-for="DamagePayeeRegister.Gender" required class="floating-label-field floating-label-field--s3">
                                            <option value="0">---Select---</option>
                                            <option value="1">Male</option>
                                            <option value="2">Female</option>
                                            <option value="3">Other</option>
                                        </select>
                                        <span asp-validation-for="DamagePayeeRegister.Gender" class="d-block text-danger field-validation-valid"></span>
                                    </div>
                                </td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.Address" maxlength="100" required type="text" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="DamagePayeeRegister.Address" class="d-block text-danger field-validation-valid"></span>

                                </td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.MobileNo" maxlength="12" required type="text" class="floating-label-field floating-label-field--s3 NoCheck" />
                                    <span asp-validation-for="DamagePayeeRegister.MobileNo" class="d-block text-danger field-validation-valid msg2"></span>

                                </td>
                                <td>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.EmailId" maxlength="30" required type="text" class="floating-label-field floating-label-field--s3 EmailCheck" />
                                    <span asp-validation-for="DamagePayeeRegister.EmailId" class="d-block text-danger field-validation-valid msg1"></span>

                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input readonly="readonly" asp-for="DamagePayeeRegister.AadharNo" type="text" maxlength="15" placeholder="Aadhar No." class=" floating-label-field--s3" />
                                        <span asp-validation-for="DamagePayeeRegister.AadharNo" class="d-block text-danger field-validation-valid msg3"></span>

                                        <a id="viewAadharId" asp-action="ViewPersonelInfoAadharFile" class="badge badge-primary px-2" target="_blank" style="display:none;">(View File)</a>
                                      
                                        <input readonly="readonly" asp-for="DamagePayeeRegister.Aadhar" type="file" class="floating-label-field floating-label-field--s3 " />
                                        <span asp-validation-for="DamagePayeeRegister.Aadhar" class="d-block text-danger field-validation-valid"></span>

                                    </div>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input readonly="readonly" asp-for="DamagePayeeRegister.PanNo" maxlength="15" type="text" placeholder="Pan No." class="  floating-label-field floating-label-field--s3" />
                                        <span asp-validation-for="DamagePayeeRegister.PanNo" class="d-block text-danger field-validation-valid"></span>

                                        <a id="viewPanId" asp-action="ViewPersonelInfoPanFile" class="badge badge-primary px-2" target="_blank" style="display:none;">(View File)</a>
                                        <input readonly="readonly" asp-for="DamagePayeeRegister.Pan" type="file" class="floating-label-field floating-label-field--s3 " />
                                      
                                        <span asp-validation-for="DamagePayeeRegister.Pan" class="d-block text-danger field-validation-valid msg4"></span>

                                    </div>
                                </td>
                                <td>
                                    <a id="viewPhotoId" asp-action="ViewPersonelInfoPhotoFile" class="badge badge-primary px-2" target="_blank" style="display:none;">(View File)</a>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.Photograph" type="file" class="floating-label-field floating-label-field--s3" />
                                  
                                    <span asp-validation-for="DamagePayeeRegister.Photograph" class="d-block text-danger field-validation-valid"></span>

                                </td>
                                <td>
                                    <a id="viewSignatureId" asp-action="ViewPersonelInfoSignautreFile" class="badge badge-primary px-2" target="_blank" style="display:none;">(View File)</a>
                                    <input readonly="readonly" asp-for="DamagePayeeRegister.SignatureFile" type="file" class="floating-label-field floating-label-field--s3" />
                                   
                                    <span asp-validation-for="DamagePayeeRegister.SignatureFile" class="d-block text-danger field-validation-valid"></span>

                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>


            </div>

        </div>
        <br />
     


        <div id="MainContentForm">
            <h3 class="my-3 d-flex sub-heading">Property Identification Details</h3>

            <div class="row">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="DamagePayeeRegister.PropertyNo" disabled type="text" class="floating-label-field floating-label-field--s3" placeholder="Municipal No." />

                        <label class="floating-label">Property/House/Municipal No.</label>

                    </div>
                </div>

                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select class="floating-label-field floating-label-field--s3" disabled asp-for="DamagePayeeRegister.LocalityId"
                                asp-items="@(new SelectList(@ViewBag.Locality,"Id","Name"))">
                            <option value="0">---Select---</option>

                        </select>
                        <label class="floating-label">Locality</label>
                    </div>
                </div>


            </div>

            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="DamagePayeeRegister.FloorNo" disabled type="text" placeholder="Floor No" class="floating-label-field floating-label-field--s3" />
                        <label class="floating-label">Floor No(if purchased floor wise)</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="DamagePayeeRegister.StreetNo" disabled type="text" placeholder="Street Name" class="floating-label-field floating-label-field--s3" />
                        <label class="floating-label">Road/Street Name</label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="DamagePayeeRegister.PinCode" disabled type="text" placeholder="Pin Code" class="floating-label-field floating-label-field--s3" />
                        <label class="floating-label">Pin Code</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="DamagePayeeRegister.DistrictId" disabled class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(ViewBag.District,"Id","Name"))">
                            <option value="0">---Select---</option>

                        </select>
                        <label class="floating-label">District</label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="DamagePayeeRegister.PlotAreaSqYard" disabled type="text" placeholder="Plot Area in Sq.yds." class="floating-label-field floating-label-field--s3" id="txtPlotyds" />
                        <label class="floating-label">Plot Area in Sq.yds.</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0 ">
                        <input asp-for="DamagePayeeRegister.PlotAreaSqMt" disabled type="text" placeholder="Sq. Mtr.-" class="floating-label-field floating-label-field--s3" id="txtPlotmts" disabled />
                        <label class="floating-label">Sq. Mtr.-</label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">

                        <input asp-for="DamagePayeeRegister.FloorAreaSqYard" disabled type="text" placeholder="Floor Area in Sq. yds" class="floating-label-field floating-label-field--s3" id="txtFlooryds" />
                        <label class="floating-label">Floor Area in Sq. yds.(if purchased floor wise)</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0 ">
                        <input asp-for="DamagePayeeRegister.FloorAreaSqMt" disabled type="text" placeholder="Sq. Mtr.-" class="floating-label-field floating-label-field--s3" id="txtFloormts" disabled />
                        <label class="floating-label">Sq. Mtr.-</label>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="form-group col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="DamagePayeeRegister.PropertyPhotoPath" />
                        @{
                            if (Model.DamagePayeeRegister.PropertyPhotoPath != null)
                            {
                                <a class="badge badge-primary px-2" asp-action="DownloadPropertyPhoto" asp-route-id="@Model.Id" target="_blank"> (View File)</a>

                                <input type="file" placeholder="Property Photo" class="floating-label-field floating-label-field--s3" readonly="readonly" asp-for="DamagePayeeRegister.PropertyPhoto" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="DamagePayeeRegister.PropertyPhoto" class="d-block text-danger field-validation-valid"></span>
                            }
                            else
                            {
                                <input type="file" readonly="readonly" asp-for="DamagePayeeRegister.PropertyPhoto" class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                <span asp-validation-for="DamagePayeeRegister.PropertyPhoto" class="d-block text-danger field-validation-valid"></span>
                                <label for="field-1" class="floating-label">Upload Photograph of Property </label>
                            }
                        }
                    </div>
                </div>
            </div>

        </div>


        <div id="Purpose">

            <h2 class="my-3 sub-heading">Purpose Of Mutation/Change Of Title</h2>
            <div class="row">
                <input type="hidden" asp-for="MutationPurpose"  id="MutationPurpose" value="Purchaser" />

                <div class="col-md-6">
                    <div class="form-check theme">
                        <input class="form-check-input" asp-for="MutationPurpose" type="radio" name="grpPurpose" id="rdbPurchaser" value="Purchaser" checked />

                        <label class="form-check-label">Purchaser</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-check theme">

                        <input class="form-check-input" asp-for="MutationPurpose" type="radio" name="grpPurpose" value="Inheritance" id="rdbInheritance" />
                        <label class="form-check-label">Inheritance </label>
                    </div>

                </div>
            </div>
        </div>

        <div id="DivForPurchasePurpose">

            <h2 class="my-3 sub-heading">If Purchased</h2>
            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">

                        <input asp-for="PurchaseDate" type="date"  class="floating-label-field floating-label-field--s3" />

                        <label class="floating-label">Date Of Purchase<span class="text-danger" style="color:red">*</span></label>

                    </div>
                </div>
            </div>

            <div class="row pt-3">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="AtsfilePath" />
                        @{
                            if (Model.AtsfilePath != null)
                            {
                                <a class="badge badge-primary px-2" asp-action="ViewAtsFileMutation" asp-route-id="@Model.Id" target="_blank"> (View File)</a>
                                <input type="file"  class="floating-label-field floating-label-field--s3" asp-for="AtsfilePathNew" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="AtsfilePathNew" class="d-block text-danger field-validation-valid"></span>
                            }
                            else
                            {
                                <input type="file" asp-for="AtsfilePathNew"  class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                <span asp-validation-for="AtsfilePathNew" class="d-block text-danger field-validation-valid"></span>
                                <label for="field-1" class="floating-label">Upload ATS(Agreement To sell)<span class="text-danger" style="color:red">*</span> </label>
                            }
                        }
                    </div>
                </div>

                <div class="col-md-6">
                    <br />
                    <input type="submit" value="Upload" class="btn btn-theme " />
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="GpafilePath" />
                        @{
                            if (Model.GpafilePath != null)
                            {
                                <a class="badge badge-primary px-2" asp-action="DownloadGPAFile" asp-route-id="@Model.Id" target="_blank"> (View File)</a>
                                <input type="file"  class="floating-label-field floating-label-field--s3" asp-for="GpafilePathNew" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="GpafilePathNew" class="d-block text-danger field-validation-valid"></span>
                            }
                            else
                            {
                                <input type="file" asp-for="GpafilePathNew"  class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                <span asp-validation-for="GpafilePathNew" class="d-block text-danger field-validation-valid"></span>
                                <label for="field-1" class="floating-label">Upload GPA(General Power Attorny If any)<span class="text-danger" style="color:red">*</span></label>
                            }
                        }
                    </div>
                </div>
                <div class="col-md-6">
                    <br />
                    <input type="submit" value="Upload" class="btn btn-theme " />
                </div>
            </div>
            <div class="row">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="MoneyRecieptFilePath" />
                        @{
                            if (Model.MoneyRecieptFilePath != null)
                            {
                                <a class="badge badge-primary px-2" asp-action="DownloadMoneyReceiptFile" asp-route-id="@Model.Id" target="_blank"> (View File)</a>
                                <input type="file"   class="floating-label-field floating-label-field--s3" asp-for="MoneyfilePathNew" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="MoneyfilePathNew" class="d-block text-danger field-validation-valid"></span>
                            }
                            else
                            {
                                <input type="file" asp-for="MoneyfilePathNew"  class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                <span asp-validation-for="MoneyfilePathNew" class="d-block text-danger field-validation-valid"></span>
                                <label for="field-1" class="floating-label">Upload Money Receipts( If any)<span class="text-danger" style="color:red">*</span></label>
                            }
                        }
                    </div>
                </div>

                <div class="col-md-6">
                    <br />
                    <input type="submit" value="Upload" class="btn btn-theme " />
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="SignatureSpecimenFilePath" />
                        @{
                            if (Model.SignatureSpecimenFilePath != null)
                            {
                                <a class="badge badge-primary px-2" asp-action="DownloadSignSpecFile" asp-route-id="@Model.Id" target="_blank"> (View File)</a>
                                <input type="file"   class="floating-label-field floating-label-field--s3" asp-for="SignSpecPathNew" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="SignSpecPathNew" class="d-block text-danger field-validation-valid"></span>
                            }
                            else
                            {
                                <input type="file" asp-for="SignSpecPathNew"  class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                <span asp-validation-for="SignSpecPathNew" class="d-block text-danger field-validation-valid"></span>
                                <label for="field-1" class="floating-label">Signature Specimen<span class="text-danger" style="color:red">*</span></label>
                            }
                        }
                    </div>
                </div>
                <div class="col-md-6">
                    <br />
                    <input type="submit" value="Upload" class="btn btn-theme " />
                </div>
            </div>
        </div>
        <div id="DivForInheritancePurpose" style="display:none;">

            <h2 class="my-3 sub-heading">If transfer through Inheritance:-</h2>
            <div class="row pt-3">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="hidden" asp-for="DeathCertificatePath" />
                        @{
                            if (Model.DeathCertificatePath != null)
                            {
                                <a class="badge badge-primary px-2" asp-action="ViewDeathCertificate" asp-route-id="@Model.Id" target="_blank"> (View File)</a>
                                <input type="file"  class="floating-label-field floating-label-field--s3" asp-for="DeathCertificatePathNew" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="DeathCertificatePathNew" class="d-block text-danger field-validation-valid"></span>
                            }
                            else
                            {
                                <input type="file" asp-for="DeathCertificatePathNew" class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                <span asp-validation-for="DeathCertificatePathNew" class="d-block text-danger field-validation-valid"></span>
                                <label for="field-1" class="floating-label">Death Certificate Original</label>
                            }
                        }
                    </div>
                </div>
                <div class="col-md-6">
                    <br />
                    <input type="submit" value="Upload" class="btn btn-theme " />
                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="hidden" asp-for="RelationshipUploadPath" />
                            @{
                                if (Model.RelationshipUploadPath != null)
                                {
                                    <a class="badge badge-primary px-2" asp-action="ViewRelationshipUpload" asp-route-id="@Model.Id" target="_blank"> (View File)</a>
                                    <input type="file"  class="floating-label-field floating-label-field--s3" asp-for="RelationshipUploadPathNew" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                    <span asp-validation-for="RelationshipUploadPathNew" class="d-block text-danger field-validation-valid"></span>
                                }
                                else
                                {
                                    <input type="file" asp-for="RelationshipUploadPathNew" class="floating-label-field floating-label-field--s3" placeholder="Property Photo">
                                    <span asp-validation-for="RelationshipUploadPathNew" class="d-block text-danger field-validation-valid"></span>
                                    <label for="field-1" class="floating-label">Proof of Relationship with Previous damage assess(ID proof,Retion Card,etc.)</label>
                                }
                            }
                        </div>
                    </div>


                </div>
                <div class="col-md-6">
                    <br />
                    <input type="submit" value="Upload" class="btn btn-theme " />
                </div>
