    <div class="row pt-3">
        <div class="col-md-12">

            <input type="submit" value="Create" class="btn btn-theme " />
            @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
            @Html.ActionLink("Back", "Create", "SubstitutionMutationDetails", null, new { @class = "btn btn-theme " })
        </div>
    </div>

