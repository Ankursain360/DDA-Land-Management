@model Libraries.Model.Entity.Noticetodamagepayee
@await Component.InvokeAsync("PageHeader", new { pageName = "Notice To Damage Payee" })

@{ ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml"; }

<div class="pt-3">

    <form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">Add Details <span class="ms-auto"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span> </h3>

        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

     

        <div id="DivForFileName">


            <div class="row mt-md-3">


                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input readonly asp-for="FileNo" placeholder="Notification"
                               class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="FileNo" class="text-danger"></span>
                        <label class="floating-label">File No <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input readonly asp-for="Name" placeholder="Name"
                               class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="Name" class="text-danger"></span>
                        <label class="floating-label">Name<span style="color:red">*</span></label>
                    </div>
                </div>
            </div>

            <div class="row mt-md-3">


                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input readonly asp-for="Address" placeholder="Notification"
                               class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="Address" class="text-danger"></span>
                        <label class="floating-label">Address<span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input readonly asp-for="PropertyDetails" placeholder="Name"
                               class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="PropertyDetails" class="text-danger"></span>
                        <label class="floating-label">Property Details<span style="color:red">*</span></label>
                    </div>
                </div>
            </div>

            <div class="row mt-md-3">


                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input readonly asp-for="Area" placeholder="Notification"
                               class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="Area" class="text-danger"></span>
                        <label class="floating-label">Area <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input readonly asp-for="InterestPercentage" placeholder="Name"
                               class="floating-label-field floating-label-field--s3 numbers" />
                        <span asp-validation-for="InterestPercentage" class="text-danger"></span>
                        <label class="floating-label">Interest Percentage <span style="color:red">*</span></label>
                    </div>
                </div>
            </div>
       
            <div class="row pt-3">
                <div class="col-md-12">
                    <input type="submit" value="Generate" class="btn btn-theme " />

                    @Html.ActionLink("Back", "Index", "NoticeToDamagePayee", null, new { @class = "btn btn-theme " })
                </div>
            </div>


        </div>





            <div id="LoadView">
            </div>
</form>

</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />