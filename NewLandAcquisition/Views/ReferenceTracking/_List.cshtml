
@model IEnumerable<Dto.Search.TrackingListDataDto>
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">

        <tr>
           
            <th>Send From</th>
            <th>Send To </th>

            <th>Remarks </th>
            <th>Send Date </th>
         




        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
                <tr>
                 


                    <td>@item.From_User</td>

                    <td>@item.To_User</td>

                    <td>@item.Remarks</td>
                  
                    <td>@(Convert.ToDateTime(item.Send_Date).ToString("dd/MMM/yyyy"))</td>


                </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="13" style="text-align:center"> Data Not Available </th>
            </tr>
        }

    </tbody>

</table>





