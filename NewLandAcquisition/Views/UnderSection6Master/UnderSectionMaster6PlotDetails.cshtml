
@{
    ViewData["Title"] = "UnderSectionMaster6PlotDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/js/jquery.min.js"></script>

<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>


<style>
    .p {
        color: black;
    }
</style>

<style>

    .bg {
        background: linear-gradient(to left,#9e1c32, #991141, #900f50,#83155d, #721e68, #68226f,#5d2675,#4f2a7a, #492b80,#422c85, #392e8b,#2c3091);
        Color: #ffffff;
        Font-size: 0.95rem;
        Font-Family: Open SansRegular;
        height: 28px;
    }
</style>


<script src="js/bootstrap.min.js"></script>
<link href="Styles/bootstrap.css" rel="stylesheet" />

<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Notification U/S-6  Plot Form</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />

<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>


                <div id="Divclsification">


                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Notification No.(U/S-6)</label>
                            <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                                <option value="0">--Select--</option>
                                <option value="1">F.4(98)/64-L&H(II)</option>
                                <option value="2">F.4(98)/64-L&H(III)</option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm"> Village Name</label>
                            <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                                <option value="0">--Select--</option>
                                <option value="1">Nimri</option>
                            </select>

                        </div>


                    </div>

                    <div class="form-row ">

                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Khasra No</label>
                            <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                                <option value="0">--Select--</option>
                                <option value="1">0//1/2</option>
                                <option value="2">0//1/14</option>
                            </select>


                        </div>

                    </div>



                    <div id="Divclsification">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class=" myapp-hed2">
                                    <div class="col-md-12">
                                        <h6 class="pull-left" style="color:white;margin-top:5px;">Area Used</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Bigha</label>
                                <input type="text" class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Biswa</label>
                                <input type="text" class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Biswanshi</label>
                                <input type="text" class="form-control" placeholder="0" />
                            </div>
                        </div>
                    </div>




                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Remarks</label>
                            <textarea type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                        </div>

                    </div>




                   
                </div>



                <br /><br />
                <div class="form-row">
                    <div class="form-group  col-md-12" style="justify-content:center">
                        <input type="submit" value="Create" class="btn btn-primary myapp-hed1" />
                     
                        @Html.ActionLink("Back", "UnderSectionMaster6PlotDetailsIndex", "UnderSection6Master", null, new { @class = "btn btn-primary myapp-hed1" })




                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<script>

            $(function () {
                $("input[name='grpSearchby']").click(function () {
                    if ($("#rdbFilename").is(":checked")) {
                        $("#DivForFileName").show();
                        $("#DivForDocumentName").hide();
                        $("#DivForGenerate").hide();
                    }
                    else if ($("#rdbDocumentname").is(":checked")) {
                        $("#DivForFileName").hide();
                        $("#DivForDocumentName").show();
                        $("#DivForGenerate").hide();
                    }
                   
                });
            });

</script>

<script>
            $(function () {
                $("input[name='grpMainFile']").click(function () {
                    if ($("#rSubsequent").is(":checked")) {
                        $("#DivForSubsequentPurchaser").show();
                    } else {
                        $("#DivForSubsequentPurchaser").hide();
                    }
                });
            });
</script>
<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.2.1.min.js">
</script>
<script>
            $(function () {
                $("#date").datepicker({ dateFormat: 'yy' });
            });
</script>
