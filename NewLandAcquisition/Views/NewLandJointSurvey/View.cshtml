@model Libraries.Model.Entity.Newlandjointsurvey
@await Component.InvokeAsync("PageHeader", new { pageName = "New Land Joint Survey" })


<div class="pt-3">
    <form asp-action="View" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">View Details</h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="Id" />
        <div class="row pt-3">

            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select disabled asp-for="ZoneId" class="floating-label-field floating-label-field--s3" onchange="return onChange(this.options[this.selectedIndex].value);"
                            asp-items="@(new SelectList(Model.ZoneList,"Id","Name"))">
                        <option value="">---Select---</option>

                    </select>
                    <span asp-validation-for="ZoneId" class="text-danger "></span>
                    <label class="floating-label">Zone </label>
                </div>
            </div>


            <div class="col-md-6">

                <div class="floating-label-wrap">
                    <select disabled class="floating-label-field floating-label-field--s3" asp-for="VillageId"
                            asp-items="@(new SelectList(Model.VillageList,"Id","Name"))"
                            onchange="return onChangeVillage(this.options[this.selectedIndex].value)" required="required" requiredMsg="The Village field is required">
                        <option selected="selected" value="">--Select-- </option>
                    </select>
                    <label asp-validation-for="VillageId" class="error"></label>
                    <label asp-for="VillageId" class="floating-label">Village </label>
                </div>
            </div>
        </div>
        <div class="row pt-3">
            <div class="col-md-6">
                <div class="floating-label-wrap">
                    <select disabled asp-for="KhasraId" class="floating-label-field floating-label-field--s3"
                            asp-items="@(new SelectList(Model.KhasraList,"Id","Name"))">
                        <option value="">---Select---</option>

                    </select>
                    <span asp-validation-for="KhasraId" class="text-danger "></span>
                    <label class="floating-label">Khasra </label>
                </div>
            </div>

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input disabled asp-for="Address" placeholder="NaamPatti" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="Address" class="text-danger"></span>
                    <label class="floating-label">Address </label>
                </div>
            </div>
        </div>
        <h3 class="my-3 d-flex sub-heading">Site Position</h3>
        <div class="row">
            <div class="col-md-3">
                <input type="hidden" asp-for="SitePosition" />
                <div class="form-check theme">
                    <input class="form-check-input chkAction" type="checkbox" asp-for="IsBuiltup" disabled>
                    <label class="form-check-label" for="checkbox">
                        Built Up
                    </label>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-check theme">
                    <input class="form-check-input chkAction" type="checkbox" asp-for="IsPartialBuiltup" disabled>
                    <label class="form-check-label" for="checkbox">
                        Partial Built Up
                    </label>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-check theme">
                    <input class="form-check-input chkAction" type="checkbox" asp-for="IsVacant" disabled>
                    <label class="form-check-label" for="checkbox">
                        Vacant
                    </label>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-check theme">
                    <input class="form-check-input chkAction" type="checkbox" asp-for="IsBoundary" disabled>
                    <label class="form-check-label" for="checkbox">
                        Boundry Wall
                    </label>
                </div>
            </div>
        </div>

        <h3 class="my-3 d-flex sub-heading pt-4">Area </h3>
        <div class="row pt-3">
            <div class="col-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input disabled asp-for="Bigha" placeholder="Notification"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="Bigha" class="text-danger"></span>
                    <label class="floating-label">Bigha    </label>
                </div>
            </div>
            <div class="col-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input disabled asp-for="Biswa" placeholder="Name"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="Biswa" class="text-danger"></span>
                    <label class="floating-label">Biswa </label>
                </div>
            </div>
            <div class="col-md-4">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input disabled asp-for="Biswanshi" placeholder="NoOfKhatauni"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="Biswanshi" class="text-danger"></span>
                    <label class="floating-label">Biswanshi </label>
                </div>
            </div>
        </div>
        <div class="row pt-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input disabled asp-for="NatureOfStructure" placeholder="NatureOfStructure" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="NatureOfStructure" class="text-danger"></span>
                    <label class="floating-label">Nature Of Structure </label>
                </div>
            </div>

            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input disabled asp-for="JointSurveyDate" type="date"
                           class="floating-label-field floating-label-field--s3" />
                    <span asp-validation-for="JointSurveyDate" class="text-danger"></span>
                    <label class="floating-label">JointSurveyDate </label>
                </div>
            </div>
        </div>
        <div id="DivPersonalInformationofDamageAssessee">
            <h3 class="my-3 d-flex sub-heading">Survey Report</h3>

            <div class="table-section bg-white border shadow rounded">
                <div class="table-responsive">
                    <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_posts">
                        <thead class="text-nowrap">

                            <tr>
                                <th>Sr.No</th>
                                <th>Document Name</th>
                                <th>Upload Document</th>

                            </tr>
                        </thead>
                        <tbody id="tbl_posts_body">
                        </tbody>
                        <tbody class="tbody" id="add">
                            <tr id="">
                                <td style="text-align:center;"><span class="sn">1</span></td>



                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input disabled asp-for="DocumentName" type="text" class="floating-label-field floating-label-field--s3" />
                                        <span asp-validation-for="DocumentName" class="d-block text-danger field-validation-valid"></span>
                                    </div>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <a id="viewDocumentId" asp-action="ViewSurveyReportFile" class="badge badge-primary px-2" target="_blank" style="display:none;">(View File)</a>
                                        <input type="hidden" asp-for="UploadFilePath" />

                                        <input asp-for="Document" type="file" class="floating-label-field floating-label-field--s3 " />
                                        <span asp-validation-for="Document" class="d-block text-danger field-validation-valid"></span>

                                    </div>
                                </td>

                            </tr>

                        </tbody>
                    </table>
                </div>


            </div>

        </div>

        <h3 class="my-3 d-flex sub-heading pt-4"> Attendance Details</h3>
        <div class="table-section bg-white border shadow rounded">
            <div class="table-responsive">
                <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_posts2">
                    <thead class="text-nowrap">
                        <tr>
                            <th>Sr.No</th>
                            <th> Name</th>
                            <th>Designation</th>
                            <th>Attendance</th>

                        </tr>
                    </thead>
                    <tbody id="tbl_posts2_body">
                    </tbody>
                    <tbody class="tbody" id="add2">
                        <tr id="">

                            <td style="text-align:center;"><span class="sn2">1</span></td>
                            <td>
                                <input disabled asp-for="AName" type="text" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="AName" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <input disabled asp-for="ADesignation" type="text" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="ADesignation" class="d-block text-danger field-validation-valid"></span>

                            </td>
                            <td>
                                <select disabled asp-for="AAttendance" class="floating-label-field floating-label-field--s3">
                                    <option disabled selected="selected">---Select---</option>
                                    <option value="1">Present</option>
                                    <option value="2">Absent</option>

                                </select>
                                <span asp-validation-for="AAttendance" class="d-block text-danger field-validation-valid"></span>

                            </td>

                        </tr>

                    </tbody>
                </table>
            </div>
        </div>

        <div class="row pt-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <textarea disabled asp-for="Remarks" placeholder="Description" class="floating-label-field floating-label-field--s3"></textarea>

                    <span asp-validation-for="Remarks" class="text-danger"></span>
                    <label class="floating-label">Remarks  </label>
                </div>
            </div>

            <div class=" col-md-6">
                <div class="floating-label-wrap">
                    <select disabled asp-for="IsActive" class="floating-label-field floating-label-field--s3">
                        <option value="1">Active</option>
                        <option value="0">Inactive</option>
                    </select>
                    <span asp-validation-for="IsActive" class="text-danger"></span>
                    <label class="floating-label">Status </label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class=" col-md-12">
                @Html.ActionLink("Back", "Index", "NewLandJointSurvey", null, new { @class = "btn btn-theme " })

            </div>
        </div>

    </form>

</div>




<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/newjointsurveyedit.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/newjointsurveyedit.min.js" asp-append-version="true"></script>
</environment>