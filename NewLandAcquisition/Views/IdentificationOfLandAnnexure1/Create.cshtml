
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Identification Of Land - Annexure I</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div id="DivProjectDetails">
                    <a data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class=" myapp-hed2">
                                    <div class="col-md-12">

                                        <h6 class="pull-left" style="color:white;margin-top:5px;">Project Details</h6>
                                        <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </a>
                    <div class="collapse" id="collapseExample">
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Project/Proposal Name</label>
                                <input type="text" class="form-control" placeholder="DDa project">
                            </div>

                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Project No/File No</label>
                                <input type="text" class="form-control" placeholder="F-1(73)CM">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Required Agency</label>
                                <input type="text" class="form-control" placeholder="DDA">
                            </div>

                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Area/Locality</label>
                                <input type="text" class="form-control" placeholder="Okhla">
                            </div>
                        </div>
                        <div class="form-row">


                            <div class="form-group col-md-6">

                                <label class="control-label lb-sm">Total Area Required</label>
                                <label class="radio-inline">
                                    <input type="radio" name="grpMainFile" id="rha" checked>HA
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="grpMainFile" id="racre">Acre
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="grpMainFile" id="rmt">Square mt.
                                </label>
                                <br /> <br />
                                <label class="control-label lb-sm">Area</label>
                                <input type="text" class="form-control">
                            </div>
                            <div class="form-row">
                                <div class="form-group col-md-12">
                                    <label class="control-label lb-sm">Purpose Of Acquisition</label>
                                    <textarea type="text" class="form-control" style=" background-color: #dddddd;" placeholder="Commercial Use"></textarea>
                                </div>
                            </div>


                            <div class="form-row">
                                <div class="form-group col-md-12">
                                    <label class="control-label lb-sm">Remarks</label>
                                    <textarea type="text" class="form-control" style=" background-color: #dddddd;" placeholder="Land is acquired"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Name of Village</label>
                        <input type="text" class="form-control" />
                    </div>

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Address</label>
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Name of Taluk/Sub-Division/Tehsil/Block(as applicable)</label>
                        <input type="text" class="form-control" />
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Name of Municipality</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">---Select---</option>
                            <option value="1">Nimri</option>
                        </select>
                    </div>

                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Name of District</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">---Select---</option>
                            <option value="1">New Delhi</option>
                        </select>
                    </div>
                </div>

                <div id="Divclsification">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Surely Not to be Required</h6>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" border="0" id="tbl_posts">
                                    <thead class="thead">
                                        <tr>
                                            <th>Sr.No</th>
                                            <th>Khasra No.</th>
                                            <th>Area</th>
                                            <th>Ownership Status</th>
                                            <th>Owner Name</th>
                                            <th>Operation</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbl_posts_body">
                                    </tbody>
                                    <tbody class="tbody" id="add">
                                        <tr id="">
                                            <td>1</td>
                                            <td>
                                                <input type="text" id="txtPersonalName" class="form-control" />
                                            </td>
                                            <td>
                                                <input type="text" id="txtPersonalName" class="form-control" placeholder="Bigha" /><br/>
                                                <input type="text" id="txtPersonalName" class="form-control" placeholder="Biswa"  /><br />
                                                <input type="text" id="txtPersonalName" class="form-control" placeholder="Biswanshi" />
                                            </td>
                                            <td>
                                                <select class="form-control" id="drpPersonalGender" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                                                    <option value="0">Private</option>
                                                    <option value="1">Occupied by other dept.</option>
                                                </select>
                                            </td>
                                            <td>
                                                <input type="text" id="txtPersonalAddress" class="form-control" />
                                            </td>
                                            <td>
                                                @*<button id="addToList" class="btn btn-primary myapp-hed1">
                                                        <i class="fa fa-plus"></i>
                                                    </button>*@
                                                <a class="btn btn-xs delete-record" data-id="0" style="display:none;"><h3><i class="glyphicon glyphicon-minus-sign"></i></h3></a>
                                                <a class="btn btn-xs add-record" data-added="0"><h3><i class="glyphicon glyphicon-plus-sign"></i></h3></a>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Total Area Under Request</label>
                        <div class="form-row">
                            <div class="form-group col-md-4">
                                <label class="radio-inline  ">
                                    <input type="radio" name="court" id="court" value="" />HA
                                </label>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="radio-inline ">
                                    <input type="radio" name="court" id="court" value="" />Acre
                                </label>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="radio-inline ">
                                    <input type="radio" name="court" id="court" value="" />Sq. Meters
                                </label>
                            </div>
                        </div>
                        <input type="text" class="form-control" />
                    </div>

                </div>
                <div id="Divclsification">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Boundaries of the Area to be Acquired</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">East</label>
                            <input type="text" class="form-control" />
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">West</label>
                            <input type="text" class="form-control" />
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">North</label>
                            <input type="text" class="form-control" />
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label lb-sm">South</label>
                            <input type="text" class="form-control" />
                        </div>
                    </div>

                </div>

                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Area of the Agricultural & Irrigated Multi Cropped Land(in HA/Acre/Sq. mtr)</label>
                        <textarea type="text" class="form-control"></textarea>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Reasons For Inclusion of Agricultural and Irrigated Multi Cropped Land</label>
                        <textarea type="text" class="form-control"></textarea>
                    </div>
                </div>


                <div id="DivBuildingDetails">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Details of Building and Other Structure</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="col-md-12">
                            <table class="table table-responsive table-bordered table-card">

                                <thead class="thead">
                                    <tr>
                                        <th>Sr.No</th>
                                        <th>Structure</th>
                                        <th>Number</th>
                                        <th>Description</th>
                                    </tr>
                                </thead>
                                <tbody class="tbody">
                                    <tr>
                                        <td>1</td>
                                        <td>Building</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                    <tr>
                                        <td>2</td>
                                        <td>Tanks</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                    <tr>
                                        <td>3</td>
                                        <td>Wells</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                    <tr>
                                        <td>4</td>
                                        <td>Trees</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                    <tr>
                                        <td>5</td>
                                        <td>Religious Building</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                    <tr>
                                        <td>6</td>
                                        <td>Graveyard or Tomb</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                    <tr>
                                        <td>7</td>
                                        <td>Others</td>
                                        <td><input type="text" class="form-control" /></td>
                                        <td><textarea type="text" class="form-control"></textarea></td>
                                    </tr>
                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group col-md-12">
                        <label class="control-label lb-sm">Reasons of the Inclusion of Building,Graveyard or Tomb</label>
                        <textarea type="text" class="form-control"></textarea>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-12">
                        <label class="control-label lb-sm">Remarks</label>
                        <textarea type="text" class="form-control"></textarea>
                    </div>
                </div>
                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Create" class="btn btn-primary myapp-hed1" />
                        @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
                        @Html.ActionLink("Back", "Index", "IdentificationOfLandAnnexure1", null, new { @class = "btn btn-primary myapp-hed1" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>





