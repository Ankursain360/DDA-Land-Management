
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Joint Survey Details</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Zone</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">---Select---</option>
                            <option value="dept1">	LM/SHAHDRA 1</option>
                            <option value="dept2">	LM/NORTH-WEST </option>
                            <option value="dept3">	LM/SOUTH-WEST </option>
                        </select>
                    </div>


                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Village Name</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">---Select---</option>
                            <option value="1">Nimri</option>
                        </select>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Khasra No.</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">---Select---</option>
                            <option value="1">0//10</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Address</label>
                        <input type="text" class="form-control" />
                    </div>
                    </div>
                    <div id="DivMainContentForm">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class=" myapp-hed2">
                                    <div class="col-md-12">
                                        <h6 class="pull-left" style="color:white;margin-top:5px;">Site Position</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-12">
                                <label class="radio-inline">
                                    <input type="checkbox" name="grpUseofpeoperty" id="rdbResidential">Built Up
                                </label>
                                <label class="radio-inline">
                                    <input type="checkbox" name="grpUseofpeoperty" id="rdbCommercial">Partial Built Up
                                </label>
                                <label class="radio-inline">
                                    <input type="checkbox" name="grpUseofpeoperty" id="rdbMixed">Vacant
                                </label>
                                <label class="radio-inline">
                                    <input type="checkbox" name="grpUseofpeoperty" id="rdbMixed">Boundary Wall
                                </label>
                            </div>

                        </div>
                    </div>

                    <div id="Divclsification">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class=" myapp-hed2">
                                    <div class="col-md-12">
                                        <h6 class="pull-left" style="color:white;margin-top:5px;">Area</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-3">
                                <label class="control-label lb-sm">Bigha</label>
                                <input type="text" class="form-control" />
                            </div>
                            <div class="form-group col-md-3">
                                <label class="control-label lb-sm">Biswa</label>
                                <input type="text" class="form-control" />
                            </div>
                            <div class="form-group col-md-3">
                                <label class="control-label lb-sm">Biswanshi</label>
                                <input type="text" class="form-control" placeholder="0" />
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Nature of Structure</label>
                            <input type="text" class="form-control" />
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Joint Survey Date</label>
                            <input type="Date" class="form-control" />
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Survey Report</label>
                            <input type="file" class="form-control" />
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Any Other Document</label>
                            <input type="file" class="form-control" />
                        </div>
                    </div>
                    <div id="DivAttendence">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class=" myapp-hed2">
                                    <div class="col-md-12">
                                        <h6 class="pull-left" style="color:white;margin-top:5px;">Attendence Details</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col-md-12">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered" border="0" id="tbl_posts">
                                        <thead class="thead">
                                            <tr>
                                                <th>Sr.No</th>
                                                <th>Name</th>
                                                <th>Designation</th>
                                                <th>Attendence</th>
                                                <th>Operation</th>
                                            </tr>
                                        </thead>
                                        <tbody id="tbl_posts_body">
                                        </tbody>
                                        <tbody class="tbody" id="add">
                                            <tr id="">
                                                <td>1</td>
                                                <td>
                                                    <input type="text" id="txtPersonalName" class="form-control" />
                                                </td>
                                                <td>
                                                    <input type="text" id="txtPersonalFatherName" class="form-control" />
                                                </td>
                                                <td>
                                                    <select class="form-control" id="drpPersonalGender" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                                                        <option value="0">---Select---</option>
                                                        <option value="1">Present</option>
                                                        <option value="2">Absent</option>
                                                    </select>
                                                </td>
                                                <td>
                                                    <a class="btn btn-xs delete-record" data-id="0" style="display:none;"><h3><i class="glyphicon glyphicon-minus-sign"></i></h3></a>
                                                    <a class="btn btn-xs add-record" data-added="0"><h3><i class="glyphicon glyphicon-plus-sign"></i></h3></a>
                                                </td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Remarks</label>
                            <textarea type="text" class="form-control"></textarea>
                        </div>
                    </div>


                    <div class="form-row ">
                        <div class="form-group col-md-12" style="margin-bottom:40px;">
                            <input type="submit" value="Create" class="btn btn-primary myapp-hed1" />
                            @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
                            @Html.ActionLink("Back", "Index", "NewLandJointSurvey", null, new { @class = "btn btn-primary myapp-hed1" })
                        </div>
                    </div>

</form>
        </div>
    </div>
</div>





