    $(function () {
        $('#drp4').change(function () {
            var id = $("#drp4").val();
            if (id == 1) {
                $("#txt4").show();

            }
            else {
                $("#txt4").hide();
            }
        });
    });
    $(function () {
        $('#drpHasLand').change(function () {
            var id = $("#drpHasLand").val();
            if (id == 0) {
                $("#txtDateLand").hide();
                $("#txtLand").hide();
                $("#fileland").hide();
            }
            else {
                $("#txtDateLand").show();
                $("#txtLand").show();
                $("#fileland").show();
            }
        });
    });
</script>



