
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Notification U/S-17 Plot Form</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Add Details </h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div id="Divclsification">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Notification U/S-17 Details</h6>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Notification No(U/S-17)</label>
                            <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                                <option value="0">F.4(25)/62-L&H</option>
                            </select>
                        </div>

                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Village Name</label>
                            <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                                <option value="0">Nimri</option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Khasra No</label>
                            <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                                <option value="0">0//10</option>0//10
                            </select>
                        </div>
                    </div>
                    <div id="Divclsification">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class=" myapp-hed2">
                                    <div class="col-md-12">
                                        <h6 class="pull-left" style="color:white;margin-top:5px;">Area</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Bigha</label>

                                <input type="text" class="form-control" />

                            </div>

                            <div class="form-group col-md-4">

                                <label class="control-label lb-sm">Biswa</label>

                                <input type="text" class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Biswanshi</label>
                                <input type="text" class="form-control" placeholder="0" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-12">
                                <label class="control-label lb-sm">Remarks</label>
                                <textarea type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                            </div>
                        </div>


                     



                        <div class="form-row ">
                            <div class="form-group col-md-12" style="margin-bottom:40px;">
                                <input type="submit" value="Create" class="btn btn-primary myapp-hed1" />
                                @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
                                @Html.ActionLink("Back", "Index", "UnderSection17DetailsPlotForm", null, new { @class = "btn btn-primary myapp-hed1" })
                            </div>
                        </div>

            </form>
        </div>
    </div>
</div>


