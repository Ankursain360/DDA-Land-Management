@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Gisdata>

<table class="table table-sm table-bordered mb-0 table-striped">

    <thead class="text-nowrap">

        <tr>

            <th>Zone Name</th>
            <th>Village Name</th>
            <th>GCP LandMarks</th>
            <th>View on Map</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Results)
        {
            <tr>

                <td>@item.Village.Zone.Name</td>
                <td>@item.Village.Name</td>
                <td>@item.Label</td>
                @* <td>@item.Xcoordinate</td>
                <td>@item.Ycoordinate</td> *@
                <td>
                    <div class="action-btns">
                        <a target="_blank" href="https://maps.google.com/?q=@item.Ycoordinate,@item.Xcoordinate" title="View Location on Map"><i class="ri-road-map-fill"></i></a>
                   
                    </div>

                </td>

            </tr>
        }

    </tbody>
</table>
@await Component.InvokeAsync("Pager", new { totalCount = Model.RowCount, pageNumber = Model.CurrentPage, pageSize = Model.PazeSize })