@model Libraries.Model.Entity.EncroachmentRegisteration
@{int FirFile = 0;}
@{int LocationMapFile = 0;}
@{int PhotoFile = 0;}
<div class="permission mt-2 accordion list-unstyled m-0 ">
    <div class="card">
        <a class="p-2 no-dec d-flex align-items-center" id="collapseapp" data-bs-toggle="collapse" href="#collapseApprroval" role="button" aria-expanded="false"
           aria-controls="collapseApprroval">
            <i class="ri-arrow-right-s-fill"></i>
            <span class="text ms-1 me-auto">View Inspection/Encroachment Register Details</span>
            <i class="arrow ri-arrow-drop-down-line slow"></i>
        </a>
        <div class="list-unstyled m-0 ps-2 pe-1 ps-md-4 pe-md-3 collapse" id="collapseApprroval">
            <div id="allDetails">
                <input type="hidden" asp-for="Id" id="hdnId" />
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <select disabled asp-for="DepartmentId" class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.DepartmentList,"Id","Name"))" onchange="return onChangeDepartment(this.options[this.selectedIndex].value);">
                                <option value="">Select</option>
                            </select>
                            <label asp-validation-for="DepartmentId" class="error"></label>
                            <label class="floating-label">Department</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <select asp-for="ZoneId" disabled class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.ZoneList,"Id","Name"))" onchange="return onChangeZone(this.options[this.selectedIndex].value)">
                                <option value="value">Select</option>
                            </select>
                            <label asp-validation-for="ZoneId" class="error"></label>
                            <label class="floating-label">Zone</label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <select asp-for="DivisionId" disabled class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.DivisionList,"Id","Name"))" onchange="return onChangeDivision(this.options[this.selectedIndex].value)">
                                <option value="value">Select</option>
                            </select>
                            <label asp-validation-for="DivisionId" class="error"></label>
                            <label class="floating-label">Division</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <select asp-for="LocalityId" disabled class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.LocalityList,"Id","Name"))">
                                <option value="value">Select</option>
                            </select>
                            <label asp-validation-for="LocalityId" class="error"></label>
                            <label class="floating-label">Locality(Village) Name</label>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <select asp-for="KhasraNo" disabled class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.PropertyInventoryKhasraList,"Id","KhasraNo"))">
                                <option value="0">Select</option>
                            </select>
                            <label asp-validation-for="KhasraNo" class="error"></label>
                            <label class="floating-label">Khasra No.</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input type="Date" asp-for="EncrochmentDate" disabled class="floating-label-field floating-label-field--s3" />
                            <span asp-validation-for="EncrochmentDate" class="text-danger"></span>
                            <label class="floating-label">Date</label>
                        </div>
                    </div>
                </div>
                <div id="DivArea">
                    <h2 class="my-3 sub-heading">Site Details</h2>
                    <div class="row mt-md-3">

                        <div class="col-md-4">
                            <div class="floating-label-wrap">
                                <select class="floating-label-field floating-label-field--s3" asp-for="AreaUnit" disabled id="inspectionAreaUnit">
                                    <option value="">--Select--</option>
                                    <option value="0">Bigha-Biswa-Biswansi</option>
                                    <option value="1">Sq Yd.</option>
                                    <option value="2">Acre</option>
                                    <option value="3">Hectare</option>
                                    <option value="4">Sq. Mt.</option>
                                </select>
                                <span asp-validation-for="AreaUnit" class="text-danger"></span>
                                <label asp-for="AreaUnit" class="floating-label">Area Unit </label>
                            </div>
                        </div>
                    </div>
                    <div id="inspectionbighabis">
                        <div class="row mt-md-3">
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input limit="4" asp-for="TotalAreaInBighaInspection" type="text" disabled class="floating-label-field floating-label-field--s3 onlynumbers inspectionbbbcalculation" placeholder="Bigha" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                    <span asp-validation-for="TotalAreaInBighaInspection" class="text-danger"></span>
                                    <label class="floating-label">Bigha</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input limit="2" asp-for="TotalAreaInBiswaInspection" type="text" disabled class="floating-label-field floating-label-field--s3 onlynumbers inspectionbbbcalculation" placeholder="Biswa" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                    <span asp-validation-for="TotalAreaInBiswaInspection" class="text-danger"></span>
                                    <label class="floating-label">Biswa</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input limit="2" asp-for="TotalAreaInBiswaniInspection" type="text" disabled class="floating-label-field floating-label-field--s3 onlynumbers inspectionbbbcalculation" placeholder="Biswani" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                    <span asp-validation-for="TotalAreaInBiswaniInspection" class="text-danger"></span>
                                    <label class="floating-label">biswansi</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="inspectionAreainSqAcreHec" style="display:none;">
                        <div class="row mt-md-3">
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="TotalAreaInSqAcreHt" id="inspectionTotalAreaInSqAcreHt" type="text" disabled class="floating-label-field floating-label-field--s3 numbers TotalCalculation" placeholder="Total Area(Sq Yd./Acre/Hectare)" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                    <span asp-validation-for="TotalAreaInSqAcreHt" class="text-danger"></span>
                                    <label class="floating-label">Total Area(Sq Yd./Acre/Hectare)</label>
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="row mt-md-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" id="inspectionArea" class="floating-label-field floating-label-field--s3 numbers" disabled placeholder="Area" asp-for="Area" />
                                <span asp-validation-for="Area" class="text-danger"></span>
                                <label class="floating-label">Area(in sq. mt.)</label>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="row mt-md-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <textarea class="floating-label-field floating-label-field--s3" limit="1000" placeholder="Location/Address with land mark" asp-for="LocationAddressWithLandMark"></textarea>
                            <span asp-validation-for="LocationAddressWithLandMark" class="text-danger"></span>
                            <label class="floating-label">Location/Address with land mark</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="row mt-md-3">
                            <div class="col-md-4">
                                <label>Encroachment</label>
                            </div>
                            <input type="hidden" asp-for="IsEncroachment" value="No" />
                            <div class="col-md-4">
                                <div class="form-check theme">
                                    <input class="form-check-input" disabled type="radio" asp-for="IsEncroachment" name="grpIsEncroachment" value="No" checked>
                                    <label class="form-check-label">No </label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-check theme">
                                    <input class="form-check-input" disabled type="radio" asp-for="IsEncroachment" name="grpIsEncroachment" value="Yes">
                                    <label class="form-check-label">Yes </label>
                                </div>
                            </div>
                        </div>
                        <span asp-validation-for="IsEncroachment" class="text-danger"></span>
                    </div>
                </div>


                <div id="Divclsification">
                    <h2 class="my-3 sub-heading">Details Of Encroachment</h2>
                    <div class="row mt-md-3 mb-md-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" placeholder="Name of Encroacher" limit="100" class="floating-label-field floating-label-field--s3" asp-for="EncroacherName" />
                                <span asp-validation-for="EncroacherName" class="text-danger"></span>
                                <label class="floating-label">Name of Encroacher</label>
                            </div>
                        </div>
                    </div>
                    <div class="table-section bg-white border shadow rounded">
                        <div class="table-responsive ">
                            <table id="tbl_posts" class="table table-sm table-bordered mb-0 table-striped">
                                <thead class="text-nowrap">
                                    <tr>
                                        <th>Sr.No</th>
                                        <th>Name of Structure</th>
                                        <th>Construction Status</th>
                                        <th>Area[Approx.]</th>
                                        <th>Type[residential/religious/compercial]</th>
                                        <th>Religious Structure</th>
                                        <th>Age of Encroachment</th>
                                        <th>Reference No. on  Location</th>
                                        <th>Count of Structure</th>

                                    </tr>
                                </thead>
                                <tbody id="tbl_posts_body">
                                </tbody>
                                <tbody id="add">
                                    <tr id="">
                                        <td style="text-align:center;"><span class="sn">1</span></td>
                                        <td>
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input type="text" asp-for="NameOfStructure" disabled class="floating-label-field floating-label-field--s3" id="NameOfStructure" />
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap">
                                                <select asp-for="ConstructionStatus" disabled class="floating-label-field floating-label-field--s3">
                                                    <option value="Select">Select</option>
                                                    <option value="Kachcha">Kachcha</option>
                                                    <option value="Pakka">Pakka</option>
                                                    <option value="Under Construction">Under Construction</option>
                                                </select>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input type="text" asp-for="AreaApprox" disabled class="floating-label-field floating-label-field--s3 numbers" id="AreaApprox" />
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input type="text" asp-for="Type" disabled class="floating-label-field floating-label-field--s3" id="Type" />
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap">
                                                <select asp-for="ReligiousStructure" disabled class="floating-label-field floating-label-field--s3">
                                                    <option value="No">No</option>
                                                    <option value="Yes">Yes</option>
                                                </select>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="DateOfEncroachment" disabled class="floating-label-field floating-label-field--s3 onlynumbers" id="DateOfEncroachment" />
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="ReferenceNoOnLocation" type="text" disabled class="floating-label-field floating-label-field--s3 number" id="ReferenceNoOnLocation" />
                                            </div>
                                        </td>
                                        <td>
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="CountOfStructure" type="text" disabled class="floating-label-field floating-label-field--s3 onlynumbers" id="CountOfStructure" />
                                            </div>
                                        </td>

                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <select asp-for="StatusOfLand" class="floating-label-field floating-label-field--s3" onchange="onChangeLandStatus(this.value)">
                                <option value="DDA Land">DDA Land</option>
                                <option value="Other Govt. Land">Other Govt. Land</option>
                                <option vlaue="Private Land">Private Land</option>
                            </select>
                            <label class="floating-label">Land Ownership</label>
                        </div>
                    </div>
                </div>
                <div id="divForLandStatus">
                    <div class="row pt-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap">

                                <select disabled class="floating-label-field floating-label-field--s3" asp-for="IsPossession">
                                    <option value="Yes">Yes</option>
                                    <option value="No">No</option>
                                </select>
                                <label class="floating-label">Possession with DDA</label>
                            </div>
                        </div>
                    </div>

                    <div class="row pt-3">
                        @*<div class="col-md-6" id="divPossessionGroup" style="display:block;">
                    <div class="row mt-md-3">
                        <div class="col-md-6">
                            <input type="hidden" asp-for="PossessionType" id="PossessionType" value="Formal Possession" />
                            <input type="hidden" asp-for="Id" id="hdnId" />
                            <input type="hidden" asp-for="IsActive" value="1" />
                            <div class="form-check theme">

                                <input type="radio" name="grpPossession" id="rdbFormal" checked disabled>
                                <label class="radio-inline">Formal Possession </label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-check theme">

                                <input type="radio" name="grpPossession" id="rdbFlexible" disabled>
                                <label class="radio-inline">Physical Possession </label>
                            </div>
                        </div>
                    </div>
                </div>*@
                        <div class="col-md-6" id="divDepartment" style="display:block;">
                            <div class="floating-label-wrap">
                                <select asp-for="OtherDepartment" disabled class="floating-label-field floating-label-field--s3" asp-items="@(new SelectList(Model.DepartmentList,"Id","Name"))">
                                    <option value="0">Select</option>
                                </select>
                                <label asp-validation-for="OtherDepartment" class="error"></label>
                                <label class="floating-label">Other Department </label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            @{
                                if (Model.EncroachmentFirFileDetails != null && Model.EncroachmentFirFileDetails.Count > 0)
                                {
                                    <input type="file" asp-for="Firfile" id="Firfile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg, image/gif" multiple />

                                    foreach (var item in Model.EncroachmentFirFileDetails)
                                    {
                                        <a class="badge bg-primary px-2" target="_blank"  asp-action="DownloadFirfile" asp-route-id="@item.Id"> (View File @(FirFile += 1) )</a>

                                    }
                                }
                                else
                                {
                                    <input type="file" asp-for="Firfile" id="Firfile" class="floating-label-field floating-label-field--s3" accept="application/pdf, image/png, image/jpeg, image/gif" multiple />

                                }
                                <label class="floating-label">FIR Details</label>
                                <span asp-validation-for="Firfile" class="text-danger"></span>
                            }
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <input type="text" asp-for="PoliceStation" disabled class="floating-label-field floating-label-field--s3" />
                            <span asp-validation-for="PoliceStation" class="text-danger"></span>
                            <label class="floating-label">Police Station</label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">

                            <input disabled type="text" asp-for="SecurityGuardOnDuty" class="floating-label-field floating-label-field--s3" />
                            <span asp-validation-for="SecurityGuardOnDuty" class="text-danger"></span>
                            <label class="floating-label">Officer on Duty</label>
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            @{
                                if (Model.EncroachmentLocationMapFileDetails != null && Model.EncroachmentLocationMapFileDetails.Count > 0)
                                {
                                    <input type="file" asp-for="LocationMapFile" id="LocationMapFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg, image/gif" multiple />
                                    foreach (var item in Model.EncroachmentLocationMapFileDetails)
                                    {
                                        <a asp-action="DownloadLocationMapFile" target="_blank"  class="badge bg-primary px-2" asp-route-id="@item.Id"> (View Document @(LocationMapFile += 1) )</a>

                                    }
                                }
                                else
                                {
                                    <input type="file" asp-for="LocationMapFile" id="LocationMapFile" class="floating-label-field floating-label-field--s3" accept="application/pdf,  image/png, image/jpeg, image/gif" multiple />

                                }
                                <label class="floating-label">Location Map</label>
                                <span asp-validation-for="LocationMapFile" class="text-danger"></span>
                            }
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            @{
                                if (Model.EncroachmentPhotoFileDetails != null && Model.EncroachmentPhotoFileDetails.Count > 0)
                                {
                                    <input type="file" class="floating-label-field floating-label-field--s3" asp-for="PhotoFile" id="PhotoFile" accept="application/pdf,  image/png, image/jpeg, image/gif" multiple />
                                    foreach (var item in Model.EncroachmentPhotoFileDetails)
                                    {
                                        <a asp-action="DownloadPhotoFile" target="_blank"  class="badge bg-primary px-2" asp-route-id="@item.Id"> (View Document @(PhotoFile += 1 ))</a>

                                    }
                                }
                                else
                                {
                                    <input type="file" class="floating-label-field floating-label-field--s3" asp-for="PhotoFile" id="PhotoFile" accept="application/pdf, image/png, image/jpeg, image/gif" multiple />

                                }
                                <span asp-validation-for="PhotoFile" class="text-danger"></span>
                                <label class="floating-label">Photo (<span style="color:red;"> max size 5 mb</span>)</label>
                            }
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-12">
                        <div class="floating-label-wrap">

                            <textarea type="text" disabled asp-for="Remarks" class="floating-label-field floating-label-field--s3"></textarea>
                            <span asp-validation-for="Remarks" class="text-danger"></span>
                            <label class="floating-label">Remarks</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/encroachmentregisterationview.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/encroachmentregisterationview.min.js" asp-append-version="true"></script>
</environment>
