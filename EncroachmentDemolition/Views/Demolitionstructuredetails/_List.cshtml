@*@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Demolitionstructuredetails>*@
@model IEnumerable<Libraries.Model.Entity.Demolitionstructuredetails>
@{int rowNo = 0;}
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">

        <tr>
            <th>Sr.No</th>

            <th>Department</th>
            <th>Zone</th>
            <th>Division</th>
            <th>Locality</th>
            <th>Date</th>
            <th>File No</th>
            <th>Police Station</th>
            <th>Name Of Area</th>
            <th>Date Of approval of demolition</th>
            <th>Start of Demolition Action</th>
            <th>End of Demolition Action</th>

            <th>Operation</th>
        </tr>
    </thead>
    <tbody>
        @*@foreach (var item in Model.Results)*@
        @foreach (var item in Model)
        {
            <tr>
                <td>@(rowNo += 1)</td>
                <td>@item.Department.Name</td>
                <td>@item.Zone.Name</td>
                <td>@item.Division.Name</td>
                <td>@item.Locality.Name</td>
                <td>@item.Date</td>
                <td>@item.FileNo</td>
                <td>@item.PoliceStation</td>
                <td>@item.NameOfAreaSite</td>
                <td>@item.DateOfApprovalDemolition</td>
                <td>@item.StartOfDemolitionActionDate</td>
                <td>@item.EndOfDemolitionActionDate</td>
                @*<td>@(item.EncrochmentDate==null?"Not Available":Convert.ToDateTime(item.EncrochmentDate).ToString("dd MMM yyyy"))</td>*@

                <td>
                    <a asp-action="View" asp-route-id="@item.Id" title="View">  <i class="glyphicon glyphicon-eye-open" aria-hidden="true"></i> </a>
                    <a asp-action="Edit" asp-route-id="@item.Id" title="edit"> &nbsp; | &nbsp;  <i class="glyphicon glyphicon-pencil" aria-hidden="true"></i> </a>
                    <a asp-action="Delete" asp-route-id="@item.Id" title="delete" onclick="return confirm('Are You Sure, You want to Delete this Record?')">   &nbsp;|&nbsp;<i class="glyphicon glyphicon-trash" aria-hidden="true"></i></a>
                </td>
            </tr>
        }
    </tbody>

</table>
@*@await Component.InvokeAsync("Pager", new { totalCount = Model.RowCount, pageNumber = Model.CurrentPage, pageSize = Model.PazeSize })*@