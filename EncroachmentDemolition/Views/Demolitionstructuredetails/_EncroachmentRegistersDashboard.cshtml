        <li class="nav-item">
            <div class="nav-link active" data-toggle="tab" id="divDemolitionDashboard" onclick="return DemolitionDashboard();" value="Demolition" role="tab" aria-controls="DemolitionDashboard" aria-selected="true">Demolition Dashboard</div>-->
            @*<a class="nav-link active" data-toggle="tab" href="#DemolitionDashboard" role="tab" aria-controls="DemolitionDashboard" aria-selected="true">Demolition Dashboard</a>*@
            <!--</li>
    <li class="nav-item">
        <div class="nav-link" data-toggle="tab" id="divEncroachmentDemolition" onclick="return EncroachmentDemolition();" value="Encroachment" role="tab" aria-controls="EncroachmentDemolition" aria-selected="false">Encroachment Dashboard</div>-->
            @*<a class="nav-link"  data-toggle="tab" href="#EncroachmentDemolition" role="tab" aria-controls="EncroachmentDemolition" aria-selected="false">Encroachment Demolition</a>*@
            <!--</li>
    </ul>-->
        </div>
    </div>
    <div id="myTabContent">
        <div class="card-body bg-light tab-pane fade show" id="encroachmentDemolition" role="tabpanel" aria-labelledby="profile-tab" style="text-decoration:none!important;outline:none!important">
            <div class="widget p-4 p-md-5 ">
                <div class="row register-form g-md-5">
                    <div class="col-md-4 col-sm-6">
                        <a href="#" onclick="encroachmentregistrationshowdata('TotalRequest')" title="Total encroachment application Received">
                            <div class="card text-white bg-primary mb-3">
                                <div class="card-header">
                                    <b>Total encroachment application Received</b>
                                </div>
                                <div class="card-body">
                                    <h5 class="card-title">@Model.Select(x => x.TotalRequest).FirstOrDefault()</h5>
                                    <p class="card-text">Total encroachment application Received</p>
                                </div>
                                <div class="card-footer text-end">
                                    @*<a href="#" class="btn btn-group-sm bg-primary">show</a>*@
                                </div>
                            </div>
                        </a>
                    </div>
                    <div class="col-md-4 col-sm-6">
                        <a href="#" onclick="encroachmentregistrationshowdata('TotalPending')" title="Total encroachment application pending">
                            <div class="card text-white bg-secondary  mb-3">
                                <div class="card-header">
                                    <b>Total encroachment application pending</b>
                                </div>
                                <div class="card-body">
                                    <h5 class="card-title">@Model.Select(x => x.TotalPending).FirstOrDefault() </h5>
                                    <p class="card-text">Total encroachment application pending</p>
                                </div>
                                <div class="card-footer text-end">
                                </div>
                            </div>
                        </a>
                    </div>
                    <div class="col-md-4 col-sm-6">
                        <a href="#" onclick="encroachmentregistrationshowdata('PendingAtyou')" title="Total encroachment application Pending At User Level">
                            <div class="card text-white bg-warning mb-3">
                                <div class="card-header">
                                    <b>Total encroachment application Pending At User Level</b>
                                </div>
                                <div class="card-body">
                                    <h5 class="card-title">@Model.Select(x => x.PendingAtYou).FirstOrDefault() </h5>
                                    <p class="card-text">Total encroachment application Pending At User Level</p>
                                    @*<a href="#" class="btn btn-primary">Go#</a>*@
                                </div>
                                <div class="card-footer text-end">
                                </div>
                            </div>
                        </a>
                    </div>
                    <div class="col-md-4 col-sm-6">
                        <a href="#" onclick="encroachmentregistrationshowdata('AcceptAndInitiateDemolition')" title="Total encroachment application Accept & initiate demolition">
                            <div class="card text-white bg-success   mb-3">
                                <div class="card-header">
                                    <b>Total encroachment application Accept & initiate demolition</b>
                                </div>
                                <div class="card-body">
                                    <h5 class="card-title">@Model.Select(x => x.AcceptAndInitiateDemolition).FirstOrDefault() </h5>
                                    <p class="card-text">Total encroachment application Accept & initiate demolition</p>
                                </div>
                                <div class="card-footer text-end">
                                </div>
                            </div>
                        </a>
                    </div>
                    <div class="col-md-4 col-sm-6">
                        <a href="#" onclick="encroachmentregistrationshowdata('TotalDemolitionToBETakenLater')" title="Total encroachment application Demolition to be taken later">
                            <div class="card text-white bg-danger mb-3">
                                <div class="card-header">
                                    <b>Total encroachment application Demolition to be taken later</b>
                                </div>
                                <div class="card-body">
                                    <h5 class="card-title">@Model.Select(x => x.TotalDemolitionToBETakenLater).FirstOrDefault() </h5>
                                    <p class="card-text">Total encroachment application Demolition to be taken later</p>
                                </div>
                                <div class="card-footer text-end">
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            </div>

        </div>


        <div id="demolitionDashboard">

        </div>

    </div>
    <div class="card-footer">
        <small class="text-white">Last updated 3 mins ago</small>
    </div>
</div>

<!-- Button trigger modal -->
<button type="button" class="btn btn-primary" style="display:none" id="btnShowModel" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
    #
</button>

<!-- Modal -->
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Dashboard Data</h5><h5 id="hfiltertextofapplication"></h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" onclick="resetModel()" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div id="divModelContent">
                    @*show data Here*@
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" onclick="resetModel()" data-bs-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>