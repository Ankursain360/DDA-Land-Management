
@model List<Dto.Search.EncroachmentRegisterDashboardDto>
<link rel="stylesheet" href="~/css/ToggleSwitch1.css" />
<div class="card text-white bg-secondary register mb-3">
    <div class="card-header ">
        <h5 style="float:left">Encroachment Dashboard</h5>
        <div>
            <div class="mid">
                <span class="DemolitionCls active" style="color: white; font-size: 15px; margin-left: 300px">Demolition <br /><span class="DemolitionCls active">Dashboard</span></span>
                <label class="rocker" style="margin-left: 15px; background-color: #6c757d">
                    <input type="checkbox">
                    <span id="divDemolitionDashboard" onclick="return DemolitionDashboard();" title="Demolition Dashboard" class="switch-left active"></span>
                    <span id="divEncroachmentDemolition" onclick="return EncroachmentDemolition();" title="Encroachment Dashboard" class="switch-right"></span>
                </label>
                <span class="EncroachmentCls" style="color: white; font-size: 15px; margin-left: 15px ">Encroachment <br /><span class="EncroachmentCls">Dashboard</span></span>
            </div>
