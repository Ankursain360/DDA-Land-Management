@model Libraries.Model.Entity.MonthlyRoaster
<table class="table">
    <thead class="thead table-striped table-bordered table">
        <tr>
            <th>Date</th>
            <th>Day</th>
            <th>Primary No</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var itemRepeater in Model.Dailyroaster)
        {

<tr>
    <td>@itemRepeater.Date</td>
    <td>@itemRepeater.Day</td>
    <td>
        <select class="roaster" multiple asp-for="PrimaryListNo">
            <option value="Codeigniter">Bahapur</option>
            <option value="CakePHP">Raipur Khurd</option>
            <option value="Laravel">Tehkhand</option>
            <option value="YII">Kilokari</option>
            <option value="Zend">Jaitpur</option>
            <option value="Symfony">Jasola</option>
            <option value="Phalcon">Aali</option>
            <option value="Slim">Tajpul</option>
        </select>
    </td>
</tr>}
    </tbody>
</table>

<environment include="Development">
    <script src="~/js/monthlyroastercreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/monthlyroastercreate.js" asp-append-version="true"></script>
</environment>
