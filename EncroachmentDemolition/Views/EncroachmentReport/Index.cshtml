@*@model Libraries.Model.Entity.Nazulland*@
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Encroachment Report</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%"> Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm"> Zone </label>
                        <select class="form-control">
                            <option value="0">All</option>
                            <option value="dept1">	LM/SHAHDRA 1</option>
                            <option value="dept2">	LM/NORTH-WEST </option>
                            <option value="dept3">	LM/SOUTH-WEST </option>SelectZone
                        </select>
                    </div>


                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm"> Division </label>
                        <select class="form-control">
                            <option value="0">All</option>
                            <option value="dept1">WD-2 </option>
                            <option value="dept2">WD-8 </option>
                            <option value="dept3">WD-9 </option>
                        </select>
                    </div>
                </div>
                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm"> Village </label>
                        <select class="form-control">
                            <option value="0">All</option>
                            <option value="dept1">	Bajitpur</option>
                            <option value="dept2">	Narela</option>
                            <option value="dept3">	Driyapur </option>SelectZone
                        </select>

                    </div>
                </div>

                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Generate" class="btn btn-primary dda-pageheading" />

                    </div>
                </div>
                @*<div id="Divclsification">*@
                <div class="row" style="margin-bottom:35px;">
                    <div class="col-sm-12">
                        <div class="dda-innerheading">
                            <div class="col-md-12">
                                <h6 class="pull-left" style="color:white;margin-top:5px;">Report</h6>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-12">
                        <div class=" table-responsive  table-card">

                            <table class="table table-responsive table-bordered ">

                                <thead class="thead">
                                    <tr>
                                        <th>Sr.No</th>
                                        <th>Zone</th>
                                        <th>Division</th>
                                        <th>Village Name</th>
                                        <th>Khasra No.</th>
                                        <th>Encroachment Date</th>

                                    </tr>
                                </thead>
                                <tbody class="tbody">
                                    <tr style="color:red;">
                                        <td>1</td>
                                        <td>LM/SHAHDRA 1</td>
                                        <td>WD-2 </td>
                                        <td>Bajitpur</td>
                                        <td>1234</td>
                                        <td>01-10-2020</td>

                                    </tr>
                                    <tr style="color:red;">
                                        <td>2</td>
                                        <td>LM/NORTH-WEST </td>
                                        <td>WD-8</td>
                                        <td>Narela</td>
                                        <td>456</td>
                                        <td>01-10-2020</td>

                                    </tr>


                                </tbody>

                            </table>

                        </div>
                    </div>
                </div>
                @*</div>*@


                <div class="form-row mt-4">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Export to Excel" class="btn btn-primary dda-pageheading" style="width:200px;" />
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>

