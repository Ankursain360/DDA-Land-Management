@model Libraries.Model.Entity.EncroachmentRegisteration
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Encroachment Report</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Encroachment Report</h6>
        </div>
        <div class="panel-body">
            @*<form>*@
            <div class="row">
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">Department <span style="color:red">*</span></label>
                    <select asp-for="DepartmentId" class="form-control dda-dropdown" required="required" requiredMsg="Please Select Department" asp-items="@(new SelectList(Model.DepartmentList,"Id","Name"))" onchange="return onChangeDepartment(this.options[this.selectedIndex].value);">
                        <option value="0">All</option>
                    </select>
                    <span asp-validation-for="DepartmentId" class="text-danger"></span>
                </div>
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">Zone <span style="color:red">*</span></label>
                    <select asp-for="ZoneId" class="form-control dda-dropdown" required="required" requiredMsg="Please Select Zone" asp-items="@(new SelectList(Model.ZoneList,"Id","Name"))" onchange="return onChangeZone(this.options[this.selectedIndex].value)">
                        <option value="0">All</option>
                    </select>
                    <span asp-validation-for="ZoneId" class="text-danger"></span>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">Division <span style="color:red">*</span></label>
                    <select asp-for="DivisionId" class="form-control dda-dropdown" required="required" requiredMsg="Please Select Division" asp-items="@(new SelectList(Model.DivisionList,"Id","Name"))" onchange="return onChangeDivision(this.options[this.selectedIndex].value)">
                        <option value="0">All</option>
                    </select>
                    <span asp-validation-for="DivisionId" class="text-danger"></span>
                </div>
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">Locality(Village) Name <span style="color:red">*</span></label>
                    <select asp-for="LocalityId" class="form-control dda-dropdown" required="required" requiredMsg="Please Select Locality" asp-items="@(new SelectList(Model.LocalityList,"Id","Name"))">
                        <option value="0">All</option>
                    </select>
                    <span asp-validation-for="LocalityId" class="text-danger"></span>
                </div>
            </div>



            <div class="row ">
                <div class="form-group col-md-12" style="margin-bottom:40px;">
                    <input type="submit" value="Generate" id="btnGenerate" class="btn btn-primary dda-pageheading" />
                    @*@Html.ActionLink("Back", "Index", "RestoreLandReport", null, new { @class = "btn btn-primary dda-pageheading" })*@
                </div>
            </div>

            <div id="LoadReportView">

            </div>
            @*</form>*@
        </div>
    </div>
</div>

<br />
<br />


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/encroachmentreportcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/encroachmentreportcreate.min.js" asp-append-version="true"></script>
</environment>
