
@model IEnumerable<Libraries.Model.Entity.EncroachmentRegisteration>
@{int rowNo = 0; }


<div class="card  table-card table-responsive">

    <table class="table table-responsive table-bordered table-card">


        <thead class="thead">

            <tr>
                <th rowspan="2">Sr.No</th>
                <th rowspan="2">Date</th>
                <th rowspan="2">Village Name</th>
                <th rowspan="2">Khasra No.</th>
                <th rowspan="2">Area</th>
                <th rowspan="2">Status on Land</th>

                <th rowspan="2">Other Department</th>
            
                <th rowspan="2">Police Station</th>
                <th rowspan="2">Security Guard on duty</th>

                <th rowspan="2">Remarks</th>
            </tr>

        </thead>
        <tbody class="tbody">
            @if (Model != null && Model.Count() > 0)
            {
                @foreach (var item in Model)
                {
            <tr>
                <td>@(rowNo += 1)</td>

                <td>@item.EncrochmentDate</td>
                <td>@item.Locality.Name</td>
                <td>@item.KhasraNo</td>
                <td>@item.Area</td>
                <td>@item.StatusOfLand</td>
                <td>@item.Department.Name</td>

                <td>@item.PoliceStation</td>
                <td>@item.SecurityGuardOnDuty</td>


                <td>@item.Remarks</td>

             
            </tr>
                }
            }
            else
            {
                <tr>
                    <th colspan="13" style="text-align:center"> Data Not Available </th>
                </tr>
            }

        </tbody>

    </table>

</div>





