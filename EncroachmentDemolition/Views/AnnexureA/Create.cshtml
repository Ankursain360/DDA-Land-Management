                                <a asp-action="DownloadFirfile" asp-route-id="@item.Id"> (View Existing Document @(FirFile += 1) )</a>
                                <br />
                            }
                            <input type="file" asp-for="Encroachment.Firfile" id="Firfile" class="form-control" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" multiple />
                            <span asp-validation-for="Encroachment.Firfile" class="text-danger"></span>
                        }
                        else
                        {
                            <input type="file" asp-for="Encroachment.Firfile" id="Firfile" class="form-control" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" multiple />
                            <span asp-validation-for="Encroachment.Firfile" class="text-danger"></span>
                        }
                    }
