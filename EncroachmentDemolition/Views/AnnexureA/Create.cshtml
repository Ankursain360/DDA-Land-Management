                        <option value="Non DDA Land">Non DDA Land</option>
                    </select>
                </div>
                <div id="divForLandStatus">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Possession </label>
                        <select class="form-control dda-dropdown" asp-for="Encroachment.IsPossession" onchange="onChangePossationStatus(this.value)" disabled>
                            <option value="Yes">Yes</option>
                            <option value="No">No</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6" id="divPossessionGroup" style="display:block;">
                        <input type="hidden" asp-for="Encroachment.PossessionType" id="PossessionType" value="Formal Possession" disabled />
                        <input type="hidden" asp-for="Id" id="hdnId" />
                        <input type="hidden" asp-for="IsActive" value="1" />
