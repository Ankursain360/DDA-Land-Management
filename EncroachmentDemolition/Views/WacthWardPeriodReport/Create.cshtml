@model Libraries.Model.Entity.Watchandward

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Report Watchandward</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Watchandward Details</h6>
        </div>
        <div class="panel-body">
            @*<form>*@
            <div class="row">
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">Village Name</label><span style="color:red">*</span>
                    <select class="form-control dda-dropdown" asp-for="VillageId" asp-items="@(new SelectList(Model.VillageList, "Id","Name"))">
                        <option>Select</option>
                    </select>
                    <span asp-validation-for="VillageId" class="text-danger"></span>
                </div>
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">From Date</label><span style="color:red">*</span>
                    <input asp-for="Date" type="date" id="txtDate" class="form-control" />
                    <span asp-validation-for="Date" class="text-danger"></span>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-6">
                   
                    <label class="control-label lb-sm">To Date</label><span style="color:red">*</span>
                    <input asp-for="Date" type="date" id="txtDate" class="form-control" />
                    <span asp-validation-for="Date" class="text-danger"></span>
                </div>
               
            </div>



            <div class="row ">
                <div class="form-group col-md-12" style="margin-bottom:40px;">
                    <input type="submit" value="Generate" id="btnGenerate" class="btn btn-primary dda-pageheading" />
                    @*@Html.ActionLink("Back", "Index", "RestoreLandReport", null, new { @class = "btn btn-primary dda-pageheading" })*@
                </div>
            </div>

            <div id="LoadReportView">

            </div>
            @*</form>*@
        </div>
    </div>
</div>

<br />
<br />


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/wacthwardperiodreportcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/wacthwardperiodreportcreate.min.js" asp-append-version="true"></script>
</environment>
