@model Libraries.Model.Entity.Onlinecomplaint
<div class="permission mt-2 accordion list-unstyled m-0 ">
    <div class="card">
        <a class="p-2 no-dec d-flex align-items-center" data-bs-toggle="collapse" href="#collapseApprroval" role="button" aria-expanded="true"
           aria-controls="collapseApprroval">
            <i class="ri-arrow-right-s-fill"></i>
            <span class="text ms-1 me-auto">View Online Complaint Details</span>
            <i class="arrow ri-arrow-drop-down-line slow"></i>
        </a>
        <div class="list-unstyled m-0 ps-2 pe-1 ps-md-4 pe-md-3 collapse" id="collapseApprroval">
            <div id="allDetails">
                <div class="row">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea type="text" disabled asp-for="Name" class="floating-label-field floating-label-field--s3" limit="400"></textarea>
                            <span asp-validation-for="Name" class="text-danger"></span>
                            <label class="floating-label">Name of Complainant</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea type="text" disabled asp-for="AddressOfComplaint" class="floating-label-field floating-label-field--s3" limit="4000"></textarea>
                            <span asp-validation-for="AddressOfComplaint" class="text-danger"></span>
                            <label class="floating-label">Address of Complainant</label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input class="floating-label-field floating-label-field--s3" disabled asp-for="Contact" limit="10" />
                            <span asp-validation-for="Contact" class="d-block text-danger field-validation-valid"></span>
                            <label class="floating-label">Contact No.</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input class="floating-label-field floating-label-field--s3" disabled asp-for="Email" limit="30" />
                            <span asp-validation-for="Email" class="d-block text-danger field-validation-valid"></span>
                            <label class="floating-label">Email Id  </label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="ComplaintTypeId" asp-items="@(new SelectList(Model.ComplaintList, "Id","Name"))">
                                <option>Select</option>
                            </select>
                            <span asp-validation-for="ComplaintTypeId" class="text-danger"></span>
                            <label class="floating-label">Type of Complaint </label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input type="hidden" asp-for="PhotoPath" />
                            @{
                                if (Model.PhotoPath != null && Model.PhotoPath != "")
                                {
                                    <br />
                                    <input disabled asp-for="Photo" id="Photolatlong" type="file" class="floating-label-field floating-label-field--s3" />
                                    <a class="badge bg-primary px-2" asp-action="ViewPhoto" asp-route-id="@Model.Id" target="_blank"> (View Photo)</a>

                                }
                                else
                                {
                                    <input disabled asp-for="Photo" id="Photolatlong" type="file" class="floating-label-field floating-label-field--s3" />
                                }
                            }
                            <span asp-validation-for="Photo" class="text-danger"></span>
                            <label class="floating-label">Photograph</label>                           
                            <label class="err" style="color:red;display:none;">
                                system is unable to find the coordinates in uploaded image.
                                kindly upload the image which is having location information or
                                enter latitude and longitude manually.
                            </label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <input class="floating-label-field floating-label-field--s3" id="slat" placeholder="Lattitude" disabled asp-for="Lattitude" />
                            <span asp-validation-for="Lattitude" class="text-danger"></span>
                            <label class="floating-label">Lattitude </label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input placeholder="Longitude" id="slong" class="floating-label-field floating-label-field--s3" disabled asp-for="Longitude" />
                            <span asp-validation-for="Longitude" class="text-danger"></span>
                            <label class="floating-label">Longitude </label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="LocationId" asp-items="@(new SelectList(Model.LocationList, "Id","Name"))">
                                <option>Select</option>
                            </select>
                            <span asp-validation-for="LocationId" class="text-danger"></span>
                            <label class="floating-label">Location </label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea type="text" disabled asp-for="AddressOfEncroachedProperty" class="floating-label-field floating-label-field--s3" limit="4000"></textarea>
                            <span asp-validation-for="AddressOfEncroachedProperty" class="text-danger"></span>
                            <label class="floating-label">Address/Landmark of Encroached Property</label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input class="floating-label-field floating-label-field--s3" disabled asp-for="EncroacherName" placeholder="name" />
                            <span asp-validation-for="EncroacherName" class="text-danger"></span>
                            <label class="floating-label">Name of Encroacher</label>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <label class="floating-label">  DDA Land</label>
                        </div>
                        </div>
                        <div class="col-md-2">
                            <input type="hidden" disabled asp-for="DDALand" id="DDAid" value="yes" />

                            <div class="form-check theme">
                                <input class="form-check-input" type="radio" disabled asp-for="DDALand" name="DDA" id="ryes" value="yes" checked="checked">
                                <label class="form-check-label" for="radio">
                                    Yes
                                </label>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-check theme">
                                <input class="form-check-input" type="radio" disabled asp-for="DDALand" name="DDA" id="rno" value="no">
                                <label class="form-check-label" for="radio">
                                    No
                                </label>
                            </div>
                        </div>
                    </div>
             
                
                
                
                <div class="row pt-3">
                    <div class="col-md-12">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <textarea type="text" disabled asp-for="Remarks" placeholder="Remarks" class="floating-label-field floating-label-field--s3" limit="4000"></textarea>
                            <label class="floating-label">Remarks</label>
                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>

