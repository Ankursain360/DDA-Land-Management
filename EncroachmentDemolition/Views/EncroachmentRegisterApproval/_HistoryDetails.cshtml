@model IEnumerable<Libraries.Model.Entity.Approvalproccess>

    <a data-toggle="collapse" href="#collapseHistoryApprroval" id="collapseapp" role="button" aria-expanded="false" aria-controls="collapseHistoryApprroval">
        <h3 class="my-3 d-flex sub-heading">Approval History Details</h3>
    </a>
    <div class="collapse" id="collapseHistoryApprroval">
        <table class="table table-sm table-bordered mb-0 table-striped">
            <thead class="text-nowrap">
                <tr>
                  
                    <th>Send From </th>
                    <th>Send To</th>
                    <th>Date</th>
                    <th>Remarks</th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null && Model.Count() > 0)
                {
                    @foreach (var item in Model)
                    {
                        <tr>
                           
                            <td>
                                @item.SendFrom
                            </td>
                            <td>
                                @item.SendTo
                            </td>
                            <td>
                                @(item.CreatedDate!= null ? Convert.ToDateTime(item.CreatedDate).ToString("dd-MMM-yyyy h:mm tt") : "Not Available")
                            </td>
                            <td>
                                @item.Remarks
                            </td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <th colspan="14" style="text-align:center"> No Record Available! </th>
                    </tr>
                }
            </tbody>
        </table>

    </div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />

