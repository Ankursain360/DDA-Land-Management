    @{
        if(@ViewBag.userRole!=null)
        {
            if (@ViewBag.userRole == 1 || @ViewBag.userRole == 2 || @ViewBag.userRole == 19 || @ViewBag.userRole == 63 || @ViewBag.userRole == 7 || @ViewBag.userRole == 3 || @ViewBag.userRole == 16 || @ViewBag.userRole == 10)
            {
                <div id="divDashboard">
                    <img class="img-fluid d-block mx-auto" src="~/images/brand.png" alt="dda">
                </div>
            }
            else
            {
                <img class="img-fluid d-block mx-auto" src="~/images/brand.png" alt="dda">
            }
        }
        else
        {
            <img class="img-fluid d-block mx-auto" src="~/images/brand.png" alt="dda">
        }
    }
   
</figure>

<environment include="Development">
    <script src="~/js/HomeIndex.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/HomeIndex.min.js" asp-append-version="true"></script>
</environment> 