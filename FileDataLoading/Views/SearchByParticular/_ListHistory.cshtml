@model IEnumerable<Dto.Search.SearchByParticularFileHistoryListDataDto>
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>

            <th>IssuedToEmployee</th>
            <th>Department</th>
            <th>IssuedDate</th>
            <th>IssuedBy</th>
            <th>ReturnedDate</th>
            <th>ReturnedBy</th>

        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
        <tr>
            <td>@item.IssuedToEmployee</td>
            <td>@item.DeptId</td>
            <td>@item.IssuedDate</td>
            <td>@item.IssuedBy</td>
            <td>@item.ReturnedDate</td>
            <td>@item.ReturnedBy</td>


        </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="14" style="text-align:center"> No Record Available! </th>
            </tr>
        }
    </tbody>
</table>
