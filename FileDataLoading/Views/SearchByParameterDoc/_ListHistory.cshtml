@*@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Datastoragedetails>*@
@model IEnumerable<Dto.Search.SearchByParticularDocHistoryListDataDto>
@{int rowNo = 0; }
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>

            <th>Issued To Employee</th>
            <th>Department</th>
            <th>Issued Date</th>
            <th>Issued By</th>
            <th>Returned Date</th>
            <th>Returned By</th>

        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
        <tr>
            <td>@item.IssuedToEmployee</td>
            <td>@item.DeptId</td>
            <td>@item.IssuedDate</td>
            <td>@item.IssuedBy</td>
            <td>@item.ReturnedDate</td>
            <td>@item.ReturnedBy</td>


        </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="14" style="text-align:center"> No Record Available! </th>
            </tr>
        }
    </tbody>
</table>
@*@await Component.InvokeAsync("Pager", new { totalCount = Model.RowCount, pageNumber = Model.CurrentPage, pageSize = Model.PazeSize })*@
