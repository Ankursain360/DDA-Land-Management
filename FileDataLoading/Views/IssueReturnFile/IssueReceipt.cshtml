@model Libraries.Model.Entity.Issuereturnfile

<style>
    .p {
        color: black;
    }

    fieldset {
        border: 1px solid #ddd !important;
        margin: 0;
        xmin-width: 0;
        padding: 10px;
        position: relative;
        border-radius: 4px;
        background-color: #f5f5f5;
        padding-left: 10px !important;
    }

    label {
        font-weight: 500;
    }

    .table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
        border-top: 0px none;
    }
</style>



<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>

<script src="js/bootstrap.min.js"></script>
<link href="Styles/bootstrap.css" rel="stylesheet" />
<link href="~/css/PrintLabel.css" rel="stylesheet" />

<div class="content contentstyle" id="DivDemandReport">


    <div class="panel panel-default">
        <div class="panel-heading" style="border: groove;background-color:white;">

            <table class="table table-bordered" border="0">


                <tr>
                    <td>
                        <img src="https://res.cloudinary.com/deb9cwxor/image/upload/v1596010902/ow9sj5vbbpymed9eydrl.png" class="img-responsive img" alt="">
                    </td>
                    <td align="center">
                        <div>
                            <h style="font-size:20px; color:black">

                                <u> <b> DELHI DEVELOPMENT AUTHORITY</b></u>

                            </h>
                            <p style="color:black; font-size:16px; text-align:center;">

                                <b align="center">
                                    <u>
                                        Vikas Sadan, New Delhi
                                    </u>
                                </b>

                            </p>
                            <p style="color:black; font-size:16px; text-align:center;"><u>Issue Receipt</u></p>
                        </div>
                    </td>
                    <td>
                        <img src="https://res.cloudinary.com/deb9cwxor/image/upload/v1596882092/dupx7btfcqjequcgsb00.jpg" class="img-responsive img" alt="">

                    </td>
                </tr>


            </table>

        </div>

        <div class="panel-body">

            <table class="table" border="0" style="border:none; border-top: none;  ">

                <tr>
                    <td class="tdstyle">
                        <p style="color:black; font-weight:700;">File No.</p>
                    </td>
                    <td class="tdstyle">
                        <label class="control-label" style="font-weight: 500;">@Model.DataStorageDetails.FileNo</label>
                    </td>
                    <td class="tdstyle">
                        <p style="color:black; font-weight:700;">File Name/Subject </p>
                    </td>
                    <td class="tdstyle">
                        <label class="control-label" >@Model.DataStorageDetails.Name</label>
                    </td>
                </tr>


            </table>

            <br />
            <fieldset>
                <legend style="font-size:medium;font-weight:700;"> Location(Village) Details</legend>

                <table class="table" border="0" style="border:none; border-top: none;  ">


                    <tr>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Compactor/Room No. :</p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >@Model.DataStorageDetails.RecordRoomNo</label>
                        </td>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Almirah No. : </p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Model.DataStorageDetails.Almirah == null?"": Model.DataStorageDetails.Almirah.AlmirahNo)

                            </label>
                        </td>
                    </tr>
                    <tr>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Row No. :</p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Model.DataStorageDetails.Row == null?"": Model.DataStorageDetails.Row.RowNo)

                            </label>
                        </td>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Column No. : </p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Model.DataStorageDetails.Column == null?"": Model.DataStorageDetails.Column.ColumnNo)

                            </label>
                        </td>
                    </tr>
                    <tr>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Bundle No. :</p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" style="font-weight: 500;">
                                @(Model.DataStorageDetails.Bundle == null?"": Model.DataStorageDetails.Bundle.BundleNo)
                            </label>
                        </td>
                    </tr>

                </table>


            </fieldset>
            <br />
            <fieldset>
                <legend style="font-size:medium;font-weight:700;"> Issuing Details</legend>
                <table class="table" border="0" style="border:none; border-top: none;  ">


                    <tr>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Issued To (Employee Name) :</p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >@Model.IssuedToEmployee</label>
                        </td>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Department : </p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Model.Department == null?"": Model.Department.Name)

                            </label>
                        </td>
                    </tr>
                    <tr>
                       
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Branch Society :</p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Model.Branch == null?"": Model.Branch.Name)

                            </label>
                        </td>

                    </tr>
                    <tr>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Designation : </p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Model.Designation == null?"": Model.Designation.Name)

                            </label>
                        </td>
                        <td class="tdstyle">
                            <p style="color:black; font-weight:700;">Issued Date :</p>
                        </td>
                        <td class="tdstyle">
                            <label class="control-label" >
                                @(Convert.ToDateTime(Model.IssuedDate).ToString("dd MMM yyyy"))

                            </label>
                        </td>
                    </tr>

                </table>

            </fieldset>
            <br />
            <br />
            <table class="table" border="0" style="border:none; border-top: none;  ">
                <tr>

                    <td class="tdstyle" align="right">
                        <label class="control-label">Signature</label>
                    </td>
                </tr>

            </table>
            <button class="btn btn-success jhide" type="submit" id="btnPrint">Print</button>
            <a asp-action="Index" asp-controller="IssueReturnFile" class="btn btn-primary jhide">Back</a>
        </div>
    </div>
</div>



<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>




<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/issuefiledata.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/issuefiledata.min.js" asp-append-version="true"></script>
</environment>
