@model Dto.Master.ApiResponseCompactor


@await Component.InvokeAsync("PageHeader", new { pageName = " Compactor Details" })


<div class="pt-3">

    <form asp-action="Index1" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex"> Details</h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="FileNo" required placeholder="Name" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span class="text-danger"></span>
                    <label class="floating-label">File No   </label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="Subject"  placeholder="Name" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span class="text-danger"></span>
                    <label class="floating-label">Subject  </label>
                </div>
            </div>
        </div>
        <div class="row mt-md-3">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">

                    <input asp-for="Scheme"  placeholder="Name" type="text"
                           class="floating-label-field floating-label-field--s3" />
                    <span class="text-danger"></span>
                    <label class="floating-label">Scheme  </label>
                </div>
            </div>

        </div>
        <div class="row mt-md-3">
            <div class=" col-md-4">
                <button class="btn btn-theme " type="submit">Submit</button>

            </div>
            @*<div class=" col-md-4">
                <button class="btn btn-link btn-block d-flex" id="btnReset" type="submit">
                    <i class="ri-refresh-line mr-2"></i>Reset
                </button>
            </div>*@
        </div>

    </form>
   
    @if (Model != null)
    {
        <div class="table-section bg-white border shadow rounded">


            <div class="table-responsive ">

                <table class="table table-sm table-bordered mb-0 table-striped">
                    <thead class="text-nowrap">
                        @*<tr  style="text-align:center"> <th colspan="2"> Compactor Details</th></tr>*@
                        <tr>
                            <th> MAIN FILE NO</th>
                            <th> @Model.cargo[0].MAIN_FILE_NO</th>
                        </tr>

                        @*<tr>

                                <th> MAIN FILE NO</th>
                                <th> PART FILE NO</th>
                                <th>SUBJECT</th>
                                <th>NAME</th>
                                <th>ROOM NO</th>
                                <th>COMPACTOR NO</th>
                                <th>ROW NO</th>
                                <th>COLUMN NO</th>
                                <th>ALMIRAH NO</th>
                                <th>FLT CTGRY ID</th>
                                <th>LOCALITY HEADER</th>
                                <th>SEQUENCE NO</th>
                                <th>YEAR</th>

                            </tr>*@
                    </thead>
                    <tbody>
                        <tr>
                            <th> PART FILE NO</th>
                            <td>@Model.cargo[0].PART_FILE_NO</td>
                        </tr>
                        <tr>
                            <th>SUBJECT</th>
                            <td>@Model.cargo[0].SUBJECT</td>
                        </tr>
                        <tr>
                            <th>NAME</th>
                            <td>@Model.cargo[0].NAME</td>
                        </tr>
                        <tr>
                            <th>ROOM NO</th>
                            <td>@Model.cargo[0].RM_NO</td>
                        </tr>
                        <tr>
                            <th>COMPACTOR NO</th>
                            <td>@Model.cargo[0].CMPCTR_NO</td>
                        </tr>
                        <tr>
                            <th>ROW NO</th>
                            <td>@Model.cargo[0].RW_NO</td>
                        </tr>
                        <tr>
                            <th>COLUMN NO</th>
                            <td>@Model.cargo[0].CLMN_NO</td>
                        </tr>
                        <tr>
                            <th>ALMIRAH NO</th>
                            <td>@Model.cargo[0].ALMRH_NO</td>
                        </tr>
                        <tr>
                            <th>FLT CTGRY ID</th>
                            <td>@Model.cargo[0].FLT_CTGRY_ID</td>
                        </tr>
                        <tr>
                            <th>LOCALITY HEADER</th>
                            <td>@Model.cargo[0].LCLTY_HDR</td>
                        </tr>
                        <tr>
                            <th>SEQUENCE NO</th>
                            <td>@Model.cargo[0].SQNC_NO</td>
                        </tr>
                        <tr>
                            <th>YEAR</th>
                            <td>@Model.cargo[0].YR</td>
                        </tr>
                        @*<tr>

                                    <td>@Model.cargo[0].MAIN_FILE_NO</td>
                                    <td>@Model.cargo[0].PART_FILE_NO</td>
                                    <td>@Model.cargo[0].SUBJECT</td>
                                    <td>@Model.cargo[0].NAME</td>
                                    <td>@Model.cargo[0].RM_NO</td>
                                    <td>@Model.cargo[0].CMPCTR_NO</td>
                                    <td>@Model.cargo[0].RW_NO</td>
                                    <td>@Model.cargo[0].CLMN_NO</td>
                                    <td>@Model.cargo[0].ALMRH_NO</td>
                                    <td>@Model.cargo[0].FLT_CTGRY_ID</td>
                                    <td>@Model.cargo[0].LCLTY_HDR</td>
                                    <td>@Model.cargo[0].SQNC_NO</td>
                                    <td>@Model.cargo[0].YR</td>

                            </tr>*@

                    </tbody>

                </table>
            </div>


        </div>

    }
    else
    {
        <p>
            <b style="text-align:center"> No Record Available! </b>
        </p>
    }
</div>



<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
