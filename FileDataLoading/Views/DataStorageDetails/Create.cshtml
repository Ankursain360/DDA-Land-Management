@model Libraries.Model.Entity.Datastoragedetails
@await Component.InvokeAsync("PageHeader", new { pageName = "Data Storage Details" })
<div class="pt-3">
    <form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">Add Details</h3>

        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

      
        <div class="row">
            <div class="col-md-6">
                <label class="radio-inline">
                    <input type="hidden" asp-for="IsFileDocument" id="IsFileDocument" value="0" />
                    <input type="radio" name="grpSearchby" id="rdbFilename" value="" checked="checked" />File
                </label>
                <label class="radio-inline">
                    <input type="radio" name="grpSearchby" id="rdbDocumentname" />Document
                </label>
            </div>
        </div>

        <div class="row mt-md-3">

            <div id="DivForFileName">

                <h3 class="mb-3 d-flex">File No</h3>


                <div class="row">

                    <div class="col-md-1">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="CategoryNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                   limit="10">
                            <span asp-validation-for="CategoryNo" class="text-danger "></span>
                            <label class="floating-label">CategoryNo</label>
                        </div>
                    </div>

                    <div class="col-md-1">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="HeaderNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                   limit="10">
                            <span asp-validation-for="HeaderNo" class="text-danger "></span>
                            <label class="floating-label">Header No</label>
                        </div>
                    </div>


                    <div class="col-md-2">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="SequenceNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                   limit="10">
                            <span asp-validation-for="SequenceNo" class="text-danger "></span>
                            <label class="floating-label">Sequence No</label>
                        </div>
                    </div>

                    <div class="col-md-1">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input asp-for="Year" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                   limit="10">
                            <span asp-validation-for="Year" class="text-danger "></span>
                            <label class="floating-label">Year No</label>
                        </div>
                    </div>
                    <div class="col-md-1">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" class="floating-label-field floating-label-field--s3" />

                        </div>
                    </div>



                    <div class="col-md-2">
                        <div class="floating-label-wrap">

                            <select asp-for="SchemeId" class="floating-label-field floating-label-field--s3"
                                    asp-items="@(new SelectList(Model.schemaList,"Id","Name"))">
                                <option value="">---Select---</option>

                            </select>
                            <label class="floating-label">Scheme </label>
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="floating-label-wrap">

                            <select asp-for="ZoneId" class="floating-label-field floating-label-field--s3"
                                    asp-items="@(new SelectList(Model.ZoneList,"Id","Name"))">
                                <option value="">---Select---</option>

                            </select>
                            <label class="floating-label">Zone </label>
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="floating-label-wrap">

                            <select asp-for="LocalityId" class="floating-label-field floating-label-field--s3"
                                    asp-items="@(new SelectList(Model.LocalityList,"Id","Name"))">
                                <option value="">---Select---</option>

                            </select>
                            <label class="floating-label">Locality(Village) </label>
                        </div>
                    </div>



                </div>

                <div class="row mt-md-3">


                    <div class="col-md-6">
                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" class="floating-label-field floating-label-field--s3" placeholder="Name/Subject" />
                            <label class="floating-label">Name/Subject</label>
                        </div>
                    </div>


                    <div class="form-group col-md-6">
                        <br />
                        <label class="control-label lb-sm">Is part of this main file?</label><span style="color:red">*</span>
                        <label class="radio-inline">
                            <input type="radio" name="grpMainFile" id="rSubsequent" checked>Yes
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="grpMainFile" id="rOriginal">No
                        </label>
                    </div>

                </div>


            </div>
        </div>


        <div id="DivForSubsequentPurchaser">

            <h3 class="mb-3 d-flex">File No</h3>

            <div class="table-section bg-white border shadow rounded">

                <div class="table-responsive">
                    <table class="table table-sm table-bordered mb-0 table-striped" id="tbl_posts">
                        <thead class="text-nowrap">

                            <tr>
                                <th>Sr.No</th>
                                <th>Category</th>
                                <th>Header</th>
                                <th>Sequence No.</th>
                                <th>Year</th>
                                <th>Scheme/Department/Branch</th>
                                <th>Locality(Village)</th>
                                <th>Subject</th>
                                <th>Operation</th>
                            </tr>
                        </thead>
                        <tbody id="tbl_posts_body">
                        </tbody>
                        <tbody class="tbody" id="add">
                            <tr>
                                <td style="text-align:center;"><span class="sn">1</span></td>

                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="text" asp-for="Category" placeholder="Category" class="floating-label-field floating-label-field--s3" id="NameOfStructure" />
                                    </div>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="text" asp-for="Header" placeholder="Header" class="floating-label-field floating-label-field--s3" id="NameOfStructure" />
                                    </div>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="text" asp-for="SequenceNoForPartFile" placeholder="Sequence No" class="floating-label-field floating-label-field--s3" id="NameOfStructure" />
                                    </div>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="text" asp-for="YearForPartFile" placeholder="Year No" class="floating-label-field floating-label-field--s3" id="NameOfStructure" />
                                    </div>
                                </td>
                                <td>
                                    <select asp-for="SchemeDptBranch" class="floating-label-field floating-label-field--s3"
                                            asp-items="@(new SelectList(Model.schemaList,"Id","Name"))">
                                        <option value="">---Select---</option>

                                    </select>
                                </td>
                                <td>
                                    <select asp-for="LocalityIdForPartFile" class="floating-label-field floating-label-field--s3"
                                            asp-items="@(new SelectList(Model.LocalityList,"Id","Name"))">
                                        <option value="">---Select---</option>

                                    </select>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="text" asp-for="Subject" placeholder="Subject" class="floating-label-field floating-label-field--s3" id="NameOfStructure" />
                                    </div>
                                </td>
                                <td>
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <a class="btn btn-theme delete-record" data-id="0" style="display:none;color:white;"><i class="fa fa-trash"></i></a>
                                        <a class="btn btn-theme add-record" data-added="0" style="color:white;"><i class="fa fa-plus"></i></a>
                                    </div>
                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>

            </div>
        </div>



        <div id="DivLocation">
            <h3 class="mb-3 d-flex">Loaction(Village) Details</h3>

            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="RecordRoomNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                               limit="50">
                        <span asp-validation-for="RecordRoomNo" class="text-danger "></span>
                        <label class="floating-label">RecordRoom No</label>
                    </div>
                </div>


                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="AlmirahId" class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(Model.AlmirahList,"Id","AlmirahNo"))">
                            <option value="">---Select---</option>

                        </select>
                        <span asp-validation-for="AlmirahId" class="d-block text-danger field-validation-valid"></span>

                        <label class="floating-label">Almirah No<span style="color:red">*</span></label>
                    </div>
                </div>




            </div>
            <div class="row mt-md-3">


                <div class="col-md-6">
                    <div class="floating-label-wrap">
                       <select class="floating-label-field floating-label-field--s3" asp-for="RowId" asp-items="@(new SelectList(Model.RowList, "Id","RowNo"))"  required="required" requiredmsg="Row No is Mandatory">

                             
                            <option value="">---Select---</option>

                        </select>
                        <span asp-validation-for="RowId" class="d-block text-danger field-validation-valid"></span>

                        <label class="floating-label">Row No<span style="color:red">*</span></label>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="ColumnId" class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(Model.ColumnList,"Id","ColumnNo"))" required="required" requiredmsg="Column No is Mandatory">
                            <option value="">---Select---</option>

                        </select>
                        <span asp-validation-for="ColumnId" class="d-block text-danger field-validation-valid"></span>

                        <label class="floating-label">Column No<span style="color:red">*</span></label>
                    </div>
                </div>



            </div>
            <div class="row mt-md-3">


                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="BundleId" class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(Model.BundleList,"Id","BundleNo"))" required="required" requiredmsg="Bundle No is Mandatory">
                            <option value="">---Select---</option>

                        </select>
                        <span asp-validation-for="BundleId" class="d-block text-danger field-validation-valid"></span>

                        <label class="floating-label">Bundle No<span style="color:red">*</span></label>
                    </div>
                </div>




            </div>
        </div>


        <div id="Divflat">

            <div class="row mt-md-3"></div>
            <h3 class="mb-3 d-flex">Property/Khasra No</h3>

            <div class="row">
                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="ZoneId" class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(Model.ZoneList,"Id","Name"))" required="required" requiredmsg="Zone No is Mandatory">>
                            <option value="">---Select---</option>

                        </select>
                        <span asp-validation-for="ZoneId" class="d-block text-danger field-validation-valid"></span>

                        <label class="floating-label">Zone No<span style="color:red">*</span></label>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="floating-label-wrap">
                        <select asp-for="LocalityId" class="floating-label-field floating-label-field--s3"
                                asp-items="@(new SelectList(Model.LocalityList,"Id","Name"))" required="required" requiredmsg="Locality No is Mandatory">>
                            <option value="">---Select---</option>

                        </select>
                        <span asp-validation-for="LocalityId" class="d-block text-danger field-validation-valid"></span>

                        <label class="floating-label">Locality <span style="color:red">*</span></label>
                    </div>
                </div>





            </div>
            <div class="row mt-md-3">

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="KhasraNoPropertyNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                               limit="50">
                        <span asp-validation-for="KhasraNoPropertyNo" class="text-danger "></span>
                        <label class="floating-label"> KhasraNo Property No</label>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input asp-for="Area" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                               limit="50">
                        <span asp-validation-for="Area" class="text-danger "></span>
                        <label class="floating-label"> Area</label>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid" id="DivForDocumentName" style="display:none">
            <div class="panel-group">
                <div class="panel">
                    <div class="row mt-md-3"></div>
                    <h3 class="mb-3 d-flex">File No</h3>

                    <div class="row">

                        <div class="col-md-1">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input asp-for="CategoryNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                       limit="50">
                                <span asp-validation-for="CategoryNo" class="text-danger "></span>
                                <label class="floating-label">CategoryNo</label>
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input asp-for="HeaderNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                       limit="50">
                                <span asp-validation-for="HeaderNo" class="text-danger "></span>
                                <label class="floating-label">Header No</label>
                            </div>
                        </div>


                        <div class="col-md-2">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input asp-for="SequenceNo" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                       limit="50">
                                <span asp-validation-for="SequenceNo" class="text-danger "></span>
                                <label class="floating-label">Sequence No</label>
                            </div>
                        </div>

                        <div class="col-md-2">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input asp-for="Year" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                                       limit="50">
                                <span asp-validation-for="Year" class="text-danger "></span>
                                <label class="floating-label">Year</label>
                            </div>
                        </div>


                        <div class="col-md-1">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" class="floating-label-field floating-label-field--s3" />

                            </div>
                        </div>


                        <div class="col-md-2">
                            <div class="floating-label-wrap">
                                <select asp-for="SchemeId" class="floating-label-field floating-label-field--s3"
                                        asp-items="@(new SelectList(Model.schemaList,"Id","Name"))">
                                    <option value="">---Select---</option>

                                </select>
                                <label class="floating-label">Scheme/Department/Branch </label>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="floating-label-wrap">

                                <select asp-for="LocalityId" class="floating-label-field floating-label-field--s3"
                                        asp-items="@(new SelectList(Model.LocalityList,"Id","Name"))">
                                    <option value="">---Select---</option>

                                </select>
                                <label class="floating-label">Locality(Village)</label>
                            </div>
                        </div>


                    </div>
                    <br />
                    <div class="row">

                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" class="floating-label-field floating-label-field--s3" placeholder="Name/Subject" />
                                <label class="floating-label">Name/Subject</label>
                            </div>
                        </div>


                    </div>
                    <div class="row mt-md-3"></div>
                    <h3 class="mb-3 d-flex">Location(Village) Details</h3>



                    <div class="row">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" class="floating-label-field floating-label-field--s3" placeholder="Record Room No" />
                                <label class="floating-label">Record Room No</label>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="floating-label-wrap">

                                <select asp-for="AlmirahId" class="floating-label-field floating-label-field--s3"
                                        asp-items="@(new SelectList(Model.AlmirahList,"Id","AlmirahNo"))">
                                    <option value="">---Select---</option>

                                </select>
                                <label class="floating-label">Almirah Id/Compactor No</label>
                            </div>
                        </div>

                    </div>
                    <div class="row mt-md-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap">
                                <select asp-for="RowId" class="floating-label-field floating-label-field--s3"
                                        asp-items="@(new SelectList(Model.RowList,"Id","RowNo"))">
                                    <option value="">---Select---</option>

                                </select>
                                <label class="floating-label">Row Id</label>
                            </div>
                        </div>

                        <div class="col-md-6">


                            <div class="floating-label-wrap">

                                <select asp-for="ColumnId" class="floating-label-field floating-label-field--s3"
                                        asp-items="@(new SelectList(Model.ColumnList,"Id","ColumnNo"))">
                                    <option value="">---Select---</option>

                                </select>
                                <label class="floating-label">Column Id</label>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-md-3">
                        <div class="col-md-6">

                            <div class="floating-label-wrap">

                                <select asp-for="BundleId" class="floating-label-field floating-label-field--s3"
                                        asp-items="@(new SelectList(Model.BundleList,"Id","BundleNo"))">
                                    <option value="">---Select---</option>

                                </select>
                                <label class="floating-label">Bundle No</label>
                            </div>

                        </div>

                    </div>
                    <div class="row">
                        <div class=" col-md-6">
                            <div class="floating-label-wrap">
                                <select class="floating-label-field floating-label-field--s3" asp-for="IsActive">

                                    <option value="1">Active</option>
                                    <option value="0">Inactive</option>
                                </select>
                                <span asp-validation-for="IsActive" class="text-danger"></span>
                                <label asp-for="IsActive" class="floating-label">Status </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
        <div class="row mt-md-3">
            <div class="col-md-12">
                <input type="submit" value="Create" class="btn btn-theme" />
                @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
                @Html.ActionLink("Back", "Index", "DataStorageDetails", null, new { @class = "btn btn-theme" })
            </div>
        </div>


    </form>


</div>


<environment include="Development">
    <script src="~/js/DataStoragecreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/DataStoragecreate.min.js" asp-append-version="true"></script>
</environment>