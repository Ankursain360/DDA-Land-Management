        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create" method="post" id="1">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>


                <div class="row" style="margin-bottom:35px;">
                    <div class="col-sm-12">
                        <div class=" myapp-hed2">
                            <div class="col-md-12">
                                <h6 class="pull-left" style="color:white;margin-top:5px;">Filing Date is between specified period</h6>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="container-fluid" id="DivForFileName">
                    <div class="panel-group">
                        <div class="panel">
                            @*<div class="form-row ">
                                    <div class="form-group col-md-12 bg">Status Report DateWise</div>

                                </div>*@
                            <div class="form-row">

                                <div class="form-group col-md-6">
                                    <label class="control-label lb-sm">Date From</label>
                                    <input type="Date" style="width:100%;max-width:100%;min-width:unset !important;" class="form-control" />
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="control-label lb-sm">To</label>
                                    <input type="Date" style="width:100%;max-width:100%;min-width:unset !important;" class="form-control" />
                                </div>





                            </div>




                            <div class="form-row">
                                <div class="form-group col-md-12">
                                    <label class="control-label lb-sm">Remarks</label>
                                    <textarea type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                                </div>

                            </div>

                            <br />

                        </div>
                    </div>


                    <div class="form-row">
                        <div class="form-group  col-md-12" style="justify-content:center">
                            <input type="button" value="Reterive" onclick="showcollection();" class="btn btn-primary myapp-hed1" />

                        </div>
                    </div>
                </div>

                <div class="container-fluid" id="showcollection" style="display:none">

                    <div class="card  table-card" style="overflow:scroll;">

                        <table class="table table-responsive table-bordered table-card" style="display:none" id="showtr">


