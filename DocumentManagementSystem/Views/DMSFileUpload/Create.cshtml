@model Libraries.Model.Entity.Dmsfileupload

@await Component.InvokeAsync("PageHeader", new { pageName = "File Upload" })
<br />
<br />
<div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
    <h3 class="mb-3 d-flex">Add Details</h3>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="pt-3">

        <div asp-action="Create" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data" autocomplete="off">
            <ul class="nav nav-tabs md-tabs" id="myTabMD" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="rbtFileUpload" data-toggle="tab" href="#home-md" role="tab" aria-controls="home-md"
                       aria-selected="true">File Upload</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="rbtBulkUpload" data-toggle="tab" href="#profile-md" role="tab" aria-controls="profile-md"
                       aria-selected="false">Bulk Upload</a>
                </li>
            </ul>
            <input type="hidden" asp-for="IsFileBulkUpload" value="File Upload" />
            <br />
            <div id="divfileupload">

                <form asp-action="Create" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data" autocomplete="off">
                    <input type="hidden" asp-for="IsFileBulkUpload" value="File Upload" />
                    @*<div class="row">
                    <div class="col-md-3">
                        <input type="hidden" asp-for="IsFileBulkUpload" value="File Upload" />

                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" asp-for="IsFileBulkUpload" name="grpIsFileBulkUpload" id="rbtFileUpload" value="File Upload" checked>

                            <label class="form-check-label" for="radio">
                                File Upload
                            </label>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-check theme">
                            <input class="form-check-input" type="radio" asp-for="IsFileBulkUpload" name="grpIsFileBulkUpload" id="rbtBulkUpload" value="Bulk Upload">
                            <label class="form-check-label" for="radio">
                                Bulk Upload
                            </label>
                        </div>
                    </div>
                </div>*@
                    <div>
                        <h3 class="my-3 d-flex sub-heading">File Upload</h3>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="FileNo" type="text" class="floating-label-field floating-label-field--s3 ">
                                    <span asp-validation-for="FileNo" class="text-danger"></span>
                                    <label class="floating-label">File No.</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="AlloteeName" type="text" class="floating-label-field floating-label-field--s3 ">
                                    <span asp-validation-for="AlloteeName" class="text-danger"></span>
                                    <label class="floating-label">Allotee Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <select class="floating-label-field floating-label-field--s3" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))">
                                        <option selected="selected" disabled="disabled" value="">--Select-- </option>
                                    </select>
                                    <span asp-validation-for="DepartmentId" class="text-danger"></span>
                                    <label asp-for="DepartmentId" class="floating-label">Department </label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <select class="floating-label-field floating-label-field--s3" asp-for="LocalityId" asp-items="@(new SelectList(Model.LocalityList, "Id","Name"))">
                                        <option selected="selected" disabled="disabled" value="">--Select-- </option>
                                    </select>
                                    <span asp-validation-for="LocalityId" class="text-danger"></span>
                                    <label asp-for="LocalityId" class="floating-label">Locality </label>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3" asp-for="KhasraNoId" asp-items="@(new SelectList(Model.KhasraNoList, "Id","KhasraNo"))">
                                        <option selected="selected" disabled="disabled" value="">--Select-- </option>
                                    </select>
                                    <span asp-validation-for="KhasraNoId" class="text-danger"></span>
                                    <label asp-for="KhasraNoId" class="floating-label">Khasra No. </label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="PropertyNoAddress" type="text" class="floating-label-field floating-label-field--s3 ">
                                    <span asp-validation-for="PropertyNoAddress" class="text-danger"></span>
                                    <label class="floating-label">Property No./Address</label>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap">
                                    <input asp-for="Title" type="text" class="floating-label-field floating-label-field--s3 ">
                                    <span asp-validation-for="Title" class="text-danger"></span>
                                    <label class="floating-label">Title</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="AlmirahNo" type="text" class="floating-label-field floating-label-field--s3 ">
                                    <span asp-validation-for="AlmirahNo" class="text-danger"></span>
                                    <label class="floating-label">Almirah No.</label>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input type="file" asp-for="FileUpload" class="floating-label-field floating-label-field--s3">
                                    <span asp-validation-for="FileUpload" class="d-block text-danger field-validation-valid"></span>
                                    <label for="field-1" class="floating-label">Upload File</label>
                                </div>
                            </div>
                            <div class=" col-md-6">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3" asp-for="IsActive" disabled>
                                        <option value="1">Active</option>
                                        <option value="0">Inactive</option>
                                    </select>
                                    <span asp-validation-for="IsActive" class="text-danger"></span>
                                    <label asp-for="IsActive" class="floating-label">Status </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-md-3">
                        <div class=" col-md-12">
                            <button class="btn btn-theme " type="submit">Create</button>
                            @Html.ActionLink("Back", "Index", "DMSFileUpload", null, new { @class = "btn btn-theme " })

                        </div>
                    </div>

                </form>

            </div>
            <div id="divBulkFileUpload" style="display:none;">
                @*divLoadData*@

                <form asp-action="CreateBulkUpload" method="post" id="frmInfo" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data" autocomplete="off">
                    <div>
                        <h3 class="my-3 d-flex sub-heading">Bulk Upload</h3>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="PdfLocationPath" type="text" class="floating-label-field floating-label-field--s3 ">
                                    <span asp-validation-for="PdfLocationPath" class="text-danger"></span>
                                    <label class="floating-label">Enter Pdf File Location </label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))">
                                        <option selected="selected" disabled="disabled" value="">--Select-- </option>
                                    </select>
                                    <span asp-validation-for="DepartmentId" class="text-danger"></span>
                                    <label asp-for="DepartmentId" class="floating-label">Department </label>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-md-3">
                            <div class="col-md-6">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input type="file" asp-for="FileUpload" class="floating-label-field floating-label-field--s3">
                                    <span asp-validation-for="FileUpload" class="d-block text-danger field-validation-valid"></span>
                                    <label for="field-1" class="floating-label">Upload CSV File</label>

                                    <a class="badge badge-primary px-2" asp-action="DownloadCSVFormat"> (Download CSV Format)</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mt-md-3">
                        <div class=" col-md-12">
                            <button class="btn btn-theme " type="submit">Create</button>
                            @Html.ActionLink("Back", "Index", "UserManagement", null, new { @class = "btn btn-theme " })
                        </div>
                    </div>
                </form>
            </div>
        </div>


    </div>

</div>




<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/dmsfileuploadcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/dmsfileuploadcreate.js" asp-append-version="true"></script>
</environment>



