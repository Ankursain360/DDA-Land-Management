
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*@model DDAPropertyREG.Models.Tblzonemaster*@


<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Relief Report</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Specify Details  </h6>
        </div>
        <div class="panel-body">
            <form asp-action="RptCasesCourtWise">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Payee Name</label><span style="color:red">*</span>
                        <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                            <option value="0">---Select---</option>

                        </select>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Locality </label><span style="color:red">*</span>
                        <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                            <option value="0">---Select---</option>

                        </select>
                    </div>


                </div>
                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">From Date  </label><span style="color:red">*</span>
                        <input required type="date" class="form-control" />
                    </div>

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm"> To Date </label><span style="color:red">*</span>
                        <input required type="date" class="form-control" />
                    </div>


                </div>
                <div class="form-row">
                    <div class=" form-group  col-md-12">
                        <input type="submit" value="Generate" class="btn btn-primary myapp-hed1" />

                    </div>
                </div>

                <div id="divsearch">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Relief Report</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-12">

                            <div id="courtwiserpt">
                                <div class="card  table-card table-responsive">

                                    <table class="table  table-bordered table-card">

                                        <thead class="thead">
                                            <tr class="myapp-hed1">
                                                <th>Sr.No</th>

                                                <th> Locality </th>

                                                <th> Payee Name </th>
                                                <th>  Property No   </th>
                                                <th>Demand Number</th>
                                                <th> Demand Amount   </th>
                                                <th> Rebate Amount </th>

                                            </tr>
                                        </thead>
                                        <tbody class="tbody">
                                            <tr>
                                                <td>1</td>
                                                <td></td>

                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>

                                            </tr>
                                            <tr>
                                                <td>2</td>
                                                <td></td>

                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>

                                            </tr>
                                        </tbody>

                                    </table>

                                </div>

                            </div>
                        </div>

                    </div>
                </div>
                <br />
                <div class="form-row">
                    <div class=" form-group  col-md-12">

                        <input type="submit" value="Export to Excel" class="btn btn-primary  myapp-hed1" style="width:150px !important;" />

                    </div>
                </div>

            </form>
        </div>
    </div>
</div>



@if (ViewData["Msg"] != null)
{
    DDAPropertyREG.Models.Message msg = ViewData["Msg"] as DDAPropertyREG.Models.Message;
    @await Html.PartialAsync("~/Views/Shared/_ShowMsg.cshtml", msg);
}


<script>

    $(function () {
        var dtToday = new Date();

        var month = dtToday.getMonth() + 1;
        var day = dtToday.getDate();
        var year = dtToday.getFullYear();
        if (month < 10)
            month = '0' + month.toString();
        if (day < 10)
            day = '0' + day.toString();

        var maxDate = year + '-' + month + '-' + day;
        //alert(maxDate);
    });



</script>
