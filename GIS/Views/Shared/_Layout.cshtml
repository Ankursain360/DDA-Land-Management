<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link href="img/favicon.ico" rel="shortcut icon" type="image/x-icon" sizes="16x16" />
    <link rel="icon" href="~/images/favicon.png" type="image/png" sizes="16x16" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>GIS- AI Based Encroachment Detection</title>
    <!--font css-->
    <link rel="stylesheet" href="~/font/open-sans/stylesheet.css" />

    <!--icon css-->
    <link rel="stylesheet" href="~/font/icon/remixicon.css" />

    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/Mainstyle.css" rel="stylesheet" />
    <link href="~/lib/toaster/toastr.min.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link href="~/lib/select2/select2.css" rel="stylesheet" />
    <script src="~/lib/select2/select2.js"></script>

</head>
<body class="bg-gray">
    <div id="loader-wrapper" style="display:none">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <div class="top-header">
        <header>
            <div class="container-fluid">
                <div class="d-flex align-items-center">
                    <div class="menu me-3 p-2 text-white order-md-2 me-md-auto open" id="mobileMenu">
                        <i class="ri-menu-fold-line"></i>
                        <i class="ri-menu-unfold-line"></i>
                    </div>
                    <div class="logo py-1 me-auto me-md-4 order-md-1" >
                        <a href="/" class="d-block">
                            <figure class="m-0">
                                <img src="~/images/header-logo.png" alt="DDA Logo" class="img-fluid">
                            </figure>
                        </a>
                    </div>
                    <div class="dropdown noti text-white my-2  me-3 order-md-3" style="margin-left:1070px">
                        <a asp-action="Usermanual" asp-controller="Home" target="_blank" role="button" title="User Manual" class='p-2 d-block text-white no-dec'>
                            <i class="ri-folder-user-fill" style="font-size:20px"></i>
                        </a>
                    </div>
                    <div class="dropdown noti text-white my-2  me-3 order-md-3" >
                        <a href="https://ddalmis.org.in/" role="button" id="notification" title="Go back to modules!" class='p-2 d-block text-white no-dec'>
                            @*<i class="ri-notification-2-line"></i>*@
                            <i class="ri-home-3-fill" style="font-size:20px"></i>
                            @*<small class="bg-success rounded-circle position-absolute nofi-active">99</small>*@
                        </a>
                    </div>
                    @await Component.InvokeAsync("SignInProfile")
                </div>
            </div>
        </header>
    </div>

    <div class="pt-3">
    </div>

    @*<aside class="left-menu small slow" id="leftMenu">
        <ul class="accordion list-unstyled mb-0">
             
            @await Component.InvokeAsync("SideNav")
        </ul>
    </aside>*@
    <aside class="left-menu small slow" id="leftMenu">
        <div class="accordion" id="leftMenu">
            <a class="no-dec d-flex align-items-center collapsed active" data-toggle="collapse" href="#m0" role="button"
               aria-expanded="false" aria-controls="m0">
                <i class="main-icon ri-dashboard-line"></i>
                <span class="text mr-auto">Dashboard</span>
                <i class="arrow ri-arrow-drop-down-line slow"></i>
            </a>
            <a class="no-dec d-flex align-items-center collapsed" data-toggle="collapse" href="#m1" role="button"
               aria-expanded="false" aria-controls="m1">
                <i class="main-icon ri-group-line"></i>
                <span class="text mr-auto">Master</span>
                <i class="arrow ri-arrow-drop-down-line slow"></i>
            </a>
            <div class="collapse sub-menu" id="m1" data-parent="#leftMenu">
                <ul class="list-unstyled m-0 p-0">
                    <li>
                        <a href="~/Zone/Index" class="d-block">Zone</a>
                    </li>
                    <li>
                        <a href="~/Acquiredvillage/Index" class="d-block">Village</a>
                    </li>
                    <li>
                        <a href="~/KhasraMaster/Index" class="d-block">Khasra</a>
                    </li>
                </ul>
            </div>
            <a class="no-dec d-flex align-items-center collapsed" data-toggle="collapse" href="#DecisionSupportSystem" role="button"
               aria-expanded="false" aria-controls="m1">
                <i class="main-icon ri-group-line"></i>
                <span class="text mr-auto">Decision Support System</span>
                <i class="arrow ri-arrow-drop-down-line slow"></i>
            </a>
            <div class="collapse sub-menu" id="DecisionSupportSystem" data-parent="#leftMenu">
                <ul class="list-unstyled m-0 p-0">
                   
                    <li>
                        <a href="~/GIS/AIChangeDetection" class="d-block">AI Change Detection</a>
                    </li>
                    <li>
                        <a href="~/DDADecisionSupportSystem/Index" class="d-block">DDA Decision Support System</a>
                    </li>
                    <li>
                        <a href="~/DDADecisionSupportSystem/Create" class="d-block">DDA Decision Support System Report</a>
                    </li>
                    
                </ul>
            </div>
          
        </div>
    </aside>
    <partial name="_CookieConsentPartial" />
    <section id="rightSection" class="slow bg-gray">
        <div class="container-fluid">
            <div class="section-wraper px-3  pb-md-0">
                @RenderBody()
            </div>
        </div>
    </section>
    <footer class="footer slow">
        <div class="container-fluid">
            <p class="my-0 py-2 text-center">
                <a asp-action="Index" asp-controller="DecisionSupportSystemHome">Home</a>
                | <a asp-action="PrivacyPolicy" asp-controller="DecisionSupportSystemHome">Privacy Policy</a>
                | <a asp-action="TermsandConditions" asp-controller="DecisionSupportSystemHome">Terms&Conditions</a>
                | <a asp-action="copyRight" asp-controller="DecisionSupportSystemHome">Copyright Policy</a>
                | <a asp-action="HyperlinkPolicy" asp-controller="DecisionSupportSystemHome">Hyperlink policy</a>
                | <a asp-action="WebInformationManager" asp-controller="DecisionSupportSystemHome">Web Information Manager</a>
                | <a asp-action="Contactus" asp-controller="DecisionSupportSystemHome">Contact Us</a>
                | <a asp-action="Help" asp-controller="DecisionSupportSystemHome">Help</a>
                | <a asp-action="Disclaimer" asp-controller="DecisionSupportSystemHome">Disclaimer</a>
                | <a asp-action="AboutUs" asp-controller="DecisionSupportSystemHome">AboutUs</a>
                | <a asp-action="ContingencyManagementPlan" asp-controller="DecisionSupportSystemHome">ContingencyManagementPlan</a><br>
                &nbsp; &copy; Content Owned by DDA, Government of India. &nbsp;&nbsp;<label>Last Updated : @HttpContextAccessor.HttpContext.Session.GetString("LastUpdatedDate")</label>
            </p>
        </div>
    </footer>

    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/chart.js/dist/Chart.min.js"></script>
    <script src="~/lib/toaster/toastr.min.js"></script>

    <environment include="Development">
        <script src="~/js/site.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
    @Html.Raw(ViewBag.Message)
    @using Microsoft.AspNetCore.Http
    @inject IHttpContextAccessor HttpContextAccessor
</body>
</html>
