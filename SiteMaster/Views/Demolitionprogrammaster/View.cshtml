@model Libraries.Model.Entity.Demolitionprogrammaster

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Demolition Program master </h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%">View Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="View" disabled="disabled">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>






                <div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Items<span style="color:red">*</span></label>
                        <input asp-for="Items" required="required" requiredMsg="The Items Name is required" class="form-control" limit="1000">
                        <span asp-validation-for="Items" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Items type<span style="color:red">*</span></label>

                        <select asp-for="ItemsType" class="form-control dda-dropdown">
                            <option value="Textbox">Textbox</option>
                            <option value="Number">Number</option>
                            <option value="Date">Date</option>
                        </select>
                        <span asp-validation-for="ItemsType" class="text-danger"></span>
                    </div>
                </div>

                <div class="row">




                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Status</label>
                        <select asp-for="IsActive" class="form-control dda-dropdown">
                            <option value="1">Active</option>
                            <option value="0">Inactive</option>
                        </select>
                        <span asp-validation-for="IsActive" class="text-danger"></span>
                    </div>
                </div>




                <div class="row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        @Html.ActionLink("Back", "Index", "Demolitionprogrammaster", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />










