@model Dto.Master.EditUserDto
@await Component.InvokeAsync("PageHeader", new { pageName = "User" })


<div class="container-fluid">
    <div class="panel">
       
        <div class="panel-body">
            <form class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
                <h3 class="mb-3 d-flex">Edit Details</h3>

                <input type="hidden" asp-for="Id" />
                <ul class="nav nav-tabs md-tabs" id="myTabMD" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="btnPersonalInfo" data-toggle="tab" href="#home-md" role="tab" aria-controls="home-md"
                           aria-selected="true">Personal Information</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="btnProfileInfo" data-toggle="tab" href="#profile-md" role="tab" aria-controls="profile-md"
                           aria-selected="false">Profile Information</a>
                    </li>
                </ul>
            </form>
                <br />
                <div id="divLoadData">

                </div>
        </div>
    </div>
</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />

<environment include="Development">
    <script src="~/js/usermanagementedit.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/usermanagementedit.min.js" asp-append-version="true"></script>
</environment>