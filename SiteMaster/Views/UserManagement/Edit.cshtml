@model Dto.Master.EditUserDto

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">User </h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%">Edit Details</h6>
        </div>
        <div class="panel-body">
            <input type="hidden" asp-for="Id" />
            <div class="row ">
                <div class="form-group col-md-12" align="center">
                    <div class="btn-group" role="group" aria-label="Basic example">
                        @*<button type="button" id="PersonalBtn" class="btn btn-secondary">Personal Information</button>
                        <button type="button" id="ProfileBtn" class="btn btn-secondary">Profile Information</button>*@

                        <input type="submit" value="Personal Information" id="PersonalBtn" class="btn btn-primary dda-pageheading"  />
                        <input type="submit" value="Profile Information" id="ProfileBtn" class="btn btn-primary dda-pageheading" />
                    </div>
                </div>
            </div>
            
            <br />
            <div id="divLoadData"></div>
        </div>
    </div>
</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />

<environment include="Development">
    <script src="~/js/usermanagementedit.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/usermanagementedit.min.js" asp-append-version="true"></script>
</environment>