@model IEnumerable<Libraries.Model.Entity.User>

    <div class="row">
        <div class="col-sm-12">
            <div class="dda-topheading">
                <div class="col-md-12">
                    <h6 class="pull-left" style="color:white;margin-top:5px;">User </h6>
                    <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
                </div>
            </div>
        </div>
    </div>
    <br />
    <br />

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <label class="control-label lb-sm" for="txtName">Name </label>
                <input class="form-control" id="txtName" />
            </div>
            <div class="col-md-3">
                <label class="control-label lb-sm" for="txtUserName">User Name </label>
                <input class="form-control" id="txtUserName" />
            </div>
            <div class="col-md-3">
                <label class="control-label lb-sm" for="txtEmail">Email </label>
                <input class="form-control" id="txtEmail" />
            </div>
            <div class="col-md-3">
                <label class="control-label lb-sm" for="txtPhoneNumber">Phone Number </label>
                <input class="form-control" id="txtPhoneNumber" />
            </div>
        </div>

        <div class="row ">
            <div class="form-group col-md-12" style="margin-top:20px;">
                <button id="btnSearch" class="btn btn-primary dda-pageheading">Search</button>
                <button id="btnReset" class="btn btn-primary dda-pageheading">Reset</button>
            </div>
        </div>
        <div>
            <a asp-action="Create" class="btn btn-primary pull-right dda-pageheading " style="margin-top: -10px;">Add New</a>
            @*@Html.ActionLink("Add New", "Create", "District", null, new { @class = "btn btn-primary pull-right myapp-hed1 ", style = "margin-top: -10px;" })*@
        </div>
    </div>
    <br />
    <div class="container-fluid">
        <div class="card  table-card" style="overflow:scroll;">
            <div id="divUser">

            </div>


        </div>
    </div>
    <partial name="../Shared/_ValidationScriptsPartial.cshtml" />
    <environment include="Development">

        <script src="~/js/usermanagementindex.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/usermanagementindex.min.js" asp-append-version="true"></script>
    </environment>
