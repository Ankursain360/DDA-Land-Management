@model IEnumerable<Libraries.Model.Entity.User>

<div class="row">
    <div class="col-md-3">
        <label class="control-label lb-sm" for="txtName">Name </label>
        <input class="form-control" id="txtName" />
    </div>
    <div class="col-md-3">
        <label class="control-label lb-sm" for="txtUserName">User Name </label>
        <input class="form-control" id="txtUserName" />
    </div>
    <div class="col-md-3">
        <label class="control-label lb-sm" for="txtEmail">Email </label>
        <input class="form-control" id="txtEmail" />
    </div>
    <div class="col-md-3">
        <label class="control-label lb-sm" for="txtPhoneNumber">Phone Number </label>
        <input class="form-control" id="txtPhoneNumber" />
    </div>
</div>
<div class="row">
    <div class="col-md-12">
