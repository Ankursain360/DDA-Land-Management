
@model Libraries.Model.Entity.Schemefileloading

@await Component.InvokeAsync("PageHeader", new { pageName = "Scheme" })

<div class="pt-3">
    <form asp-action="Edit" disabled="disabled" id="myForm" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">View Details</h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>


        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                   
                    <input asp-for="SchemeName" disabled placeholder="District Name" type="text"
                           class="floating-label-field floating-label-field--s3" limit="100" />

                    <span asp-validation-for="SchemeName" class="text-danger"></span>

                    <label class="floating-label">Scheme Name   </label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                   
                    <input  disabled asp-for="SchemeCode" placeholder="Code" type="text"
                           class="floating-label-field floating-label-field--s3" limit="10" />

                    <span asp-validation-for="SchemeCode" class="text-danger"></span>

                    <label class="floating-label">Scheme Code   </label>
                </div>
            </div>
        </div>

        <div class="row">

            <div class=" col-md-6">

                <div class="floating-label-wrap">
                    <select class="floating-label-field floating-label-field--s3" asp-for="IsActive">

                        <option value="1">Active</option>
                        <option value="0">Inactive</option>
                    </select>
                    <span asp-validation-for="IsActive" class="text-danger"></span>
                    <label asp-for="IsActive" class="floating-label">Status </label>
                </div>

            </div>
        </div>

        <div class="row mt-md-3">
            <div class="form-group col-md-12" style="margin-bottom:40px;">
                @Html.ActionLink("Back", "Index", "Schemefileloading", null, new { @class = "btn btn-theme " })

            </div>
        </div>

    </form>


</div>



<partial name="../Shared/_ValidationScriptsPartial.cshtml" />

