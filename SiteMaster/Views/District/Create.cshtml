@model Libraries.Model.Entity.District



<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">District</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading dda-pageheading">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form id="myForm" asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                       
                        <label class="control-label lb-sm">District Name</label><span style="color:red">*</span>
                        <input type="text" asp-for="Name" class="form-control" limit="100" />
                        <span asp-validation-for="Name" class="text-danger"></span>
                    </div>


                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">District Code</label><span style="color:red">*</span>
                        <input type="text" asp-for="Code" class="form-control" limit="200" />
                        <span asp-validation-for="Code" class="text-danger"></span>
                    </div>


                </div>

                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Status</label>
                        <select asp-for="IsActive" class="form-control dda-dropdown">
                            <option value="1">Active</option>
                            <option value="0">Inactive</option>
                        </select>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group  col-md-12" style="justify-content:center">
                        <input type="submit" value="Create" class="btn btn-primary dda-pageheading" />
                        @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
                        @Html.ActionLink("Back", "Index", "District", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/designationindex.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/designationindex.min.js" asp-append-version="true"></script>
</environment>





