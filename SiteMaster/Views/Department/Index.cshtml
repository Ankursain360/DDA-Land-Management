@model IEnumerable<Libraries.Model.Entity.Department>

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Department  </h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div>
    @Html.ActionLink("Add New", "Create", "Department", null, new { @class = "btn btn-primary pull-right dda-pageheading", style = "margin-top: -10px;" })
</div>
<br />
<br />
<div class="container-fluid">
    <div class="card  table-card table-responsive">
        <div id="divDepartmentTable"></div>
    </div>
</div>




<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/departmentindex.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/departmentindex.min.js" asp-append-version="true"></script>
</environment>
