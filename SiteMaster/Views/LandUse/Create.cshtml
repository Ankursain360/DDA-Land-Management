@model Libraries.Model.Entity.Landuse
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Land Use Master</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white " style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create"  method="post">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Land Use </label><span class="text-danger">*</span>
                        <input asp-for="Name" class="form-control" limit="200" required="required" requiredMsg="Land Use is Mandatory">
                        <span asp-validation-for="Name" class="text-danger" ></span>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Status</label><span class="text-danger">*</span>
                        <select asp-for="IsActive" class="form-control dda-dropdown">
                            <option value="1">Active</option>
                            <option value="0">Inactive</option>
                        </select>
                        <span asp-validation-for="IsActive" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Create" class="btn btn-primary dda-pageheading" onclick="return ValidateForm();"/>
                        @Html.ActionLink("Back", "Index", "LandUse", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/landusecreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/landusecreate.js" asp-append-version="true"></script>
</environment>