@model Libraries.Model.Entity.Designation

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .bg {
        background: linear-gradient(to left,#9e1c32, #991141, #900f50,#83155d, #721e68, #68226f,#5d2675,#4f2a7a, #492b80,#422c85, #392e8b,#2c3091);
        Color: #ffffff;
        Font-size: 0.95rem;
        Font-Family: Open SansRegular;
        height: 28px;
    }
</style>

<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Designation Master</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%"> Delete</h6>
        </div>
        <div class="panel-body">


            <br />
            <form asp-action="Delete" method="post">
                <div class="form-group">
                    <h6 class="p-4">Are you sure you want to delete this?</h6>
                    <input type="hidden" asp-for="Id" />
                </div>
                <div class="card-footer text-center">
                    <div class="row col-12" style="justify-content:center;">
                        <div class="col-1">
                            <input type="submit" value="Delete" class="btn btn-primary myapp-hed1" />
                            <a asp-action="Index" class="btn btn-primary myapp-hed1">Back</a>
                            @*@Html.ActionLink("Back", "Index", "ModuleMaster", null, new { @class = "btn btn-primary myapp-hed1" })*@
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>





@if (ViewData["Msg"] != null)
{
    SiteMaster.Models.Message msg = ViewData["Msg"] as SiteMaster.Models.Message;
    @await Html.PartialAsync("~/Views/Shared/_ShowMsg.cshtml", msg);
}
