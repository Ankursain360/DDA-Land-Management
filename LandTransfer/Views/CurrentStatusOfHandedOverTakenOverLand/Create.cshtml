@*@model Libraries.Model.Entity.Landtransfer*@



<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Current Status Of Handed Over Taken Over Land</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <a data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">

                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Handed Over /Taken Over Details </h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
                <div class="collapse" id="collapseExample">
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Department <span style="color:red">*</span></label>
                            <select class="form-control dda-dropdown" style="width:100%;">
                                <option value="">Select</option>
                            </select>
                            @*<span asp-validation-for="DepartmentId" class="text-danger"></span>*@
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Zone <span style="color:red">*</span></label>
                            <select class="form-control dda-dropdown" style="width:100%;">
                                <option value="value">Select</option>
                            </select>
                            @*<span asp-validation-for="ZoneId" class="text-danger"></span>*@
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Division <span style="color:red">*</span></label>
                            <select class="form-control dda-dropdown" style="width:100%;">
                                <option value="value">Select</option>
                            </select>
                            @*<span asp-validation-for="DivisionId" class="text-danger"></span>*@
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Locality(Village) Name <span style="color:red">*</span></label>
                            <select class="form-control dda-dropdown" style="width:100%;">
                                <option value="value">Select</option>
                            </select>
                            @*<span asp-validation-for="LocalityId" class="text-danger"></span>*@
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Khasra No.</label>
                            <input type="text" class="form-control" />
                            @*<span asp-validation-for="KhasraNo" class="text-danger"></span>*@
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Address</label>
                            <textarea type="text" class="form-control"></textarea>
                            @*<span asp-validation-for="Address" class="text-danger"></span>*@
                        </div>
                    </div>

                    @*<div id="LoadView">
            </div>*@
                    <div id="DivHandedOver">

                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Handed Over By</h6>
                                    </div>
                                </div>
                            </div>
                        </div>



                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Department</label><span style="color:red">*</span>
                                <select class="form-control dda-dropdown" style="width:100%;">
                                    <option value="">Select</option>
                                </select>
                                @*<span asp-validation-for="HandedOverDepartmentId" class="text-danger"></span>*@
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Handed Over By(Name/Designation)</label>
                                <input type="text" class="form-control" />
                                @*<span asp-validation-for="HandedOverByNameDesingnation" class="text-danger"></span>*@
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Date of Hand Over</label>
                                <input type="date" class="form-control" id="txtHandOverDate" pattern="\d{4}/\d{2}/\d{2}" min="15/Aug/1947" />
                                @*<span asp-validation-for="HandedOverDate" class="text-danger"></span>*@
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Order No.</label>
                                <input type="text" class="form-control" />
                                @*<span asp-validation-for="OrderNo" class="text-danger"></span>*@
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Copy of Order</label>
                                <input type="file" class="form-control" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                @*<span asp-validation-for="CopyofOrder" class="text-danger"></span>*@
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Transfer order Issue Authority</label>
                                <input type="text" class="form-control" />
                                @*<span asp-validation-for="TransferorderIssueAuthority" class="text-danger"></span>*@
                            </div>
                        </div>
                    </div>

                    <div id="DivTakenOver">

                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Taken Over By</h6>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Department</label><span style="color:red">*</span>
                                <select class="form-control dda-dropdown" style="width:100%;">
                                    <option value="">Select</option>
                                </select>
                                @*<span asp-validation-for="TakenOverDepartmentId" class="text-danger"></span>*@
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Taken Over By(Name/Designation)</label>
                                <input type="text" class="form-control" />
                                @*<span asp-validation-for="TakenOverByNameDesingnation" class="text-danger"></span>*@
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Date of Taken Over</label>
                                <input type="date" class="form-control" id="txtTakenOverDate" />
                                @*<span asp-validation-for="DateofTakenOver" class="text-danger"></span>*@
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-12">
                                <label class="control-label lb-sm">Remarks</label>
                                <textarea type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                                @*<span asp-validation-for="Remarks" class="text-danger"></span>*@
                            </div>
                        </div>



                    </div>

                </div>

                <div id="DivAddStatusDetails ">


                    @*<a data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">*@

                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Current Status Of Handed Over /Taken over details </h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*</a>*@
                    @*<div class="collapse" id="collapseExample">*@

                    <div id="tsssurvey">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">TSS Survey </h6>
                                        <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">TSS Survey  <span style="color:red">*</span></label>
                                <select class="form-control dda-dropdown" id="DD1">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                            </div>
                            @*<span asp-validation-for="DepartmentId" class="text-danger"></span>*@
                            <div id="TssSurvey" class="form-group col-md-6" style="display:none;">
                                <label class="control-label lb-sm">Upload document/report file</label>
                                <input type="file" class="form-control" id="txtTakenOverDate" />
                            </div>


                        </div>

                    </div>
                    <div id="EncroachmentDiv">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Encroachment</h6>
                                        <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Encroachment  <span style="color:red">*</span></label>
                                <select class="form-control  dda-dropdown" id="DD2">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>

                                </select>
                            </div>

                            <div id="Encroachment" style="display:none;">
                                <div class="form-group col-md-6">
                                    <label class="control-label lb-sm">Area Of  Encroachment</label>
                                    <input type="text" class="form-control" id="txtTakenOverDate" />
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="control-label lb-sm">Action On Encroachment</label>
                                    <textarea type="text" class="form-control" id="txtTakenOverDate"></textarea>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="control-label lb-sm">Upload document/report file</label>
                                    <input type="file" class="form-control" id="txtTakenOverDate" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="FencingBoundaryWall">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Fencing/Boundary Wall </h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Fencing/Boundary Wall   <span style="color:red">*</span></label>
                            <select class="form-control  dda-dropdown" id="DD3">
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            @*<span asp-validation-for="DepartmentId" class="text-danger"></span>*@
                        </div>
                        <div id="Boundary" style="display:none;">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Area Covered</label>
                                <input type="text" class="form-control" id="txtTakenOverDate" />
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Dimension (L*B*H)</label>
                                <input type="text" class="form-control" id="txtTakenOverDate" />
                            </div>
                        </div>
                    </div>
                </div>
                <div id="AreaPlotUtilization">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Area /Plot Utilization</h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Area /Plot Utilization<span style="color:red">*</span></label>
                            <select class="form-control  dda-dropdown" id="DD4">
                                <option value="0">No</option>
                                <option value="1">Yes</option>

                            </select>
                        </div>
                        <div id="Utilization" style="display:none;">
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Area Utilised </label>
                                <input type="text" class="form-control" id="txtTakenOverDate" />
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Balance Area </label>
                                <input type="text" class="form-control" id="txtTakenOverDate" />
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label lb-sm">Status </label>
                                <input type="text" class="form-control" id="txtTakenOverDate" />
                            </div>
                        </div>



                    </div>
                </div>

                <div class="row">
                    <div class="form-group col-md-12">
                        <label class="control-label lb-sm">Remarks</label>
                        <textarea type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                        @*<span asp-validation-for="Remarks" class="text-danger"></span>*@
                    </div>
                </div>

                @*</div>*@

                <div class="form-row">
                    <div class="form-group col-md-12" style="justify-content:center">
                        <input type="submit" value="Create" class="btn btn-primary dda-pageheading" />
                        <a asp-action="Index" class="btn dda-pageheading">Back </a>
                        @*@Html.ActionLink("Back", "Index", "CurrentStatusOfHandedOverTakenOverLand", null, new { @class = "btn btn-primary dda-pageheading" })*@
                    </div>
                </div>
            </form>
    </div>
    </div>
</div>
<partial name="../Shared/_ValidationScriptsPartial.cshtml" />


<script>
    $(function () {
        $('#DD1').change(function () {
            var id = $("#DD1").val();
            if (id == 1) {
                $("#TssSurvey").show();

            }
            else {
                $("#TssSurvey").hide();
            }
        });
    });

    $(function () {
        $('#DD2').change(function () {
            var id = $("#DD2").val();
            if (id == 1) {
                $("#Encroachment").show();

            }
            else {
                $("#Encroachment").hide();
            }
        });
    });


    $(function () {
        $('#DD3').change(function () {
            var id = $("#DD3").val();
            if (id == 1) {
                $("#Boundary").show();

            }
            else {
                $("#Boundary").hide();
            }
        });
    });


    $(function () {
        $('#DD4').change(function () {
            var id = $("#DD4").val();
            if (id == 1) {
                $("#Utilization").show();

            }
            else {
                $("#Utilization").hide();
            }
        });
    });
</script>