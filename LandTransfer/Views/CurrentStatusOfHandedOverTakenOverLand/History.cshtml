
@model Libraries.Model.Entity.Currentstatusoflandhistory
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;"> Current Status Of Handed Over Taken Over Land History</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div>
    <a asp-action="Index" class="btn btn-primary pull-right dda-pageheading" style="margin-top: -10px;">Back</a>
</div>
<br />
<br />
<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="History">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="Id" id="txtid"/>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">From Date</label><span style="color:red">*</span>
                        <input asp-for="CreatedDate" type="date" id="txtFromDate" class="form-control" />
                        <span asp-validation-for="CreatedDate" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-6">

                        <label class="control-label lb-sm">To Date</label><span style="color:red">*</span>
                        <input asp-for="CreatedDate" type="date" id="txtToDate" class="form-control" />
                        <span asp-validation-for="CreatedDate" class="text-danger"></span>
                    </div>

                </div>
                <div class="row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="button" value="Filter" id="btnFilter" class="btn btn-primary dda-pageheading" />
                        <input type="button" value="Refresh" onclick="location.reload(true);" class="btn btn-primary dda-pageheading" />
                    </div>
                </div>

                <div id="LoadView">

                </div>


                @*<br />
        <div class="form-row">
            <div class=" form-group  col-md-12">
                <input type="submit" value="Export to Excel" class="btn btn-primary  dda-pageheading" style="width:150px !important;" />
            </div>
        </div>*@

            </form>
        </div>
    </div>
</div>


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">

    <script src="~/js/currentStatusOfLandHistory.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/currentStatusOfLandHistory..min.js" asp-append-version="true"></script>
</environment>


