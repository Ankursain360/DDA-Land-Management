@model Libraries.Model.Entity.Landtransfer
@{int rowNo = 0;}
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Hand Over/Take Over</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <a data-toggle="collapse" href="#collapseExample" id="collapse" role="button" aria-expanded="false" aria-controls="collapseExample">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Property Registration Details </h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
                <div class="collapse" id="collapseExample">
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.ClassificationOfLandId">Classification of Land</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.ClassificationOfLandId" asp-items="@(new SelectList(Model.Propertyregistration.ClassificationOfLandList, "Id","Name"))" required="required" requiredmsg="Classification of Land is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.ClassificationOfLandId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.DepartmentId">Department</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.DepartmentId" asp-items="@(new SelectList(Model.Propertyregistration.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.DepartmentId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.ZoneId">Zone</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.ZoneId" asp-items="@(new SelectList(Model.Propertyregistration.ZoneList, "Id","Name"))" onchange="return GetDivisionList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Zone is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.ZoneId" class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">

                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.DivisionId">Division</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.DivisionId" asp-items="@(new SelectList(Model.Propertyregistration.DivisionList, "Id","Name"))" required="required" requiredmsg="Division is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.DivisionId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.LocalityId">Locality(Village)</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.LocalityId" asp-items="@(new SelectList(Model.Propertyregistration.LocalityList, "Id","Name"))" required="required" requiredmsg="Locality is Mandatory">
                            </select>
                            <span asp-validation-for="Propertyregistration.LocalityId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Khasra No</label>
                            <input asp-for="Propertyregistration.KhasraNo" class="form-control" />
                            <span asp-validation-for="Propertyregistration.KhasraNo" class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Primary List No.</label>
                            <input asp-for="Propertyregistration.PrimaryListNo" class="form-control" />
                            <span asp-validation-for="Propertyregistration.PrimaryListNo" class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Plot No. & Address with Landmark</label>
                            <textarea asp-for="Propertyregistration.Palandmark" class="form-control"></textarea>
                            <div id="feedbackPalandmark"></div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Encroachment Status</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.EncroachmentStatusId">
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.EncroachmentStatusId" class="text-danger lb-sm"></span>
                        </div>

                    </div>
                    <div class="row">
                        <div class="form-group col-md-12" id="divEncroachmentDetails">
                            <label id="l1" class="control-label lb-sm">Encroachment Details</label>
                            <input asp-for="Propertyregistration.EncraochmentDetails" class="form-control" />
                            <span asp-validation-for="Propertyregistration.EncraochmentDetails" class="text-danger lb-sm"></span>
                        </div>
                    </div>

                    <div id="DivArea">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Area</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Boundary Wall/Fencing</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.Boundary">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                                <span asp-validation-for="Propertyregistration.Boundary" class="text-danger lb-sm"></span>
                            </div>
                            <div class="form-group col-md-4" id="divBoundaryRemarks" style="display:none;">
                                <label id="l1" class="control-label lb-sm">Boundary Remarks</label>
                                <input asp-for="Propertyregistration.BoundaryRemarks" class="form-control" />
                                <span asp-validation-for="Propertyregistration.BoundaryRemarks" class="text-danger lb-sm"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm lb-sm">Total Area(In bigha-biswa/Sq Yd./Acre)</label>
                                <input asp-for="Propertyregistration.TotalAreaInBigha" type="text" class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Total Area(in sq. mt.)</label><span class="text-danger">*</span>
                                <input asp-for="Propertyregistration.TotalArea" class="form-control numbers" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                <span asp-validation-for="Propertyregistration.TotalArea" class="text-danger lb-sm"></span>
                            </div>


                        </div>

                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Encroched(in sq. mt.)</label>
                                <input asp-for="Propertyregistration.Encroached" class="form-control numbers" disabled onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span id="EncroachedValidation" asp-validation-for="Propertyregistration.Encroached" class="text-danger lb-sm"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Builtup in Encroachment Area(in sq. mt.)</label>
                                <input asp-for="Propertyregistration.BuiltUpEncraochmentArea" class="form-control numbers" id="BuiltupEncroachmentArea" disabled onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span asp-validation-for="Propertyregistration.BuiltUpEncraochmentArea" class="text-danger lb-sm"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Vacant(in sq. mt.)</label>
                                <input asp-for="Propertyregistration.Vacant" class="form-control numbers" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span asp-validation-for="Propertyregistration.Vacant" class="text-danger lb-sm"></span>
                            </div>
                        </div>
                    </div>
                    <div id="DivLandUse">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Planned/Unplanned Land</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm" asp-for="Propertyregistration.PlannedUnplannedLand">Planned/Unplanned Land</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.PlannedUnplannedLand">
                                    <option value="Planned Land">Planned Land</option>
                                    <option value="Unplanned Land">Unplanned Land</option>
                                    <option value="Under Process">Under Process</option>
                                </select>
                                <span asp-validation-for="Propertyregistration.PlannedUnplannedLand" class="text-danger lb-sm"></span>
                            </div>

                            <div id="divPlannedLand">
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Main Land Use</label>
                                    <select class="form-control dda-dropdown" asp-for="Propertyregistration.MainLandUseId" asp-items="@(new SelectList(Model.Propertyregistration.LandUseList, "Id","Name"))">
                                        <option value="0">--Select--</option>
                                    </select>
                                    <span asp-validation-for="Propertyregistration.MainLandUseId" class="text-danger lb-sm"></span>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Sub Use</label>
                                    <input type="text" asp-for="Propertyregistration.SubUse" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="DivOtherDetails">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Other Details</h6>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Built-Up</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.BuiltUp">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4" id="divBuilttype" style="display:none;">
                                <label class="control-label lb-sm">Built-Up Remarks</label>
                                <input asp-for="Propertyregistration.BuiltUpRemarks" class="form-control" limit="4000" />
                                <span asp-validation-for="Propertyregistration.BuiltUpRemarks" class="text-danger lb-sm"></span>
                            </div>

                            @*<div class="form-group col-md-4">
                                <label class="control-label lb-sm">Layout Plan</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.LayoutPlan">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>

                            </div>*@
                            <div class="form-group col-md-4" id="divLayoutPlan" style="display:none;">
                                <label class="control-label lb-sm" asp-for="Propertyregistration.FileData">Upload Layout Plan</label>
                                <input type="hidden" asp-for="Propertyregistration.LayoutFilePath" />
                                @{
                                    @if (ViewBag.LayoutDocView != null)
                                    {
                                        @Html.ActionLink(" (View Existing Document)", "Download", new { id = Model.Id })
                                        <input type="file" class="form-control" id="Assignfile" name="Assignfile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                    }
                                    else
                                    {
                                        <input type="file" class="form-control" id="Assignfile" name="Assignfile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                    }
                                }
                                <span asp-validation-for="Propertyregistration.FileData" class="text-danger lb-sm"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Litigation Status</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.LitigationStatus">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4" id="divLitigationStatus" style="display:none;">
                                <label class="control-label lb-sm">Remarks</label>
                                <input asp-for="Propertyregistration.LitigationStatusRemarks" class="form-control" limit="4000" />
                                <span asp-validation-for="Propertyregistration.LitigationStatusRemarks" class="text-danger lb-sm"></span>
                            </div>
                        </div>
                        <div class="row">


                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">GEO Referencing Done</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.GeoReferencing">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>

                            </div>
                            <div class="form-group col-md-4" id="divGEOReferencing" style="display:none;">
                                <label class="control-label lb-sm" asp-for="Propertyregistration.GeoFileData">Upload Geo Referencing File</label>
                                <input type="hidden" asp-for="Propertyregistration.GeoFilePath" />
                                @{
                                    @if (ViewBag.GeoDocView != null)
                                    {
                                        @Html.ActionLink(" (View Existing Document)", "GeoDownload", new { id = Model.Id })
                                        <input type="file" class="form-control" id="GeoAssignFile" name="GeoAssignFile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                    }
                                    else
                                    {
                                        <input type="file" class="form-control" id="GeoAssignFile" name="GeoAssignFile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                    }
                                }
                                <span asp-validation-for="Propertyregistration.GeoFileData" class="text-danger lb-sm"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <a data-toggle="collapse" href="#collapseExample2" id="collapse" role="button" aria-expanded="false" aria-controls="collapseExample2">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">History </h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
                <div class="collapse" id="collapseExample2">
                    <table class="table table-responsive table-bordered table-card">
                        <thead class="thead">
                            <tr class="dda-pageheading">
                                <th>Sr.No</th>
                                <th>Department</th>
                                <th>Zone</th>
                                <th>Division</th>
                                <th>Locality(Village)</th>
                                <th>Khasra No.</th>
                                <th>Handed Over By</th>
                                <th>Date of Hand Over</th>
                                <th>Taken Over By</th>
                                <th>Date of Taken Over</th>
                            </tr>
                        </thead>
                        <tbody class="tbody">
                            @foreach (var item in Model.LandTransferList)
                            {
                                <tr>
                                    <td>@(rowNo += 1)</td>
                                    <td>@item.PropertyRegistration.Department.Name</td>
                                    <td>@item.PropertyRegistration.Zone.Name</td>
                                    <td>@item.PropertyRegistration.Division.Name</td>
                                    <td>@item.PropertyRegistration.Locality.Name</td>
                                    <td>@item.PropertyRegistration.KhasraNo</td>
                                    <td>
                                        @item.HandedOverByNameDesingnation
                                    </td>
                                    <td>
                                        @(item.HandedOverDate != null ? Convert.ToDateTime(item.HandedOverDate).ToString("dd MMM yyyy") : "Not Available")
                                    </td>
                                    <td>
                                        @item.TakenOverByNameDesingnation
                                    </td>
                                    <td>
                                        @(item.DateofTakenOver != null ? Convert.ToDateTime(item.DateofTakenOver).ToString("dd MMM yyyy") : "Not Available")
                                    </td>
                                </tr>
                            }
                        </tbody>

                    </table>
                </div>
                <div class="row" style="margin-bottom:35px;">
                    <div class="col-sm-12">
                        <div class="dda-innerheading">
                            <div class="col-md-12">
                                <h6 class="pull-left color-white" style="margin-top:5px;">Handed Over /Taken Over Details </h6>
                                <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="DivHandedOver">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Handed Over</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm" asp-for="HandedOverDepartmentId">Department</label>
                            <select class="form-control dda-dropdown" asp-for="HandedOverDepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="HandedOverDepartmentId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Name/Designation</label>
                            <input class="form-control" />
                            <span class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Date</label>
                            <input asp-for="HandedOverDate" type="date" class="form-control" />
                            <span asp-validation-for="HandedOverDate" class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Email Id</label>
                            <input class="form-control" />
                            <span class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Mobile No.</label>
                            <input class="form-control onlynumbers" maxlength="10" />
                            <span class="text-danger lb-sm"></span>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Land Line No.</label>
                            <input class="form-control onlynumbers" maxlength="10" />
                            <span class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">

                        <div class="form-group col-md-6">
                            <label class="control-label lb-sm">Document</label>
                            <input type="hidden" />
                            @{
                                @if (ViewBag.HandedOverDocView != null)
                                {
                                    @Html.ActionLink(" (View Existing Document)", "HandedOverDownload", new { id = Model.Id })
                                    <input type="file" class="form-control" id="HandedOverAssignFile" name="HandedOverAssignFile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                }
                                else
                                {
                                    <input type="file" class="form-control" id="HandedOverAssignFile" name="HandedOverAssignFile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                }
                            }
                            <span class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Comments</label>
                            <textarea class="form-control"></textarea>
                            <span class="text-danger lb-sm"></span>
                            <div id="feedbackHandedOverComments"></div>
                        </div>

                    </div>
                </div>
