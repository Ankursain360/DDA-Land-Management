
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*@model DDAPropertyREG.Models.Tblzonemaster*@


<link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
<script src="~/js/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-1.10.2.js" integrity="sha256-it5nQKHTz+34HijZJQkpNBIHsjpV8b6QzMJs9tmOBSo=" crossorigin="anonymous"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">Land Transfer Reports</h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />

<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Specify Details  </h6>
        </div>
        <div class="panel-body">
            <form asp-action="RptCasesCourtWise">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-row">

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Zone/Division</label><span style="color:red">*</span>
                        <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                            <option value="0">---Select---</option>
                            <option value="1">Har</option>
                            <option value="2">Someother</option>
                        </select>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Locality</label><span style="color:red">*</span>
                        <select class="form-control" style="background-color:#dddddd; height:30px; border-color:#b1b1b1;">
                            <option value="0">---Select---</option>
                            <option value="1">Har</option>
                            <option value="2">Someother</option>
                        </select>
                    </div>


                </div>
                <div class="form-row">
                    <div class=" form-group  col-md-12">
                        <input type="submit" value="Retrieve" class="btn btn-primary myapp-hed1" />

                    </div>
                </div>

                <div id="divsearch">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Report of Land Transfer Division Locality Wise</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-12">

                            <div id="courtwiserpt">
                                <div class="card  table-card table-responsive">

                                    <table class="table  table-bordered table-card">

                                        <thead class="thead">
                                            <tr class="myapp-hed1">
                                                <th>Sr.No</th>
                                                <th>Zone/Division  </th>
                                                <th> Locality </th>
                                                <th>  Property No   </th>
                                                <th> Handed Over By  </th>
                                                <th> Handed Over Date</th>
                                                <th> Taken Over By  </th>
                                                <th> Taken Over Date  </th>
                                                <th> Transfer order Issue Authority</th>
                                                <th>Remarks </th>
                                            </tr>
                                        </thead>
                                        <tbody class="tbody">

                                            @*<tr>  <td colspan="10"><strong>Court Name : High Court Delhi</strong> </td> </tr>*@

                                            <tr>
                                                <td>1</td>
                                                <td>North Zone</td>
                                                <td>Dwarka</td>
                                                <td>23018</td>
                                                <td>LM</td>
                                                <td>30-MAY-2018</td>
                                                <td>ENGG</td>
                                                <td>30-MAY-2018</td>
                                                <td>DDA</td>
                                                <td>Other details</td>

                                            </tr>
                                            <tr>
                                                <td>2</td>
                                                <td>South Zone</td>
                                                <td>Dwarka</td>
                                                <td>23018</td>
                                                <td>LM</td>
                                                <td>30-MAY-2018</td>
                                                <td>ENGG</td>
                                                <td>30-MAY-2018</td>
                                                <td>DDA</td>
                                                <td>Other details</td>

                                            </tr>
                                        </tbody>

                                    </table>

                                </div>

                            </div>
                        </div>

                    </div>
                </div>
                <br />
                <div class="form-row">
                    <div class=" form-group  col-md-12">

                        <input type="submit" value="Export to Excel" class="btn btn-primary  myapp-hed1" style="width:150px !important;" />

                    </div>
                </div>

            </form>
        </div>
    </div>
</div>



@if (ViewData["Msg"] != null)
{
    LandTransfer.Models.Message msg = ViewData["Msg"] as LandTransfer.Models.Message;
    @await Html.PartialAsync("~/Views/Shared/_ShowMsg.cshtml", msg);
}


<script>

    $(function () {
        var dtToday = new Date();

        var month = dtToday.getMonth() + 1;
        var day = dtToday.getDate();
        var year = dtToday.getFullYear();
        if (month < 10)
            month = '0' + month.toString();
        if (day < 10)
            day = '0' + day.toString();

        var maxDate = year + '-' + month + '-' + day;
        //alert(maxDate);
    });



</script>
