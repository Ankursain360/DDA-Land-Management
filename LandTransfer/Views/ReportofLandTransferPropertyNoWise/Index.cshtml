@model Libraries.Model.Entity.Landtransfer
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Hand Over/Take Over Report(Khasra No. Wise)</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading dda-pageheading">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            <form novalidate="novalidate">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label lb-sm">Khasra No <span style="color:red">*</span></label>
                        <select asp-for="KhasraNo" class="form-control dda-dropdown" required="required" requiredMsg="Please Select Khasra Number" asp-items="@(new SelectList(Model.LandTransferList,"Id","KhasraNo"))">
                            <option value="">Select</option>
                        </select>
                        <span asp-validation-for="DepartmentId" class="text-danger"></span>
                    </div>
                </div>
                <div class="row">
                    <div class=" form-group  col-md-12">
                        <input type="button" value="Generate" id="btnGenerate" class="btn btn-primary dda-pageheading" />
                    </div>
                </div>
                <div id="LoadView">
                </div>
            </form>
        </div>
    </div>
</div>

<environment include="Development">
    <script src="~/js/ReportofLandTransferPropertyNoWise.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/ReportofLandTransferPropertyNoWise.min.js" asp-append-version="true"></script>
</environment>