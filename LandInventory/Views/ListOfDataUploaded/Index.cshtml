                        <div class="form-row">
                            <div class="form-group col-md-12">
                                <label class="control-label lb-sm">From Date </label>
                                <input class="form-control" required type="date" />
                                <span class="text-danger"></span>
                            </div>
                        </div>



                        <br />
                      
                        <br />



                        <br>


                    </form>
                </div>
                <div class="col-md-6">

