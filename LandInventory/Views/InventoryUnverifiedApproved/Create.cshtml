@model Libraries.Model.Entity.Propertyregistration

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Unverified/Approved Property</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            @*<form>*@
            <div class="row">

                <div class="form-group col-md-4">
                    <label class="control-label lb-sm">Inventoried In</label>
                    <select class="form-control dda-dropdown" asp-for="InventoriedInId">
                        <option value="0">All</option>
                        <option value="1">VLMS</option>
                        <option value="2">Used</option>
                    </select>
                    <span asp-validation-for="InventoriedInId" class="text-danger lb-sm"></span>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="PlannedUnplannedLand">Planned/Unplanned Land</label>
                    <select class="form-control dda-dropdown" asp-for="PlannedUnplannedLand">
                        <option value="0">All</option>
                        <option value="Planned Land">Planned Land</option>
                        <option value="Unplanned Land">Unplanned Land</option>
                    </select>
                    <span asp-validation-for="PlannedUnplannedLand" class="text-danger lb-sm"></span>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="ClassificationOfLandId">Classification of Land</label>
                    <select class="form-control dda-dropdown" asp-for="ClassificationOfLandId" asp-items="@(new SelectList(Model.ClassificationOfLandList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                    <span asp-validation-for="ClassificationOfLandId" class="text-danger lb-sm"></span>
                </div>

            </div>
            <div class="row">
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="DepartmentId">Department</label>

                    <select class="form-control dda-dropdown" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="ZoneId">Zone</label>

                    <select class="form-control dda-dropdown" asp-for="ZoneId" onchange="return GetDivisionList(this.options[this.selectedIndex].value);">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="DivisionId">Division</label>
                    <select class="form-control dda-dropdown" asp-for="DivisionId" onchange="return GetPrimaryNoList(this.options[this.selectedIndex].value);">
                        <option value="0">All</option>
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="LocalityId">Locality(Village)</label>
                    <select class="form-control dda-dropdown" asp-for="LocalityId">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="LocalityId">Unverified/Approved</label>
                    <select class="form-control dda-dropdown" asp-for="unverifiedverfied">
                        <option value="0">Unverified</option>
                        <option value="1">Approved</option>
                    </select>
                </div>
            </div>


            <div class="row ">
                <div class="form-group col-md-12" style="margin-bottom:40px;">
                    <input type="submit" value="Generate" id="btnGenerate" class="btn btn-primary dda-pageheading" />
                    @Html.ActionLink("Back", "Index", "InventoryUnverifiedApproved", null, new { @class = "btn btn-primary dda-pageheading" })
                </div>
            </div>

            <div id="LoadReportView">

            </div>
            @*</form>*@
        </div>
    </div>
</div>

<br />
<br />


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/inventoryunverifiedapproved.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/inventoryunverifiedapproved.min.js" asp-append-version="true"></script>
</environment>
