@model Libraries.Model.Entity.Propertyregistration
@await Component.InvokeAsync("PageHeader", new { pageName = "User Wise Land Status Report" })
<div class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
    <h3 class="mb-3 d-flex">
        View Details<span class="ms-auto"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span>
    </h3>

    <div class="row">


        <div class="col-md-4">
            <div class="floating-label-wrap">
                <select class="floating-label-field floating-label-field--s3" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))"
                        onchange="return GetZoneList(this.options[this.selectedIndex].value);">
                    <option selected="selected" value="0">All</option>
                </select>
                <label asp-validation-for="DepartmentId" class="error"></label>
                <label class="floating-label" asp-for="DepartmentId"> Select Department</label>
            </div>
        </div>
        <div class="col-md-4">

            <div class="floating-label-wrap">
                <select class="floating-label-field floating-label-field--s3" asp-for="ZoneId" onchange="return GetDivisionList(this.options[this.selectedIndex].value);">
                    <option selected="selected" value="0">All</option>
                </select>
                <label asp-validation-for="ZoneId" class="error"></label>
                <label class="floating-label" asp-for="ZoneId">Select Zone</label>
            </div>
        </div>
        <div class="col-md-4">

            <div class="floating-label-wrap">
                <select class="floating-label-field floating-label-field--s3" asp-for="DivisionId" onchange="return GetLocalityList(this.options[this.selectedIndex].value);">
                    <option selected="selected" value="0">All</option>
                </select>
                <label asp-validation-for="DivisionId" class="error"></label>
                <label class="floating-label" asp-for="DivisionId">Select Division</label>
            </div>
        </div>
    </div>
    <div class="row">


  
    </div>

</div>

<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/restorepropertyreportcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/restorepropertyreportcreate.min.js" asp-append-version="true"></script>
</environment>
