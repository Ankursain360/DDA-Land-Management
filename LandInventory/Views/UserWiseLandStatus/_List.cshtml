@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Vacantlandimage>



@{int rowNo = 0; }
<table class="table table-sm table-bordered mb-0 table-striped">
    <thead class="text-nowrap">
        <tr>
            <th rowspan="2">Department</th>
            <th rowspan="2">Zone</th>
            <th rowspan="2">Division</th>
            <th rowspan="2">User Id</th>
            <th rowspan="2">User Name</th>
            <th rowspan="2">Designation</th>
            <th rowspan="2">Photos</th>


        </tr>

    </thead>
    <tbody>
        @if (Model != null && Model.Results.Count() > 0)
        {

            @foreach (var item in Model.Results)
            {
        <tr>
            <td>@item.DepartmentNavigation.Name</td>
            <td>@item.ZoneNavigation.Name</td>
            <td>@item.DivisionNavigation.Name</td>
        



        </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="13" style="text-align:center"> Data Not Available </th>
            </tr>
        }

    </tbody>

</table>


@await Component.InvokeAsync("Pager", new { totalCount = Model.RowCount, pageNumber = Model.CurrentPage, pageSize = Model.PazeSize })
