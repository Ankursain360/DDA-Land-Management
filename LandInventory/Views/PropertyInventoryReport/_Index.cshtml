@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Propertyregistration>
@{int rowNo = 0; }


<table class="table table-sm table-bordered mb-0 table-striped">


    <thead class="text-nowrap">
        <tr>
            <th rowspan="2">Inventoried In</th>
            <th rowspan="2">Planned/Unplanned Land</th>
            <th rowspan="2">Classification of Land</th>
            <th rowspan="2">Department</th>
            <th rowspan="2">Zone</th>
            <th rowspan="2">Division</th>
            <th rowspan="2">Locality(Village)</th>
            <th rowspan="2">Khasra No.</th>
            <th rowspan="2">Colony</th>
            <th rowspan="2">Sector</th>
            <th rowspan="2">Block</th>
            <th rowspan="2">Pocket</th>
            <th rowspan="2">Plot No.</th>
            <th rowspan="2">Primary List No.</th>
            <th rowspan="2">Address with Landmark</th>
            <th colspan="3" style=" text-align:center">Area</th>
            <th colspan="7" style=" text-align:center">Encroachment Status</th>
            <th colspan="4" style="text-align:center">Boundary Wall/Fencing</th>
            <th rowspan="2">Built & type</th>
            <th colspan="5" style="text-align:center">Litigation Status</th>
            <th rowspan="2">GEO Referencing</th>
            <th colspan="9" style=" text-align:center">Handed Over</th>
            <th colspan="7" style=" text-align:center">Taken Over</th>
            @*<th colspan="3" style=" text-align:center">Disposal</th>*@
            <th rowspan="2">Remarks</th>
        </tr>
        <tr>
            <th>Area Unit</th>
            <th>Total Area(In bigha-biswa-biswani/Sq Yd./Acre/Hectare)</th>
            <th>Total Area(Sq.mt.)</th>
            <th>Encroachment</th>
            <th>Encroachment Status</th>
            <th>Encrochment Area(in Sq. Mt.)</th>
            <th>Builtup in Encroachment Area(in sq. mt.)</th>
            <th>Vacant(in Sq. Mt.)</th>
            <th>Action On Encroachment</th>
            <th>Encroachment Details</th>
            <th>Protection of Land</th>
            <th>Area Covered(Sq. mtr)</th>
            <th>Dimension (L*B*H)(mtr)</th>
            <th>Boundary Remarks</th>
            <th>Litigation Status</th>
            <th>Court Name</th>
            <th>Case No.</th>
            <th>Opposite Party</th>
            <th>Remarks</th>
            <th>Department</th>
            <th>Zone</th>
            <th>Division</th>
            <th>Name/Designation</th>
            <th>Date</th>
            <th>Mobile No.</th>
            <th>Order No.</th>
            @*<th>Copy of Order</th>*@
            <th>Transfer order Issue Authority</th>
            <th>Comments</th>
            <th>Department</th>
            <th>Zone</th>
            <th>Division</th>
            <th>Name/Designation</th>
            <th>Date</th>
            <th>Mobile No.</th>
            <th>Comments</th>
            @*<th>type</th>
                <th>Date</th>
                <th>Comments</th>*@

        </tr>
    </thead>
    <tbody class="tbody">
        @if (Model != null && Model.Results.Count() > 0)
        {
            @foreach (var item in Model.Results)
            {
                <tr>
                    <td>@(item.InventoriedInId.ToString() == "1" ? "VLMS" : "Used" ) </td>
                    <td>@(item.PlannedUnplannedLand)</td>
                    <td>@item.ClassificationOfLand.Name</td>
                    <td>@item.Department.Name</td>
                    <td>@item.Zone.Name</td>
                    <td>@item.Division.Name</td>
                    <td>@(item.LocalityId == null ? "" : item.Locality.Name)</td>
                    <td>@item.KhasraNo</td>
                    <td>@item.Colony</td>
                    <td>@item.Sector</td>
                    <td>@item.Block</td>
                    <td>@item.Pocket</td>
                    <td>@item.PlotNo</td>
                    <td>@item.PrimaryListNo</td>
                    <td>@item.Palandmark</td>
                    <td>@(item.AreaUnit == 0 ? "bigha-biswa-bishwani" : item.AreaUnit == 1 ? "Sq Yd." : item.AreaUnit == 2 ? "Acre" : "Hectare" )</td>
                    @if ((item.AreaUnit == 0))
                    {
                        <td>@(Convert.ToString(item.TotalAreaInBigha)+" - "+ Convert.ToString(item.TotalAreaInBiswa)+ " - " + Convert.ToString(item.TotalAreaInBiswani) )</td>
                    }
                    else
                    {
                        <td>@item.TotalAreaInSqAcreHt</td>
                    }
                    <td>@item.TotalArea</td>
                    <td>@(item.EncroachmentStatusId== 0 ? "No" : "Yes")</td>
                    <td>@(item.EncroachedPartiallyFully.ToString() == "0" ? "Partially Encroached" : "Fully Encroached")</td>
                    <td>@item.EncrochedArea</td>
                    <td>@item.BuiltUpEncraochmentArea</td>
                    <td>@item.Vacant</td>
                    <td>@item.ActionOnEncroachment</td>
                    <td>@item.EncraochmentDetails</td>
                    @*<td>@item.TotalArea</td>*@
                    <td>@(item.Boundary == 0 ? "Boundary Wall" : item.AreaUnit == 1 ? "Fencing" :  "None")</td>
                    <td>@item.BoundaryAreaCovered</td>
                    <td>@item.BoundaryDimension</td>
                    <td>@item.BoundaryRemarks</td>
                    <td>@(item.BuiltUp == 0 ? "No" : "Yes")</td>
                    <td>@(item.LitigationStatus == 0 ? "No" : "Yes")</td>
                    <td>@item.CourtName</td>
                    <td>@item.CaseNo</td>
                    <td>@item.OppositeParty</td>
                    <td>@item.LitigationStatusRemarks</td>
                    @*<td>@(item.LitigationStatus  == 0 ? "No" : "Yes")</td>*@
                    <td>@(item.GeoReferencing == 0 ? "No" : "Yes")</td>
                    @*<td>
                            @if (item.LayoutFilePath != null)
                            {
                                @Html.ActionLink("Download", "Download", new { id = item.Id })
                            }
                            else
                            {
                                <p class="text-danger">No Document</p>
                            }
                        </td>*@
                    <td>@(item.HandedOverDepartmentId == null ? "" : item.HandedOverDepartment.Name)</td>
                    <td>@(item.HandedOverZoneId == null ? "" :item.HandedOverZone.Name)</td>
                    <td>@(item.HandedOverDivisionId == null ? "" : item.HandedOverDivision.Name)</td>
                    <td>@item.HandedOverName</td>
                    <td>@item.HandedOverDate</td>
                    <td>@item.HandedOverMobileNo</td>
                    <td>@item.HandedOverOrderNo</td>
                    @*<td>@item.HandedOverCopyofOrderDoc</td>*@
                    <td>@item.HandedTransferOrder</td>
                    <td>@item.HandedOverComments</td>
                    <td>@(item.TakenOverDepartmentId == null ? "" : item.TakenOverDepartment.Name)</td>
                    <td>@(item.TakenOverZoneId == null ? "" : item.TakenOverZone.Name)</td>
                    <td>@(item.TakenOverDivisionId == null ? "" : item.TakenOverDivision.Name)</td>
                    <td>@item.TakenOverName</td>
                    <td>@item.TakenOverDate</td>
                    <td>@item.TakenOverMobileNo</td>
                    <td>@item.TakenOverComments</td>
                    @*<td>@item.DisposalType.Name</td>
                        <td>@item.DisposalDate</td>
                        <td>@item.DisposalComments</td>*@
                    <td>@item.Remarks</td>
                    @*<td>
                            <a asp-action="View" asp-route-id="@item.Id" title="View">  <i class="glyphicon glyphicon-eye-open" aria-hidden="true"></i> </a>

                            <a asp-action="Edit" asp-route-id="@item.Id" title="edit"> &nbsp; | &nbsp;  <i class="glyphicon glyphicon-pencil" aria-hidden="true"></i> </a>

                            <a asp-action="DeleteConfirmed" asp-route-id="@item.Id" title="delete" onclick="return confirm('Are You Sure, You want to Delete this Record?')">   &nbsp;|&nbsp;<i class="glyphicon glyphicon-trash" aria-hidden="true"></i></a>
                        </td>*@
                </tr>
            }
        }
        else
        {
            <tr>
                <th colspan="14" style="text-align:center"> No Record Available! </th>
            </tr>
        }

