@model Libraries.Model.Entity.Propertyregistration

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Property Inventory Report</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            @*<form>*@
            <div class="row">
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="ClassificationOfLandId">Classification of Land</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="ClassificationOfLandId" asp-items="@(new SelectList(Model.ClassificationOfLandList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                </div>

                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="DepartmentId">Department</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="ZoneId">Zone</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="ZoneId" onchange="return GetDivisionList(this.options[this.selectedIndex].value);">
                        <option value="0">All</option>
                    </select>
                </div>
            </div>
            <div class="row">

                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="DivisionId">Division</label><span class="text-danger">*</span>
                                                                                            <select class="form-control dda-dropdown" asp-for="DivisionId">
                                                                                                <option value="0">All</option>
                                                                                            </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="LocalityId">Locality</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="LocalityId"> 
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm" asp-for="PlannedUnplannedLand">Planned/Unplanned Land</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="PlannedUnplannedLand">
                        <option value="0">All</option>
                        <option value="Planned Land">Planned Land</option>
                        <option value="Unplanned Land">Unplanned Land</option>
                    </select>

                </div>
            </div>

            <div class="row">
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm">Main Land Use</label>
                    <select class="form-control dda-dropdown" asp-for="MainLandUseId" asp-items="@(new SelectList(Model.LandUseList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm">Litigation Status</label><span class="text-danger">*</span>
                    <select asp-for="LitigationStatus" class="form-control dda-dropdown">
                        <option value="2">All</option>
                        <option value="0">Yes</option>
                        <option value="1">No</option>
                    </select>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label lb-sm">Encroachment</label><span class="text-danger">*</span>
                    <select asp-for="Encroached" class="form-control dda-dropdown">
                        <option value="2">All</option>
                        <option value="0">Yes</option>
                        <option value="1">No</option>
                    </select>
                </div>

            </div>
            @*<div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label">Department</label><span class="text-danger">*</span>
                        <select class="form-control dda-dropdown">
                            <option>All</option>
                            <option>All</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label">Land Use</label><span class="text-danger">*</span>
                        <select class="form-control dda-dropdown">
                            <option>All</option>
                            <option>Planned</option>
                            <option>Unplanned</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label">Litigation Status</label><span class="text-danger">*</span>
                        <select class="form-control dda-dropdown">
                            <option>All</option>
                            <option>Yes</option>
                            <option>No</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label">Encroachment</label><span class="text-danger">*</span>
                        <select class="form-control dda-dropdown">
                            <option>All</option>
                            <option>Yes</option>
                            <option>No</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>

                </div>*@
            <div class="row ">
                <div class="form-group col-md-12" style="margin-bottom:40px;">
                    <input type="submit" value="Generate" id="btnGenerate" class="btn btn-primary dda-pageheading" />
                    @Html.ActionLink("Back", "Index", "PropertyInventoryReport", null, new { @class = "btn btn-primary dda-pageheading" })
                </div>
            </div>

            <div id="LoadReportView">

            </div>
            @*</form>*@
        </div>
    </div>
</div>

<br />
<br />


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/propertyregisterationreportcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/propertyregisterationreportcreate.js" asp-append-version="true"></script>
</environment>