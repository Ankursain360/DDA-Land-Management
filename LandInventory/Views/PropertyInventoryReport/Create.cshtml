@model Libraries.Model.Entity.Propertyregistration

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Property Inventory Report</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            @*<form>*@
                <div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label" asp-for="DepartmentId">Department</label><span class="text-danger">*</span>
                        <select class="form-control dda-dropdown" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))">
                            <option value="0">All</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label" asp-for="LandUseId">Land Use</label><span class="text-danger">*</span>
                        <select class="form-control dda-dropdown" asp-for="LandUseId" asp-items="@(new SelectList(Model.LandUseList, "Id","Name"))">
                            <option value="0">All</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label class="control-label">Litigation Status</label><span class="text-danger">*</span>
                        <select asp-for="LitigationStatus" class="form-control dda-dropdown">
                            <option value="2">All</option>
                            <option value="0">Yes</option>
                            <option value="1">No</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="control-label">Encroachment</label><span class="text-danger">*</span>
                        <select asp-for="Encroached" class="form-control dda-dropdown">
                            <option value="2">All</option>
                            <option value="0">Yes</option>
                            <option value="1">No</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>

                </div>
