@model Libraries.Model.Entity.Propertyregistration
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Archive land</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Details</h6>
        </div>
        <div class="panel-body">
            @*<form>*@
            <div class="row">
                <div class="form-group col-md-6">

                    <label class="control-label lb-sm" asp-for="DepartmentId"> Select Department</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm" asp-for="ZoneId">Select Zone</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="ZoneId" asp-items="@(new SelectList(Model.ZoneList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                </div>
            </div>





            <div class="row">
                <div class="form-group col-md-6">

                    <label class="control-label lb-sm">Division</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="DivisionId" asp-items="@(new SelectList(Model.DivisionList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                </div>
                <div class="form-group col-md-6">
                    <label class="control-label lb-sm">Primary No</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="LocalityId" asp-items="@(new SelectList(Model.LocalityList, "Id","Name"))">
                        <option value="0">All</option>
                    </select>
                </div>
            </div>




            <div class="row ">
                <div class="form-group col-md-12" style="margin-bottom:40px;">
                    <input type="submit" value="Generate" id="btnGenerate" class="btn btn-primary dda-pageheading" />
                    @Html.ActionLink("Back", "Index", "Archive", null, new { @class = "btn btn-primary dda-pageheading" })
                </div>
            </div>

        </div>
    </div>
        
        <div id="LoadReportView">

            </div>
            @*</form>*@
        </div>
    


<br />
<br />



