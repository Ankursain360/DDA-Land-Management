
    @model Libraries.Model.Entity.Propertyregistration
    @await Component.InvokeAsync("PageHeader", new { pageName = "OTP For Verifiy Property" })

<div class="pt-3">

    <form asp-action="OTP" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3">
        <h3 class="mb-3 d-flex">Add Details<span class="ms-auto"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span></h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
      
        <div class="row">
            <div class="col-md-6">
                <div class="floating-label-wrap mb-3 mb-md-0">
                    <input asp-for="Otp" type="text" class="floating-label-field floating-label-field--s3 " placeholder="Name"
                           limit="6">
                    <span asp-validation-for="Otp" class="text-danger "></span>
                    <label class="floating-label">Enter OTP<span style="color:red">*</span></label>
                </div>
            </div>
           
                <div class="col-md-4">
                    <label class="control-label lb-sm">Please check for Validate</label>
                    <input type="checkbox" asp-for="IsValidateData" />
                </div>

        </div>
        <div class="row mt-md-3">
            <div class=" col-md-12">
                <button class="btn btn-theme " type="submit">Submit</button>
                @Html.ActionLink("Back", "Create", "InventoryUnverified", null, new { @class = "btn btn-theme " })

            </div>
        </div>

    </form>

</div>

