@model Libraries.Repository.Common.PagedResult<Libraries.Model.Entity.Nazulland>
@{int rowNo = 0; }
<table class="table table-responsive table-bordered table-card">
    <thead class="thead">
        <tr>
            @*<th rowspan="2">Sr.No</th>*@
            <th rowspan="2">Khasra No.</th>
            <th rowspan="2">Land Area Acquired</th>
            <th rowspan="2">Award No</th>
            <th rowspan="2">Award Date</th>
            <th rowspan="2">Date of Possession</th>
            <th rowspan="2">Area of which possession taken over</th>
            <th rowspan="2">Date on which possession taken over</th>
            <th rowspan="2">Scheme for which Acquired</th>
            <th rowspan="2">Amount of Award</th>
            <th colspan="3">Enhanced compensation if any</th>
            <th rowspan="2">Certificate by Land Record Officer to the correctness of entry</th>
            <th rowspan="2">Division to Which Land has been transfered</th>
            <th rowspan="2">Date of Transfer</th>
            <th rowspan="2">Status</th>
            <th rowspan="2">Operation</th>

        </tr>
        <tr>
            <th>ADI Court</th>
            <th>High Court</th>
            <th>Supreme Court</th>
        </tr>

    </thead>
    <tbody class="tbody">
        @foreach (var item in Model.Results)
        {
            <tr>
                <td>@(rowNo += 1)</td>

                <td>@item.KhasraNo</td>

                <td>@item.LandAreaAcquired</td>
                <td>@item.AwardNo </td>
                <td>@item.AwardDate</td>

                <td>@(Convert.ToDateTime(item.DateOfPossession).ToString("dd-MMM-yyyy"))</td>

                <td>@item.AreaOfWhichPossessionTakenOver</td>
                <td>@(Convert.ToDateTime(item.DateOnWhichPossessionTakenOver).ToString("dd-MMM-yyyy"))</td>

                <td>@item.SchemeForWhichAcquired</td>

                <td>@item.AmountOfAward</td>
                <td>@item.AdiCourt</td>
                <td>@item.HighCourt</td>
                <td>@item.SupremeCourt</td>
                <td>@item.CertificateToCorrectnessOfEntry</td>
                <td>@item.Division.Name</td>
                <td>@(Convert.ToDateTime(item.DateOfTransfer).ToString("dd-MMM-yyyy"))</td>


                <td>
                    @(item.IsActive  == 1 ? "Active" : "Inactive" )
                </td>
                <td>
                    <a asp-action="View" asp-route-id="@item.Id" title="View">  <i class="glyphicon glyphicon-eye-open" aria-hidden="true"></i> </a>

                    <a asp-action="Edit" asp-route-id="@item.Id" title="edit"> &nbsp; | &nbsp;  <i class="glyphicon glyphicon-pencil" aria-hidden="true"></i> </a>

                    <a asp-action="Delete" asp-route-id="@item.Id" title="delete" onclick="return confirm('Are You Sure, You want to Delete this Record?')">   &nbsp;|&nbsp;<i class="glyphicon glyphicon-trash" aria-hidden="true"></i></a>

                </td>

            </tr>
        }
    </tbody>
</table>
@await Component.InvokeAsync("Pager", new { totalCount = Model.RowCount, pageNumber = Model.CurrentPage, pageSize = Model.PazeSize })