                                    <label class="control-label lb-sm">Remarks</label>
                                    <input asp-for="LandTransfer.Propertyregistration.LitigationStatusRemarks" disabled class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="DivOtherDetails">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Other Details</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Built-Up</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown select2destroy" asp-for="LandTransfer.Propertyregistration.BuiltUp" disabled>
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4" id="divBuilttype" style="display:none;">
                                <label class="control-label lb-sm">Built-Up Remarks</label>
                                <input asp-for="LandTransfer.Propertyregistration.BuiltUpRemarks" class="form-control" disabled />
                            </div>

                            <div class="form-group col-md-4" id="divLayoutPlan" style="display:none;">
                                <label class="control-label lb-sm">Upload Layout Plan</label>
                                <br /> <a target="_blank" href="/LandTransfer/ViewDocument?path=@(Model.LandTransfer.Propertyregistration.LayoutFilePath)">@(Model.LandTransfer.Propertyregistration.LayoutFilePath=="" || Model.LandTransfer.Propertyregistration.LayoutFilePath == string.Empty || Model.LandTransfer.Propertyregistration.LayoutFilePath == null ? "Not Available": "Layout Plan")</a>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">GEO Referencing Done</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown select2destroy" asp-for="LandTransfer.Propertyregistration.GeoReferencing" disabled>
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>

                            </div>
                            <div id="divGEOReferencingSelection" style="display:none;">
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Upload Geo Referencing</label>
                                    <br /> <a target="_blank" href="/LandTransfer/ViewDocument?path=@(Model.LandTransfer.Propertyregistration.GeoFilePath)">@(Model.LandTransfer.Propertyregistration.GeoFilePath=="" || Model.LandTransfer.Propertyregistration.GeoFilePath == string.Empty || Model.LandTransfer.Propertyregistration.GeoFilePath == null ? "Not Available": "Geo Referencing")</a>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Lattitude</label>
                                    <input asp-for="LandTransfer.Propertyregistration.GeoLattitude" class="form-control" disabled />
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Longitude</label>
                                    <input asp-for="LandTransfer.Propertyregistration.GeoLongitude" class="form-control" disabled />
                                </div>
                                <div class="form-group col-md-4">
                                    <a class="btn btn-primary dda-pageheading" style="width:auto;margin-top:15px;">Get Lat Long</a>
                                </div>
                            </div>
                        </div>
                    </div>

