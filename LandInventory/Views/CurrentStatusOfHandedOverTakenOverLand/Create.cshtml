                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <select asp-for="PlotUtilization" class="floating-label-field floating-label-field--s3">
                                            <option value="No">No</option>
                                            <option value="Yes">Yes</option>
                                        </select>
                                        <label class="floating-label">Area /Plot Utilization</label>
                                    </div>
                                </div>
                                <div class="col-12" id="Utilization" style="display:none;">
                                    <div class="row mt-md-3">
                                        <div class="col-md-4">
                                            <div class="floating-label-wrap">
                                                <select class="floating-label-field floating-label-field--s3" asp-for="NatureOfUtilization" onchange="ChangeNatureOfUtilizationStatus(this.value)">
                                                    <option value="0">Temporary Allocation/Relocation</option>
                                                    <option value="1">Function Site</option>
                                                    <option value="2">Other</option>
                                                </select>
                                                <label class="floating-label">Nature of Utilization</label>
                                            </div>
                                        </div>
                                        <div class="col-md-4" id="divNatureOfUtilization">
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="VacationDate" placeholder="Vacation Date" class="floating-label-field floating-label-field--s3" type="Date" />
                                                <label class="floating-label">Utilization Date</label>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="floating-label-wrap">
                                                <select class="floating-label-field floating-label-field--s3" asp-for="AreaUnit">
                                                    <option value="0">bigha-biswa-bishwani</option>
                                                    <option value="1">Sq Yd.</option>
                                                    <option value="2">Acre</option>
                                                    <option value="3">Hectare</option>
                                                    <option value="4">Sq. Mt.</option>
                                                </select>
                                                <span asp-validation-for="AreaUnit" class="text-danger lb-sm"></span>
                                                <label class="floating-label">Area Unit</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="bighabisForCurrentStatus">
                                        <div class="row mt-md-3">
                                            <div class="col-md-2">
                                                <div class="floating-label-wrap">
                                                    <input asp-for="TotalAreaInBigha" placeholder="Bigha" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                    <span asp-validation-for="TotalAreaInBigha" class="text-danger lb-sm"></span>
                                                    <label class="floating-label">Bigha</label>

                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="floating-label-wrap mb-3 mb-md-0">

                                                    <input asp-for="TotalAreaInBiswa" placeholder="Biswa" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                    <span asp-validation-for="TotalAreaInBiswa" class="text-danger lb-sm"></span>
                                                    <label class="floating-label">Biswa</label>

                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="floating-label-wrap mb-3 mb-md-0">

                                                    <input asp-for="TotalAreaInBiswani" placeholder="Biswani" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                    <span asp-validation-for="TotalAreaInBiswani" class="text-danger lb-sm"></span>
                                                    <label class="floating-label">Biswani</label>

                                                </div>
                                            </div>

                                        </div>


                                    </div>
                                    <div id="AreainSqAcreHecForCurrentStatus" class="col-md-3" style="display:none;">
                                        <div class="row mt-md-3">
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="TotalAreaInSqAcreHt" placeholder="Total Area" type="text" class="floating-label-field floating-label-field--s3 numbers TotalCalculation" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                                <span asp-validation-for="TotalAreaInSqAcreHt" class="text-danger lb-sm"></span>
                                                <label class="floating-label" id="LabelTotalAreaSqAcreHecForCurrentStatus">Total Area(Sq Yd./Acre/Hectare)</label>

                                            </div>
                                        </div>
                                    </div>
                                    <div class="row mt-md-3">
                                        <div class="col-md-6">
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="TotalArea" value="" placeholder="Total Area(in sq. mt.)"
                                                       class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                <span asp-validation-for="TotalArea" class="text-danger lb-sm"></span>
                                                <label class="floating-label">Total Area(in sq. mt.)</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row mt-md-3">
                                        <div class="col-md-4">
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="Status" type="text" placeholder="Status" class="floating-label-field floating-label-field--s3" id="txtTakenOverDate" />
                                                <label class="floating-label">Status </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row mt-md-4">
                            <div class="col-md-12">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <textarea asp-for="Remarks" placeholder="Remarks" type="text" class="floating-label-field floating-label-field--s3"></textarea>
                                    @*<span asp-validation-for="Remarks" class="text-danger"></span>*@
                                    <label class="floating-label">Remarks</label>

                                </div>
                            </div>
                        </div>

                        @*</div>*@
                    </div>
                    <div class="row mt-md-4">
                        <div class="col-md-12">
                            <input type="submit" value="Create" class="btn btn-theme" />
                            <a asp-action="Index" class="btn btn-theme">Back </a>

                        </div>
                    </div>
</form>

