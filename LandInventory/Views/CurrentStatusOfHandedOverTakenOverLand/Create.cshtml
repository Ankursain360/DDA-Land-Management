@model Libraries.Model.Entity.Currentstatusoflandhistory
@{int rowNo = 0;}

@await Component.InvokeAsync("PageHeader", new { pageName = "Update Plot Details" })



<form asp-action="Create" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data">
    <h3 class="mb-3 d-flex">Details</h3>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="accordion mb-4" id="accordionExample">
        <div class="permission mt-2 accordion list-unstyled m-0 ">
            <div class="card">
                <a class="p-2 no-dec d-flex align-items-center" data-toggle="collapse" href="#collapseOne" role="button" aria-expanded="false"
                   aria-controls="collapseApprroval">
                    <i class="ri-arrow-right-s-fill"></i>
                    <span class="text ml-1 mr-auto">View  Property Inventory Details</span>
                    <i class="arrow ri-arrow-drop-down-line slow"></i>
                </a>
                <div class="list-unstyled m-0 pl-2 pr-1 pl-md-4 pr-md-3 collapse" id="collapseOne">
                    <div id="allDetails">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.InventoriedInId">
                                        <option value="">--Select--</option>
                                        <option value="1">VLMS</option>
                                        <option value="2">Used/Allocated</option>
                                    </select>
                                    <label class="floating-label">Inventoried In</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.PlannedUnplannedLand">
                                        <option value="Planned Land">Planned Land</option>
                                        <option value="Unplanned Land">Unplanned Land</option>
                                    </select>
                                    <label class="floating-label" asp-for="LandTransfer.Propertyregistration.PlannedUnplannedLand">Planned/Unplanned Land</label>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.ClassificationOfLandId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.ClassificationOfLandList, "Id","Name"))" required="required" requiredmsg="Classification of Land is Mandatory">
                                        <option value="">--Select--</option>
                                    </select>
                                    <label class="floating-label" asp-for="LandTransfer.Propertyregistration.ClassificationOfLandId">Classification of Land</label>

                                </div>
                            </div>
                        </div>
                        <div class="row mt-md-3">
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.DepartmentId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.TakenOverDepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                                        <option value="">--Select--</option>
                                    </select>
                                    <label class="floating-label" asp-for="LandTransfer.Propertyregistration.DepartmentId">Department</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.ZoneId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.ZoneList, "Id","Name"))">
                                        <option value="">--Select--</option>
                                    </select>
                                    <label class="floating-label" asp-for="LandTransfer.Propertyregistration.ZoneId">Zone</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.DivisionId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.DivisionList, "Id","Name"))">
                                        <option value="">--Select--</option>
                                    </select>
                                    <label class="floating-label" asp-for="LandTransfer.Propertyregistration.DivisionId">Division</label>
                                </div>
                            </div>
                        </div>
                        <div id="divUnplannedSelection" style="display:none;">
                            <div class="row pt-3">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy" disabled asp-for="LandTransfer.Propertyregistration.LocalityId" asp-items="@(new SelectList(Model.LandTransfer.PropertyRegistration.LocalityList,"Id","Name"))">
                                            <option value="">--Select--</option>
                                        </select>
                                        <label class="floating-label" asp-for="LandTransfer.Propertyregistration.LocalityId">Locality(Village)</label>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.KhasraNo" placeholder="Khasra No" disabled class="floating-label-field floating-label-field--s3" />
                                        <label class="floating-label">Khasra No</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="divPlannedSelection">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.Colony" placeholder="Khasra No" disabled class="floating-label-field floating-label-field--s3" />
                                        <label class="floating-label">Colony</label>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.Sector" placeholder="Sector" disabled class="floating-label-field floating-label-field--s3" />
                                        <label class="floating-label">Sector</label>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="text" class="form-control" placeholder="Block" disabled asp-for="LandTransfer.Propertyregistration.Block" />
                                        <label class="floating-label">Block</label>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.Pocket" placeholder="Pocket" disabled class="floating-label-field floating-label-field--s3" />
                                        <label class="floating-label">Pocket</label>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.PlotNo" placeholder="Plot No." disabled class="floating-label-field floating-label-field--s3" />
                                        <label class="floating-label">Plot No.</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row pt-3">
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="LandTransfer.Propertyregistration.PrimaryListNo" placeholder="Primary List No." disabled class="floating-label-field floating-label-field--s3" />
                                    <label class="floating-label">Primary List No.</label>
                                </div>
                            </div>
                        </div>
                        <div class="row pt-3">
                            <div class="col-md-12">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <textarea asp-for="LandTransfer.Propertyregistration.Palandmark" placeholder="Address with Landmark" disabled class="floating-label-field floating-label-field--s3"></textarea>
                                    <label class="floating-label">Address with Landmark</label>

                                    <div id="feedbackPalandmark"></div>
                                </div>
                            </div>
                        </div>

                        <div id="DivArea">
                            <h2 class="my-3 sub-heading">Area</h2>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy"
                                                disabled asp-for="LandTransfer.Propertyregistration.AreaUnit">
                                            <option value="0">bigha-biswa-bishwani</option>
                                            <option value="1">Sq Yd.</option>
                                            <option value="2">Acre</option>
                                        </select>
                                        <label class="floating-label">Area Unit</label>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.TotalArea" placeholder="Total Area(in sq. mt.)" class="floating-label-field floating-label-field--s3 numbers" disabled />
                                        <label class="floating-label">Total Area(in sq. mt.)</label>
                                    </div>
                                </div>
                            </div>
                            <div id="bighabis">
                                <div class="row pt-3">
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.TotalAreaInBigha" placeholder="Bigha" disabled class="floating-label-field floating-label-field--s3 numbers" />
                                            <label class="floating-label">Bigha</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.TotalAreaInBiswa" placeholder="Biswa" class="floating-label-field floating-label-field--s3 numbers" disabled />
                                            <label class="floating-label">Biswa</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.TotalAreaInBiswani" placeholder="Biswani" class="floating-label-field floating-label-field--s3 numbers" disabled>
                                            <label class="floating-label">Biswani</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="AreainSqAcreHec" style="display:none;">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.TotalAreaInSqAcreHt" placeholder="Total Area(Sq Yd./Acre/Hectare)" type="text" disabled class="floating-label-field floating-label-field--s3 numbers" />
                                            <label class="floating-label" id="LabelTotalAreaSqAcreHec">Total Area(Sq Yd./Acre/Hectare)</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="divEncroachment">
                            <h2 class="my-3 sub-heading">Encroached Details</h2>
                            <div class="row pt-3">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.EncroachmentStatusId" disabled>
                                            <option value="0">No</option>
                                            <option value="1">Yes</option>
                                        </select>
                                        <label class="floating-label">Encroachment </label>
                                    </div>
                                </div>
                            </div>
                            <div id="divEncroachmentYesSelection" style="display:none;">
                                <div class="row pt-3">
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap">
                                            <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.EncroachedPartiallyFully" disabled>
                                                <option value="0">Vacant</option>
                                                <option value="1">Partially Vacant</option>
                                            </select>
                                            <label class="floating-label">Encroachment Status</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.Encroached" placeholder="Encroched Area(in sq. mt.)"
                                                   class="floating-label-field floating-label-field--s3 numbers" disabled />
                                            <label class="floating-label">Encroched Area(in sq. mt.)</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.BuiltUpEncraochmentArea" placeholder="Builtup in Encroachment Area(in sq. mt.)" class="floating-label-field floating-label-field--s3 numbers" />
                                            <label class="floating-label">Builtup in Encroachment Area(in sq. mt.)</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row pt-3">
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.Vacant" placeholder="Vacant(in sq. mt.)" class="floating-label-field floating-label-field--s3 numbers" />
                                            <label class="floating-label">Vacant(in sq. mt.)</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <textarea type="text" class="floating-label-field floating-label-field--s3"
                                                      id="txtTakenOverDate" disabled placeholder="Action On Encroachment"
                                                      asp-for="LandTransfer.Propertyregistration.ActionOnEncroachment"></textarea>
                                            <label class="floating-label">Action On Encroachment</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <br /> <a target="_blank" href="/LandTransfer/ViewDocument?path=@(Model.LandTransfer.Propertyregistration.EncroachAtrfilepath)">@(Model.LandTransfer.Propertyregistration.EncroachAtrfilepath=="" || Model.LandTransfer.Propertyregistration.EncroachAtrfilepath == string.Empty || Model.LandTransfer.Propertyregistration.EncroachAtrfilepath == null ? "Not Available": "Action Taken Report(ATR)")</a>

                                            <label class="floating-label">Upload Action Taken Report(ATR)</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row pt-3">
                                <div id="divEncroachmentDetails" style="display:none;">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="floating-label-wrap mb-3 mb-md-0">
                                                <input asp-for="LandTransfer.Propertyregistration.EncraochmentDetails" placeholder="Encroachment Details" class="floating-label-field floating-label-field--s3" disabled />
                                                <label class="floating-label">Encroachment Details</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="divFencingBoundaryWall">
                            <h2 class="my-3 sub-heading">Boundary Wall/Fencing </h2>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.Boundary" disabled>
                                            <option value="0">Boundary Wall</option>
                                            <option value="1">Fencing</option>
                                            <option value="2">None</option>
                                        </select>
                                        <label class="floating-label">Protection of Land</label>
                                    </div>
                                </div>
                            </div>
                            <div id="divBoundarySelection" style="display:none;">
                                <div class="row pt-3">
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input type="text" asp-for="LandTransfer.Propertyregistration.BoundaryAreaCovered" placeholder="Area Covered(Sq. mtr)" class="floating-label-field floating-label-field--s3" disabled />
                                            <label class="floating-label">Area Covered(Sq. mtr)</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input type="text" class="floating-label-field floating-label-field--s3" placeholder="Dimension (L*B*H)(mtr)" disabled asp-for="LandTransfer.Propertyregistration.BoundaryDimension" />

                                            <label class="floating-label">Dimension (L*B*H)(mtr)</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.BoundaryRemarks" placeholder="Boundary Remarks" class="floating-label-field floating-label-field--s3" disabled />

                                            <label id="l1" class="floating-label">Boundary Remarks</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div id="DivLandUse">
                            <h2 class="my-3 sub-heading">Land Use</h2>

                            <div id="divPlannedLand">
                                <div class="row pt-3">
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap">
                                            <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.MainLandUseId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.LandUseList, "Id","Name"))" disabled>
                                                <option value="0">--Select--</option>
                                            </select>
                                            <label class="floating-label">Main Land Use</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input type="text" asp-for="LandTransfer.Propertyregistration.SubUse" placeholder="Sub use" class="floating-label-field floating-label-field--s3" disabled />
                                            <label class="floating-label">Sub Use</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="divLitigation">
                            <h2 class="my-3 sub-heading">Litigation</h2>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.LitigationStatus" disabled>
                                            <option value="0">No</option>
                                            <option value="1">Yes</option>
                                        </select>
                                        <label class="floating-label">Litigation Status</label>
                                    </div>
                                </div>
                            </div>

                            <div id="divLitigationStatusSelection" style="display:none;">
                                <div class="row pt-3">
                                    <div class="col-md-6">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.CourtName" placeholder="Court Name" class="floating-label-field floating-label-field--s3" disabled />
                                            <label class="floating-label">Court Name</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.CaseNo" placeholder="Case No." class="floating-label-field floating-label-field--s3" disabled />
                                            <label class="floating-label">Case No.</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row  pt-3">
                                    <div class="col-md-6">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.OppositeParty" placeholder="Opposite Party" class="floating-label-field floating-label-field--s3" disabled />
                                            <label class="floating-label">Opposite Party</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.LitigationStatusRemarks" placeholder="Remarks" disabled class="floating-label-field floating-label-field--s3" />
                                            <label class="floating-label">Remarks</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="DivOtherDetails">
                            <h2 class="my-3 sub-heading"> Other Details</h2>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.BuiltUp" disabled>
                                            <option value="0">No</option>
                                            <option value="1">Yes</option>
                                        </select>
                                        <label class="floating-label">Built-Up</label>
                                    </div>
                                </div>
                                <div class="col-md-4" id="divBuilttype" style="display:none;">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="LandTransfer.Propertyregistration.BuiltUpRemarks" placeholder="Built-Up Remarks" class="floating-label-field floating-label-field--s3" disabled />
                                        <label class="floating-label">Built-Up Remarks</label>
                                    </div>
                                </div>
                                <div class="col-md-4" id="divLayoutPlan" style="display:none;">
                                    <br /> <a target="_blank" href="/LandTransfer/ViewDocument?path=@(Model.LandTransfer.Propertyregistration.LayoutFilePath)">@(Model.LandTransfer.Propertyregistration.LayoutFilePath=="" || Model.LandTransfer.Propertyregistration.LayoutFilePath == string.Empty || Model.LandTransfer.Propertyregistration.LayoutFilePath == null ? "Not Available": "Layout Plan")</a>
                                    <label class="floating-label">Upload Layout Plan</label>
                                </div>
                            </div>
                            <div class="row pt-3">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3 select2destroy" asp-for="LandTransfer.Propertyregistration.GeoReferencing" disabled>
                                            <option value="0">No</option>
                                            <option value="1">Yes</option>
                                        </select>
                                        <label class="floating-label">GEO Referencing Done</label>
                                    </div>
                                </div>
                            </div>
                            <div id="divGEOReferencingSelection" style="display:none;">
