@model Libraries.Model.Entity.Currentstatusoflandhistory
@{int rowNo = 0;}
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Update Plot Details</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <a data-toggle="collapse" href="#collapseExample" id="collapse" role="button" aria-expanded="false" aria-controls="collapseExample">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Property Inventory Details </h6>
                                    <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
                <div class="collapse" id="collapseExample">

                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Inventoried In</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.InventoriedInId">
                                <option value="">--Select--</option>
                                <option value="1">VLMS</option>
                                <option value="2">Used</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="LandTransfer.Propertyregistration.PlannedUnplannedLand">Planned/Unplanned Land</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.PlannedUnplannedLand">
                                <option value="Planned Land">Planned Land</option>
                                <option value="Unplanned Land">Unplanned Land</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="LandTransfer.Propertyregistration.ClassificationOfLandId">Classification of Land</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.ClassificationOfLandId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.ClassificationOfLandList, "Id","Name"))" required="required" requiredmsg="Classification of Land is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="LandTransfer.Propertyregistration.DepartmentId">Department</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.DepartmentId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.TakenOverDepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="LandTransfer.Propertyregistration.ZoneId">Zone</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.ZoneId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.ZoneList, "Id","Name"))">
                                <option value="">--Select--</option>
                            </select>
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm" asp-for="LandTransfer.Propertyregistration.DivisionId">Division</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.DivisionId" asp-items="@(new SelectList(Model.LandTransfer.Propertyregistration.DivisionList, "Id","Name"))">
                                <option value="">--Select--</option>
                            </select>
                        </div>
                        <div id="divUnplannedSelection" style="display:none;">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm" asp-for="LandTransfer.Propertyregistration.LocalityId">Locality(Village)</label>
                                <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.LocalityId" asp-items="@(new SelectList(Model.LandTransfer.PropertyRegistration.LocalityList,"Id","Name"))">
                                    <option value="">--Select--</option>
                                </select>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Khasra No</label>
                                <input asp-for="LandTransfer.Propertyregistration.KhasraNo" disabled class="form-control" />
                            </div>
                        </div>
                        <div id="divPlannedSelection">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Colony</label>
                                <input asp-for="LandTransfer.Propertyregistration.Colony" disabled class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Sector</label>
                                <input asp-for="LandTransfer.Propertyregistration.Sector" disabled class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Block</label>
                                <input type="text" class="form-control" disabled asp-for="LandTransfer.Propertyregistration.Block" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Pocket</label>
                                <input asp-for="LandTransfer.Propertyregistration.Pocket" disabled class="form-control" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Plot No.</label>
                                <input asp-for="LandTransfer.Propertyregistration.PlotNo" disabled class="form-control" />
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Primary List No.</label>
                            <input asp-for="LandTransfer.Propertyregistration.PrimaryListNo" disabled class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Address with Landmark</label>
                            <textarea asp-for="LandTransfer.Propertyregistration.Palandmark" disabled class="form-control"></textarea>
                            <div id="feedbackPalandmark"></div>
                        </div>
                    </div>
                    <div id="DivArea">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Area</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm lb-sm">Area Unit</label>
                                <select class="form-control dda-dropdown select2destroy" disabled asp-for="LandTransfer.Propertyregistration.AreaUnit">
                                    <option value="0">bigha-biswa-bishwani</option>
                                    <option value="1">Sq Yd.</option>
                                    <option value="2">Acre</option>
                                </select>
                            </div>
                            <div id="bighabis">
                                <div class="form-group col-md-1">
                                    <label class="control-label lb-sm">Bigha</label>
                                    <input asp-for="LandTransfer.Propertyregistration.TotalAreaInBigha" disabled class="form-control numbers" />
                                </div>
                                <div class="form-group col-md-1">
                                    <label class="control-label lb-sm">Biswa</label>
                                    <input asp-for="LandTransfer.Propertyregistration.TotalAreaInBiswa" class="form-control numbers" disabled />
                                </div>
                                <div class="form-group col-md-1">
                                    <label class="control-label lb-sm">Biswani</label>
                                    <input asp-for="LandTransfer.Propertyregistration.TotalAreaInBiswani" class="form-control numbers" disabled>
                                </div>
                            </div>
                            <div id="AreainSqAcreHec" class="form-group col-md-4" style="display:none;">
                                <label class="control-label lb-sm lb-sm" id="LabelTotalAreaSqAcreHec">Total Area(Sq Yd./Acre/Hectare)</label>
                                <input asp-for="LandTransfer.Propertyregistration.TotalAreaInSqAcreHt" type="text" disabled class="form-control numbers" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Total Area(in sq. mt.)</label><span class="text-danger">*</span>
                                <input asp-for="LandTransfer.Propertyregistration.TotalArea" class="form-control numbers" disabled />
                            </div>
                        </div>
                    </div>
                    <div id="divEncroachment">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Encroached Details</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Encroachment </label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown select2destroy" asp-for="LandTransfer.Propertyregistration.EncroachmentStatusId" disabled>
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                            </div>
                            <div id="divEncroachmentYesSelection" style="display:none;">
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Encroachment Status</label>
                                    <select class="form-control dda-dropdown select2destroy" asp-for="LandTransfer.Propertyregistration.EncroachedPartiallyFully" disabled>
                                        <option value="0">Vacant</option>
                                        <option value="1">Partially Vacant</option>
                                    </select>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Encroched Area(in sq. mt.)</label>
                                    <input asp-for="LandTransfer.Propertyregistration.Encroached" class="form-control numbers" disabled />
                                </div>

                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Builtup in Encroachment Area(in sq. mt.)</label>
                                    <input asp-for="LandTransfer.Propertyregistration.BuiltUpEncraochmentArea" class="form-control numbers" />
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Vacant(in sq. mt.)</label>
                                    <input asp-for="LandTransfer.Propertyregistration.Vacant" class="form-control numbers" />
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Action On Encroachment</label>
                                    <textarea type="text" class="form-control" id="txtTakenOverDate" disabled asp-for="LandTransfer.Propertyregistration.ActionOnEncroachment"></textarea>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Upload Action Taken Report(ATR)</label>
                                    <br /> <a target="_blank" href="/LandTransfer/ViewDocument?path=@(Model.LandTransfer.Propertyregistration.EncroachAtrfilepath)">@(Model.LandTransfer.Propertyregistration.EncroachAtrfilepath=="" || Model.LandTransfer.Propertyregistration.EncroachAtrfilepath == string.Empty || Model.LandTransfer.Propertyregistration.EncroachAtrfilepath == null ? "Not Available": "Action Taken Report(ATR)")</a>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-12" id="divEncroachmentDetails" style="display:none;">
                                <label class="control-label lb-sm">Encroachment Details</label>
                                <input asp-for="LandTransfer.Propertyregistration.EncraochmentDetails" class="form-control" disabled />
                            </div>
                        </div>
                    </div>

                    <div id="divFencingBoundaryWall">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">
                                            Boundary Wall/Fencing
                                        </h6>
                                        <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-4">
                                <label class="control-label lb-sm">Protection of Land</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown select2destroy" asp-for="LandTransfer.Propertyregistration.Boundary" disabled>
                                    <option value="0">Boundary Wall</option>
                                    <option value="1">Fencing</option>
                                    <option value="2">None</option>
                                </select>
                            </div>
                            <div id="divBoundarySelection" style="display:none;">
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Area Covered(Sq. mtr)</label>
                                    <input type="text" asp-for="LandTransfer.Propertyregistration.BoundaryAreaCovered" class="form-control" disabled />
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label lb-sm">Dimension (L*B*H)(mtr)</label>
                                    <input type="text" class="form-control" disabled asp-for="LandTransfer.Propertyregistration.BoundaryDimension" />
                                </div>
                                <div class="form-group col-md-4">
                                    <label id="l1" class="control-label lb-sm">Boundary Remarks</label>
                                    <input asp-for="LandTransfer.Propertyregistration.BoundaryRemarks" class="form-control" disabled />
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="DivLandUse">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Land Use</h6>
                                    </div>
                                </div>
                            </div>
