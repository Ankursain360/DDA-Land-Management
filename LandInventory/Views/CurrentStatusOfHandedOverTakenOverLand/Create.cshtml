                                            <input asp-for="LandTransfer.Propertyregistration.GeoLattitude" placeholder="Lattitude" class="floating-label-field floating-label-field--s3" disabled />
                                            <label class="floating-label">Lattitude</label>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="floating-label-wrap mb-3 mb-md-0">
                                            <input asp-for="LandTransfer.Propertyregistration.GeoLongitude" placeholder="Longitude" class="floating-label-field floating-label-field--s3" disabled />

                                            <label class="floating-label">Longitude</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row pt-3">
                                    <div class="col-md-4">
                                        <a class="btn btn-theme" style="color:white;">Get Lat Long</a>
