
@{
    ViewData["Title"] = "Index";
}




<style>
    .alert-info {
        color: #31708F;
        background-color: rebeccapurple;
        border-color: #BCE8F1;
    }

    .hedingcss {
        background-color: rebeccapurple;
    }

    .panel-default > .panel-heading-custom {
        background: #2c3091;
        color: #fff;
        height: 44px;
        margin-bottom: 2px;
        font-size: 16px;
    }

    .alert-custom {
        top: -10px;
        color: #fff;
        background: linear-gradient(to left, #9e1c32, #991141, #900f50, #83155d, #721e68, #68226f, #5d2675, #4f2a7a, #492b80, #422c85, #392e8b, #2c3091);
        position: relative;
        font-family: Arial, Helvetica, sans-serif;
    }

    .btn {
        width: 143px !important;
    }
</style>




<div class="row">
    <div class="col-sm-12">
        <div class="myapp-hed">
            <div class="col-md-12">
                <h6 class="pull-left" style="color:white;margin-top:5px;">
                    Vacant Land Monitoring System
                    (Through Picture Uploading)
                </h6>
                <i class="fa fa-arrow-left pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>



<br />
<br />







<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  myapp-hed1">
            <h6 class="pull-left" style="color:white; margin-top:-0.4%">
                List of properties(Images not uploaded during selected dates)
            </h6>
        </div>
        <div class="panel-body">
            <form class="form-horizontal" action="" method="post">
                <fieldset>

                    <label class="control-label" for="name">Dates</label>
                    <div class="form-group">
                        <div class="col-md-6">
                            <label class="control-label" for="name">From</label>
                            <input id="name" name="name" type="date" placeholder="Your name" class="form-control">

                        </div>
                        <div class="col-md-6">
                            <label class="control-label" for="name">To</label>
                            <input id="email" name="email" type="date" placeholder="Your email" class="form-control">
                        </div>
                    </div>




                    <!-- Name input-->
                    <div class="form-group">
                        <div class="col-md-6">
                            <label class="control-label">Department </label>
                            <select style="height:30px; background-color: #dddddd;" type="text" required class="form-control">
                                <option value="1">-Select-</option>
                                <option value="dept1">Engineering</option>
                                <option value="dept2">Land Management</option>
                                <option value="dept3">MOR</option>
                                <option value="dept3">Horticulture</option>
                                <option value="dept3">Pre-Monsoon</option>
                                <option value="dept3">  Yamuna Flood Plain </option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label class="control-label">Zone </label>
                            <select style="height:30px; background-color: #dddddd;" type="text" required class="form-control">
                                <option value="1">-Select-</option>
                                <option value="dept1">CE (Project)</option>
                                <option value="dept2">Dwarka</option>
                                <option value="dept3">South</option>
                                <option value="dept3">Rohini</option>
                                <option value="dept3">North</option>
                                <option value="dept3"> East </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-12 widget-right">
                            <button type="submit" class="btn btn-info btn-md pull-center myapp-hed1">Retrieve Report </button>
                        </div>
                    </div>

                    <div class="form-group">

                        <div class="col-md-6">
                            <label class="control-label" for="name">Email</label>
                            <input id="email" name="email" type="text" placeholder="Your email" class="form-control">
                        </div>
                    </div>


                    <div class="form-group">
                        <div class="col-md-12 widget-right">
                            <button type="submit" class="btn btn-info btn-md pull-center myapp-hed1">Send</button>
                        </div>
                    </div>
                    <!-- Form actions -->

                </fieldset>
            </form>
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel-heading  myapp-hed1">
                        <h6 class="pull-left" style="color:white; margin-top:-0.4%">
                            List of vacant land of CE(PROJECT) Zone of Engineering Department for not uploaded photos during period from 11-08-2020 to 24-08-2020
                        </h6>
                    </div>
                </div>
            </div>
            <div class="panel panel-container" style="border-bottom:double; border-color:rebeccapurple; overflow:hidden">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-body ">
                                <div class="table-responsive">
                                    <table id="mytable" class="table table-striped table-bordered" border="0">

                                        <thead class="thead">

                                            <tr>
                                            <tr>
                                                <th>Sr.No</th>
                                                <th>Locality</th>
                                                <th>Primary List No.</th>
                                                <th>Division</th>
                                                <th>
                                                    Zone
                                                </th>

                                            </tr>
                                        </thead>
                                        <tbody class="tbody">
                                            <tr>
                                                <td>1</td>
                                                <td>Nehru Place District Centre, Plot No. A-2</td>
                                                <td>8/FOD-4</td>
                                                <td>FOD-4</td>
                                                <td>CE(PROJECT)</td>

                                            </tr>
                                            <tr>
                                                <td>2</td>
                                                <td>Nehru Place District Centre, Plot No. A-4</td>
                                                <td>8/FOD-4</td>
                                                <td>FOD-4</td>
                                                <td>CE(PROJECT)</td>

                                            </tr>



                                            <tr>
                                                <td>2</td>
                                                <td>Nehru Place District Centre, Plot No. A-4</td>
                                                <td>8/FOD-4</td>
                                                <td>FOD-4</td>
                                                <td>CE(PROJECT)</td>

                                            </tr>


                                        </tbody>
                                    </table>
                                </div>
                                <div>
                                    <input type="submit" value="Export to Excel" class="btn btn-primary myapp-hed1" style="width:200px" />
                                </div>
                            </div>


                            <br />


                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>






</div>