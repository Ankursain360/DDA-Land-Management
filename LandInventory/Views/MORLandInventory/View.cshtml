@model Libraries.Model.Entity.Propertyregistration

@await Component.InvokeAsync("PageHeader", new { pageName = "Ministry of rehabilitation(MOR) Land Inventory" })




<div class="container-fluid">
    <div class="panel">

        <div class="panel-body">
            <form asp-action="Edit" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" id="myForm" method="post" enctype="multipart/form-data" autocomplete="off">
                <h3 class="mb-3 d-flex">View Details</h3>

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="Id" />

                <div class="row">
                    <div class="col-md-4">
                        <div class="floating-label-wrap">

                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="InventoriedInId">
                                <option value="">--Select--</option>
                                <option value="1">VLMS</option>
                                <option value="2">Used</option>
                            </select>
                            <label class="floating-label">Inventoried In</label>

                            <span asp-validation-for="InventoriedInId" class="text-danger lb-sm"></span>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="PlannedUnplannedLand">
                                <option value="Planned Land">Planned Land</option>
                                <option value="Unplanned Land">Unplanned Land</option>
                            </select>
                            <span asp-validation-for="PlannedUnplannedLand" class="text-danger lb-sm"></span>

                            <label class="floating-label" asp-for="PlannedUnplannedLand">Planned/Unplanned Land</label>

                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="ClassificationOfLandId" asp-items="@(new SelectList(Model.ClassificationOfLandList, "Id","Name"))" required="required" requiredmsg="Classification of Land is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="ClassificationOfLandId" class="text-danger lb-sm"></span>

                            <label class="floating-label" asp-for="ClassificationOfLandId">Classification of Land</label>

                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap">

                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="DepartmentId" class="text-danger lb-sm"></span>
                            <label class="floating-label" asp-for="DepartmentId">Maintaining Department</label>

                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="ZoneId" asp-items="@(new SelectList(Model.ZoneList, "Id","Name"))" onchange="return GetDivisionList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Zone is Mandatory">
                                @*asp-items="@(new SelectList(Model.ZoneList, "Id","Name"))"*@
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="ZoneId" class="text-danger lb-sm"></span>

                            <label class="floating-label" asp-for="ZoneId">Zone</label>

                        </div>
                    </div>

                    <div class="col-md-4">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" disabled asp-for="DivisionId" asp-items="@(new SelectList(Model.DivisionList, "Id","Name"))" required="required" requiredmsg="Division is Mandatory">
                                @*asp-items="@(new SelectList(Model.DivisionList, "Id","Name"))"*@
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="DivisionId" class="text-danger lb-sm"></span>

                            <label class="floating-label" asp-for="DivisionId">Division</label>

                        </div>
                    </div>
                </div>

                <div id="divUnplannedSelection" style="display:none;">
                    <div class="row">

                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <select class="floating-label-field floating-label-field--s3" disabled asp-for="LocalityId" asp-items="@(new SelectList(Model.LocalityList, "Id","Name"))">
                                    @*asp-items="@(new SelectList(Model.LocalityList, "Id","Name"))"*@
                                    <option value="">--Select--</option>
                                </select>
                                <span asp-validation-for="LocalityId" class="text-danger lb-sm"></span>
                                <label class="floating-label" asp-for="LocalityId">Locality(Village)</label>

                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input disabled asp-for="KhasraNo" class="floating-label-field floating-label-field--s3" limit="200" />
                                <span asp-validation-for="KhasraNo" class="text-danger lb-sm"></span>
                                <label class="floating-label">Khasra No</label>

                            </div>
                        </div>
                    </div>
                </div>
                <div id="divPlannedSelection">
                    <div class="row mt-md-3">

                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input disabled asp-for="Colony" type="text" class="floating-label-field floating-label-field--s3" />
                                <span class="text-danger lb-sm"></span>

                                <label class="floating-label">Colony</label>

                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input disabled asp-for="Sector" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="Sector" class="text-danger lb-sm"></span>
                                <label class="floating-label">Sector</label>

                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input type="text" class="floating-label-field floating-label-field--s3" disabled maxlength="50" asp-for="Block" />
                                <span class="text-danger lb-sm" asp-validation-for="Block"></span>

                                <label class="floating-label">Block</label>

                            </div>
                        </div>
                    </div>
                    <div class="row mt-md-3">

                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input disabled asp-for="Pocket" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="Pocket" class="text-danger lb-sm"></span>
                                <label class="floating-label">Pocket</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input disabled asp-for="PlotNo" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="PlotNo" class="text-danger lb-sm"></span>
                                <label class="floating-label">Plot No.</label>

                            </div>
                        </div>
                    </div>

                </div>

                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input disabled asp-for="PrimaryListNo" class="floating-label-field floating-label-field--s3" limit="200" />
                            <label class="floating-label">Primary List No.</label>

                            <span asp-validation-for="PrimaryListNo" class="text-danger lb-sm"></span>
                        </div>
                    </div>
                </div>

                <div class="row mt-md-3">

                    <div class="col-md-12">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <textarea disabled asp-for="Palandmark" class="floating-label-field floating-label-field--s3" maxlength="4000" limit="4000"></textarea>
                            <label class="floating-label">Address with Landmark</label>

                            <div id="feedbackPalandmark"></div>
                        </div>
                    </div>
                </div>
                <div class="row mt-md-3">

                    <div id="DivArea">
                        <h3 class="mb-3 d-flex">Area</h3>


                        <div class="row">
                            <div class="col-md-4">
                                <div class="floating-label-wrap">
                                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="AreaUnit">
                                        <option value="0">bigha-biswa-bishwani</option>
                                        <option value="1">Sq Yd.</option>
                                        <option value="2">Acre</option>
                                        <option value="3">Hectare</option>
                                        <option value="4">Sq. Mt.</option>
                                    </select>

                                    <label class="floating-label">Area Unit</label>

                                    <span asp-validation-for="AreaUnit" class="text-danger lb-sm"></span>
                                </div>
                            </div>
                            <div id="bighabis">
                                <div class="col-md-1">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled asp-for="TotalAreaInBigha" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                        <span asp-validation-for="TotalAreaInBigha" class="text-danger lb-sm"></span>
                                        <label class="floating-label">Bigha</label>

                                    </div>
                                </div>
                                <div class="col-md-1">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled asp-for="TotalAreaInBiswa" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                        <span asp-validation-for="TotalAreaInBiswa" class="text-danger lb-sm"></span>
                                        <label class="floating-label">Biswa</label>
                                    </div>
                                </div>
                                <div class="col-md-1">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled asp-for="TotalAreaInBiswani" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                        <span asp-validation-for="TotalAreaInBiswani" class="text-danger lb-sm"></span>
                                        <label class="floating-label">Biswani</label>

                                    </div>
                                </div>
                            </div>
                            <div id="AreainSqAcreHec" class="col-md-4" style="display:none;">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="TotalAreaInSqAcreHt" type="text" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                    <span asp-validation-for="TotalAreaInSqAcreHt" class="text-danger lb-sm"></span>
                                    <label class="floating-label" id="LabelTotalAreaSqAcreHec">Total Area(Sq Yd./Acre/Hectare)</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="TotalArea" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                    <span asp-validation-for="TotalArea" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Total Area(in sq. mt.)</label>
                                </div>
                            </div>
                        </div>

                        <div class="row">


                        </div>
                    </div>
                </div>

                <div id="divEncroachment">
                    <div class="row mt-md-3">

                        <h3 class="mb-3 d-flex">Encroached Details</h3>

                        <div class="row">
                            <div class="col-md-4">
                                <div class="floating-label-wrap">

                                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="EncroachmentStatusId">
                                        <option value="0">No</option>
                                        <option value="1">Yes</option>
                                    </select>
                                    <span asp-validation-for="EncroachmentStatusId" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Encroachment</label>

                                </div>
                            </div>
                            <div id="divEncroachmentYesSelection" style="display:none;">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap">
                                        <select class="floating-label-field floating-label-field--s3" disabled asp-for="EncroachedPartiallyFully">
                                            <option value="0">Partially Encroached</option>
                                            <option value="1">Fully Encroached</option>
                                        </select>
                                        <span asp-validation-for="EncroachedPartiallyFully" class="text-danger lb-sm"></span>

                                        <label class="floating-label">Encroachment Status</label>

                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled asp-for="EncrochedArea" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                        <span id="EncroachedValidation" asp-validation-for="Encroached" class="text-danger lb-sm"></span>
                                        <label class="floating-label">Encroched Area(in sq. mt.)</label>

                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input disabled asp-for="BuiltUpEncraochmentArea" class="floating-label-field floating-label-field--s3 numbers" id="BuiltupEncroachmentArea" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                        <span asp-validation-for="BuiltUpEncraochmentArea" class="text-danger lb-sm"></span>

                                        <label class="floating-label">Builtup in Encroachment Area(in sq. mt.)</label>

                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input disabled asp-for="Vacant" class="floating-label-field floating-label-field--s3 numbers" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                        <span asp-validation-for="Vacant" class="text-danger lb-sm"></span>

                                        <label class="floating-label">Vacant Area(in sq. mt.)</label>

                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input disabled asp-for="ActionOnEncroachment" class="floating-label-field floating-label-field--s3" />
                                        <span asp-validation-for="ActionOnEncroachment" class="text-danger lb-sm"></span>
                                        <label class="floating-label">Action On Encroachment</label>

                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input type="hidden" asp-for="EncroachAtrfilepath" />
                                        @{
                                                        @if (ViewBag.EncroachAtrDocView != null)
                                                        {
                                            <a>(View File)</a>
                                                            <input type="file" class="floating-label-field floating-label-field--s3" disabled asp-for="EncroachAtrDoc" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" /> }
                                                        else
                                                        {
                                            <input type="file" class="form-control" disabled asp-for="EncroachAtrDoc" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />} }
                                        <span asp-validation-for="EncroachAtrDoc" class="text-danger lb-sm"></span>

                                        <label class="floating-label">Upload Action Taken Report(ATR)</label>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12" id="divEncroachmentDetails" style="display:none;">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="EncraochmentDetails" class="floating-label-field floating-label-field--s3" maxlength="4000" limit="4000" />
                                    <span asp-validation-for="EncraochmentDetails" class="text-danger lb-sm"></span>
                                    <label id="l1" class="floating-label">Encroachment Details</label>
                                </div>
                            </div>
                        </div>

                    </div>


                </div>
                <div id="divFencingBoundaryWall">

                    <h3 class="mb-3 d-flex">Boundary Wall/Fencing</h3>

                    <div class="row">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">
                                <select class="floating-label-field floating-label-field--s3" disabled asp-for="Boundary">
                                    <option value="0">Boundary Wall</option>
                                    <option value="1">Fencing</option>
                                    <option value="2">None</option>
                                </select>
                                <span asp-validation-for="Boundary" class="text-danger lb-sm"></span>

                                <label class="floating-label">Protection of Land</label>

                            </div>
                        </div>
                        <div id="divBoundarySelection" style="display:none;">
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input type="text" class="floating-label-field floating-label-field--s3 numbers" disabled asp-for="BoundaryAreaCovered" />
                                    <label class="floating-label">Area Covered(Sq. mtr)</label>


                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input type="text" class="floating-label-field floating-label-field--s3" disabled asp-for="BoundaryDimension" />
                                    <label class="floating-label">Dimension (L*B*H)(mtr)</label>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="BoundaryRemarks" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="BoundaryRemarks" class="text-danger lb-sm"></span>
                                    <label id="l1" class="floating-label">Boundary Remarks</label>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="DivLandUse">
                    <h3 class="mb-3 d-flex">Land Use</h3>


                    <div class="row">
                        <div id="divPlannedLand">
                            <div class="col-md-4">
                                <div class="floating-label-wrap">

                                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="MainLandUseId" asp-items="@(new SelectList(Model.LandUseList, "Id","Name"))">
                                        <option value="0">--Select--</option>
                                    </select>
                                    <span asp-validation-for="MainLandUseId" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Main Land Use</label>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input type="text" disabled asp-for="SubUse" class="floating-label-field floating-label-field--s3" limit="500" />
                                    <label class="floating-label">Sub Use</label>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="divLitigation">
                    <h3 class="mb-3 d-flex">Litigation</h3>

                    <div class="row">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">
                                <select class="floating-label-field floating-label-field--s3" disabled asp-for="LitigationStatus">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>

                                <label class="floating-label">Litigation Status</label>

                            </div>
                        </div>
                        <div id="divLitigationStatusSelection" style="display:none;">
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="CourtName" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="CourtName" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Court Name</label>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="CaseNo" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="CaseNo" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Case No.</label>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="OppositeParty" class="floating-label-field floating-label-field--s3" />
                                    <span asp-validation-for="OppositeParty" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Opposite Party</label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">

                                    <input disabled asp-for="LitigationStatusRemarks" class="floating-label-field floating-label-field--s3" limit="4000" />
                                    <span asp-validation-for="LitigationStatusRemarks" class="text-danger lb-sm"></span>
                                    <label class="floating-label">Remarks</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="DivOtherDetails">
                    <h3 class="mb-3 d-flex">Other Details</h3>


                    <div class="row">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <select class="floating-label-field floating-label-field--s3" disabled asp-for="BuiltUp">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                                <label class="floating-label">Built-Up</label>

                            </div>
                        </div>
                        <div class="col-md-4" id="divBuilttype" style="display:none;">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input disabled asp-for="BuiltUpRemarks" class="floating-label-field floating-label-field--s3" />
                                <span asp-validation-for="BuiltUpRemarks" class="text-danger lb-sm"></span>

                                <label class="floating-label">Built-Up Remarks</label>

                            </div>
                        </div>

                        <div class="col-md-4" id="divLayoutPlan" style="display:none;">
                            <input type="hidden" asp-for="LayoutFilePath" />
                            @{
                                            @if (ViewBag.LayoutDocView != null)
                                            {
                                @Html.ActionLink(" (View File)", "Download", new { id = Model.Id })
                                                <input type="file" disabled class="floating-label-field floating-label-field--s3" id="Assignfile" name="Assignfile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" /> }
                                            else
                                            {
                                <input type="file" disabled class="floating-label-field floating-label-field--s3" id="Assignfile" name="Assignfile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />} }
                            <span asp-validation-for="FileData" class="text-danger lb-sm"></span>
                            <label class="floating-label" asp-for="FileData">Upload Layout Plan</label>

                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <select class="floating-label-field floating-label-field--s3" disabled asp-for="GeoReferencing">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                                <label class="floating-label">GEO Referencing Done</label>
                            </div>
                        </div>
                        <div id="divGEOReferencingSelection" style="display:none;">
                            <div class="form-group col-md-4" id="divGEOReferencing">
                                <label class="control-label lb-sm" asp-for="GeoFileData">Upload Geo Referencing File</label>
                                <input type="hidden" asp-for="GeoFilePath" />
                                @{
                                                @if (ViewBag.GeoDocView != null)
                                                {
                                    <a>(View File)</a>
                                                    <input type="file" disabled class="form-control" id="GeoAssignFile" name="GeoAssignFile" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" /> }
                                                else
                                                {
