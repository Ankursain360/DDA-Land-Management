@model Libraries.Model.Entity.Propertyregistration

@await Component.InvokeAsync("PageHeader", new { pageName = "Ministry of rehabilitation (MOR)/LDO Land Inventory" })

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="pt-3">
    <form asp-action="Create" method="post" class="d-block rounded border p-3 bg-white search-filer shadow mb-3" enctype="multipart/form-data" autocomplete="off">
        <h3 class="mb-3 d-flex">
            Add Details <span class="ms-auto"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span>
        </h3>@*id="myForm"*@
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div id="allDetails">
            <div class="row">
                <div class="col-md-4">
                    <div class="floating-label-wrap">
                        <select class="floating-label-field floating-label-field--s3" asp-for="InventoriedInId">
                            <option value="1">VLMS</option>
                            <option value="2">Used/Allocated</option>
                        </select>
                        <span asp-validation-for="InventoriedInId" class="text-danger"></span>
                        <label asp-for="InventoriedInId" class="floating-label">Inventoried In </label>
                        @*<select class="form-control dda-dropdown" asp-for="InventoriedInId" required="required" requiredmsg="Inventoried In is Mandatory">
                        <option value="">--Select--</option>
                        <option value="1">VLMS</option>
                        <option value="2">Used</option>
                    </select>*@

                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap">
                        @*<label class="control-label lb-sm" asp-for="PlannedUnplannedLand">Planned/Unplanned Land</label><span class="text-danger">*</span>*@

                        <select class="floating-label-field floating-label-field--s3" asp-for="PlannedUnplannedLand">
                            <option value="Planned Land">Planned Land</option>
                            <option value="Unplanned Land">Unplanned Land</option>
                        </select>

                        <span asp-validation-for="PlannedUnplannedLand" class="text-danger"></span>
                        <label asp-for="PlannedUnplannedLand" class="floating-label">Planned/Unplanned Land <span style="color:red">*</span></label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap">
                        <select class="floating-label-field floating-label-field--s3" asp-for="ClassificationOfLandId" asp-items="@(new SelectList(Model.ClassificationOfLandList,"Id","Name"))">
                            <option selected="selected" disabled="disabled" value="">--Select-- </option>
                        </select>
                        <span asp-validation-for="ClassificationOfLandId" class="text-danger"></span>
                        <label asp-for="ClassificationOfLandId" class="floating-label">Classification of Land<span style="color:red">*</span> </label>
                    </div>
                    @*<label class="control-label lb-sm" asp-for="ClassificationOfLandId">Classification of Land</label><span class="text-danger">*</span>
                <select class="form-control dda-dropdown" asp-for="ClassificationOfLandId" asp-items="@(new SelectList(Model.ClassificationOfLandList, "Id","Name"))" required="required" requiredmsg="Classification of Land is Mandatory">
                    <option value="">--Select--</option>
                </select>
                <span asp-validation-for="ClassificationOfLandId" class="text-danger lb-sm"></span>*@
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-4">
                    <div class="floating-label-wrap">
                        @*<label class="control-label lb-sm" asp-for="DepartmentId">Maintaining Department</label><span class="text-danger">*</span>
                    <select class="form-control dda-dropdown" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                        <option value="">--Select--</option>
                    </select>
                    <span asp-validation-for="DepartmentId" class="text-danger lb-sm"></span>*@
                        <select class="floating-label-field floating-label-field--s3" asp-for="DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                            <option selected="selected" disabled="disabled" value="">--Select-- </option>
                        </select>
                        <span asp-validation-for="DepartmentId" class="text-danger"></span>
                        <label asp-for="DepartmentId" class="floating-label">Maintaining Department<span style="color:red">*</span> </label>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="floating-label-wrap">

                        <select class="floating-label-field floating-label-field--s3" asp-for="ZoneId" onchange="return GetDivisionList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Zone is Mandatory">
                            <option selected="selected" disabled="disabled" value="">--Select-- </option>
                        </select>
                        <span asp-validation-for="ZoneId" class="text-danger"></span>
                        <label asp-for="ZoneId" class="floating-label">Zone<span style="color:red">*</span> </label>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="floating-label-wrap">

                        <select class="floating-label-field floating-label-field--s3" asp-for="DivisionId" required="required" requiredmsg="Division is Mandatory">
                            <option selected="selected" disabled="disabled" value="">--Select-- </option>
                        </select>
                        <span asp-validation-for="DivisionId" class="text-danger"></span>
                        <label asp-for="DivisionId" class="floating-label">Division<span style="color:red">*</span> </label>
                    </div>
                </div>
            </div>

            <div id="divUnplannedSelection" style="display:none;">
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap">

                            <select class="floating-label-field floating-label-field--s3" asp-for="LocalityId">
                                <option selected="selected" disabled="disabled" value="">--Select-- </option>
                            </select>
                            <span asp-validation-for="LocalityId" class="text-danger"></span>
                            <label asp-for="LocalityId" class="floating-label">Locality(Village) </label>
                        </div>
                    </div>
                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="KhasraNo" limit="30" class="floating-label-field floating-label-field--s3" placeholder="Khasra No" />
                            <label for="txtKhasraNo" class="floating-label">Khasra No</label>
                        </div>
                    </div>
                </div>
            </div>
            <div id="divPlannedSelection">
                <div class="row mt-md-3">
                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Colony" maxlength="50" class="floating-label-field floating-label-field--s3" placeholder="Colony" />
                            <label for="txtName" class="floating-label">Colony</label>
                        </div>
                    </div>
                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Sector" maxlength="50" class="floating-label-field floating-label-field--s3" placeholder="Sector" />
                            <label for="txtName" class="floating-label">Sector</label>
                        </div>
                    </div>
                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Block" maxlength="50" class="floating-label-field floating-label-field--s3" placeholder="Block" />
                            <label for="txtName" class="floating-label">Block</label>
                        </div>
                    </div>
                </div>
                <div class="row pt-3">
                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="Pocket" maxlength="50" class="floating-label-field floating-label-field--s3" placeholder="Pocket" />
                            <label for="txtName" class="floating-label">Pocket</label>
                        </div>
                    </div>
                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="PlotNo" maxlength="50" class="floating-label-field floating-label-field--s3" placeholder="Plot No." />
                            <label for="txtName" class="floating-label">Plot No.</label>
                        </div>
                    </div>

                    <div class="col-md-4">

                        <div class="floating-label-wrap mb-3 mb-md-0">
                            <input type="text" asp-for="PrimaryListNo" limit="20" class="floating-label-field floating-label-field--s3" placeholder="PrimaryListNo" />
                            <label for="txtName" class="floating-label">Primary List No.</label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row mt-md-3">
                <div class="col-md-12">

                    <div class="floating-label-wrap mb-3 mb-md-0">
                        <input type="text" asp-for="Palandmark" maxlength="4000" limit="4000" class="floating-label-field floating-label-field--s3" placeholder="Address with Landmark" />
                        <label for="txtName" class="floating-label">Address with Landmark</label>
                    </div>
                </div>
            </div>


            <div id="DivArea">
                <h2 class="my-3 sub-heading">Area</h2>
                <div class="row mt-md-3">

                    <div class="col-md-4">
                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="AreaUnit">
                               
                                <option value="0">Bigha-Biswa-Biswansi</option>
                                <option value="1">Sq Yd.</option>
                                <option value="2">Acre</option>
                                <option value="3">Hectare</option>
                                <option value="4">Sq. Mt.</option>
                            </select>
                            <span asp-validation-for="AreaUnit" class="text-danger"></span>
                            <label asp-for="AreaUnit" class="floating-label">Area Unit </label>
                        </div>
                    </div>

                </div>
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div id="bighabis">
                            <div class="row mt-md-3">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input limit="4"  asp-for="TotalAreaInBigha" type="text" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" placeholder="Bigha" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                        <span asp-validation-for="TotalAreaInBigha" class="text-danger"></span>
                                        <label class="floating-label">Bigha</label>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input limit="2"  asp-for="TotalAreaInBiswa" type="text" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" placeholder="Biswa" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                        <span asp-validation-for="TotalAreaInBiswa" class="text-danger"></span>
                                        <label class="floating-label">Biswa</label>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">

                                        <input limit="2"  asp-for="TotalAreaInBiswani" type="text" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" placeholder="Biswani" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                        <span asp-validation-for="TotalAreaInBiswani" class="text-danger"></span>
                                        <label class="floating-label">Biswansi</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="AreainSqAcreHec" style="display:none;">
                    <div class="row mt-md-3">
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">
                                <input asp-for="TotalAreaInSqAcreHt" type="text" class="floating-label-field floating-label-field--s3 numbers TotalCalculation" placeholder="Total Area(Sq Yd./Acre/Hectare)" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span asp-validation-for="TotalAreaInSqAcreHt" class="text-danger"></span>
                                <label class="floating-label">Total Area(Sq Yd./Acre/Hectare)</label>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input asp-for="TotalArea" value="" class="floating-label-field floating-label-field--s3  numbers" placeholder="Total Area(in sq. mt.)" onkeypress="return numericDecimal(this);" onpaste="return false;" required="required" requiredmsg="Total Area is Mandatory">
                            <span asp-validation-for="TotalArea" class="text-danger"></span>
                            <label class="floating-label">Total Area(in sq. mt.)<span style="color:red">*</span></label>
                        </div>
                    </div>
                </div>

            </div>

            <div id="divEncroachment">
                <h2 class="my-3 sub-heading">Encroached Details</h2>
                <div class="row mt-md-3">
                    <div class="col-md-4">
                        <div class="floating-label-wrap">

                            <select class="floating-label-field floating-label-field--s3" asp-for="EncroachmentStatusId">
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <span asp-validation-for="EncroachmentStatusId" class="text-danger"></span>
                            <label class="floating-label">Encroachment</label>
                        </div>
                    </div>
                </div>
                <div id="divEncroachmentYesSelection" style="display:none;">
                    <div class="row pt-3">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <select class="floating-label-field floating-label-field--s3" asp-for="EncroachedPartiallyFully">
                                    <option value="0">Partially Encroached</option>
                                    <option value="1">Fully Encroached</option>
                                </select>
                                <span asp-validation-for="EncroachedPartiallyFully" class="text-danger"></span>
                                <label class="floating-label">Encroachment Status</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <input asp-for="EncrochedArea" value="" class="floating-label-field floating-label-field--s3 numbers" placeholder="Encroched Area(in sq. mt.)" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span id="EncroachedValidation" asp-validation-for="EncrochedArea" class="text-danger"></span>
                                <label class="floating-label">Encroached Area(in sq. mt.)</label>
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <input asp-for="BuiltUpEncraochmentArea" class="floating-label-field floating-label-field--s3 numbers" id="BuiltupEncroachmentArea" placeholder="Builtup in Encroachment Area(in sq. mt.)" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span asp-validation-for="BuiltUpEncraochmentArea" class="text-danger"></span>
                                <label class="floating-label">Builtup in Encroachment Area(in sq. mt.)</label>
                            </div>
                        </div>
                    </div>
                    <div class="row pt-3">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">

                                <input asp-for="Vacant" value="" class="floating-label-field floating-label-field--s3 numbers" placeholder="Vacant Area(in sq. mt.)" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                <span asp-validation-for="Vacant" class="text-danger"></span>
                                <label class="floating-label">Vacant Area(in sq. mt.)</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="ActionOnEncroachment" class="floating-label-field floating-label-field--s3" placeholder="Action On Encroachment" maxlength="200" />
                                <span asp-validation-for="ActionOnEncroachment" class="d-block text-danger field-validation-valid"></span>
                                <label class="floating-label">Action On Encroachment</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input type="file" class="floating-label-field floating-label-field--s3" placeholder="Upload Action Taken Report(ATR)" asp-for="EncroachAtrDoc" accept="application/pdf, application/vnd.ms-excel, application/vnd.ms-word, image/png, image/jpeg, image/gif" />
                                <span asp-validation-for="EncroachAtrDoc" class="text-danger"></span>
                                <label for="EncroachAtrDoc" class="floating-label">Upload Action Taken Report(ATR)</label>
                            </div>

                        </div>
                    </div>

                </div>
                <div class="row pt-3">
                    <div class="col-md-12" id="divEncroachmentDetails" style="display:none;">
                        <div class="floating-label-wrap">

                            <input asp-for="EncraochmentDetails" class="floating-label-field floating-label-field--s3" placeholder="Encroachment Details" maxlength="4000" limit="5000" />
                            <span asp-validation-for="EncraochmentDetails" class="text-danger lb-sm"></span>
                            <label id="l1" class="floating-label">Encroachment Details</label>
                        </div>
                    </div>

                </div>
            </div>

            <div id="divFencingBoundaryWall">
                <h2 class="my-3 sub-heading">Boundary Wall/Fencing</h2>
                <div class="row mt-md-3">
                    <div class="col-md-4">

                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="Boundary">

                                <option value="0">Boundary Wall</option>
                                <option value="1">Fencing</option>
                                <option value="2">None</option>
                            </select>
                            <span asp-validation-for="Boundary" class="text-danger"></span>
                            <label asp-for="Boundary" class="floating-label">Protection of Land </label>
                        </div>
                    </div>
                </div>
                <div id="divBoundarySelection" style="display:none;">
                    <div class="row pt-3">
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="BoundaryAreaCovered" type="text" class="floating-label-field floating-label-field--s3 numbers" />
                                <span asp-validation-for="BoundaryAreaCovered" class="text-danger "></span>
                                <label for="txtName" class="floating-label">Area Covered(Sq. mtr)</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="BoundaryDimension" type="text" class="floating-label-field floating-label-field--s3" placeholder="Dimension (L*B*H)(mtr)">

                                <span asp-validation-for="BoundaryDimension" class="text-danger "></span>
                                <label class="floating-label">Dimension (L*B*H)(mtr)</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="BoundaryRemarks" type="text" class="floating-label-field floating-label-field--s3" placeholder="Boundary Remarks">

                                <span asp-validation-for="BoundaryRemarks" class="text-danger "></span>
                                <label class="floating-label">Boundary Remarks</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div id="DivLandUse">
                <h2 class="my-3 sub-heading">Land Use</h2>
                <div id="divPlannedLand">


                    <div class="row mt-md-3">
                        <div class="col-md-4">
                            <div class="floating-label-wrap">
                                <select class="floating-label-field floating-label-field--s3" asp-for="MainLandUseId"
                                        asp-items="@(new SelectList(Model.LandUseList, "Id","Name"))">
                                    <option selected="selected" disabled="disabled" value="">--Select-- </option>
                                </select>
                                <label asp-validation-for="MainLandUseId" class="error"></label>
                                <label asp-for="MainLandUseId" class="floating-label">Main Land Use </label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="SubUse" type="text" class="floating-label-field floating-label-field--s3" limit="50" placeholder="Boundary Remarks">

                                <span asp-validation-for="SubUse" class="text-danger "></span>
                                <label class="floating-label">Sub Use</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div id="divLitigation">
                <h2 class="my-3 sub-heading">Litigation</h2>
                <div class="row mt-md-3">
                    <div class="col-md-4">

                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="LitigationStatus">

                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <span asp-validation-for="LitigationStatus" class="text-danger"></span>
                            <label asp-for="LitigationStatus" class="floating-label">Litigation Status </label>
                        </div>
                    </div>
                </div>

                <div id="divLitigationStatusSelection" style="display:none;">
                    <div class="row pt-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="CourtName" class="floating-label-field floating-label-field--s3" maxlength="50" placeholder="Court Name">

                                <span asp-validation-for="CourtName" class="text-danger "></span>
                                <label class="floating-label">Court Name</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="CaseNo" class="floating-label-field floating-label-field--s3" maxlength="50" placeholder="Case No.">

                                <span asp-validation-for="CaseNo" class="text-danger "></span>
                                <label class="floating-label">Case No.</label>
                            </div>
                        </div>
                    </div>
                    <div class="row pt-3">
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="OppositeParty" class="floating-label-field floating-label-field--s3" maxlength="100" placeholder="Opposite Party">

                                <span asp-validation-for="OppositeParty" class="text-danger "></span>
                                <label class="floating-label">Opposite Party</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="LitigationStatusRemarks" class="floating-label-field floating-label-field--s3" limit="5000" placeholder="Remarks">

                                <span asp-validation-for="LitigationStatusRemarks" class="text-danger "></span>
                                <label class="floating-label">Remarks</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="DivOtherDetails">
                <h2 class="my-3 sub-heading">Other Details</h2>

                <div class="row mt-md-3">
                    <div class="col-md-4">

                        <div class="floating-label-wrap">
                            <select class="floating-label-field floating-label-field--s3" asp-for="BuiltUp">

                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <span asp-validation-for="BuiltUp" class="text-danger"></span>
                            <label asp-for="BuiltUp" class="floating-label">Built-Up </label>
                        </div>
                    </div>
                    <div class="col-md-4" id="divBuilttype" style="display:none;">
                        <div class="floating-label-wrap mb-3 mb-md-0">

                            <input asp-for="BuiltUpRemarks" class="floating-label-field floating-label-field--s3" placeholder="Remarks">

                            <span asp-validation-for="BuiltUpRemarks" class="text-danger "></span>
                            <label class="floating-label">Built-Up Remarks</label>
                        </div>
                    </div>
                    <div class="form-group col-md-4" id="divLayoutPlan" style="display:none;">
