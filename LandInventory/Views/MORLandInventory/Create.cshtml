                                <label class="floating-label">Biswa</label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="floating-label-wrap mb-3 mb-md-0">

                                <input asp-for="TotalAreaInBiswani" type="text" id="txtName" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" placeholder="Biswani" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                <span asp-validation-for="TotalAreaInBiswani" class="text-danger"></span>
                                <label class="floating-label">Biswani</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
