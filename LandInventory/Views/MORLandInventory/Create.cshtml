                                <input asp-for="TotalAreaInBiswa" type="text" id="txtName" value="" class="floating-label-field floating-label-field--s3 onlynumbers bbbcalculation" placeholder="Biswa" onkeypress="return numericDecimal(this);" onpaste="return false;">
