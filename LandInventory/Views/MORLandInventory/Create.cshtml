                        <option value="3">Hectare</option>
                        <option value="4">Sq. Mt.</option>
                    </select>
                    <span asp-validation-for="AreaUnit" class="text-danger"></span>
                    <label asp-for="AreaUnit" class="floating-label">Area Unit </label>
                </div>
            </div>

        </div>
        <div class="row pt-3">
