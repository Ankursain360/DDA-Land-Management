@*@model Libraries.Model.Entity.Interest*@

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Property Registration</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>


                <div class="form-row">
                    <div class="form-group col-md-4">
                        <label class="control-label lb-sm">Classification of Land</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="0">DIT/Nazul-1 Land</option>
                            <option value="1">Acquired/Nazul-2 Land</option>
                            <option value="2">(MOR)Ministry of Rehabilitation Land</option>
                            <option value="3">Planned Land</option>
                            <option value="4">L&DO Land</option>
                            <option value="5">Damage Payee Properties Land</option>
                        </select>
                    </div>

                    <div class="form-group col-md-4">
                        <label class="control-label lb-sm">Unique ID</label>
                        <input type="text" class="form-control">
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label lb-sm">ZOne/Division</label>
                        <select style="height:30px; background-color: #dddddd;" id="Division" required="" class="form-control">
                            <option value="No">Finance</option>
                            <option value="Yes">Technical</option>
                        </select>

                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-4">
                        <label class="control-label lb-sm">Locality</label>
                        <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                            <option value="">Select</option>
                            <option value="1">Delhi</option>
                            <option value="2">Mumbai</option>
                            <option value="3">Chennai</option>
                        </select>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label lb-sm">Khasra No</label>
                        <input type="text" class="form-control">
                    </div>


                </div>



                <div id="DivArea">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Area (in sq. mt.)</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Boundary</label>
                            <select style="height:30px; background-color: #dddddd;" id="Boundary" class="form-control">
                                <option value="No">No</option>
                                <option value="Yes">Yes</option>
                            </select>

                        </div>
                        <div class="form-group col-md-4">
                            <label id="l1" class="control-label lb-sm">Boundary Remarks</label>
                            <input type="text" class="form-control" id="txtBoundaryRemarks" disabled>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Total Area</label>
                            <input id="TotalArea" type="text" class="form-control">
                        </div>


                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Encroched</label>
                            <input type="text" class="form-control">
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Vacant</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>

                <div id="DivLandUse">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Land Use</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Land Use</label>
                            <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                                <option value="">Select</option>
                                <option value="1">Main Use</option>
                                <option value="2">Sub Use</option>
                                <option value="2">Not Known</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div id="DivOtherDetails">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Other Details</h6>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Built-Up</label>
                            <select style="height:30px; background-color: #dddddd;" id="Buit" required="" class="form-control">
                                <option value="No">No</option>
                                <option value="Yes">Yes</option>
                            </select>
                            <div id="divBuilttype" style="display:none;">
                                <label class="control-label lb-sm">Remarks</label>
                                <input type="text" class="form-control" id="t2">
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Layout Plan</label>
                            <select style="height:30px; background-color: #dddddd;" id="Layout" required="" class="form-control">
                                <option value="No">No</option>
                                <option value="Yes">Yes</option>
                            </select>
                            <div id="divLayoutPlan" style="display:none;">
                                <label class="control-label lb-sm">Upload Layout File</label>
                                <input type="file" id="fileupload" name="file" multiple class="form-control" />
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Litigation Status</label>
                            <select style="height:30px; background-color: #dddddd;" id="Litigation" required="" class="form-control">
                                <option value="No">No</option>
                                <option value="Yes">Yes</option>
                            </select>
                            <div id="divLitigationStatus" style="display:none;">
                                <label class="control-label lb-sm">Remarks</label>
                                <input type="text" class="form-control" id="t4">
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">GEO Referencing Done</label>
                            <select style="height:30px; background-color: #dddddd;" id="GEO" required="" class="form-control">
                                <option value="No">No</option>
                                <option value="Yes">Yes</option>
                            </select>
                            <div id="divGEOReferencing" style="display:none;">
                                <label class="control-label lb-sm">Upload Geo Referencing File</label>
                                <input type="file" id="t5" name="file" multiple class="form-control" />
                            </div>
                        </div>

                    </div>
                </div>
                <div id="DivTakenOver">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Taken Over</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Name/Designation</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Date</label>
                            <input type="date" class="form-control">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Comments</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
                <div id="DivHandedOver">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Handed Over</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Name/Designation</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Date</label>
                            <input type="date" class="form-control">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Comments</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
                <div id="DivDisposal">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class=" myapp-hed2">
                                <div class="col-md-12">
                                    <h6 class="pull-left" style="color:white;margin-top:5px;">Disposal</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Type</label>
                            <select style="height:30px; background-color: #dddddd;" id="" required="" class="form-control">
                                <option value="">Select</option>
                                <option value="1">Alloted</option>
                                <option value="2">Leased</option>
                                <option value="2">Scheme</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Date</label>
                            <input type="Date" class="form-control">
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Comments</label>
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group col-md-12">
                        <label class="control-label lb-sm">Remarks</label>
                        <textarea type="text" class="form-control" style=" background-color: #dddddd;"></textarea>
                    </div>
                </div>



                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Create" class="btn btn-primary myapp-hed1" />
                        @*<input type="submit" value="Back" class="btn btn-primary myapp-hed1" />*@
                        @Html.ActionLink("Back", "Index", "PropertyRegistration", null, new { @class = "btn btn-primary myapp-hed1" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/propertyregisterationcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/propertyregisterationcreate.js" asp-append-version="true"></script>
</environment>

<environment include="Development">
    <script src="~/js/common.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/common.js" asp-append-version="true"></script>
</environment>



