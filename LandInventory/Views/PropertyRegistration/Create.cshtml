@model Libraries.Model.Entity.Propertyregistration

<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Property Registration</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />


<div class="container-fluid">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">Add Details</h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="row">
                    <div class="form-group col-md-4">
                        <label class="control-label" asp-for="ClassificationOfLandId">Classification of Land</label>
                        <select class="form-control dda-dropdown" asp-for="ClassificationOfLandId" asp-items="@(new SelectList(Model.ClassificationOfLandList, "Id","Name"))">
                            <option>--Select--</option>
                        </select>
                        <span asp-validation-for="ClassificationOfLandId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label">Unique ID</label>
                        <input asp-for="UniqueId" class="form-control">
                        <span asp-validation-for="UniqueId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label" asp-for="ZoneDivisionId">Zone/Division</label>
                        <select class="form-control dda-dropdown" asp-for="ZoneDivisionId" asp-items="@(new SelectList(Model.ZoneList, "Id","Name"))">
                            <option>--Select--</option>
                        </select>
                        <span asp-validation-for="ZoneDivisionId" class="text-danger"></span>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-4">
                        <label class="control-label" asp-for="LocalityId">Locality</label>
                        <select class="form-control dda-dropdown" asp-for="LocalityId" asp-items="@(new SelectList(Model.LocalityList, "Id","Name"))">
                            <option>--Select--</option>
                        </select>
                        <span asp-validation-for="LocalityId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label">Khasra No</label>
                        <input asp-for="KhasraNo"  class="form-control">
                        <span asp-validation-for="KhasraNo" class="text-danger"></span>
                    </div>
                </div>
                
                <div id="DivArea">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Area (in sq. mt.)</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label">Boundary</label>
                            <select class="form-control dda-dropdown" asp-for="Boundary">
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <span asp-validation-for="Boundary" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label id="l1" class="control-label">Boundary Remarks</label>
                            <input asp-for="BoundaryRemarks"  class="form-control" disabled>
                            <span asp-validation-for="BoundaryRemarks" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Total Area</label>
                            <input asp-for="TotalArea" value="" class="form-control">
                            <span asp-validation-for="TotalArea" class="text-danger"></span>
                        </div>


                    </div>

                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label">Encroched</label>
                            <input asp-for="Encroached" value="" class="form-control">
                            <span asp-validation-for="Encroached" class="text-danger"></span>
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label">Vacant</label>
                            <input asp-for="Vacant" value="" class="form-control">
                            <span asp-validation-for="Vacant" class="text-danger"></span>
                        </div>
                    </div>
                </div>

                <div id="DivLandUse">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Land Use</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        
                        <div class="form-group col-md-4">
                            <label class="control-label" asp-for="LandUseId">Land Use</label>
                            <select class="form-control dda-dropdown" asp-for="LandUseId" asp-items="@(new SelectList(Model.LandUseList, "Id","Name"))">
                                <option>--Select--</option>
                            </select>
                            <span asp-validation-for="LandUseId" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div id="DivOtherDetails">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Other Details</h6>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Built-Up</label>
                            <select class="form-control dda-dropdown" asp-for="BuiltUp" id="Buit">
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <div id="divBuilttype" style="display:none;">
                                <label class="control-label">Built-Up Remarks</label>
                                <input asp-for="BuiltUpRemarks" class="form-control">
                                <span asp-validation-for="BuiltUpRemarks" class="text-danger"></span>
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Layout Plan</label>
                            <select class="form-control dda-dropdown" asp-for="LayoutPlan" id="Layout">
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <div id="divLayoutPlan" style="display:none;">
                                <label class="control-label">Upload Layout File</label>
                                <input asp-for="LayoutPlan" class="form-control">
                                <span asp-validation-for="LayoutPlan" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">Litigation Status</label>
                            <select  id="Litigation" class="form-control dda-dropdown" asp-for="LitigationStatus" >
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <div id="divLitigationStatus" style="display:none;">
                                <label class="control-label">Remarks</label>
                                <input asp-for="LitigationStatusRemarks" class="form-control">
                                <span asp-validation-for="LitigationStatusRemarks" class="text-danger"></span>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label lb-sm">GEO Referencing Done</label>
                            <select class="form-control dda-dropdown" asp-for="GeoReferencing" id="GEO" >
                                <option value="0">No</option>
                                <option value="1">Yes</option>
                            </select>
                            <div id="divGEOReferencing" style="display:none;">
                                <label class="control-label lb-sm">Upload Geo Referencing File</label>
                                <input asp-for="GeoReferencing" type="file"  multiple class="form-control" />
                                <span asp-validation-for="GeoReferencing" class="text-danger"></span>
                            </div>
                        </div>

                    </div>
                </div>
                <div id="DivTakenOver">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Taken Over</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label">Name/Designation</label>
                            <input asp-for="TakenOverName" class="form-control">
                            <span asp-validation-for="TakenOverName" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Date</label>
                            <input asp-for="TakenOverDate" type="date" value="" class="form-control">
                            <span asp-validation-for="TakenOverDate" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Comments</label>
                            <input asp-for="TakenOverComments" class="form-control">
                            <span asp-validation-for="TakenOverComments" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div id="DivHandedOver">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Handed Over</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label">Name/Designation</label>
                            <input asp-for="HandedOverName" class="form-control">
                            <span asp-validation-for="HandedOverName" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Date</label>
                            <input asp-for="HandedOverDate" type="date" value="" class="form-control">
                            <span asp-validation-for="HandedOverDate" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label">Comments</label>
                            <input asp-for="HandedOverComments" class="form-control">
                            <span asp-validation-for="HandedOverComments" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div id="DivDisposal">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Disposal</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label class="control-label" asp-for="DisposalTypeId">Type</label>
                            <select class="form-control dda-dropdown" asp-for="DisposalTypeId" asp-items="@(new SelectList(Model.DisposalTypeList, "Id","Name"))">
                                <option>--Select--</option>
                            </select>
                            <span asp-validation-for="DisposalTypeId" class="text-danger"></span>
                        </div>
                            <div class="form-group col-md-4">
                                <label class="control-label">Date</label>
                                <input asp-for="DisposalDate" type="date" value="" class="form-control">
                                <span asp-validation-for="DisposalDate" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label">Comments</label>
                                <input asp-for="DisposalComments" class="form-control">
                                <span asp-validation-for="DisposalComments" class="text-danger"></span>
                            </div>
                        </div>
                    </div>

                <div class="row">
                    <div class="form-group col-md-12">
                        <label class="control-label">Remarks</label>
                        <textarea asp-for="Remarks" class="form-control" style=" background-color: #dddddd;"></textarea>
                    </div>
                </div>



                <div class="form-row ">
                    <div class="form-group col-md-12" style="margin-bottom:40px;">
                        <input type="submit" value="Create" class="btn btn-primary dda-pageheading" />
                        @Html.ActionLink("Back", "Index", "PropertyRegistration", null, new { @class = "btn btn-primary dda-pageheading" })
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>


<partial name="../Shared/_ValidationScriptsPartial.cshtml" />
<environment include="Development">
    <script src="~/js/propertyregisterationcreate.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/propertyregisterationcreate.js" asp-append-version="true"></script>
</environment>

<environment include="Development">
    <script src="~/js/common.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/common.js" asp-append-version="true"></script>
</environment>



