@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header card-header-primary">
                        <div class="row">
                            <div class="col-md-5">
                                <h4 class="card-title">Property Registration </h4>
                                <p class="card-title">Delete</p>
                            </div>
                            <div class="col-md-7">
                                <a asp-action="Index" class="btn btn-default pull-right" style="margin-top: -5px;">Back</a>
