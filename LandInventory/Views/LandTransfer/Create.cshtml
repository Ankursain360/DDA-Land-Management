                    <div id="divEncroachment">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Encroached Details</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Encroachment Status</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.EncroachmentStatusId">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                                <span asp-validation-for="Propertyregistration.EncroachmentStatusId" class="text-danger lb-sm"></span>
                            </div>
                            <div id="divEncroachmentYesSelection" style="display:none;">
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Land</label>
                                    <select class="form-control dda-dropdown" asp-for="Propertyregistration.EncroachmentStatusId">
                                        <option value="0">Vacant</option>
                                        <option value="1">Partially Vacant</option>
                                    </select>
                                    <span asp-validation-for="Propertyregistration.EncroachmentStatusId" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Encroched Area(in sq. mt.)</label>
                                    <input asp-for="Propertyregistration.Encroached" class="form-control numbers" onchange="return ComapareTotalArea(this);" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                    <span id="EncroachedValidation" asp-validation-for="Propertyregistration.Encroached" class="text-danger lb-sm"></span>
                                </div>

                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Builtup in Encroachment Area(in sq. mt.)</label>
                                    <input asp-for="Propertyregistration.BuiltUpEncraochmentArea" class="form-control numbers" id="BuiltupEncroachmentArea" onchange="return ComapareTotalArea(this);" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                    <span asp-validation-for="Propertyregistration.BuiltUpEncraochmentArea" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Vacant(in sq. mt.)</label>
                                    <input asp-for="Propertyregistration.Vacant" class="form-control numbers" onkeypress="return numericDecimal(this);" onpaste="return false;" onchange="return ComapareTotalArea(this);" />
                                    <span asp-validation-for="Propertyregistration.Vacant" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Action On Encroachment</label>
                                    <textarea type="text" class="form-control" asp-for="PropertyRegistration.ActionOnEncroachment"></textarea>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Upload Action Taken Report(ATR)</label>
                                    <input type="file" class="form-control" id="txtTakenOverDate" />
                                </div>

                            </div>

                        </div>
                        <div class="row">
                            <div class="form-group col-md-12" id="divEncroachmentDetails" style="display:none;">
                                <label id="l1" class="control-label lb-sm">Encroachment Details</label>
                                <input asp-for="Propertyregistration.EncraochmentDetails" class="form-control" maxlength="4000" limit="4000" />
                                <span asp-validation-for="Propertyregistration.EncraochmentDetails" class="text-danger lb-sm"></span>
                            </div>
                        </div>

                    </div>

                    <div id="divFencingBoundaryWall">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Fencing/Boundary Wall </h6>
                                        <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Boundary Wall/Fencing</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.Boundary">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                                <span asp-validation-for="Propertyregistration.Boundary" class="text-danger lb-sm"></span>
                            </div>
                            <div id="divBoundarySelection" style="display:none;">
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Area Covered(Sq. mtr)</label>
                                    <input type="text" class="form-control" id="txtTakenOverDate" />
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Dimension (L*B*H)(mtr)</label>
                                    <input type="text" class="form-control" id="txtTakenOverDate" />
                                </div>
                                <div class="col-md-4">
                                    <label id="l1" class="control-label lb-sm">Boundary Remarks</label>
                                    <input asp-for="Propertyregistration.BoundaryRemarks" class="form-control" />
                                    <span asp-validation-for="Propertyregistration.BoundaryRemarks" class="text-danger lb-sm"></span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="DivLandUse">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Land Use</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div id="divPlannedLand">
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Main Land Use</label>
                                    <select class="form-control dda-dropdown" asp-for="Propertyregistration.MainLandUseId" asp-items="@(new SelectList(Model.Propertyregistration.LandUseList, "Id","Name"))">
                                        <option value="0">--Select--</option>
                                    </select>
                                    <span asp-validation-for="Propertyregistration.MainLandUseId" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Sub Use</label>
                                    <input type="text" asp-for="Propertyregistration.SubUse" class="form-control" limit="50" />
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="divLitigation">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Litigation</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Litigation Status</label><span class="text-danger">*</span>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.LitigationStatus">
                                    <option value="0">No</option>
                                    <option value="1">Yes</option>
                                </select>
                            </div>
                            <div id="divLitigationStatusSelection" style="display:none;">
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Court Name</label>
                                    <input asp-for="Propertyregistration.LitigationStatusRemarks" class="form-control" />
                                    <span asp-validation-for="Propertyregistration.LitigationStatusRemarks" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Case No.</label>
                                    <input asp-for="Propertyregistration.LitigationStatusRemarks" class="form-control" />
                                    <span asp-validation-for="Propertyregistration.LitigationStatusRemarks" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Opposite Party</label>
                                    <input asp-for="Propertyregistration.LitigationStatusRemarks" class="form-control" />
                                    <span asp-validation-for="Propertyregistration.LitigationStatusRemarks" class="text-danger lb-sm"></span>
                                </div>
                                <div class="col-md-4">
                                    <label class="control-label lb-sm">Remarks</label>
                                    <input asp-for="Propertyregistration.LitigationStatusRemarks" class="form-control" limit="4000" />
                                    <span asp-validation-for="Propertyregistration.LitigationStatusRemarks" class="text-danger lb-sm"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="DivOtherDetails">
                        <div class="row" style="margin-bottom:35px;">
                            <div class="col-sm-12">
                                <div class="dda-innerheading">
                                    <div class="col-md-12">
                                        <h6 class="pull-left color-white" style="margin-top:5px;">Other Details</h6>
