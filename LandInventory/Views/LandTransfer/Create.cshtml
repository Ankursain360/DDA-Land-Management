@model Libraries.Model.Entity.Landtransfer
@{int rowNo = 0;}
<div class="row">
    <div class="col-sm-12">
        <div class="dda-topheading">
            <div class="col-md-12">
                <h6 class="pull-left color-white" style="margin-top:5px;">Hand Over/Take Over</h6>
                <i class="glyphicon glyphicon-arrow-right pull-right" aria-hidden="true" style="padding-top:4px;"></i>
            </div>
        </div>
    </div>
</div>
<br />
<br />
<div class="container-fluid" id="MainContentForm">
    <div class="panel">
        <div class="panel-heading  dda-pageheading">
            <h6 class="pull-left color-white" style="margin-top:-0.4%">
                Add Details <span class="ms-auto"><span style="color:red">*&nbsp;</span>Marked are mandatory fields</span>

            </h6>
        </div>
        <div class="panel-body">
            <form asp-action="Create" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <a data-bs-toggle="collapse" href="#collapseExample" id="collapse" role="button" aria-expanded="false" aria-controls="collapseExample">
                    <div class="row" style="margin-bottom:35px;">
                        <div class="col-sm-12">
                            <div class="dda-innerheading">
                                <div class="col-md-12">
                                    <h6 class="pull-left color-white" style="margin-top:5px;">Property Registration Details </h6>
                                    <i class="fa-angle-right fa pull-right" aria-hidden="true" style="padding-top:4px;color:white"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
                <div class="collapse" id="collapseExample">

                    <div class="row">
                        <div class="col-md-4">
                            <label class="control-label lb-sm">Inventoried In</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.InventoriedInId">
                                <option value="">--Select--</option>
                                <option value="1">VLMS</option>
                                <option value="2">Used/Allocated</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.ClassificationOfLandId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.PlannedUnplannedLand">Planned/Unplanned Land</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.PlannedUnplannedLand">
                                <option value="Planned Land">Planned Land</option>
                                <option value="Unplanned Land">Unplanned Land</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.PlannedUnplannedLand" class="text-danger lb-sm"></span>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.ClassificationOfLandId">Classification of Land</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.ClassificationOfLandId" asp-items="@(new SelectList(Model.Propertyregistration.ClassificationOfLandList, "Id","Name"))" required="required" requiredmsg="Classification of Land is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.ClassificationOfLandId" class="text-danger lb-sm"></span>
                        </div>

                        <div class="col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.DepartmentId">Department</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.DepartmentId" asp-items="@(new SelectList(Model.DepartmentList, "Id","Name"))" onchange="return GetZoneList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Department is Mandatory">
                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.DepartmentId" class="text-danger lb-sm"></span>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.ZoneId">Zone</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.ZoneId" onchange="return GetDivisionList(this.options[this.selectedIndex].value);" required="required" requiredmsg="Zone is Mandatory">

                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.ZoneId" class="text-danger lb-sm"></span>
                        </div>

                        <div class="col-md-4">
                            <label class="control-label lb-sm" asp-for="Propertyregistration.DivisionId">Division</label><span class="text-danger">*</span>
                            <select class="form-control dda-dropdown" asp-for="Propertyregistration.DivisionId" required="required" requiredmsg="Division is Mandatory">

                                <option value="">--Select--</option>
                            </select>
                            <span asp-validation-for="Propertyregistration.DivisionId" class="text-danger lb-sm"></span>
                        </div>
                        <div id="divUnplannedSelection" style="display:none;">
                            <div class="col-md-4">
                                <label class="control-label lb-sm" asp-for="Propertyregistration.LocalityId">Locality(Village)</label>
                                <select class="form-control dda-dropdown" asp-for="Propertyregistration.LocalityId">

                                    <option value="">--Select--</option>
                                </select>
                                <span asp-validation-for="Propertyregistration.LocalityId" class="text-danger lb-sm"></span>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Khasra No</label>
                                <input asp-for="Propertyregistration.KhasraNo" class="form-control" limit="30" />
                                <span asp-validation-for="Propertyregistration.KhasraNo" class="text-danger lb-sm"></span>
                            </div>
                        </div>
                        <div id="divPlannedSelection">
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Colony</label>
                                <input type="text" class="form-control" asp-for="PropertyRegistration.Colony" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Sector</label>
                                <input asp-for="Propertyregistration.KhasraNo" class="form-control" />
                                <span asp-validation-for="Propertyregistration.KhasraNo" class="text-danger lb-sm"></span>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Block</label>
                                <input type="text" class="form-control" asp-for="PropertyRegistration.Block" />
                                <span class="text-danger lb-sm"></span>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Pocket</label>
                                <input asp-for="Propertyregistration.KhasraNo" class="form-control" />
                                <span asp-validation-for="Propertyregistration.KhasraNo" class="text-danger lb-sm"></span>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label lb-sm">Plot No.</label>
                                <input asp-for="Propertyregistration.KhasraNo" class="form-control" />
                                <span asp-validation-for="Propertyregistration.KhasraNo" class="text-danger lb-sm"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label lb-sm">Primary List No.</label>
                            <input asp-for="Propertyregistration.PrimaryListNo" class="form-control" limit="20" />
                            <span asp-validation-for="Propertyregistration.PrimaryListNo" class="text-danger lb-sm"></span>
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-group col-md-12">
                            <label class="control-label lb-sm">Address with Landmark</label>
                            <textarea asp-for="Propertyregistration.Palandmark" class="form-control" maxlength="4000" limit="4000"></textarea>
                            <div id="feedbackPalandmark"></div>
                        </div>
                    </div>

                    <div id="DivArea">
                        <h2 class="my-3 sub-heading">Area</h2>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="floating-label-wrap">

                                    <select class="floating-label-field floating-label-field--s3" disabled asp-for="Propertyregistration.AreaUnit">
                                        <option value="0">Bigha-Biswa-Biswansi</option>
                                        <option value="1">Sq Yd.</option>
                                        <option value="2">Acre</option>
                                        <option value="3">Hectare</option>
                                        <option value="4">Sq. Mt.</option>
                                    </select>
                                    <span asp-validation-for="Propertyregistration.AreaUnit" class="text-danger"></span>
                                    <label class="floating-label">Area Unit</label>
                                </div>
                            </div>
                        </div>
                        <div class="row pt-3">
                            <div class="col-md-4">
                                <div id="bighabis">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="floating-label-wrap mb-3 mb-md-0">

                                                <input asp-for="Propertyregistration.TotalAreaInBigha" disabled class="floating-label-field floating-label-field--s3 numbers bbbcalculation" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                <span asp-validation-for="Propertyregistration.TotalAreaInBigha" class="text-danger"></span>
                                                <label class="floating-label">Bigha</label>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="floating-label-wrap mb-3 mb-md-0">

                                                <input asp-for="Propertyregistration.TotalAreaInBiswa" disabled class="floating-label-field floating-label-field--s3 numbers bbbcalculation" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                <span asp-validation-for="Propertyregistration.TotalAreaInBiswa" class="text-danger"></span>
                                                <label class="floating-label">Biswa</label>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="floating-label-wrap mb-3 mb-md-0">

                                                <input asp-for="Propertyregistration.TotalAreaInBiswani" disabled class="floating-label-field floating-label-field--s3 numbers bbbcalculation" onkeypress="return numericDecimal(this);" onpaste="return false;">
                                                <span asp-validation-for="Propertyregistration.TotalAreaInBiswani" class="text-danger"></span>
                                                <label class="floating-label">Biswansi</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div id="AreainSqAcreHec" style="display:none;">
                            <div class="row pt-3">
                                <div class="col-md-4">
                                    <div class="floating-label-wrap mb-3 mb-md-0">
                                        <input asp-for="Propertyregistration.TotalAreaInSqAcreHt" type="text" disabled class="floating-label-field floating-label-field--s3 numbers TotalCalculation" onkeypress="return numericDecimal(this);" onpaste="return false;" />
                                        <span asp-validation-for="Propertyregistration.TotalAreaInSqAcreHt" class="text-danger"></span>
                                        <label class="floating-label" id="LabelTotalAreaSqAcreHec">Total Area(Sq Yd./Acre/Hectare)</label>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="row mt-md-3">
                            <div class="col-md-4">
                                <div class="floating-label-wrap mb-3 mb-md-0">
                                    <input asp-for="Propertyregistration.TotalArea" class="floating-label-field floating-label-field--s3 numbers" disabled onkeypress="return numericDecimal(this);" onpaste="return false;" required="required" requiredmsg="Total Area is Mandatory">
                                    <span asp-validation-for="Propertyregistration.TotalArea" class="text-danger"></span>
                                    <label class="floating-label">Total Area(in sq. mt.)</label>
                                </div>
                            </div>
                        </div>
                    </div>
